@干渉強化関数_1(ARGS, キャラ番号)
#DIM キャラ番号

TSTR:コマンド名受渡 = 
SELECTCASE ARGS
	CASE "名前"
		TSTR:コマンド名受渡 = 干涉・友情增幅
	CASE "詳細文章"
		TSTR:コマンド名受渡 = 提升好感度要素「友情」增长量的干涉。<br>
		TSTR:コマンド名受渡 += "使其更容易对作为艾拉契约者的你产生友谊之情。<br>"
		TSTR:コマンド名受渡 += "以下两种效果取较高者生效。<br>"
		TSTR:コマンド名受渡 += "・好感度要素「友情」增加量 ＋［干涉力等级✕２０］％<br>"
		TSTR:コマンド名受渡 += "・好感度要素「友情」增加量 ＋［（干涉力等级＋２）✕１０］<br>"
		TSTR:コマンド名受渡 += "<br>"
		TSTR:コマンド名受渡 += "经验值获取时机：友情度提升时<br>"
	CASE "値段"
		RETURN 1
	CASE "適用条件"
		SIF 干渉強化変数:キャラ番号:干渉種類 != 1
			RETURN 0
		RETURN NOWEX:キャラ番号:友情度上昇格納
	CASE "実補正処理"
		IF NOWEX:キャラ番号:友情度上昇格納
			LOCAL = MAX(NOWEX:キャラ番号:友情度上昇格納 * (干渉強化変数:キャラ番号:干渉ランク) / 5, (干渉強化変数:キャラ番号:干渉ランク + 2) * 10)
			NOWEX:キャラ番号:友情度上昇格納 += LOCAL
			干渉強化変数:キャラ番号:干渉経験値 += 1
		ENDIF
	CASE "対象値"
		TSTR:コマンド名受渡 = 友情度：{MIN(CFLAG:キャラ番号:友情度,100000),6}
ENDSELECT

