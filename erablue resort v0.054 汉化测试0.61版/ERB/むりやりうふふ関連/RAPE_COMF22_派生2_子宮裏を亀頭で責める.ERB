
@RAPE_COMNAME_S22_2
#FUNCTIONS
TSTR:コマンド名受渡 = 龟头刺激子宫后壁
;ペニバンにも亀頭はあるんだしまぁこれでいいかなって

;===================================================
;コマンドタイプ
;===================================================
@RAPE_COMTYPE_S_22_2
#FUNCTION
TFLAG:コマンドタイプ受渡 = RAPE_COMTYPE("Ａ系")


@RAPE_COM_TOOLTIP_S22_2
LOCALS = <br>■龟头刺激子宫后壁<br>
LOCALS += "从肠道方向用龟头强烈刺激子宫后壁的指令。<br>仅在Ａ插入系模式中可执行。"
LOCALS += "<br>强奸指令类型：Ａ系<br>获取来源：快Ａ・快Ｖ・接触・痛苦"
LOCALS += "<br>专属经验值：Ａ性交经验(TARGET)・子宫口开发(TARGET)・Ｃ性交经验_Ａ插入(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@RAPE_COM_S22_2
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------





RCVAR:ポルチオ開発フラグ = 1
CALL SOURCE_CALC_快Ａ(TARGET, PLAYER, 500)
CALL SOURCE_CALC_快Ｖ(TARGET, PLAYER, 200)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)
CALL SOURCE_CALC_快Ｃ(PLAYER, TARGET, 500)


;苦痛
LOCAL = 500 * PAIN_CHECK_A(TARGET) / 1000
CALL SOURCE_CALC_苦痛(TARGET, PLAYER, LOCAL)


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:PLAYER:Ｃ性交経験_Ａ挿入 ++
EXP:Ａ性交経験 ++

;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:同性愛経験 += 4
	EXP:PLAYER:同性愛経験 += 4
ENDIF

EXP:愛情経験 += TALENT:恋慕 * 2

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@RAPE_COM_ABLE_S22_2
;子宮裏を責める実行判定
;一括管理
SIF GLOBAL_RAPE_COMABLE_S("22_2")
	RETURN RESULT
;Ａ挿入系モードじゃない時はアウト
SIF MODE_確認_完全一致("Ａ挿入系", PLAYER, TARGET) < 0
	RETURN 0
;Vがない場合アウト
SIF TALENT:TARGET:Ｖ感度 == -2
	RETURN 0
SIF GETBIT(TALENT:TARGET:性別, 0) == 0
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@RAPE_MESSAGE_COM_S22_2


PRINTFORML %CALLNAME:PLAYER%通过直肠%TEXTR("用龟头摩擦/用龟头撞击")%刺激%CALLNAME:TARGET%的子宫后壁…


