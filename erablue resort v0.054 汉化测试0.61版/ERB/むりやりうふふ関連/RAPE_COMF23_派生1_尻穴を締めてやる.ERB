
@RAPE_COMNAME_S23_1
#FUNCTIONS
TSTR:コマンド名受渡 = 收紧菊穴

;===================================================
;コマンドタイプ
;===================================================
@RAPE_COMTYPE_S_23_1
#FUNCTION
TFLAG:コマンドタイプ受渡 = RAPE_COMTYPE("Ａ系_被挿入中")

@RAPE_COM_TOOLTIP_S23_1
LOCALS = <br>■收紧菊穴<br>
LOCALS += "收缩括约肌对插入的阴茎施加强烈刺激的指令。"
LOCALS += "<br>强奸指令类型：Ａ系or被插入中<br>获取来源：快Ｃ・暴露・接触"
LOCALS += "<br>专属经验值：Ｃ性交经验_Ａ插入(TARGET)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@RAPE_COM_S23_1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------

CALL SOURCE_CALC_快Ａ(PLAYER, TARGET, 600)
CALL SOURCE_CALC_露出(TARGET, PLAYER, 50)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)
CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 600)

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｃ性交経験_Ａ挿入 ++
EXP:PLAYER:Ａ性交経験 ++
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:同性愛経験 += 4
	EXP:PLAYER:同性愛経験 += 4
ENDIF

EXP:愛情経験 += TALENT:恋慕 * 2

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@RAPE_COM_ABLE_S23_1
;腸内を締める実行判定
;一括管理
SIF GLOBAL_RAPE_COMABLE_S("23_1")
	RETURN RESULT
;挿入系モードじゃない時はアウト
SIF MODE_確認_完全一致("Ａ挿入系", TARGET, PLAYER) < 0
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@RAPE_MESSAGE_COM_S23_1

PRINTFORML %CALLNAME:PLAYER%绷紧腹部，用%CALLNAME:TARGET%的阴茎%TEXTR("摩擦肠壁/挤压括约肌")%
CALL PRINT_STR(@"_H_")
PRINTFORML 发出%TEXTR("嗯唔/嗯啊")%的娇喘用力收缩…

