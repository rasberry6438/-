@系統リスト_Ａ自慰系(モード名)
#FUNCTION
#DIMS モード名
IF GROUPMATCH(モード名, "アナニー")
	モード確認結果 = 1
	RETURNF 1
ENDIF


@MODE_絶頂相手_Ａ自慰系(絶頂部位, 処理モード, 判定結果)
#FUNCTION
#DIM 絶頂部位
#DIMS 処理モード
#DIM REF 判定結果

IF 処理モード == "ターゲット"
	IF 絶頂部位 == 部位_Ａ
		判定結果 = 絶頂相手_自分
		RETURNF 絶頂相手_自分
	ENDIF
ENDIF


;----------------------------------------------------------------------------
;アナニー
;----------------------------------------------------------------------------
@MODE_持続快楽_アナニー(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号
#DIM 快楽基礎値
#DIM 詳細設定補正

RCVAR:対象キャラ番号:露出コマンドフラグ = 1
CALL SOURCE_CALC_快Ａ(対象キャラ番号, -1, 100)
SIF TALENT:対象キャラ番号:器用な指
	CALL SOURCE_CALC_快Ａ(対象キャラ番号, -1, 50)

RETURN 1


@MODETYPE_アナニー(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;ここは来ないはず
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = 用手指%TEXTR("玩弄/抠挖")%自己的%TEXTR("肛门/后庭/菊穴")%，沉溺于快感
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;自慰のためプレイヤー側は一切場所を専有しない
SELECTCASE 部位番号
	CASE 部位_アナル
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
	CASE 部位_指
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
ENDSELECT


@MODE_絶頂相手_アナニー(絶頂部位, 処理モード, 判定結果)
#FUNCTION
#DIM 絶頂部位
#DIMS 処理モード
#DIM REF 判定結果
RETURNF MODE_絶頂相手_Ａ自慰系(絶頂部位, 処理モード, 判定結果)


@MESSAGE_MODE_アナニー解除(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

PRINTFORML %CALLNAME:対象キャラ番号%将手指从自己肛门中抽离…
