@MODE_持続快楽_足コキ(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

CALL SOURCE_CALC_快Ｃ(対象キャラ番号, 実行キャラ番号, MIN(ABL:実行キャラ番号:技巧 * 25, 100))
CALL SOURCE_CALC_征服(実行キャラ番号, 対象キャラ番号, 90)
CALL SOURCE_CALC_接触(実行キャラ番号, 対象キャラ番号, 50)
CALL SOURCE_CALC_接触(対象キャラ番号, 実行キャラ番号, 50)
CALL SOURCE_CALC_奉仕(実行キャラ番号, 対象キャラ番号, 20)
;奉仕快楽
RCVAR:実行キャラ番号:奉仕コマンドフラグ = 1
;サドは嗜虐快楽
SIF ABL:実行キャラ番号:サド性感
	EXP:実行キャラ番号:嗜虐快楽経験 ++

RETURN 1


@MODETYPE_足コキ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	IF ABL:プレイヤー番号:サド性感 > 2
		詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%的%TEXTR("阴茎/肉棒")%被%TEXTR("用双腿夹住玩弄/用脚趾挑逗/用脚踩住")%
	ELSE
		詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%的%TEXTR("阴茎/肉棒")%被双腿夹住玩弄
	ENDIF
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	SELECTCASE (ABL:プレイヤー番号:サド性感 > 2) ? RAND:3 # 0
	CASE 0
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的双腿间正玩弄着%TEXTR("阴茎/阳具/肉棒")%
	CASE 1
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的脚趾正挑逗着%TEXTR("阴茎/阳具/肉棒")%
	CASE 2
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的脚正踩着%TEXTR("阴茎/阳具/肉棒")%
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
IF 処理モード == "ターゲット" && 部位番号 == 部位_ペニス
	;ターゲットのペニスを占有
	RETURN 部位状況_占有
ELSE
	;他は全て届かない
	RETURN 部位状況_届かない
ENDIF


@MODE_絶頂相手_足コキ(絶頂部位, 処理モード, 判定結果)
#FUNCTION
#DIM 絶頂部位
#DIMS 処理モード
#DIM REF 判定結果

IF 処理モード == "ターゲット"
	IF 絶頂部位 == 部位_Ｃ
		判定結果 = 絶頂相手_モード相手
		RETURNF 絶頂相手_モード相手
	ENDIF
ENDIF


@MESSAGE_MODE_足コキ解除(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

IF 対象キャラ番号 == MASTER
	PRINTFORML %CALLNAME:対象キャラ番号%制止了%CALLNAME:実行キャラ番号%，让其将脚从阴茎上移开
ELSE
	PRINTFORML %CALLNAME:実行キャラ番号%将脚从%CALLNAME:対象キャラ番号%的阴茎上移开
ENDIF



