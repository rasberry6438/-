@MODE_持続快楽_首絞め(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

IF ABL:実行キャラ番号:技巧 < 3
	;技巧がないと苦しいだけ
	CALL SOURCE_CALC_苦痛(対象キャラ番号, 実行キャラ番号, 1000)
ELSEIF ABL:対象キャラ番号:マゾ性感 < 3 && ABL:実行キャラ番号:技巧 < 7
	CALL SOURCE_CALC_苦痛(対象キャラ番号, 実行キャラ番号, 50 * MAX(4, 14 - ABL:実行キャラ番号:技巧))
ELSE
	;上手い人だとあまり苦しくないらしいよ
	CALL SOURCE_CALC_苦痛(対象キャラ番号, 実行キャラ番号, 50 * MAX(4, 14 - ABL:実行キャラ番号:技巧))
	CALL SOURCE_CALC_快Ｓ(対象キャラ番号, 実行キャラ番号, 50 * MAX(2, ABL:実行キャラ番号:技巧 - 4))
ENDIF
;苦痛快楽
RCVAR:対象キャラ番号:苦痛コマンドフラグ = 1
RCVAR:実行キャラ番号:嗜虐コマンドフラグ = 1
;喘ぎを減らす
RCVAR:対象キャラ番号:汎用喘ぎ_文節低減フラグ = 1

RETURN 1


@MODETYPE_首絞め(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	;詳細文文字列受け渡し変数 = 
	;基本あなた専用なのでここに来ることはないはず
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的手正掐住脖子
	SELECTCASE 性癖素質:ターゲット番号:首絞め
		CASE 3
			;中毒
			詳細文文字列受け渡し変数 += @"露出%TEXTR("阿嘿颜/恍惚表情/陶醉神情")%"
		CASE 2
			;愛好
			詳細文文字列受け渡し変数 += "彻底沦为快感的俘虏"
		CASEELSE
			;それ以外
			IF ABL:ターゲット番号:マゾ性感 > 2
				詳細文文字列受け渡し変数 += "从掐脖子本身获得快感"
			ELSE
				詳細文文字列受け渡し変数 += "浮现痛苦的表情"
			ENDIF
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
SELECTCASE 部位番号
	CASE 部位_指
		;実行者は両手を使わなければならない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_強占有
ENDSELECT


