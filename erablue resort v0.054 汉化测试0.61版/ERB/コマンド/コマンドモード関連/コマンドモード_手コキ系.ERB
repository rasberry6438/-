@系統リスト_手コキ系(モード名)
#FUNCTION
#DIMS モード名
IF GROUPMATCH(モード名, "手コキ", "授乳手コキ")
	モード確認結果 = 1
	RETURNF 1
ENDIF



@MODE_持続快楽_手コキ(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

CALL SOURCE_CALC_快Ｃ(対象キャラ番号, 実行キャラ番号, 100)
CALL SOURCE_CALC_接触(実行キャラ番号, 対象キャラ番号, 70)
CALL SOURCE_CALC_接触(対象キャラ番号, 実行キャラ番号, 80)
CALL SOURCE_CALC_奉仕(実行キャラ番号, 対象キャラ番号, 90)
EXP:実行キャラ番号:手淫経験 ++
;奉仕快楽
RCVAR:実行キャラ番号:奉仕コマンドフラグ = 1

RETURN 1


@MODE_持続快楽_授乳手コキ(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

RCVAR:実行キャラ番号:乳首開発フラグ = 2
CALL SOURCE_CALC_快Ｃ(対象キャラ番号, 実行キャラ番号, 100)
CALL SOURCE_CALC_快Ｂ(実行キャラ番号, 対象キャラ番号, 100)
CALL SOURCE_CALC_接触(実行キャラ番号, 対象キャラ番号, 100)
CALL SOURCE_CALC_接触(対象キャラ番号, 実行キャラ番号, 80)
CALL SOURCE_CALC_奉仕(実行キャラ番号, 対象キャラ番号, 120)
EXP:実行キャラ番号:手淫経験 ++
;奉仕快楽
RCVAR:実行キャラ番号:奉仕コマンドフラグ = 1

RETURN 1



@MODETYPE_手コキ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%的%TEXTR("阴茎/肉棒")%正被%TEXTR("手指挑逗/指圈套弄/指圈上下撸动/手交/手掌撸动/手掌爱抚")%
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	SELECTCASE RAND:4
	CASE 0
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的手指正玩弄着%TEXTR("阴茎/阳具/肉棒")%
	CASE 1
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%用指圈套弄着%TEXTR("阴茎/阳具/肉棒")%%TEXTR("被玩弄着/被上下撸动着")%
	CASE 2
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的%TEXTR("阴茎/阳具/肉棒")%正被手掌%TEXTR("爱抚着/上下撸动着")%
	CASE 3
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的%TEXTR("阴茎/阳具/肉棒")%正被手交侍奉着
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
	CASE 部位_指
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
ENDSELECT


@MODETYPE_授乳手コキ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	SELECTCASE RAND:4
	CASE 0 TO 2
		詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%一边吮吸着%TEXTR("乳房/乳头")%，一边用%TEXTR("手指挑逗/指圈套弄/指圈上下撸动/手交/手掌撸动/手掌爱抚")%侍奉%TEXTR("阴茎/肉棒")%
	CASE 3
		詳細文文字列受け渡し変数 = 以哺乳的姿势为%CALLNAME:ターゲット番号%%TEXTR("手交/用手撸动/用手爱抚")%%TEXTR("阴茎/肉棒")%
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	SELECTCASE RAND:4
	CASE 0 TO 2
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的%TEXTR("乳房/乳头")%被%TEXTR("吸吮/含住")%的同时，%TEXTR("阴茎/阳具/肉棒")%正被%TEXTR("手掌/手指/手指环握")%%TEXTR("爱抚着/玩弄着/上下撸动着/一上一下地爱抚着")%
	CASE 3
		詳細文文字列受け渡し変数 = 以哺乳的姿勢，%CALLNAME:プレイヤー番号%的%TEXTR("阴茎/阳具/肉棒")%正被%TEXTR("手掌/手指/手指环握")%%TEXTR("爱抚着/玩弄着/上下撸动着/一上一下地爱抚着")%
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;キスは両者口のみ専有
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
	CASE 部位_クリトリス
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_指
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
	CASE 部位_口
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_胸
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
	CASE 部位_膣
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_アナル
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
ENDSELECT



@MODE_絶頂相手_手コキ(絶頂部位, 処理モード, 判定結果)
#FUNCTION
#DIM 絶頂部位
#DIMS 処理モード
#DIM REF 判定結果

IF 処理モード == "ターゲット"
	IF 絶頂部位 == 部位_Ｃ
		判定結果 = 絶頂相手_モード相手
		RETURNF 絶頂相手_モード相手
	ENDIF
ENDIF


@MODE_絶頂相手_授乳手コキ(絶頂部位, 処理モード, 判定結果)
#FUNCTION
#DIM 絶頂部位
#DIMS 処理モード
#DIM REF 判定結果

IF 処理モード == "プレイヤー"
	IF 絶頂部位 == 部位_Ｂ
		判定結果 = 絶頂相手_モード相手
		RETURNF 絶頂相手_モード相手
	ENDIF
ELSEIF 処理モード == "ターゲット"
	IF 絶頂部位 == 部位_Ｃ
		判定結果 = 絶頂相手_モード相手
		RETURNF 絶頂相手_モード相手
	ENDIF
ENDIF



@MESSAGE_MODE_手コキ解除(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

IF 対象キャラ番号 == MASTER
	PRINTFORML %CALLNAME:対象キャラ番号%制止了%CALLNAME:実行キャラ番号%并让其松开阴茎
ELSE
	PRINTFORML %CALLNAME:実行キャラ番号%将手从%CALLNAME:対象キャラ番号%的阴茎上移开
ENDIF


@MESSAGE_MODE_授乳手コキ解除(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

IF 対象キャラ番号 == MASTER
	PRINTFORML %CALLNAME:対象キャラ番号%从%CALLNAME:実行キャラ番号%的乳房移开嘴唇，并让其松开阴茎
ELSE
	PRINTFORML %CALLNAME:実行キャラ番号%将手从%CALLNAME:対象キャラ番号%的阴茎上移开
ENDIF
