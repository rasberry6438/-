		PRINTFORML %"关系尚浅", 25, LEFT%：＋%NUM_FORMAT(LOCAL * -4), 8%卢比
		値段計算 -= LOCAL * 4
	ELSE
	;ただの友達だと若干の値上げ。値上げっていうかここまで含めて基本料金みたいなとこがある
		PRINTFORML %"普通朋友", 25, LEFT%：＋%NUM_FORMAT(LOCAL * -1), 8%卢比
		値段計算 -= LOCAL
	ENDIF
ELSE
	;親友以上で値下げしてくれる。基本料金1万円で、好感度100で100ルピ≒1000円引きならまぁ妥当っちゃ妥当か
	SETCOLOR カラーパレット("ピンク")
	PRINTFORML %"友情折扣", 25, LEFT%：－%NUM_FORMAT(LOCAL), 8%卢比
	RESETCOLOR
	値段計算 -= LOCAL
ENDIF

