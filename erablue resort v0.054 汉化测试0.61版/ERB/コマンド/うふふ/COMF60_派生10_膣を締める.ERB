
@COMNAME_S60_10
#FUNCTIONS
TSTR:コマンド名受渡 = 收紧阴道

;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_S_60_10
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("Ｖ系_被挿入中")
弱点コマンドカテゴリ = C_ちんこ
オートコマンド許可 = 1
連続オート許可 = 1

@COM_TOOLTIP_S60_10
LOCALS = <br>■收紧阴道<br>
LOCALS += "收紧阴道内部，对插入的阴茎施加强烈刺激的指令。"
LOCALS += "<br>指令类型：Ｖ系or被插入中<br>获取来源：快Ｃ・暴露・接触"
LOCALS += "<br>专属经验值：Ｃ性交经验_Ｖ插入(TARGET)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM_S60_10
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------

CALL SOURCE_CALC_快Ｖ(PLAYER, TARGET, 600)
CALL SOURCE_CALC_露出(TARGET, PLAYER, 50)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)
CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 600)

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｃ性交経験_Ｖ挿入 ++
EXP:PLAYER:Ｖ性交経験 ++
;同性愛
SIF 同性愛性癖判定(TARGET,PLAYER)
	EXP:TARGET:同性愛経験 += 4
SIF 同性愛性癖判定(PLAYER,TARGET)
	EXP:PLAYER:同性愛経験 += 4

EXP:愛情経験 += TALENT:恋慕 * 2

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE_S60_10
;腸内を締める実行判定
SIF SAVESTR:ゲームフェイズ管理 != "通常モード"
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE_S("60_10")
	RETURN RESULT
;挿入系モードじゃない時はアウト
SIF !MODE_存在判定_完全一致("Ｖ挿入系", TARGET, PLAYER)
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@MESSAGE_COM_S60_10
PRINTFORML %CALLNAME:PLAYER%绷紧小腹，用膣肉绞紧%CALLNAME:TARGET%的阴茎…


