	CALL SOURCE_CALC_快Ｃ(自慰ターゲット保存, -1, 100)
CALL SOURCE_CALC_露出(自慰ターゲット保存, MASTER, 150)

SELECTCASE EXP:自慰ターゲット保存:調教自慰経験
	CASE IS > 50
		CALL SOURCE_CALC_歓楽(自慰ターゲット保存, MASTER, 100)
