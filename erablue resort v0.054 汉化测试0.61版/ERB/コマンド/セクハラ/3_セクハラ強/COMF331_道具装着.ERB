				PRINT  + 
			実行値 += ABL:Ａ感覚 * 2
			PRINTFORM Ａ感觉LV{ABL:Ａ感覚}({ABL:Ａ感覚 * 2})
			＋表示フラグ = 1
		ENDIF
		;A経験
		IF EXP:Ａ経験 < EXPLV:2
			PRINT  - 
			実行値 -= 15
			PRINT Ａ经验不足(15)
			＋表示フラグ = 1
		ENDIF
	CASE 6
		;乳头夹
		;ABL:Ｂ感覚
		IF ABL:Ｂ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ｂ感覚 * 2
			PRINTFORM Ｂ感觉LV{ABL:Ｂ感覚}({ABL:Ｂ感覚 * 2})
			＋表示フラグ = 1
		ENDIF
		;B経験
		IF EXP:Ｂ経験 < EXPLV:2
			PRINT  - 
			実行値 -= 15
			PRINT Ｂ经验不足(15)
			＋表示フラグ = 1
		ENDIF
ENDSELECT

;合計を表示(40以上で実行)
PRINTFORM  = {実行値}

SIF 実行値 < 必要実行値
	PRINT  < 
SIF 実行値 == 必要実行値
	PRINT  = 
SIF 実行値 > 必要実行値
	PRINT  > 
PRINTFORM 执行值{必要実行値}

WAIT
SKIPDISP 0

;実行できない
SIF 実行値 < 必要実行値
	RETURN 0

RETURN 1


