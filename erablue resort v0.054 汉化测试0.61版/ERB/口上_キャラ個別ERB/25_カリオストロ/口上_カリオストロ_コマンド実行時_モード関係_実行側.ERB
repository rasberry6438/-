@KOJO_COMPLAYER_25_81(対象キャラ)
#DIM 対象キャラ
;取得快楽：なし

;あなたがフェラチオをする方
CALL 口上変数初期化

IF TARGET == MASTER
	;相手があなたの時
	IF MODE_存在判定_プレイヤー側("パイズリフェラ", TARGET)
		;パイズリ中はパイズリフェラに派生
		IF TALENT:対象キャラ:恋慕
			KSTR:(K++) = 
		ELSEIF TALENT:対象キャラ:セフレ
			KSTR:(K++) = 
		ELSE
			KSTR:(K++) = 
		ENDIF
	ELSE
		;通常フェラ
		IF TALENT:対象キャラ:恋慕
			KSTR:(K++) = 
		ELSEIF TALENT:対象キャラ:セフレ
			KSTR:(K++) = 
		ELSE
			KSTR:(K++) = 
		ENDIF
	ENDIF
ENDIF

CALL 口上変数表示(対象キャラ, 0)

