@奥義追加効果呼び出し(ARG)
#DIM 先頭得意武器
VARSET LOCALS

;一旦数値をリセットしてから、文字列表示用に奥義名部分を呼び出す
CALL アビ汎用変数文字列リセット
CALL 奥義名呼び出し(ARG)
アビテンプレート用_アビ名 = 奥義：%TSTR:コマンド名受渡%

TRYCCALLFORM 固有奥義_{NO:ARG}("奥義追加効果", ARG)
	SIF RESULT == -1
		RETURN -1
CATCH
	FOR LOCAL, 0, 10
		IF GETBIT(基礎BATTLE_STATE:ARG:得意武器, LOCAL)
			先頭得意武器 = LOCAL
			BREAK
		ENDIF
	NEXT
	TRYCCALLFORM 汎用奥義_%得意武器数値文字列変換(先頭得意武器)%("奥義追加効果", ARG)
		SIF RESULT == -1
			RETURN -1
	CATCH
	ENDCATCH
ENDCATCH
SPLIT DT_CELL_GETS("所持耳飾りデータベース", 装備ステータス補正保存:ARG:装備耳飾り番号, "奥義補正", 1), "_", LOCALS
FOR LOCAL, 0, 10
	SIF LOCALS:LOCAL == ""
		BREAK
	SIF LOCALS:LOCAL == "奥義威力"
		CONTINUE
	CALL アビ汎用変数文字列リセット
	TRYCALLFORM 奥義補正処理関数_%LOCALS:LOCAL%_%DT_CELL_GETS("所持耳飾りデータベース", 装備ステータス補正保存:ARG:装備耳飾り番号, "耳飾り名", 1)%
NEXT


@汎用奥義_剣(ARGS, キャラ番号)
#DIM キャラ番号
#DIM 奥義威力
#DIM 効果数値
#DIMS アビ名
IF 基礎BATTLE_STATE:キャラ番号:Lv >= 38 && 陥落チェック(キャラ番号) == 1
	;恋慕
	SELECTCASE 基礎BATTLE_STATE:キャラ番号:Lv
		CASE IS >= 74
			アビ名 = 终结斩击＋＋
			奥義威力 = 900
			効果数値 = 250
		CASE IS >= 62
			アビ名 = 终结斩击＋
			奥義威力 = 750
			効果数値 = 200
		CASEELSE
			アビ名 = 终结斩击
			奥義威力 = 550
			効果数値 = 130
	ENDSELECT
ELSEIF 基礎BATTLE_STATE:キャラ番号:Lv >= 38 && 陥落チェック(キャラ番号)
	;セフレ
	SELECTCASE 基礎BATTLE_STATE:キャラ番号:Lv
		CASE IS >= 74
			アビ名 = 临界刃锋＋＋
			奥義威力 = 900
			効果数値 = 200
		CASE IS >= 62
			アビ名 = 临界刃锋＋
			奥義威力 = 750
			効果数値 = 150
		CASEELSE
			アビ名 = 临界刃锋
			奥義威力 = 550
			効果数値 = 100
	ENDSELECT
ELSE
	SELECTCASE 基礎BATTLE_STATE:キャラ番号:Lv
		CASE IS >= 26
			アビ名 = 阔剑横扫＋＋
			奥義威力 = 450
		CASE IS >= 14
			アビ名 = 阔剑横扫＋
			奥義威力 = 400
		CASEELSE
			アビ名 = 阔剑横扫
			奥義威力 = 300
	ENDSELECT
ENDIF

SELECTCASE ARGS
	CASE "奥義名"
		TSTR:コマンド名受渡 '= アビ名
	CASE "奥義説明文"
		詳細文文字列受け渡し変数 '= @"奥义威力：{奥義威力}％"
		IF 基礎BATTLE_STATE:キャラ番号:Lv >= 38 && 陥落チェック(キャラ番号) == 1
			;恋慕
			詳細文文字列受け渡し変数 += @"<br>对全体敌人造成［攻击力✕{効果数値}％］的%属性数値文字列変換(基礎BATTLE_STATE:キャラ番号:属性)%属性伤害。"
		ELSEIF 基礎BATTLE_STATE:キャラ番号:Lv >= 38 && 陥落チェック(キャラ番号)
			;セフレ
			詳細文文字列受け渡し変数 += @"<br>对全体敌人造成［攻击力✕{効果数値}％］无视防御的无属性伤害。"
		ENDIF
		RETURN 奥義威力
	CASE "奥義前効果"
		;NOOP
	CASE "奥義追加効果"
		IF 基礎BATTLE_STATE:キャラ番号:Lv >= 38 && 陥落チェック(キャラ番号) == 1
			;恋慕
			CALL アビ対象選択テンプレート_敵全体_生者のみ
			アビ汎用変数:効果割合 = 効果数値
			アビ汎用文字列:ダメージ種類 = 奥義ダメージ
			アビ汎用変数:クリティカル率補正値 = 100
			CALL アビテンプレート_ダメージ処理_属性付き(基礎BATTLE_STATE:キャラ番号:属性)
		ELSEIF 基礎BATTLE_STATE:キャラ番号:Lv >= 38 && 陥落チェック(キャラ番号)
			;セフレ
			CALL アビ対象選択テンプレート_敵全体_生者のみ
			アビ汎用変数:効果割合 = 効果数値
			アビ汎用文字列:ダメージ種類 = 奥義ダメージ
			アビ汎用変数:防御無視オプション_割合 = 100
			CALL アビテンプレート_ダメージ処理_無属性
		ENDIF
ENDSELECT

