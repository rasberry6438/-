@COM74
;背面座位肛門
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:前回指令 = %STR:今回指令%

;-------------------------------------------------
;射精格子
;-------------------------------------------------
CALL SAMEN_CHECK

RETURN 1

@FLAG_COM74, ARG
CALL FLAG_INSERTION, "Ａ挿入", ARG, 70
CALL FLAG_COM5, 50

@SOURCE_COM74, ARG
CALL SOURCE_INSERTION, "Ａ挿入", ARG, 70
CALL SOURCE_COM5, 50

CALL VARSET_COMF

SOURCE:0:露出 = 1500
SOURCE:0:情愛 = 300

;PALAM:欲情をみる
SELECTCASE PALAM:欲情
CASE IS < PALAMLV:1
	TIMES SOURCE:0:情愛, 0.30
CASE IS < PALAMLV:2
	TIMES SOURCE:0:情愛, 0.60
CASE IS < PALAMLV:3
	TIMES SOURCE:0:情愛, 1.00
CASE IS < PALAMLV:4
	TIMES SOURCE:0:情愛, 1.50
CASE IS >= PALAMLV:4
	TIMES SOURCE:0:情愛, 1.80
ENDSELECT

CALL CALC, "調教ソース", ARG


@TRAIN_MESSAGE_COM74
#DIM MEMO_LINECOUNT

MEMO_LINECOUNT = LINECOUNT
CALL TRAIN_MESSAGE_INSERTION
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1
PRINTFORM %CALLNAME:PLAYER%将
IF COND("前Ｔから対象のみ変更") && VAU_INSERT_PLAY(PREVCOM:1)
	CALL ADD_TEXT, "性器から性器へ"
	PRINTFORM 讓%CALLNAME:TARGET%將身體倚靠過來、然後用%TEXTS("肛門")%肛門
;穴開きでない胖次を是ていて、ずらされていないなら、必ずずらす
ELSEIF !CHECK_CLO("Ａ露出") && CHECK_CLO("Ａ露出可")
	PRINTFORM %CALLNAME:TARGET%的
	CALL SETFLAG, "下衣を露出：テ接吻ト有些", TARGET
	PRINTFORM 、將身體倚靠過來、然後用%TEXTS("肛門")%肛門
ELSE
	PRINTFORM 讓%CALLNAME:TARGET%將身體倚靠過來、然後用%TEXTS("肛門")%肛門
ENDIF

IF (TALENT:淫乱 || ABL:露出癖 >= 3) && RAND:2 == 0
	PRINTFORM 將陰莖慢慢地吞入體內、
	IF TEQUIP:接吻
		PRINTFORMW 同時熱情地親吻著……
		CALL SETFLAG, "追加接吻フラグ", TARGET
	ELSE
		PRINTFORMW 同時將雙腿像是展示一樣大大地張開著……
	ENDIF
ELSEIF TEQUIP:接吻
	PRINTFORMW 將迫不及待地陰莖慢慢容納、貫穿、同時熱情地親吻著……
	CALL SETFLAG, "追加接吻フラグ", TARGET
ELSE
	PRINTFORMW 將迫不及待地陰莖慢慢容納、貫穿……
ENDIF

