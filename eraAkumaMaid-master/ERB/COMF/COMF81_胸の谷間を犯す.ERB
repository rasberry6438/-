@COM81
;強制乳交(雙乳間抽插)
CALL PRINT_TRAIN_NAME(SELECTCOM)


CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:前回指令 = %STR:今回指令%

;-------------------------------------------------
;射精格子
;-------------------------------------------------
CALL SAMEN_CHECK

RETURN 1

@FLAG_COM81, ARG
;対象のＢ⇔調教者のＰ的污垢が移動
CALL EQ_STAIN, TARGET, "Ｂ", PLAYER, "Ｐ"
SIF ASSIPLAY == 0 && ABL:Ｂ感覚 >= 3
	TFLAG:好感度BOUNS += 1


@SOURCE_COM81, ARG
CALL VARSET_COMF

CALL SOURCE_DOWNBASE_HP, 81

SOURCE:0:屈従 = 3000
SOURCE:0:逸脱 = 900
SOURCE:0:不潔 = COM_ORDER_STAIN("Ｐ", "Ｂ")*40

;ABL:奉仕精神をみる
SELECTCASE ABL:奉仕精神
CASE 0
	SOURCE:0:性行動 = 420
	SOURCE:0:達成感 = 150
CASE 1
	SOURCE:0:性行動 = 500
	SOURCE:0:達成感 = 300
CASE 2
	SOURCE:0:性行動 = 580
	SOURCE:0:達成感 = 600
CASE 3
	SOURCE:0:性行動 = 660
	SOURCE:0:達成感 = 900
CASE 4
	SOURCE:0:性行動 = 740
	SOURCE:0:達成感 = 1500
CASEELSE
	SOURCE:0:性行動 = 820
	SOURCE:0:達成感 = 2200
ENDSELECT

;ABL:Ｂ感覚をみる
SOURCE:0:Ｂ快楽 = CALCF("Ｂ刺激", 2, 70)
SOURCE:0:Ｂ快楽 = SOURCE_EXABL(SOURCE:0:Ｂ快楽, "指靈活", PLAYER)

SIF TALENT:Ｂ性向
	CALL CALC, "好みのPLAY", SOURCE:0:Ｂ快楽

CALL CALC, "調教ソース", ARG


@TRAIN_MESSAGE_COM81
IF SELECTCOM == PREVCOM
	PRINTFORMW %CALLNAME:PLAYER%將%CALLNAME:TARGET%的胸部持續侵犯著……
	RETURN 0
ENDIF

IF TALENT:巨乳 && RAND:3 == 0
	LOCALS = %TOFULL(CONDS("罩杯サイズ") )%罩杯的%TALENT_NAME(GETNUM(TALENT, "巨乳"), TALENT:巨乳)%
ELSEIF RAND:2
	IF TALENT:巨乳
		IF TALENT:巨乳 >= 2 && RAND:2
			IF RAND:2
				LOCALS = 有著壓倒性魄力的乳房
			ELSE
				LOCALS = 有著充分成熟的重量的乳房
			ENDIF
		ELSE
			IF RAND:2
				LOCALS = 有著富有魅力的線條的乳房
			ELSE
				LOCALS = 宛如熟成果實一般的乳房
			ENDIF
		ENDIF
	ELSE
		LOCALS = 乳房
	ENDIF
ELSE
	LOCALS = 胸
ENDIF

;胸元が露出しているなら、そこではさむ
IF CHECK_CLO("谷間露出")
;半脱られるのに、胸元を半脱ていないなら、ずらす
ELSEIF CHECK_CLO("半脱可")
	CALL SETFLAG, "胸を半脱る", TARGET
ENDIF

PRINTFORM %CALLNAME:PLAYER%将
;前回は乳交
IF PREVCOM == 92 && COND("前Ｔと同ペア")
	;汚れてますか？汚臭鈍感で無いならば、実行値の10分の一の値とRAND:5を比べて判定
	IF CHECK_STAIN(STAIN:PLAYER:Ｐ, "Ｖ", "Ａ") && TALENT:汚臭鈍感 == 0 && RAND:6 > TCVAR:干劲/10
		PRINTFORM 因精液的淫臭而皺著眉頭
	;恋慕or献身的or奉仕精神3以上or汚臭鈍感
	ELSEIF (TALENT:恋慕 || TALENT:献身 || ABL:奉仕精神 >= 3 || TALENT:汚臭鈍感) && RAND:2 == 0
		PRINT 滿臉幸福快樂的
	;害羞
	ELSEIF TALENT:害羞 && RAND:3 == 0
		PRINT 害羞地
	;欲情4以上
	ELSEIF PALAM:欲情 >= PALAMLV:4 && RAND:3 == 0
		SIF TALENT:倒錯的 && RAND:3 == 0
			PRINTFORM 因慾望的%TEXTS("气味")%而
		PRINT 興奮地
	;恋慕かつ奉仕精神5or献身的かつ奉仕精神3以上
	ELSEIF ( (TALENT:恋慕 && ABL:奉仕精神 >= 5) || (TALENT:献身 && ABL:奉仕精神 >= 3) ) && RAND:3 == 0
		PRINT 熱情地
	;恋慕or献身的or奉仕精神3以上
	ELSEIF TALENT:恋慕 || TALENT:献身 || ABL:奉仕精神 >= 3
		PRINT 好像很開心
	ELSEIF RAND:2 == 0 && ABL:技巧 < 2
		PRINT 用生疏笨拙的手法
	ENDIF
	PRINTFORM 侍奉著的%CALLNAME:TARGET%那%LOCALS%\@ RAND:2 ? 從兩側 # 用雙手 \@\@ RAND:2 ? 揉搓著 # 按壓著 \@、
;床に拘束しているなら相手に跨る
ELSE
	IF BIND("床", TEQUIP:拘束)
		PRINTFORM 被拘束具固定在床上的%CALLNAME:TARGET%那
		IF AGE(TARGET) >= 3
			SELECTCASE CONDS("体型")
			CASE "太め"
				PRINTFORM 豐腴而綿軟的
			CASE "細め"
				IF TALENT:大力士
					PRINTFORM 緊繃而富含彈性的
				ELSE
					PRINTFORM 纖細而修長的
				ENDIF
			CASEELSE
				SIF TALENT:大力士
					PRINTFORM 緊繃而富含彈性的
			ENDSELECT
		ENDIF
		PRINTFORM 腹部輕輕跨上、將
	ELSE
		PRINTFORM %CALLNAME:TARGET%的
	ENDIF
	IF RAND:4 == 0
		PRINTFORM %LOCALS%的股間用陰莖前後滑動著、
	ELSEIF RAND:3 == 0
		PRINTFORM %LOCALS%的股間用陰莖快速地突刺著、
	ELSEIF RAND:2
		PRINTFORM %LOCALS%的峽間為目標反復擺動腰肢、
	ELSE
		PRINTFORM %LOCALS%將陰莖包裹住、
	ENDIF
	PRINTFORM \@ RAND:2 ? 從兩側 # 用雙手 \@\@ RAND:2 ? 揉搓著 # 按壓著 \@、
ENDIF

;快楽を与える
IF RAND:2
	IF RAND:3 == 0 && CALCF("Ｂ発情")
		PRINTFORM 將乳頭用手指輕輕地揉捏著
	ELSEIF RAND:2
		PRINTFORM 將乳輪用親指不斷地摩挲著
	ELSE
		PRINTFORM 將乳房盡情的揉搓著
	ENDIF
ELSE
	IF RAND:4 == 0
		PRINTFORM 將內側不斷摩擦陰莖
	ELSEIF RAND:3 == 0
		PRINTFORM 將峰巒緊緊貼住陰莖
	ELSEIF RAND:2
		PRINTFORM 將谷間緊緊捆住陰莖
	ELSE
		PRINTFORM 將乳房用力壓迫陰莖
	ENDIF
ENDIF

;SIF EXP:PLAYER:Ｖ挿入経験 + EXP:PLAYER:Ａ挿入経験
;	PRINT 就像是在正常性交一樣

IF ABL:PLAYER:施虐属性 && RAND:3 == 0
	PRINTFORMW 粗暴地擺動腰肢……
ELSEIF RAND:2
	PRINTFORMW 將腰肢激烈地擺動著……
ELSE
	PRINTFORMW 前後动起了腰…
ENDIF

