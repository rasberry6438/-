;周回BOUNS
IF FLAG:周回数
	IF FLAG:陥落人数
		SELECTCASE FLAG:陥落人数
		CASE IS <= 4
			TCVAR:基本実行値 += 1
			STR:実行値 = %STR:実行値% + 陥落人数BOUNS(1)
		CASE IS <= 8
			TCVAR:基本実行値 += 2
			STR:実行値 = %STR:実行値% + 陥落人数BOUNS(2)
		CASE IS <= 16
			TCVAR:基本実行値 += 3
			STR:実行値 = %STR:実行値% + 陥落人数BOUNS(3)
		CASE IS <= 24
			TCVAR:基本実行値 += 4
			STR:実行値 = %STR:実行値% + 陥落人数BOUNS(4)
		CASE IS <= 32
			TCVAR:基本実行値 += 5
			STR:実行値 = %STR:実行値% + 陥落人数BOUNS(5)
		CASE IS <= 40
			TCVAR:基本実行値 += 6
			STR:実行値 = %STR:実行値% + 陥落人数BOUNS(6)
		CASEELSE
			TCVAR:基本実行値 += 7
			STR:実行値 = %STR:実行値% + 陥落人数BOUNS(7)
		ENDSELECT
