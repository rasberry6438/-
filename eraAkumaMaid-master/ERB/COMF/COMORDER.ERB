LOCAL = LOCAL:0 + LOCAL:1 + LOCAL:2 + LOCAL:3 + LOCAL:4 + LOCAL:5 + LOCAL:6 + LOCAL:7 + LOCAL:8

SIF TALENT:汚臭鈍感
	LOCAL = MAX(DIVIDE(LOCAL, 200), LOCAL - 20)
SIF TALENT:汚臭敏感
	LOCAL = MIN(MULTIPLY(LOCAL, 200), LOCAL + 20)

;陶酔中は汚れに鈍感になる
SIF LOCAL > 0 && COND("陶酔")
	LOCAL /= 2

LOCAL = MAX(LOCAL, 0)

RETURNF LOCAL

