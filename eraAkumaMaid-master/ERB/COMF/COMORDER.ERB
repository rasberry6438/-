	TCVAR:基本実行値 += 5
	STR:実行値 = %STR:実行値% + 鼓舞(5)
ENDIF
IF TEQUIP:室内露出PLAY
	TCVAR:基本実行値 -= 5
	STR:実行値 = %STR:実行値% - 室内露出(5)
ENDIF
IF TEQUIP:攝像機
	TCVAR:基本実行値 -= 10
	STR:実行値 = %STR:実行値% - 撮影中(10)
ENDIF
;IF TEQUIP:野外PLAY
;	IF COND("人里")
