@COM86
;尿道対面座位
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:前回指令 = %STR:今回指令%

;-------------------------------------------------
;射精格子
;-------------------------------------------------
CALL SAMEN_CHECK

RETURN 1

@FLAG_COM86, ARG
CALL FLAG_INSERTION, "尿道挿入", ARG, 50

@SOURCE_COM86, ARG
CALL SOURCE_INSERTION, "尿道挿入", ARG, 50

CALL VARSET_COMF

SOURCE:0:露出 = 300
SOURCE:0:情愛 = 1000

;PALAM:欲情をみる
SELECTCASE PALAM:欲情
CASE IS < PALAMLV:1
	TIMES SOURCE:0:情愛, 0.30
CASE IS < PALAMLV:2
	TIMES SOURCE:0:情愛, 0.60
CASE IS < PALAMLV:3
	TIMES SOURCE:0:情愛, 1.00
CASE IS < PALAMLV:4
	TIMES SOURCE:0:情愛, 1.50
CASE IS >= PALAMLV:4
	TIMES SOURCE:0:情愛, 1.80
ENDSELECT

CALL CALC, "調教ソース", ARG


@TRAIN_MESSAGE_COM86
#DIM MEMO_LINECOUNT

MEMO_LINECOUNT = LINECOUNT
CALL TRAIN_MESSAGE_INSERTION
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

PRINTFORM %CALLNAME:PLAYER%将
IF EXP:尿道性交経験 == 0
	;前回放尿
	IF PREVPLAY("放尿")
		PRINTFORM 剛剛排泄出晶瑩尿液的
	ELSEIF PREVEX("尿道絶頂")
		PRINTFORM 絕頂之後十分敏感的
	ELSEIF PREVPLAY("膀胱内浴尿")
		PRINTFORM 被強行灌入尿液的
	ELSEIF PENIS(TARGET) && PREVEX("射精")
		PRINTFORM 射精之后十分敏感的
	ELSEIF PREVCOM == 15
		PRINTFORM 被手指愛撫過擴張開來的
	ELSEIF PREVCOM == 149
		PRINTFORM 導尿管剛剛拔出的
	ELSEIF PREVCOM == 151
		PRINTFORM 被尿道探針蹂躪之後的
	ENDIF
	IF PENIS(TARGET)
		PRINTFORMW %CALLNAME:TARGET%的柔軟陰莖握緊、將尿道口撥開、就像是在使用飛機杯一樣嘗試著將自己的陰莖插入其中……
	ELSE
		PRINTFORMW %CALLNAME:TARGET%的尿道將陰莖尖端壓上、稍稍用力嘗試插入其中……
	ENDIF
ELSE
	IF COND("前Ｔから対象のみ変更") && PREVCOM:1 >= 60 && PREVCOM:1 <= 88
		CALL ADD_TEXT, "性器から性器へ"
		PRINTFORM 從%CALLNAME:TARGET%的正面相互擁抱、讓尿道將陰莖緊緊包裹並吮吸起來
	;穴開きでない胖次を是ていて、ずらされていないなら、必ずずらす
	ELSEIF !CHECK_CLO("Ｖ露出") && CHECK_CLO("Ｖ露出可")
		PRINTFORM %CALLNAME:TARGET%的
		CALL SETFLAG, "下衣を露出：テ接吻ト有些", TARGET
		PRINTFORM 正面相互擁抱、讓尿道將陰莖緊緊包裹並吮吸起來
	ELSE
		PRINTFORM 從%CALLNAME:TARGET%的正面相互擁抱、讓尿道將陰莖緊緊包裹並吮吸起來
	ENDIF

	IF (CHECK_COM("接吻") && TALENT:恋慕) || TEQUIP:接吻
		PRINTFORMW 、一边激烈地舌吻着一边猛烈地摇晃腰肢……
		CALL SETFLAG, "追加接吻フラグ", TARGET
	ELSE
		PRINTW ……
	ENDIF
ENDIF
