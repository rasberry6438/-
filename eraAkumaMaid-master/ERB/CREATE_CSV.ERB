;質問ごとに全質問中の何問目かを横に出す
;TFLAG:页数 で何問目かを記録
@CREATE_CSV, ARG
REDRAW 0
TFLAG:页数 = 0
TFLAG:指令中止 = 0
CALL CREATE_CSV_MAIN, ARG

@CREATE_CSV_MAIN, ARG
#DIM LCOUNT
#DIM MEMO_LINECOUNT
#DIM QNUM
#DIMS DYNAMIC MEMO_BASE, 100
#DIMS DYNAMIC MEMO_TALENT, 100
#DIMS DYNAMIC MEMO_ABL, 100
#DIMS DYNAMIC MEMO_EXP, 100
#DIMS DYNAMIC MEMO_CSTR, 100
#DIMS MEMO_CSV

MEMO_LINECOUNT = LINECOUNT

;現在的結果表示
VARSET LOCALS
MEMO_CSV = 
SELECTCASE TFLAG:页数
CASE 0
	VARSET MEMO_BASE
	VARSET MEMO_TALENT
	VARSET MEMO_ABL
	VARSET MEMO_EXP
	VARSET MEMO_CSTR
CASEELSE
	FOR LCOUNT, 1, 100
		SELECTCASE LCOUNT
		CASE IS <= TFLAG:页数
			MEMO_CSV += @"%MEMO_BASE:LCOUNT%"
		CASEELSE
			MEMO_BASE:LCOUNT = 
		ENDSELECT
	NEXT
	FOR LCOUNT, 1, 100
		SELECTCASE LCOUNT
		CASE IS <= TFLAG:页数
			MEMO_CSV += @"%MEMO_TALENT:LCOUNT%"
		CASEELSE
			MEMO_TALENT:LCOUNT = 
		ENDSELECT
	NEXT
	FOR LCOUNT, 1, 100
		SELECTCASE LCOUNT
		CASE IS <= TFLAG:页数
			MEMO_CSV += @"%MEMO_ABL:LCOUNT%"
		CASEELSE
			MEMO_ABL:LCOUNT = 
		ENDSELECT
	NEXT
	FOR LCOUNT, 1, 100
		SELECTCASE LCOUNT
		CASE IS <= TFLAG:页数
			MEMO_CSV += @"%MEMO_EXP:LCOUNT%"
		CASEELSE
			MEMO_EXP:LCOUNT = 
		ENDSELECT
	NEXT
	FOR LCOUNT, 1, 100
		SELECTCASE LCOUNT
		CASE IS <= TFLAG:页数
			MEMO_CSV += @"%MEMO_CSTR:LCOUNT%"
		CASEELSE
			MEMO_CSTR:LCOUNT = 
		ENDSELECT
	NEXT
	IF MEMO_CSV != ""
		PRINTFORML 現在設定済みのパラメータ：
		SPLIT MEMO_CSV, "+", LOCALS
		FOR LCOUNT, 0, 100
			SELECTCASE LOCALS:LCOUNT
			CASE ""
				BREAK
			CASEELSE
				PRINTFORML %LOCALS:LCOUNT%
			ENDSELECT
		NEXT
	ENDIF
ENDSELECT
PRINTL 

QNUM = TFLAG:页数 + 1
