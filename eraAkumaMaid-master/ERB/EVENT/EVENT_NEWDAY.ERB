	IF CHECK_CLO("胖次露出")
		CALL SETFLAG_DAILY, "掀起裙子"
		CALL PRINT_CUTIN, "見たまま正面", TARGET
		CALL PRINT_STRL, @"冷静下来之后%CALLNAME:MASTER%顺便进行了检查……今日的%CALLNAME:TARGET%穿着的是%PANTIES(TARGET)%的样子"
	ENDIF
	SIF CHECK_CLO("Ｖ露出") && TEXTS("Ｖコンディション", TARGET) != ""
		CALL PRINT_STRL, @"(现在的%CALLNAME:TARGET%的Ｖ状况是_黄色_%TEXTS("Ｖコンディション", TARGET)%_)"

CASE "朝騎乗位"
	PRINTFORML 早上起来感觉腰上有什么东西压着
	PRINTFORM 睁開眼睛就发现%CALLNAME:TARGET%
	IF SIZE("阴茎", PLAYER) >= 2 && RANDIF(1, TALENT:巨乳)
		IF ABL:Ｖ拡張 >= SIZE("阴茎", PLAYER)
			PRINTFORM 跨在上面、把
			IF STRCOUNTS(CONDS("体型"), "苗条", "高挑")
				PRINTFORM 纤细的身体把
			ELSEIF TALENT:娇小
				PRINTFORM 小小的身体
			ENDIF
		ENDIF
		PRINTFORM %CALLNAME:MASTER%的
		SELECTCASE SIZE("阴茎", PLAYER)
		CASE IS >= 4
			PRINTFORM 規格外的巨根
		CASEELSE
			PRINTFORM 巨根
		ENDSELECT
		PRINTFORM \@ABL:Ｖ拡張 >= SIZE("阴茎", PLAYER) ? 用膣一直吞到最根部 # 压在胯下摩擦 \@
	ELSE
		PRINTFORM 跨在上面
		IF TALENT:巨乳
			IF RAND:3 == 0
				PRINTFORM 、%TOFULL(CONDS("罩杯サイズ") )%罩杯的%TALENT_NAME(GETNUM(TALENT, "巨乳"), TALENT:巨乳)%揺晃着
			ELSEIF TALENT:巨乳 >= 2
				IF RAND:2
					PRINTFORM 、豊満的胸部夸张地摇晃着
				ELSE
					PRINTFORM 、看上去很重的胸部悠悠地摇晃着
				ENDIF
			ELSEIF RAND:2
				PRINTFORM 、大大的胸部摇晃着
			ELSE
				PRINTFORM 、一边揉着自己的胸
			ENDIF
		ENDIF
	ENDIF
	PRINTFORML 发出了舒服的喘息……
	IF TARGET == ASSI
		IF RAND:2
			PRINTFORML 发现%CALLNAME:MASTER%醒过来之后、%CALLNAME:TARGET%緩緩動起了腰、一边夹杂着嬌声一边開始说明今天的日程…
		ELSE
			PRINTFORML 刚刚睡醒頭脑还处于混乱状态下的%CALLNAME:MASTER%、一边抓着%CALLNAME:TARGET%的腰向上突进、一边让其在嬌声说明今天的日程…
		ENDIF
