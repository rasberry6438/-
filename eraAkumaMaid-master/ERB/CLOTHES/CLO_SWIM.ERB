		ELSEIF STRCOUNT(ARGS, "死库水")
			SUNTAN_BOT += "死库水"
		ENDIF
	ENDSELECT

	SELECTCASE ARGS:1
	CASE "晒痕", "晒痕トップ"
		IF STRCOUNT(ARGS, "褶边比基尼胸罩")
			SUNTAN_TOP += "褶边比基尼胸罩"
