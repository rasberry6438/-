#FUNCTIONS
SIF ARG == 0 && TARGET > 0
	ARG = TARGET

VARSET LOCALS

LOCALS += @"黄色/%CONDS("体型", ARG)%\/"

SIF TALENT:ARG:扶她
	LOCALS += "黄色/扶她/"
SIF TALENT:ARG:女性 == 0
	LOCALS += @"黄色\/阴茎尺寸：%CONDS("阴茎尺寸", ARG)%\/"

