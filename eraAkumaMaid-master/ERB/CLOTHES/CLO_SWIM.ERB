	ELSEIF STRCOUNT(ARGS, "高開衩連衣裙")
		RETURNF "高開衩連衣裙"
	ELSEIF STRCOUNT(ARGS, "連体泳装")
		RETURNF "連体泳装"
	ELSEIF STRCOUNT(ARGS, "彈弓泳衣")
		RETURNF "彈弓泳衣"
	ENDIF

;基本的には種類と一緒だがセパレートなどを考慮
CASE "晒痕", "晒痕トップ", "晒痕下衣"
	SUNTAN_TOP = 
	SUNTAN_BOT = 

	SELECTCASE ARGS:1
	CASE "晒痕", "晒痕下衣"
		IF STRCOUNT(ARGS, "男式比基尼")
			SUNTAN_BOT += "男式比基尼"
		ELSEIF STRCOUNT(ARGS, "衝浪胖次")
