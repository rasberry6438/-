;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K29, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝舐陰", "朝六九式"
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「呜（吞咽声）、早上好……」
		CASE 1
			PRINTFORML 「嗯，已经起来了」
			IF TEXTS("曜日") == "日"
				PRINTFORMW 「继续？在那之前先吃早饭吧…？」
			ELSE
				PRINTFORMW 「差不多该到了上学的时间了、接下来就是在学校里……可以吗？」
			ENDIF
		CASEELSE
			PRINTFORML 「啊、早上好、%CNAME("二人称")%。」
		ENDSELECT
	CASE "朝騎乗位"
		PRINTFORML 「%CNAME("二人称")%、该起床了～？」
		PRINTFORMW 「下面倒是先起床了呢……。」
	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1

	IF COND("体育")
		PRINTFORMW 「今天有体育课、要是把体操服忘在家里可就没劲咯」
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「……今天、有什么计划吗？」
			CASE 1
				PRINTFORMW 「等一下，我去泡绿茶……。」
			CASEELSE
				PRINTFORMW 「早上好、睡得还好吗？」
		ENDSELECT
	ENDIF
CASE "一番乗り"
	SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「%CNAME("二人称")%怎么这么早就来了？」
		CASE 1
			PRINTFORMW 「第一名~。」
		CASEELSE
			PRINTFORMW 「哦、还以为是谁来了呢……」
	ENDSELECT
CASE "ただいま"
		PRINTFORML 「我回来了~」
		PRINTFORMW 「哦、%CNAME("二人称")%也在啊」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「欢迎回来。已经等了半天了～……」
	ELSE
		PRINTFORMW 「欢、欢迎回来……」
	ENDIF

CASE "入室打招呼"
	SELECTCASE CSTR:PREV_DAILYCOM
		CASE "甜言蜜语"
			PRINTFORMW （打、打扰了…………）
		CASE "打招呼舐陰", "打招呼口交"
			PRINTFORMW 「呜、哇。你在干什么……」
			CALL EMOTION_PLUS, "重置/〇〇/困扰眉/ω"
		CASEELSE
			PRINTFORMW 「%TEXTR("失礼了/打扰了")%。」
	ENDSELECT
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「打扰了。大家差不多都到齐了吧」
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「你好…今天要做些什么呢？」
			CASE 1
				PRINTFORMW %CALLNAME:TARGET%轻轻地点了点頭。
			CASEELSE
				PRINTFORMW 「你好」
		ENDSELECT
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『您好、%CNAME(CALLNAME_ACTOR)%。』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 『大半已经到齐了。』
			CASE 1
				PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、你好。』
			CASEELSE
				PRINTFORMW 『你好~。』
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 『你好~。』
			CASE 1
				PRINTFORMW %CALLNAME:TARGET%轻轻地点了点頭。
			CASEELSE
				PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、你好。』
		ENDSELECT
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "便当"
	PRINTFORM 「%CNAME("二人称")%、今天用
	IF !RAND:4
		;時々、地元特産品弁当
		PRINTFORMW %TEXTR("明太子便当/高菜便当/梅果羊栖菜便当/迷你饺子便当/干菜便当")%来招待，%TEXTR("来尝尝吧/请慢用")%。」
		;※かしわめし…鶏肉の炊き込みご飯
	ELSE
		PRINTFORMW %TEXTR("海苔便当/鸡蛋烧便当/生姜烧便当/饭团便当/盐烤鲑鱼便当")%来招待，%TEXTR("来尝尝吧/请慢用")%。」
	ENDIF
	SELECTCASE RAND:4
		CASE 0
			PRINTFORMW 「甜点的%TEXTR("梨子/猕猴桃/橘子/草莓/羊羹")%也请收下」
		CASE 1
			PRINTFORMW 「这可是%CNAME("一人称")%的自信作品哦。味道怎么样？」
		CASE 2
			PRINTFORMW %CALLNAME:TARGET%把便当的一部分递到了%CALLNAME:MASTER%嘴边。
			PRINTFORMW 「来%CNAME("二人称")%、啊～唔★」
		CASEELSE
			PRINTFORMW 「量好像有点少呢、能够吃饱吗？」
			PRINTFORMW 虽然这么说、但其实只是以%CALLNAME:TARGET%的标准来说很少、对%CALLNAME:MASTER%是正好的分量。
	ENDSELECT
ENDSELECT

