;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K29, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 僕_僕
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF NAME:PLAYER == "你"
			LOCALS = 部长_部长
		ELSEIF TRUE_SEX(MASTER) == "男性"
			LOCALS = %NAME:PLAYER%君_%CSTR:PLAYER:ふりがな%君
		ELSE
			LOCALS = %NAME:PLAYER%酱_%CSTR:PLAYER:ふりがな%酱
		ENDIF
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部", "１年生", "２年生"
			IF TALENT:PLAYER:男性
				LOCALS = %CALLNAME:PLAYER%君_%CSTR:PLAYER:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:PLAYER%酱_%CSTR:PLAYER:ふりがな%酱
			ENDIF
		CASE "３年生", "研究生"
			LOCALS = %CALLNAME:PLAYER%前輩_%CSTR:PLAYER:ふりがな%前輩
		CASEELSE
			IF COND("教員", PLAYER)
				LOCALS = %CALLNAME:PLAYER%老师_%CSTR:PLAYER:ふりがな%老师
			ELSEIF TALENT:PLAYER:男性
				LOCALS = %CALLNAME:PLAYER%君_%CSTR:PLAYER:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:PLAYER%酱_%CSTR:PLAYER:ふりがな%酱
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 部长_部长
CASE "助手"
	SELECTCASE CSTR:ASSI:学年
	CASE "初等部", "中等部", "１年生", "２年生"
		IF TALENT:PLAYER:男性 || TRUE_SEX(ASSI) == "男性"
			LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
		ELSE
			LOCALS = %CALLNAME:ASSI%酱_%CSTR:ASSI:ふりがな%酱
		ENDIF
	CASE "３年生", "研究生"
		LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
	CASEELSE
		IF COND("教員", ASSI)
			LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
		ELSEIF TALENT:PLAYER:男性
			LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
		ELSE
			LOCALS = %CALLNAME:ASSI%酱_%CSTR:ASSI:ふりがな%酱
		ENDIF
	ENDSELECT
ENDSELECT
RETURNF LOCALS

