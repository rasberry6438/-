;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K57, ARGS
#FUNCTIONS
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 俺_俺
CASE "二人称"
	LOCALS = %C_DETAIL(PLAYER)%
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	IF NAME:MASTER == "你"
		LOCALS = 前輩_前輩
	ELSE
		LOCALS = %NAME:MASTER%前輩_%CSTR:MASTER:ふりがな%前輩
	ENDIF
CASE "助手"
	LOCALS = %C_DETAIL(ASSI)%
CASEELSE
	ACTOR = FIND_NO(ARGS)
	LOCALS = %C_DETAIL(ACTOR)%
ENDSELECT
RETURNF LOCALS

