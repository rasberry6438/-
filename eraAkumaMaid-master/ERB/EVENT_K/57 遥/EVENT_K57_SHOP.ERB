	IF SEVENT(ARGS) == 0 || SEVENT("商品購入") == 0

	ELSE

	ENDIF
CASE "询问有什么想要的东西"
	IF CFLAG:メス堕ち度 >= 3 || TALENT:淫乱
		PRINTFORML 「欲しい物かぁ……」
		;ここで足りない/欲しい商品があると、それについて話す
		;話の順番とかを自然にするためにBIT変数を使って必須道具は管理
		;……だったが、ryusimi桑のおかげでグンとやりやすくなった
		;必須道具は潤滑液と避孕套
		;足りない商品がある時は、そちら優先
		NEED_TABLE = 
		SIF ITEM:潤滑液 <= 5
			NEED_TABLE += "潤滑液"
		SIF ITEM:避孕套 <= 10
			NEED_TABLE += "避孕套"

		IF STRCOUNTS(NEED_TABLE, "潤滑液", "避孕套")
			IF STRCOUNTS(NEED_TABLE, "潤滑液")
				PRINTFORML 「唔嗯……买点润滑液比较好吧？」
				PRINTFORML 「H的时候没有润滑剂就惨了……」
			ENDIF
			IF STRCOUNTS(NEED_TABLE, "避孕套")
				IF STRCOUNTS(NEED_TABLE, "潤滑液")
					PRINTFORML 「还要买避孕套呢ー」
				ELSE
					PRINTFORML 「……说起来，好像没有避孕套了呢」
				ENDIF
				IF VAGINA(MASTER)
					PRINTFORML 「%CNAME("二人称")%が妊娠していっていうならいいけど、ナマでするのは色々と大変だろうし、あったほうがいいんじゃないかな？」
				ELSE
					PRINTFORML 「避妊は大人のエチケットだって聞いたぞ」
					PRINTFORML 「……まあ、相手が構わないならいいんだけど」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF

		NEED_TABLE = 
		SIF ITEM:創口貼 <= 1
			NEED_TABLE += "創口貼"
		SIF ITEM:撮影機材 == 0
			NEED_TABLE += "撮影機材"
		SIF ITEM:飞机杯 == 0
			NEED_TABLE += "飞机杯"
		SIF ITEM:肛用振動棒 == 0
			NEED_TABLE += "肛用振動棒"
		SIF PENIS(MASTER) == 0 && ITEM:假阳具 == 0
			NEED_TABLE += "假阳具"

		SELECTCASE TEXTR(NEED_TABLE)
		CASE "創口貼"
			PRINTFORML 「……那么，买点创可贴怎么样？」
			PRINTFORML 「你看，没准会有受伤的时候。那个时候就方便了吧？」
			CALL PRINT_STRW, @"「……而且、H的时候没准也可以用……_H_」"
		CASE "撮影機材"
			PRINTFORML 「那么，那个相机怎么样？」
			PRINTFORML 「……嘛、那个、%CNAME("一人称")%有点好奇用这个拍照会拍成什么样……」
			CALL PRINT_STRL, @"「有点羞耻但也有点兴奋……_H_」"
			PRINTFORML 「……呜，感觉自己变得越来越变态了……」
		CASE "飞机杯"
			PRINTFORML 「……那，那这个飞机杯怎么样」
			PRINTFORML 「%CNAME("一人称")%没用过……不过%CNAME("二人称")%不会没用过吧……」
			CALL PRINT_STRW, @"「……说起来，这个飞机杯可以用来啾啵啾啵的套弄%CNAME("二人称")%的鸡鸡……_H_」"
		CASE "肛用振動棒"
			PRINTFORML 「……啊ー、那个……这个是肛、肛用振動棒……」
			CALL PRINT_STRW, @"「……怎、怎么说呢、这个插进去的话会很爽吧……_H_」"
		CASE "假阳具"
			PRINTFORML 「唔嗯……那么，买这个穿戴式假陰茎？　怎么样？　虽然不知道怎么用」
			CALL PRINT_STRL, @"「嘛、那个、H的时候也许能用上……_H_」"
		CASEELSE
			PRINTFORML 「……唔嗯、想不出来呢……」
		ENDSELECT
	ELSE
		PRINTFORML 「就算说想要的东西、%CNAME("一人称")%对这些东西也不太熟悉……」
	ENDIF
