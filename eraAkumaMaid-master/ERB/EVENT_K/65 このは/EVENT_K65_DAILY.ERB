;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K65, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝面部騎乗"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		CALL EMOTION_PLUS, "重置/紅潮２/小口２/闭眼", TARGET
		PRINTFORMW 「はむっ、んむ、ちゅ」
		PRINTFORMW 「あ、起きたね……おはよ～」
		PRINTFORMW 「……なんで、って……あ～……」
		CALL EMOTEFACE, "躁动难忍", TARGET
		PRINTFORMW 「それより、こっちも起きてきたみたいだけど～？」
		PRINTFORMW %CALLNAME:TARGET%は顔の紅潮を隠し的同时、%CALLNAME:MASTER%の下半身を指さしている……
	CASE "朝騎乗位"
		CALL EMOTEFACE, "惊嚇", TARGET
		PRINTFORMW 「あ」
		PRINTFORMW 「……起きちゃったか～」
		PRINTFORMW 「おはよ～、その、……続けたい？」
		PRINTFORMW 「……」
		CALL EMOTEFACE, "害羞", TARGET
		PRINTFORMW 「や、%CNAME("一人称")%に聞かないでよ」
	;ろりっこ添い寝！ろりっこ添い寝！
	CASE "朝添い寝"
		IF TALENT:恋慕
			IF COND("欲求不満") >= 2
				;甘えたがり
				PRINTFORMW 「……んっ、すぅ……ふぁ、ぁ……？」
				PRINTFORMW 困倦たままの%CALLNAME:TARGET%を置いて%CALLNAME:MASTER%が被窝から出ようとすると、ずしっと下半身に重みがかかる。何事かと見てみれば……
				PRINTFORMW 「……ゃ」
				PRINTFORMW %CALLNAME:TARGET%が%CALLNAME:MASTER%の腰にしがみついてきていた。
				PRINTFORMW 「……らめ」
				PRINTFORMW 「……っ、や……！」
				PRINTFORMW 引きずられながらも、%CALLNAME:TARGET%は%CALLNAME:MASTER%に密着したまま、出口ようとしない。
				PRINTFORMW それどころか就这様%CALLNAME:MASTER%の背中にのしかかってきて、身動きが取れなくなる。
				PRINTFORMW 「ひとりに、しないで……！」
				PRINTFORMW 「いっしょに、いて……いかないれ……」
				PRINTFORMW 「いっしょに……%CNAME("一人称")%、めちゃくちゃに、して……」
				PRINTFORMW 「……んぅ、すき、すきなの、すきっ、すきっ」
				PRINTFORMW 全力で腕も足も%CALLNAME:MASTER%に絡め的同时、%CALLNAME:TARGET%が腰を擦り付けてくる。
				PRINTFORMW 細っこいやわらかな身体が、ぬくもりを持った肌が、繰り返し%CALLNAME:MASTER%に触れる。
				PRINTFORMW しかしそれは次第に遅くなっていき?
				PRINTFORMW 「すき、すきぃっ、す……き……？～～～っ！！！？」
				PRINTFORMW 「ぼしゅっ！！！！！」
				PRINTFORMW %CALLNAME:TARGET%は目覚めた。顔から爆風の如き湯気を出しながら……
				PRINTFORML
				PRINTFORML
				PRINTFORML
				PRINTFORML
				PRINTFORMW ……
				PRINTFORMW 「……おはよー、ございます……うぅ」
				PRINTFORMW ちょこんと座り込んだ%CALLNAME:TARGET%に、改めて朝の打招呼をされた。
				PRINTFORMW 「あんまり%CNAME("一人称")%のこと、ほったらかしちゃやだよ……」
			ELSEIF RAND:4 == 0
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「ん、おはよ～」
				CALL EMOTEFACE, "甜言蜜语", TARGET
				PRINTFORMW 「あんまりあったかいから、抱きついちゃった、ふふ」
				PRINTFORMW 「どうする？もうちょっとゆったりしてみない？ふふっ」
			ELSEIF RAND:3 == 0
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「……んぅ……？ぁ、ぉ、おはよ～」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「ふふっ、なんか居心地よかったから、くっついちゃった」
				PRINTFORMW 「……もうちょっと一緒にいたいな～、な～んて」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「んぅ……くー……っ、ふゃ……ぁ？」
				CALL EMOTEFACE, "刚睡醒/紅潮２", TARGET
				PRINTFORMW 「ぉはよ……ゃ、まって、まだ……」
				PRINTFORMW 「もうちょっと、ねよ？ねよーよ、ねよ～……や、だーめ、だめ……いかないで……ぅぅ……」
				PRINTFORMW 困倦て抱きついてくる%CALLNAME:TARGET%をどうにか引き剥がし、あるいはしばらくくっついてから、%CALLNAME:MASTER%は被窝から出た……
			ELSE
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「すー……ん、んぁあ……？ふぁ……」
				CALL EMOTEFACE, "害羞", TARGET
				PRINTFORMW 「あれ、%CNAME("一人称")%……いつの間に……」
				PRINTFORMW 「……安心しちゃったのかも、ふふっ」
			ENDIF
		ELSE
			IF RAND:4
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「……んっ、う……？ぁ、あぇ？……っ！？」
				CALL EMOTEFACE, "害羞", TARGET
				PRINTFORMW 「お、おはよー……や、夜中にちょっと様子が気になってさ」
				PRINTFORMW 「%CNAME("二人称")%のとこ行って、なんとなく、もぐっちゃって」
				PRINTFORMW 「就这様寝ちゃってた……みたい……」
				CALL EMOTEFACE, "害羞/闭眼", TARGET
				PRINTFORMW 「……」
				CALL EMOTEFACE, "ガチ害羞", TARGET
				PRINTFORMW 「……たまに是いじゃん！おこ大人って言わないで！」
			ELSEIF RAND:3 == 0
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「……んぅ……？ぁ、ぉ、おはよ～」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「ふふっ、なんか居心地よかったから、くっついちゃった」
				PRINTFORMW 「……もうちょっと一緒にいたいな～、な～んて」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「ん……ふぁ、ぉ、おはよ～……」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「や、なんとなくくっつきたくなっちゃってさ」
				PRINTFORMW 「そういう時もあるの～、あるんだってば」
			ELSE
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「すー……ん、んぁあ……？ふぁ……」
				CALL EMOTEFACE, "害羞", TARGET
				PRINTFORMW 「あれ、%CNAME("一人称")%……いつの間に……」
				PRINTFORMW 「あんまり落ち着くからさ、もう……恥ずかし……」
			ENDIF
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		IF TALENT:恋慕
			PRINTFORMW 「今日体育なの～、そっか」
			PRINTFORMW 「いっぱい動いて疲れちゃうね～」
			PRINTFORMW 「……そしたら、「休憩」だね、ふふ……」
		ELSE
			PRINTFORMW 「今日体育なんだ、へ～」
			PRINTFORMW 「身体動かしたら疲れちゃうね、疲惫ら、あんまりその、えっちな……ことも……」
			CALL EMOTEFACE, "心情差", TARGET
			PRINTFORMW 「疲惫ら「休憩」したくなる？う～あ～、そっか～……」
		ENDIF
	ELSEIF COND("水泳")
		PRINTFORMW 「今日水泳あるんだ～」
		IF RAND:2
			PRINTFORMW 「%CNAME("二人称")%は泳ぐの、得意なの？」
		ELSE
			PRINTFORMW 「そっか、がんばってきてね」
		ENDIF

	ELSE
		;朝ごはんイベント
		IF CFLAG:料理回数 < 6
			IF RAND:3 == 0
				CALL EMOTION_PLUS, "笑顔", TARGET
				PRINTFORMW 「おはよ～、ささ、朝ごはん食べようよ」
				PRINTFORMW 「今日は%CNAME("一人称")%が作ったんだよ～」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「ちょっと失敗しちゃったかもだけど……よかったら食べて～？」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "重置", TARGET
				PRINTFORMW 「ふふん、ふん、ふん……♪」
				PRINTFORMW 「あ、おはよ～、ちょうどごはんできるところだよ～」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「おみそしる、ちょっと味見してみる？なんかしょっぱい気がして……」
			ELSE
				PRINTFORMW 「おはよ～、そ、その……朝ごはん作ってみたんだけど、どう？」
			ENDIF
		ELSE
			IF RAND:3 == 0
				CALL EMOTION_PLUS, "笑顔", TARGET
				PRINTFORMW 「おっはよ～、ほら、はやく座って座って」
				PRINTFORMW 「今日のごはんは%CNAME("一人称")%が作ったんだよ～」
				PRINTFORMW 「けっこう自信あるんだから、食べて食べて」
			ELSEIF RAND:2
				PRINTFORMW 目を覚ました%CALLNAME:MASTER%が居間に入ると、廚房の方から楽しげな鼻歌が聞こえてきた。
				CALL EMOTION_PLUS, "笑顔", TARGET
				PRINTFORMW 「ふふん、ふん、ふん……♪」
				CALL EMOTEFACE, "重置", TARGET
				PRINTFORMW 「あ、おはよ～、ちょうどごはんできるところだよ～」
				PRINTFORMW 「ふふ、今日のおかずは%TEXTR("目玉焼きとベーコン/焼きじゃけ/涼拌菜と炒め物/焼きさば/卵焼き/ソーセージ/豚のしょうが焼き")%でーす」
			ELSE
				PRINTFORMW 目を覚ました%CALLNAME:MASTER%が居間に入ると、廚房から食欲をそそる香りが漂ってくるのに気づいた。
				CALL EMOTION_PLUS, "重置", TARGET
				PRINTFORMW 「おはよ～、いいにおいする？おみそしる作ってるの～」
				PRINTFORMW 「%TEXTR("油油炸/じゃがいも/なす/捲心菜/お豆腐/わかめ/溶き卵")%入れてみたんだけど、どうかな～」
				PRINTFORMW 「もうちょっと待っててね、ふふ」
			ENDIF
		ENDIF
		CFLAG:料理回数 += 1
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")

	ELSEIF DAYEV("試験返却日")

	ELSEIF COND("朝袭相手")

	ELSEIF COND("体育")

	ELSEIF COND("水泳")

	ELSE

	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
		PRINTFORMW 「あれ、誰也不在みたいだね～」
		CALL EMOTEFACE, "笑顔", TARGET
		PRINTFORMW 「はしゃぎ放題、ってこと？」
CASE "ただいま"
	IF RAND:3 == 0
		PRINTFORMW 「回来了哟～」
	ELSEIF RAND:2
		PRINTFORMW 「たっだいま～」
	ELSE
		PRINTFORMW 「ただいま、さみしくなかった～？」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		;確率を偏らせる == 0の入れ忘れでは有ません
		IF RAND:3
			PRINTFORMW 「おかえりなさ～い」
		ELSEIF RAND:2
			PRINTFORMW 「おかえり～、ふふ」
		ELSE
			PRINTFORMW 「お、おかえり……ちょっとねむたくなってきちゃった……」
		ENDIF
	ELSE
		;確率を偏らせる == 0の入れ忘れでは有ません
		IF RAND:3
			PRINTFORMW 「あ、おつかれ～」
		ELSEIF RAND:2
			PRINTFORMW 「こんにちは～」
		ELSE
			PRINTFORMW 「ほふふぁれひゃま」
			PRINTFORMW 「……お先にくつろいでまーす」
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
			IF TALENT:恋慕
				IF RAND:2
					PRINTFORMW 「ふふーん、你の%CALLNAME:TARGET%酱だよ～」
					PRINTFORMW 「な～んて、ふふふ」
				ELSE
					PRINTFORMW 「来たよ～、どしたの？」
				ENDIF
			ELSE
				PRINTFORMW 「来たよ～、どしたの？」
			ENDIF
	;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF TALENT:恋慕
			CALL EMOTEFACE, "害羞", TARGET
			PRINTFORMW 「こん、わぁっ、ちょ、ちょっと、なにやってんのさ～……」
		ELSE
			CALL EMOTEFACE, "害羞", TARGET
			PRINTFORMW 「こん、わわっ、えぇ……なにしてるの……」
		ENDIF
	ELSE
			PRINTFORMW 「こんにちは～」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			PRINTFORMW 「えっと、おじゃましま～す……ちょっと繞道しすぎちゃったかな」
	ELSEIF CFLAG:呼び出された
			PRINTFORMW 「こんにちは～、来たよ～、どしたの？」
	ELSE
			PRINTFORMW 「おじゃましま～す」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
			PRINTFORML 『こんにち～……あ』
			PRINTFORML 『あ、%CNAME("一人称")%は、その、生徒じゃないけど、見学中というか……』
			PRINTFORMW 『お、おつかれ大人？です』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			PRINTFORML 『ほんひひふぁ』
			PRINTFORMW %CALLNAME:TARGET%は%TEXTS("点心")%を頬張ってくつろいでいる……
	ELSE
			PRINTFORMW 『こんにちは～、おつかれ大人～』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「来たよ～、で、どしたの？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「回来了哟～」
	ELSE
		PRINTFORMW 「お、いるいる」
	ENDIF
CASE "抜け出された"
		PRINTFORML 「あらら、いってらっしゃい？」
		PRINTFORMW （じー）
CASE "便当"
	;初めて
	IF CFLAG:料理回数 == 0
		PRINTFORMW 「便当、どう？おいしい？」
		CALL EMOTEFACE, "害羞", TARGET
		PRINTFORMW 「ものすごーく久しぶりに作ったから、ちょっと心配でさ」
		PRINTFORMW 「味が单薄的？ん、確かにそうかも……」
		PRINTFORMW 「……まだまだ練習しなきゃね」
		CALL EMOTEFACE, "重置", TARGET
		PRINTFORML 
		PRINTFORMW 「また、作るね？次はもうちょっとがんばるからさ」
	;不慣れ
	ELSEIF CFLAG:料理回数 >= 1 && CFLAG:料理回数 < 6
		IF RAND:3 == 0
			PRINTFORMW 「ど、どう……？」
			PRINTFORMW 「しょっぱい？やっぱり？」
			PRINTFORMW 「……ごめんなさいっ、あ、ぜ、全部は食べなくていいよ」
			PRINTFORMW 「折角作ってもらったし食べる？そっか、……有がと」
		ELSEIF RAND:2
			PRINTFORMW 「唔嗯……」
			PRINTFORMW 「もうちょっとながーく茹でた方がよかったか～」
			PRINTFORMW 「次は気を付けなきゃ」
			PRINTFORMW 「ともかく、食べてくれて有がと」
		ELSE
			;多分煮崩れした具材がべちゃっとしてるやつ
			PRINTFORMW 「今回是つもよりうまくいったと思うんだけど……」
			PRINTFORMW 「あ、ほんとだ、し无なになっちゃってる……」
			PRINTFORMW 「ふふ、おいしく作れるまで、もうしばらくかかっちゃいそうだね」
			PRINTFORMW 「でもいっしょに食べるの、楽しいな……」
		ENDIF
	;好吃！好吃！
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「どう、おいしい？」
			CALL EMOTEFACE, "笑顔", TARGET
			PRINTFORMW 「やった～、ふふ、今回は自信あったからね～」
			PRINTFORMW 「ふふふ、ふふ……」
		ELSEIF RAND:2
			CALL EMOTEFACE, "笑顔", TARGET
			PRINTFORMW 「こうやって一緒に便当食べるの、楽しいね」
			PRINTFORMW 「なんか仲良し～っていうかさ」
			PRINTFORMW 「おいしく食べてもらえるの、嬉しいし」
		ELSE
			CALL EMOTEFACE, "笑顔", TARGET
			PRINTFORMW 「ふふーん、おいしいでしょ？」
			PRINTFORMW 「ほら、あーん」
			PRINTFORMW 「ふふ……や、うまく作れたからなんか嬉しくなっちゃってさ」
			CALL EMOTEFACE, "笑顔/紅潮２", TARGET
			PRINTFORMW 「しょ、しょうがないじゃん、ほら、もう一口」
		ENDIF
	ENDIF
	CFLAG:料理回数 += 1
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	PRINTFORMW 「わわっ、げ、元気だね～……」
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;动画ーションの関係上、１ターンに何度も呼ばれたりするのでフラグ操作をする場合には
;IF CFLAG:KOJO_DAILY済 == 0
;で分岐させるのが良いでしょう
;-------------------------------------------------
@KOJO_DAILY_K65, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
		PRINTFORML 「えっと、%CNAME("一人称")%は%CALLNAME:TARGET%っていうの、よろしく……」
		PRINTFORML 見知らぬ人に緊張しているのか%CALLNAME:TARGET%は辺りをきょろきょろ見回している。
CASE "浑身是汗"
		PRINTFORML 「あ、あつい……とけちゃうよ～」
CASE "刚睡醒"
		IF CHOSE_DEVENT("睡眠姦口交")
			CALL EMOTEFACE, "憤怒", TARGET
			PRINTFORML 「んぅ……っ、うぇっ、なんか、のど、へんなかんじする……」
			PRINTFORML 「う～……」
			PRINTFORML 「ん」
			PRINTFORML 「ん……！」
