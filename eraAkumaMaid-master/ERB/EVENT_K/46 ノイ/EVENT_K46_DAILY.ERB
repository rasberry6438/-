;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K46, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 3+RAND:3)%」"
	CALL PRINT_STRW, @"「你玩腻了就不管我里面、别太过分了_H_」"
	SELECTCASE RAND:6
	CASE 0
		CALL PRINT_STRW, @"「没错、这是回礼。现在我会好好『使用』你的。觉悟吧_H_」"
	CASE 1
		CALL PRINT_STRW, @"「那么、必须要好好惩罚你_H_」"
	CASE 2
		PRINTFORMW 「さて、そんな薄情なヤツにはしっかりと教育してやらねえといけないなぁ？」
	CASE 3
		CALL PRINT_STRW, @"「こんなことが二度とないように、しっかり身体に教え込んでやらねえと_H_」"
	CASE 4
		PRINTFORMW 「こりゃあしっかり教え込んでやらねえといけないなぁ」
		CALL PRINT_STRW, @"「……二度とこんなこと出来ねえように、な_H_」"
	CASE 5
		CALL PRINT_STRW, @"「そんなヤツにゃ、それ相応の罰ってモンを与えねてやんねぇとなぁ_H_」"
	ENDSELECT
;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝拘束"
		IF RAND:2
			PRINTFORMW 「我说、最近你好久都没来了」
			PRINTFORMW 「健全な生活に戻ったってんなら結構なことだが、……大方、そうじゃないんだろ？」
			PRINTFORMW 「だったら%CNAME("一人称")%だって、おこぼれをもらっていいはずだ」
			PRINTFORMW 「なのにそれがないってんだから、こんな手に走っちまったって仕方ないだろ？」
			CALL PRINT_STRW, @"「……所以、做好觉悟吧_H_」"
		ELSE
			PRINTFORMW 「おっと失礼、最近ご無沙汰だったモンでな」
			PRINTFORMW 「……さて、これでも%CNAME("一人称")%はそれなりに%CNAME("二人称")%に対して義理立てしてやってんだ」
			PRINTFORMW 「ま、別のヤツに手を出すのは元々そういうヤツだって知ってるから構わないんだが……」
			PRINTFORMW 「……就这様放置するってのはさすがに酷いってヤツじゃあないか？」
			PRINTFORMW 「っつー訳で、今日は%CNAME("一人称")%の好きにさせてもらおうと思ってな、こういった物を用意させて貰った訳だ」
			CALL PRINT_STRW, @"「……さて、たっぷり味合わせてもらうからな。覚悟しておけよ_H_」"	
		ENDIF
	CASE "朝逆正常位"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 3+RAND:3)%」"
		SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRW, @"「这个洞……%PANT_K46(TARGET, "要", "通常")%……库、_H_真好_H_」"
		CASE 1
			CALL PRINT_STRW, @"「クク、随分と蕩けた声しやがって……_H_」"
		CASE 2
			CALL PRINT_STRW, @"「……チッ、こっちが翻弄されちまってるな……っ_H_」"
		CASE 3
			CALL PRINT_STRW, @"「……ったく、寝てる間に犯されてるってのに興奮しやがって、この淫乱が……_H_」"
		CASE 4
			CALL PRINT_STRW, @"「ったく、寝てる間もやらしいヤツだ……_H_」"
		ENDSELECT
	CASE "朝舐陰"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "Ｍ異物", 3+RAND:3)%……啊、你起来了」"
		IF RAND:2
			PRINTFORMW 「有点激动啊、……怎么、%CNAME("二人称")%也兴奋了吗？」
			CALL PRINT_STRW, @"「%CNAME("一人称")%来之前兴奋得内裤都湿了……_H_」"
		ELSE
			PRINTFORMW 「朝っぱらからすまないな……ま、%CNAME("二人称")%も嫌じゃあないんだろ？」
			CALL PRINT_STRW, @"「なんせこんだけ濡らしてるんだからな_H_」"
		ENDIF
	CASE "朝足交"

	CASE "朝の新妻PLAY"
		IF VAGINA(TARGET) && RAND:2
			PRINTFORMW 「お、似合ってるか？　そいつぁ良かった」
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「早上好、\@VAGINA(PLAYER) ? 亲爱的 # 亲爱的\@」
				PRINTFORMW 「……怎么。活了这么久了我还没当过这角色，这感觉很新鲜的」
			CASE 1
				PRINTFORMW 「お、ちょっと失礼してるぞ」
				PRINTFORMW 「……クク、たまにはこういうのも面白色だろ？」
			CASE 2
				PRINTFORMW 「っと、喜んでくれたようで何よりだな」
				CALL PRINT_STRW, @"「まったく、ノリで選んだ圍裙だってのに、『そういうの』専用になっちまうじゃねえか_H_」"
			ENDSELECT
		ENDIF
	CASE "朝乳交"
		PRINTFORMW 「おっとおはよう」
		CALL PRINT_STRW, @"「……クク_H_朝っぱらからおっ勃てやがって、生粋のスケベだなぁ_H_」"
	CASE "朝騎乗位"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 5)%」"
		CALL PRINT_STRW, @"「……これ、クセになっちまいそう……っ_H_」"
	CASE "朝Ａ騎乗位"
		IF RAND:2
			CALL PRINT_STRW, @"「ったく、朝っぱらからおっ勃てちまってなぁ_H_」"
			CALL PRINT_STRW, @"「そんなモンから……っ_H_%CNAME("一人称")%も興奮しちまったじゃねえか……あっ_H_」"
		ELSE
			CALL PRINT_STRW, @"「んっ_H_まったく、この精力超群がっ_H_」"
			CALL PRINT_STRW, @"「寝てるくせして随分と硬くしやがって……_H_」"
		ENDIF
	CASE "朝接吻"
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「早上好、睡美人」
			IF RAND:2
				PRINTFORMW 「……哎啊，有点尴尬」
			ELSE
				IF TRUE_SEX(MASTER) == "男性"
					PRINTFORMW 「……被这样说也尴尬吗？」
				ELSE
					PRINTFORMW 「库库、你脸红了，喜欢这样吗？」
				ENDIF
			ENDIF
		CASE 1
			PRINTFORMW 「お、今起きたか」
			PRINTFORMW 「何、たまにはこういう罗曼蒂克なことをしてみるのも良いモンだろ？」
		CASE 2
			PRINTFORMW 「おう、おはよう」
			PRINTFORMW 「……何だ、こういうの、%CNAME("二人称")%は好きじゃないのか？」
			PRINTFORMW 「……クク、顔が赤くなってるぞ」
		ENDSELECT
	CASE "Ｗ朝口交"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "Ｍ異物", 3+RAND:3)%」"
		PRINTFORMW 「……啊、你醒了」
		PRINTFORMW 「啊、既然碰到了。就顺路一起来把%CNAME("二人称")%『送上天』了」
		CALL PRINT_STRW, @"「ってことで、ま、もう少し付き合ってくれ_H_」"
	CASE "朝添い寝"
		PRINTFORMW 「……哦，你醒得真早啊」
		PRINTFORMW 「啊，也没什么事……」
		PRINTFORMW 「……昨晚谢谢你了」
	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		SELECTCASE RAND:10
		CASE 9
			PRINTFORMW 「お、ようやくお目覚めか。……夢見はどうだった？」
			CALL PRINT_STRW, @"「……さて、こっから第二ラウンドと洒落こもうじゃねえか。もちろん、%CNAME("二人称")%がしっかりと見える特等席でな_H_」"
		CASE 8
			PRINTFORMW 「おっと失礼、ま、すまなかったな」
			CALL PRINT_STRW, @"「こんな無防備な姿を見せられちまったら、満足できなくなっちまったのさ」"
		CASE 7
			PRINTFORMW 「……ん、何だ、接吻でもしてほしいのか？」
			PRINTFORMW 「ったくしょうがねえなぁ。ちょっと待ってろ……」
			CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "接吻", 3+RAND:3)%」"
		CASE 6
			PRINTFORMW 「……にしても、こいつぁ天賦の才、とでも言うべきかねぇ」
			CALL PRINT_STRW, @"「だってそう思うしかないだろ。こんなに男に喰らい付いて離さない様子なんて見せられちまったらさ_H_」"
		CASE 5
			PRINTFORMW 「ククッ、おはよう。随分と具合がよかったじゃねえか」
			PRINTFORMW 「……まだまだ満足してないみたいだし、もうしばらく%CNAME("一人称")%の女性にならないか？」
		CASE 4
			PRINTFORMW 「……ったく、寝ても覚めても淫乱なヤツだ」
			CALL PRINT_STRW, @"「だってそうだろう？　こんなにもマンコを湿透に濡らしちまってんだからさ_H_」"
		CASE 3
			PRINTFORMW 「哦、你终于醒了」
			PRINTFORMW 「这么激烈我还以为你早就醒了、没想到到现在才醒」
			PRINTFORMW 「如果你感觉不舒服的话就告诉我，不然在你射之前我都不会停的」
		CASE 2
			PRINTFORMW 「早、早上好」
			PRINTFORMW 「……哎呀、做爱这么激烈都现在才醒、你很累吗？」
			PRINTFORMW 「下次来叫你的时候、%CNAME("一人称")%就给你做个爽快的『按摩』吧」
		CASE 1
			PRINTFORMW 「哦、早上好」
			PRINTFORMW 「%CNAME("二人称")%很想干吧」
			CALL PRINT_STRW, @"「……看，刚睡醒，你的下面就在不停地要吃的了。_H_」"
		CASE 0
			PRINTFORMW 「早、早上好」
			PRINTFORMW 「……嗯、什么？　听不见啊」
			PRINTFORMW 「什么？　不想这样？　那由不得你」
			CALL PRINT_STRW, @"「你看、%CNAME("二人称")%的下面吸得好紧_H_」"
		ENDSELECT
	CASE "朝口交", "朝舐陰"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "Ｍ異物", 4+RAND:3)%」"
		SELECTCASE RAND:5
		CASE 0
			PRINTFORMW 「……嗯？ 啊、醒了？」
			PRINTFORMW 「再坚持一会」
			CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "Ｍ異物", 4+RAND:3)%」"
			SIF RAND:3 == 0
				CALL PRINT_STRW, @"「……库库_H_一大早就\@CSTR:朝袭 == "朝口交" ? 硬 # 湿\@得这么厉害啊_H_」"
		CASE 1
			PRINTFORMW 「……啊、你醒了」
			CALL PRINT_STRW, @"「最近好寂寞啊、照顾一下我这个老年人吧_H_」"
		CASE 2
			PRINTFORMW 「……っと、驚かせてすまないな」
			PRINTFORMW 「ま、そうは言っても気持ちよかっただろ？」
		CASE 3
			PRINTFORMW 「……っと、起きたのか」
			PRINTFORMW 「なに、まだまだ朝も早いからな」
			CALL PRINT_STRW, @"「もう少し床に眠ってろ_H_」"
		CASE 4
			PRINTFORMW 「……お、おはよう%CALLNAME:MASTER%」
			PRINTFORMW 「ま、つってもまだまだ満足してないんだろう？」
			IF CSTR:朝袭 == "朝口交"
				CALL PRINT_STRW, @"「だってこんだけチンコガッチガチにしてんだからな_H_」"
			ELSE
				CALL PRINT_STRW, @"「だってこんだけマンコビショビショにしてんだからな_H_」"
			ENDIF
		ENDSELECT
	CASE "朝騎乗位"
		CALL PRINT_STRW, @"「ん……_H_おっと、おはよう」"
		CALL PRINT_STRW, @"「ククッ、子宮に熱い精液が流れ込んでいるのがわかるぞ……_H_」"
		SELECTCASE RAND:3
		CASE 0
			CALL PRINT_STRW, @"「んっ……_H_まだやるのか？」"
		CASE 1
			CALL PRINT_STRW, @"「……っ_H_急に動きやがって……_H_」"
		CASE 2
			CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 5)%」"
			CALL PRINT_STRW, @"「いきなりっ_H_元気になりやがってっ_H_」"
		ENDSELECT
	CASE "朝Ａ騎乗位"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 4+RAND:3)%」"
		CALL PRINT_STRW, @"「嗯、_H_喂、_H_噫、_H_」"
		SELECTCASE RAND:4
		CASE 0
			CALL PRINT_STRW, @"「再来_H_再来点_H_我要_H_」"
		CASE 1
			CALL PRINT_STRW, @"「真厉害_H_就是这里_H_%PANT_K46(TARGET, "要", "通常")%你早上真精神_H_」"
		CASE 2
			CALL PRINT_STRW, @"「嗯、_H_突然变得好大……_H_%PANT_K46(TARGET, "要", "通常", 2)%」"
		CASE 3
			CALL PRINT_STRW, @"「……ククッ_H_もう少しするんだな……っ_H_」"
			CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 5)%」"
		ENDSELECT
	CASE "朝接吻"
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「哦、起来了」
			PRINTFORMW 「嗯？ 我在给你早安吻啊」
			IF TALENT:相思相愛
				PRINTFORMW 「我们是爱人。当然可以接吻啊」
			ELSE
				PRINTFORMW 「……没提前问你是我的错。我道歉」
			ENDIF
		CASE 1
			CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "接吻", 5+RAND:2)%」"
			PRINTFORMW 「……咦、终于醒了」
			PRINTFORMW 「哎呀、亏我刚才还在认真地亲你呢」
			PRINTFORMW 「明明这么热情都没有感受到、我有点受伤啊」
			PRINTFORMW 「……哎」
		CASE 2
			PRINTFORMW 「早、早上好%CNAME("主人")%」
			PRINTFORMW 「嗯？　为什么要和你接吻？」
			PRINTFORMW 「当然了，这是给睡美人的吻啊……给我说出鸡皮疙瘩了」
			PRINTFORMW 「……算了，你先醒一醒」
			CALL PRINT_STRW, @"「既然醒了、我们再来一次怎么样？　%CNAME("主人")%_H_」"
		ENDSELECT
	CASE "朝添い寝"
		PRINTFORMW 「嗯？　……啊……早上好」
		IF RAND:2
			IF COND("冬") && RAND:2
				PRINTDATA
					DATA 「思っていたより寒いからな、
					DATA 「……っと、身体が冷えちまってるな、
					DATA 「ほう、こりゃまた随分と冷えちまってるじゃないか。
				ENDDATA
			ELSE
				PRINTDATA
					DATA 「中々抱き心地が良いからな、
					DATA 「ま、偶にゃこういうのも良いだろ、せっかくだし
				ENDDATA
			ENDIF
			PRINTFORMW 再抱我一会把」
		ELSE
			IF TALENT:相思相愛 && RAND:2
				PRINTFORMW 「ん？　もしかして恥ずかしいのか？」
				PRINTFORMW 「お生憎様、案外こういうのも嫌いじゃないタチでね」
			ELSE
				PRINTFORMW 「何、偶にゃこういう甜ことしてみるのも乙ってモンだろ？」
			ENDIF
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今天有运动会吧」
		SELECTCASE RAND:3
		CASE 0
			IF COND("夏")
				PRINTFORMW 「天气很热啊。不要光喝水，还要多摄入无机盐和营养」
			ELSEIF COND("冬")
				PRINTFORMW 「天气好冷啊。记得多喝水」
			ELSE
				PRINTFORMW 「你要加油啊」
			ENDIF
		CASE 1
			PRINTFORMW 「しっかり\@FIND_COND("焰") ? %CNAME("焰")% # \@老师に絞られるんだぞー」
		CASE 2
			PRINTFORMW 「サボったからってこっちから言うことは特に無いが、後の事はフォローしてやらんからな」
		ENDSELECT
	ELSE
		IF TEXTS("曜日") == "日"
			PRINTFORMW 「嗯、今天是休息日啊」
			IF COND("朝袭相手") && COND("欲求不満")
				SELECTCASE RAND:3
				CASE 0
					CALL PRINT_STRW, @"「……せっかくだ、このまま続けちまおうじゃないか_H_」"
				CASE 1
					PRINTFORMW 「……さて、せっかくだしこのまま続けちまうか？」
					CALL PRINT_STRW, @"「%CNAME("二人称")%も随分と干劲みたいだしな_H_」"
				CASE 2
					PRINTFORMW 「つってもまだ満足できてねえんだよなぁ……」
					CALL PRINT_STRW, @"「……よし、%CNAME("二人称")%も付き合ってくれるよな_H_」"
				ENDSELECT
			ELSE
				PRINTDATAW
					DATA 「哈……再睡一下吧……」
					DATA 「玩会游戏吧……」
					DATAFORM 「要不要来一次？　%CNAME("主人")%」
					DATA 「……啊、还有好多游戏没玩……」
					DATA 「仕事は終わっているし、今日はどうするかねぇ……」
					DATA 「んー……さて、仕事は終わっているし、何をしようかねぇ……」
				ENDDATA
			ENDIF
		ELSE
			;IFが大量的あってわかりづらいのでひとつをSELECTCASEにすることで可読性を高める
			SELECTCASE RAND:3
			CASE 0
				PRINTFORM 「今天是新年啊……
				IF RAND:3 == 0
					PRINTFORMW 还要上课」
					PRINTDATAW
						DATAFORM 「%CNAME("二人称")%相手だからって贔屓する気はないからな。酱と勉強しろよ？」
						DATA 「……作業忘れたりとかしていないだろうな？」
						DATA 「今日の範囲は中々重要だからな、しっかりノートに書いてお君だぞー」
						DATA 「別に寝たりしてもどうとも言わないが、だからと言って手下留情してやるつもりはないからな」
						DATAFORM 「さて、せっかくだし今日の質問は%CNAME("二人称")%に集中させてやろうか……冗談だよ」
						DATA 「教科書を忘れてきたりとかはしていないよな？」
						DATA 「今日は最初の方だから、遅れたりするなよー」
						DATA 「今日は遅い方だが、豐腴したりするなよー」
						DATALIST
							DATAFORM 「……と、%CNAME("一人称")%は準備があるし、この辺りで失礼する」
							DATAFORM 「%CNAME("二人称")%も遅れたりはするなよー」
						ENDLIST
						DATALIST
							DATA 「と是え、まだそれなりに時間の余裕もあるからな。ゆっくり準備をしておいてくれ」
							DATAFORM 「%CNAME("一人称")%は先に行かせてもらうよ。授業の準備もあるからな」
						ENDLIST
						DATA 「ま、随意やっても問題ないが、他の老师を困らせるなよー」
						DATA 「今日は割合難しい箇所を取り扱うから、あんまり豐腴するなよー」
					ENDDATA
				ELSE
					PRINTFORMW 不用上课」
					PRINTDATAW
						DATA 「今日は部活まで合わないからって、手を拔出んじゃないぞー」
						DATA 「勉強酱と頑張れよー」
						DATA 「わからない所があったら放課後にでも聞いてくれ」
						DATAFORM 「%CNAME("二人称")%の勝手ではあるが、あんまり随意はやるなよ？」
						DATA 「ま、お互い頑張るとするか」
						DATA 「用事があれば職員室にでも来てくれ」
						DATA 「授業サボったりはすんなよー」
						DATA 「忘れものには気を付けろよー」
						DATA 「……と、ここに制服置いておくからな」
						DATA 「ま、擅長い具合にがんばれよ」
					ENDDATA
				ENDIF
			CASE 1
				PRINTFORMW 「啊，到时候了」
				PRINTFORMW 「%CNAME("一人称")%先走了、%CNAME("二人称")%不要迟到了」
			CASE 2
				PRINTFORMW 「っと、そろそろ出ないといけないな」
				PRINTFORMW 「早くに登校しても構わないなら送っていくがどうだ？」
			ENDSELECT
		ENDIF
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")
		PRINTFORMW 「今天是测试的日子啊」
		PRINTDATAW
