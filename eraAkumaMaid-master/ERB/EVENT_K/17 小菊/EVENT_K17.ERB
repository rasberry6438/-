		SELECTCASE CEVENTS("立場")
		CASE "主人様"
			LOCALS = 主人大人_主人大人
		CASE "父亲"
			LOCALS = 父亲_父亲
		CASE "お母桑"
			LOCALS = 母亲_母亲
		CASE "爸爸"
			LOCALS = 爸爸_爸爸
		CASE "妈妈"
			LOCALS = 妈妈_妈妈
		CASE "欧尼酱"
			LOCALS = 欧尼酱_欧尼酱
		CASE "欧内酱"
			LOCALS = 欧内酱_欧内酱
		CASEELSE
			LOCALS = YOU_YOU
		ENDSELECT
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
CASE "主人"
	LOCALS = 主人大人_主人大人
CASE "助手"
	LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
ENDSELECT
RETURNF LOCALS

