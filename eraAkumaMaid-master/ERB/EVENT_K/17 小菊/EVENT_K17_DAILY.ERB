	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1

	IF COND("体育")

	ELSE

	ENDIF
CASE "一番乗り"

CASE "ただいま"
	IF RAND:4 == 0 && CEVENT("イベント：学校")
		PRINTFORMW 「学校、通ってたの」
		PRINTFORMW なにをしていたか訊ねると、%CALLNAME:TARGET%は当たり前のように答えた
		PRINTFORMW 保健室登校ならぬ美術準備室登校とでも言うのか
		PRINTFORMW しかし、いたりいなかったり（？）するのは構わないのだろうか……？

	ELSEIF RAND:3 == 0 && CEVENT("イベント：学校")
		PRINTFORMW 「学校で……モデル、してたの」
		PRINTFORMW 当被问起在做什么的时候、%CALLNAME:TARGET%的身上飘出了%TEXTR("得意……大概吧、/得意/疲惫")%的气息
		PRINTW ……モデルと言っても、静物画のモチーフなわけだが……

	ELSEIF RAND:2 == 0 && ABL:信頼 >= 5
		PRINTFORMW 「我回来了、%CNAME("二人称")%」
		PRINTFORMW %CALLNAME:TARGET%提着购物袋。
		IF RAND:3 == 0 && COND("同居中",FINDCHARA(NO, 1))
			PRINTFORMW %CALLNAME:FINDCHARA(NO, 1)%のおつかいだろうか……

		ELSEIF RAND:2 == 0 && COND("同居中",FINDCHARA(NO, 10))
			PRINTFORMW %CALLNAME:FINDCHARA(NO, 10)%のおつかいだろうか……

		ELSE
			PRINTW 买了什么东西呢……

		ENDIF

	ELSEIF RAND:2 == 0
		PRINTFORMW 「去散步了」
		PRINTFORMW 问她去干什么的时候、%CALLNAME:TARGET%看起来%TEXTR("有些失落/有些開心")%的样子。
		PRINTFORMW 散步的时候发生了什么呢……

	ELSE
		PRINTFORMW 「我回来了、%CNAME("二人称")%」
		PRINTFORMW 不知道从哪里回来的%CALLNAME:TARGET%突然低下頭。

	ENDIF
	
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF COND("Ｈ的サプライズ可能") && RAND:5 == 0
			PRINTFORMW 「おかえり、%CNAME("二人称","甘え")%……%CNAME("一人称")%、%CNAME("二人称")%にかまって……ほしいな……」
			PRINTFORMW %CALLNAME:TARGET%靠近%CALLNAME:MASTER%不停地诱惑着……

		ELSE
			SELECTCASE RAND:20
				CASE 0 TO 9
					PRINTFORMW 「……欢迎回来、%CNAME("二人称")%」
					PRINTFORMW %CALLNAME:TARGET%似乎在%TEXTR("看电视/读书/发呆/打瞌睡")%……

				CASE 10 TO 17
					PRINTFORM 「欢迎回来、%CNAME("二人称")%……
					SIF ABL:信頼 >=3
						PRINT 有什么想要%CNAME("一人称")%做的事情吗……？
					PRINTW 」
