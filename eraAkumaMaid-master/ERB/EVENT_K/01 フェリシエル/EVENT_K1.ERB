;-------------------------------------------------
;女仆の悪魔 口上
;台詞が少ないせいも有角色が掴みづらいと思われるので、エロシーンなどのセリフが書かれるまでは加筆は待ってください
;-------------------------------------------------
;呼び名の設定。""の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K1, ARGS
#FUNCTIONS
#DIM LCOUNT
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	ACTOR = PLAYER
CASE "主人"
	LOCALS = 主人大人_主人大人
CASE "助手"
	ACTOR = ASSI
CASEELSE
	;ARGSに角色名が入れられてる場合には角色への呼び方なので格子
	ACTOR = FIND_NO(ARGS)
ENDSELECT

SIF LOCALS != ""
	RETURNF LOCALS

;相手が主人
IF ACTOR == MASTER
	LOCALS = 主人大人_主人大人
;相手が主人ではない
ELSE
	SELECTCASE CSTR:ACTOR:称号
	CASE "未亡人悪魔(実母)"
		LOCALS = 母亲大人_母亲大人
	CASE "魔貴族", "没落魔貴族"
		LOCALS = %CALLNAME:ACTOR%様_%CSTR:ACTOR:ふりがな%大人
	CASEELSE
		SELECTCASE CONDS("学年", ACTOR)
		CASE "初等部", "中等部", "１年生"
			IF TALENT:ACTOR:男性
				LOCALS = %CALLNAME:ACTOR%桑_%CSTR:ACTOR:ふりがな%桑
			ELSE
				LOCALS = %CALLNAME:ACTOR%酱_%CSTR:ACTOR:ふりがな%酱
			ENDIF
		CASE "２年生"
			LOCALS = %CALLNAME:ACTOR%桑_%CSTR:ACTOR:ふりがな%桑
		CASE "３年生"
			LOCALS = %CONDS("略称", ACTOR)%前輩_%CONDS("略称", ACTOR)%前輩
		CASE "教員"
			LOCALS = %CALLNAME:ACTOR%老师_%CSTR:ACTOR:ふりがな%老师
		CASEELSE
			LOCALS = %CALLNAME:ACTOR%桑_%CSTR:ACTOR:ふりがな%桑
		ENDSELECT
	ENDSELECT
ENDIF

RETURNF LOCALS

