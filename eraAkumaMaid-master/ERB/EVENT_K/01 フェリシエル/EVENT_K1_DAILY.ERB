;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K1, ARGS
#DIM MEMO_LINECOUNT
;打招呼
#DIMS GREETING
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

IF COND("夜")
	GREETING = 晚上好
ELSEIF COND("昼")
	GREETING = 你好
ELSE
	GREETING = 早上好
ENDIF

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STRW, "「呼呼呼……_H_」"
	CALL PRINT_STRW, "「离晚上还有些时间呢……充分、一起享受吧……_H_」"
CASE "朝袭"

CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝騎乗位"
		IF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STRW, @"「%CNAME("二人称")%、早上好……_H_」"
		ELSE
			PRINTFORMW 「%CNAME("二人称")%、早上好」
		ENDIF
	CASEELSE
		PRINTFORMW 「%CNAME("二人称")%、早上好」
	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1
	IF COND("体育")
		IF RAND:2
			PRINTFORMW 「今天是有体育课的日子、体操服和毛巾有带着吗？」
		ELSE
			PRINTFORMW 「今天是有体育的日子、不能一直盯着女孩子的屁股看哟」
		ENDIF
	ELSEIF RAND:2
		PRINTFORMW 「那么今天要做什么呢？」
	ELSE
		PRINTFORMW 「已经准备好饭菜了……来、这边请」
	ENDIF
CASE "登校"

CASE "一番乗り"
	IF RAND:2
		PRINTFORMW 「嗯。其他人好像迟到了」
	ELSE
		PRINTFORMW 「今天的%CNAME("一人称")%包场了哟」
		PRINTFORMW 「……開玩笑的、哼哼♪」
	ENDIF
	;部活の標語
	SELECTCASE SAVESTR:標語
	CASE "部活中は内衣姿で"
		PRINTFORMW 专心悬挂标语的%CALLNAME:TARGET%无意中露出了内衣、春光乍泄的一幕正好被%CALLNAME:MASTER%看见了……
		CALL SETFLAG, "变成内衣姿态", TARGET
	CASEELSE
		IF COND("Ｈ的サプライズ可能") && TALENT:巨乳 && RAND:3 == 0
			PRINTFORMW 「……要先開始吗？」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の腕を取り、%TOFULL(CONDS("罩杯サイズ") )%罩杯の%TALENT_NAME(GETNUM(TALENT, "巨乳"), TALENT:巨乳)%で挟んできた……
		ENDIF
	ENDSELECT
CASE "ただいま"
	PRINTFORMW 「我回来了」
	PRINTFORMW 「不在家的时候%CNAME("一人称")%寂寞了吗？呼呼♪」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		MEMO_LINECOUNT = LINECOUNT
		IF COND("Ｈ的サプライズ可能") && RAND:3 == 0
			IF TEXTS("曜日") == "土" && EQUIP:服 && ABL:露出癖 >= 3
				CALL SETFLAG, "变成内衣姿态", TARGET
				CALL EMOTION_PLUS, "交流中"
				PRINTFORMW 「%CNAME("二人称")%%TEXTR("、辛苦了/、今天一天也辛苦了")%」
				PRINTFORMW %CALLNAME:TARGET%出来迎接了……不知道为什么只穿着内衣。
				PRINTFORMW 「这周终于结束了呢。稍稍给你一点%CNAME("二人称")%的杀必死服务哟~」
				PRINTFORMW 「呼呼♪」
			ELSEIF PENIS(MASTER) && CFLAG:MASTER:汗 && TALENT:气味弱点
				PRINTFORMW 「%CNAME("二人称")%%TEXTR("、辛苦了/、今天一天也辛苦了")%」
				PRINTFORMW 「今天出汗了呢……你看、真是了不得的程度啊」
				PRINTFORMW %CALLNAME:TARGET%很自然地跪到了%CALLNAME:MASTER%面前、把%CALLNAME:MASTER%的阴茎取了出来。
				PRINTFORMW 「呼呼呼、啊啦啊啦。取出这个\@ COND("假性包茎", MASTER) ? て皮を剥い # \@的瞬間%TEXTR("气味")%好惊人哦」
				PRINTFORMW 「……怎么了嘛、一副奇怪的表情」
				PRINTFORMW 「%CNAME("二人称")%之类的女性、双腿被打開的时候%TEXTR("气味")%之类的、你不也是有点想闻闻看嘛」
				CALL PRINT_STR, "「……阴茎单纯只有臭味吗？真是的、真是个不坦率的孩子呢。别扭的家伙……就要这様_H_」_W"
				PRINTFORMW %CALLNAME:TARGET%伸出软滑的舌頭把\@ COND("假性包茎", MASTER) ? 的外皮退下 # 的前端包住 \@、用口叼住\@ COND("体育") ? 在昼の体育上 # 一日の間に \@蓄積的脏东西掃除干净……
			ENDIF
		ENDIF

		;何か発言してたらここまで
		SIF LINECOUNT > MEMO_LINECOUNT
			RETURN 1

		PRINTFORMW 「%CNAME("二人称")%%TEXTR("、辛苦了/、今天一天也辛苦了")%」
		IF RAND:5 == 0
			PRINTFORMW 「今天一天也很辛苦呢。不要拼命过頭了哦」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「饭做好了、先来喝茶吧」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「啊啦啊啦、就这様随便放着的话衣服会起皱褶的」
			PRINTFORMW %CALLNAME:TARGET%把%CALLNAME:MASTER%随手一扔的衣服好好挂了起来。
		ELSEIF RAND:2
			PRINTFORMW 「口渴了吗？茶已经沏好了」
		ELSE
			PRINTFORMW 「我正在泡茶、先去洗把脸吧。」
		ENDIF
		CALL SETFLAG_DAILY, "茶を用意", TARGET
	ELSE
		IF COND("同伴中", MASTER) && RAND:2
			PRINTFORMW 「%CNAME("partner")%也在一起。说了什么有趣的话题吗？」
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「还以为要迟到了……呼（松了一口气)……」
			ELSEIF RAND:2
				PRINTFORMW 「啊啦、%CNAME("二人称")%今天可有得忙了♪」
			ELSE
				PRINTFORMW 「%CNAME("二人称")%呀、到底是从哪条近道传过来的啊」
			ENDIF
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF COND("Ｈ的いちゃつき中", MASTER)
		IF RAND:2
			PRINTFORMW 「打扰了…………啊啦啊啦、看来是真的打扰了呢」
			PRINTFORMW 「啊呀、继续继续、还请不要在意%CNAME("一人称")%」
		ELSE
			PRINTFORMW 「对不起、我迟到了……」
			PRINTFORMW 「啊啦、在干什么呢？%CNAME("二人称")%」
			PRINTFORMW 「真是的、太色情了！」
		ENDIF
	;其他特定の行動に対する反応
	ELSE
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "甜言蜜语"
			CALL PRINT_STRW, "「失礼了……呼呼_H_」"
			PRINTFORMW %CALLNAME:TARGET%对着正说教到一半的%CALLNAME:MASTER%送去一个微笑。
		ENDSELECT
	ENDIF

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF CFLAG:呼び出された
		PRINTFORMW 「来啦、%CNAME("二人称")%的忠实的仆从、%NAME:TARGET%哟。请问有什么需求吗」
		PRINTFORMW %CALLNAME:TARGET%发出啪嗒啪嗒的脚步声站着进了房间
	ELSEIF CFLAG:MASTER:茶 == 0
		PRINTFORMW 「对不起、我迟到了……马上为您准备茶水」
		CALL SETFLAG_DAILY, "茶を用意", TARGET
	ELSE
		PRINTFORMW 「早上好」
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「来啦、%CNAME("二人称")%的忠实的仆从、%NAME:TARGET%哟。请问有什么需求吗」
		PRINTFORMW %CALLNAME:TARGET%发出啪嗒啪嗒的脚步声站着进了房间
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;既に部員の３分の２が集合済
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*2/3 && RAND:3 == 0
		PRINTFORMW 「对不起，我迟到了ー」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「来啦、%CNAME("二人称")%的忠实的仆从、%NAME:TARGET%哟。请问有什么需求吗」
		PRINTFORMW %CALLNAME:TARGET%发出啪嗒啪嗒的脚步声站着进了房间
	ELSE
		PRINTFORMW 「早上好」
	ENDIF
;簡易入室打招呼に対する返し。ここでは括弧に『』を使う事。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		ACTOR = FIND_COND("教員", "入室打招呼中")
		IF RAND:2
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%您好。刚刚失礼了』
		ELSE
			PRINTFORMW 『啊、%CNAME(CALLNAME_ACTOR)%。您辛苦了』
		ENDIF
	ELSEIF PLACE("自宅")

	ELSE
		IF RAND:2
			PRINTFORMW 『贵安、%CNAME(CALLNAME_ACTOR)%』
		ELSE
			PRINTFORMW 『啊啦、早上好』
		ENDIF
	ENDIF

CASE "抜け出された"
	IF RAND:2
		PRINTFORMW 「唔嗯嗯嗯嗯（〃｀ 3′〃）～～……」
		PRINTFORMW 「不。没什么╭(╯^╰)╮」
		PRINTFORMW 稍稍透露着不满表情的%CALLNAME:TARGET%、没有引起别人的注意……
	ELSE
		PRINTFORMW 「あら。%CNAME("partner")%との用事がお有でしたか……善后工作は%CNAME("一人称")%がしておきますね」
		PRINTFORMW 「请继续享受快乐时光吧~」
		PRINTFORMW %CALLNAME:TARGET%透露出少许寂寞的表情、目送着%CALLNAME:MASTER%离去
	ENDIF
CASE "便当"
	IF RAND:4 == 0
		PRINTFORMW 「一起吃便当真開心呢……哎呀？」
		PRINTFORMW 「饭粒沾到脸上咯、%CNAME("二人称")%」
		PRINTFORMW %CALLNAME:TARGET%伸手把%CALLNAME:MASTER%の%TEXTR("唇/脸颊")%上的饭粒摘了下来。
	ELSEIF RAND:3 == 0
		PRINTFORMW 「这様真好哟。总觉得、像是関系很好的正常人类朋友一様」
		PRINTFORMW 「……嗯？正常朋友才不会给对方做便当吗？那你怎么看？」
	ELSEIF RAND:2
		PRINTFORMW 「来、%CNAME("二人称")%、啊～～～ん♪」
		PRINTFORML 「啊~（张嘴」
		PRINTFORMW %CALLNAME:TARGET%送往%CALLNAME:MASTER%嘴中的筷子、突然掉了个頭自己一口吃掉了！
		PRINTFORMW 「啊嗯啊嗯……呼呼呼~、開个玩笑而已嘛、不要摆出那様别扭的表情♪」
	ELSE
		PRINTFORMW 「味道怎么様？这道凉拌%TEXTR("菠菜/小油菜/芜菁叶")%」
		PRINTFORMW 「这种清脆的口感、连我自己都觉得做得很不错哦」
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K1, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「从今天起、和%CNAME("一人称")%在一起的共同生活開始了哦。呼呼~」
	PRINTFORML %CALLNAME:TARGET%坐在%CALLNAME:MASTER%的床上、像偷到了鱼儿的猫一様笑眯眯的
CASE "浑身是汗"
	IF CHECK_CLO("体操服姿") && RAND:2
		IF RAND:2
			PRINTFORML 「%CNAME("二人称")%一起来怎様？偶尔做一下对身体比较好哦」
			PRINTFORML 身着体操服的%CALLNAME:TARGET%在床上做着柔软体操
		ELSE
			PRINTFORML 「嗯？在意为什么是穿着体操服的様子吗？」
			PRINTFORML 「不是很好吗？你喜欢对吧？这様的打扮」
			PRINTFORML 运动了一会后、%CALLNAME:TARGET%的汗水从頭发上洒了下来、空气中漂浮着%TEXTS("气味")%的味道……
		ENDIF
	ELSEIF RAND:2
		PRINTFORML 「嗯……有点累了」
		PRINTFORML %CALLNAME:TARGET%双手抱在頭后露出娇嫩的腋下、一边扭动着身体、伸展着肌肉。
	ELSE
		PRINTFORML 「呜。汗水流出来了」
		PRINTFORML 「……不会有臭味吧、%CNAME("一人称")%？」
		PRINTFORML %CALLNAME:TARGET%凑向领口嗅了嗅、%TEXTS("气味")%
	ENDIF
CASE "刚睡醒"
	IF RAND:3 == 0 && CHECK_CLO("体操服姿")
		PRINTFORML 「一、二、三…………」
		PRINTFORML %CALLNAME:TARGET%想要轻轻地活动身体驱散睡意。
	ELSEIF RAND:2
		PRINTFORML 「…………」
		PRINTFORML %CALLNAME:TARGET%看起来很困的様子……声音逐渐低沉了下去。
	ELSE
		PRINTFORML 「唔～～嗯………睡得真好啊」
		PRINTFORML %CALLNAME:TARGET%心情舒畅地伸了个懒腰。
	ENDIF
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K1
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「啊啊啊……%CNAME("一人称")%、以及%CNAME("二人称")%……」
	CALL PRINT_STR, "「呼呼呼_H_被喜欢的人说我爱你……最棒了_H_」_L"
	PRINTFORML %CALLNAME:TARGET%展露出至今为止从未见过的幸福表情、身子都激动得微微颤抖…………
CASE "契約成立"
	IF RAND:3 == 0
		PRINTFORML 「呼。这様的话就可以暂且不介意工作指标一起生活了」
		PRINTFORML 说起来%CALLNAME:TARGET%、真的有工作指标这种说法吗？
	ELSEIF RAND:2
		PRINTFORML 「复活技能真的好厉害啊……身体真的没什么问题吗？」
		PRINTFORML 「如果是普通人的话、渐渐变得衰弱下去也不奇怪。」
	ELSE
		PRINTFORML 「我们做了很好的交易呢……呼呼♪」
		PRINTFORML %CALLNAME:TARGET%欣赏着收集到的灵魂、看起来很開心的様子。
	ENDIF
CASE "買東西"
	;買ってきた道具などで分岐
	IF COND("巨大振動棒購入") && DEVENT("巨大振動棒感想") == 0
		IF COND("買上品数") >= 3
			PRINTFORML 「买了很多东西呢」
			PRINTFORML 「这个什么啊、好大一根……什么时候才有用到的机会呢？你在打什么坏主意呢？」
		ELSE
			PRINTFORML 「辛苦了呢、%CNAME("二人称")%」
			PRINTFORML 「但是这个……也太大了！什么时候才能塞进去？你在想什么呢？」
		ENDIF
		PRINTFORML %CALLNAME:TARGET%看着战利品、玩味着从背后抱住%CALLNAME:MASTER%、羞红的脸颊蹭了过去。
		CALL SET_DEVENT, "巨大振動棒感想"
	ELSE
		SELECTCASE COND("買上品数")
		CASE IS >= 4
			PRINTFORML 「您购物辛苦了、%CNAME("二人称")%」
			PRINTFORML 「话说回来……买了很多东西呢」
		CASEELSE
			PRINTFORML 「呼。您工作结束了、%CNAME("二人称")%」
			PRINTFORML 「之后的工作就交给%CNAME("一人称")%吧！」
			PRINTFORML %CALLNAME:TARGET%仿佛跳舞一様、优雅又迅速地将纸袋的内容分类放好。
		ENDSELECT
	ENDIF
CASE "摆脱", "主人公的房間に連れ込まれた", "自室に招待"
	IF RAND:2
		PRINTFORML 「呐、%CNAME("二人称")%、现在……你准备干什么呢？」
		PRINTFORML 「可以理解为是在刚才那个地方做不到的事情吧？」
		PRINTFORML 「……呼呼呼♪」
	ELSE
		IF RAND:3 == 0
			PRINTFORML 「呼呼…真是可爱的様子呢」
		ELSE
			PRINTFORML 「是要这様吗……好紧张呢♪」
		ENDIF
		IF PENIS(MASTER) && RAND:2
			IF RAND:2 == 0
				PRINTFORML %CALLNAME:TARGET%用嘴把%CALLNAME:MASTER%胯间的拉链打開、并亲了一口弹在脸上硬邦邦的肉棒。
			ELSE
				PRINTFORML %CALLNAME:TARGET%跪在%CALLNAME:MASTER%面前、環抱住腰并笑吟吟地用脸蹭着硬邦邦的肉棒。
			ENDIF
		ELSE
			PRINTFORML %CALLNAME:TARGET%心里描绘着接下来要发生的事情、脸上泛着红晕
		ENDIF
	ENDIF
CASE "自室に訪問された"
	;指导者を伴っている場合
	IF FIND_COND("現在存在", "助手")
		PRINTFORML 「啊啦、%CNAME("二人称")%に%CNAME(CSVCALLNAME(NO:ASSI) )%、欢迎光临」
		PRINTFORML 「要来点什么？」
	ELSE
		PRINTFORML 「哦呀%CNAME("二人称")%。要来点什么？」
		CALL PRINT_STRL, @"「难道、%CNAME("一人称")%不在就会寂寞吗？呼呼_H_」"
	ENDIF
CASE "指导者部屋訪問"
	PRINTFORML 「%CNAME(CSVCALLNAME(NO:ASSI) )%、真是失礼了」
	PRINTFORML %CALLNAME:TARGET%在不妨碍%CALLNAME:MASTER%和%CALLNAME:ASSI%的旁边坐了下来。
;自宅移動の後に你に連れられて居間に回来了場合など。優先度は低いです
CASE "帰還"
	;PRINTFORML 
CASE "オナバレ"
	PRINTFORML 「啊、那个、%CNAME("二人称")%刚才不是那様的……不、那个……呜呜呜」
	PRINTFORML %CALLNAME:TARGET%对自己的行为感到羞耻、声音越来越小……
CASE "露脐装超短裙胖次チラ"
	;超短迷你の時にはスルーしたり
	IF CHECK_SKIRT("超短迷你") && RAND:2
	ELSEIF RAND:2 && PENIS(MASTER)
		PRINTFORML 「%CNAME("二人称")%……看见了吧？」
		PRINTFORML 「装糊涂也不行哟……你看、为什么会变成那様？」
		PRINTFORML %CALLNAME:TARGET%指着%CALLNAME:MASTER%的股间窃笑道。
	ELSEIF CFLAG:甜言蜜语 || CFLAG:心情好 || CHECK_SKIRT("超短迷你")
		PRINTFORML 「怎么了？看到什么好东西了吗？」
		CALL PRINT_STR, "「……フフ_H_」_L"
		PRINTFORML %CALLNAME:TARGET%面色微红、開心地微笑着。
	ELSE
		PRINTFORML 「…………？ ……！」
		PRINTFORML 「%CNAME("二人称")%、难道说……看到了吗？」
		PRINTFORML 注意到%CALLNAME:MASTER%的视线、%CALLNAME:TARGET%一下子红了脸、伸手护住春光。
	ENDIF
