		IF TALENT:ARG:淫乱
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("Morning/早上好")%_HH_」_L"
			IF RAND:2 == 0
				CALL PRINT_STRW, @"「%TEXTR(@"人家可是忍耐了一晚上/看到这么精神的肉棒……怎么可能忍得住啊")%_HH_」"
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("哈啊/嗯")%_HH_　%TEXTR(@"%CNAME("二人称")%的量还是一如既往的可怕啊/射出来这么多、清理起来可是很麻烦的呀")%_HH_」"
			ENDIF
		ELSE
			PRINTFORML 
		ENDIF
	CASE "朝舐陰"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("ちゅっ…じゅるるっ、ちゅっちゅっ/じゅるるっ…んっ_HH_　ちゅっ、ちゅう")%_HH_」_L"
			CALL PRINT_STR, @"「%TEXTR("おはようございます/お目覚めですか、おはようございます")%_HH_　%TEXTR("綺麗にしますから待っててくださいね/綺麗になるまでペロペロしてさしあげますからね")%_HH_」_W"
		ENDIF

	CASE "朝足交"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("Bonjour（法：早上好）/早上好")%_HH_　%CNAME("主人")%」_L"
			PRINTFORMW 「%TEXTR("お願いされた/頼まれていた")%街道に起こしに来ました。あの……%TEXTR("痛かったらいってくださいね/痛くないでしょうか？")%」
		ENDIF
	CASE "朝の新妻PLAY"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 「%TEXTR("早上好/你好（Bonjour）")%%TEXTR(@"吃早饭了/今天是%CNAME("一人称")%亲手做的菜")%」
			PRINTFORML 「%TEXTR("きゃあっ/えっ、何です")%！？　%TEXTR(@"ご飯より先に%CNAME("一人称")%が食べたいんですか……もう/こらっ……してもいいですけど、怖い女仆桑が起きてくるまでですよ？/もう……いっぱい中に出してくれるって約束してくださいね/……本当はダメですからね、今回は特別ですよ")%」
		ENDIF

	CASE "朝乳交"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("はぁっ…んっ……/んぅ…んっ……")%　おはようございます、%CNAME("主人")%_HH_」_L"
			CALL PRINT_STR, @"「朝なのに起きてくださらないから……%TEXTR("ちょっと悪戯しちゃいました/ご奉仕して起こしてみました")%_HH_」_W"
		ENDIF
	CASE "朝騎乗位"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			IF RAND:2 == 0
				CALL PRINT_STRL, @"「%TEXTR("あっ、あんっ…ああっ、あっ/んっ、んんっ、んぅっ、んうっ")%_HH_」"
				CALL PRINT_STRW, @"「%TEXTR("おはようございます、/おはよう、ちゅっ_HH_　")%%CNAME("主人")%。%TEXTR("朝から元気いっぱいで、その……したくなってしまって……ごめんなさい/起こし来了んですけど、起きてくださらないのに立派になっておられたので……はしたないですよね……")%」"
			ELSE
				CALL PRINT_STRL, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_　%TEXTR("ボンジュール（Bonjour）/おはようございます")%。%CNAME("主人")%」"
				CALL PRINT_STRW, @"「%TEXTR("んっ、あっ/んっ、んっ、んんっ")%、%TEXTR(@"久しく乗馬の練習をしていないなって思って、%CNAME("主人")%に跨っちゃいました/乗馬は聖騎士の嗜みですから、%CNAME("主人")%も乗りこ无てみせますよ")%_HH_」"
			ENDIF
		ENDIF
	CASE "朝Ａ騎乗位"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			CALL PRINT_STRL, @"「%TEXTR("ボンジュール（Bonjour）/おはようございます")%_HH_　%CNAME("主人")%」"
			IF RAND:2 == 0
				CALL PRINT_STRW, @"「%CNAME("一人称")%の……%TEXTR("お尻/肛門/不浄の穴")%が%TEXTR("昨日/昨晩/夜")%から%TEXTR("落ち着かなくって/ウズウズしちゃって")%、%TEXTR(@"自己も慰めたんですけど……ダメだったんです/どうしても%CNAME("主人")%のが欲しくなっちゃいまいた")%……%TEXTR("あっ/んっ")%_HH_」"
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_　%TEXTR("今朝/今日")%の%CNAME("一人称")%の%TEXTR("お尻/肛門/不浄の穴")%の%TEXTR("具合/締めつけ/心地よさ")%はどうですか？　%TEXTR("昨晩/昨日の夜")%から%TEXTR(@"ずっと%CNAME("二人称")%サイズのディルドで拡張してきたんです。こんな悪い子にはお仕置きが必要ですよね？/入れていた塞子を抜いたばかりですから、%CNAME("一人称")%も疼いちゃって……ごめんなさい。はしたないですよね")%」"
			ENDIF
		ENDIF
	CASE "朝接吻"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("ちゅ/ちゅっ/ちゅぅ")%_HH_」_L"
			IF RAND:2 == 0
				CALL PRINT_STR, @"「%TEXTR(@"元気が出る接吻ですよ。今日一日頑張ってくださいね/おはようございます、%CNAME("主人")%/いい朝ですね、今日も一日頑張りましょう")%」_W"
			ENDIF
		ENDIF
	CASE "朝添い寝"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			PRINTFORML 「%TEXTR("ボンジュール（Bonjour）/おはようございます")%……%TEXTR(@"起こし来了のですが寝顔を見ていたら%CNAME("一人称")%も眠たくなってしまいました/起こしにきたつもりが一緒に寝ちゃってましたね")%
			PRINTFORMW 「%TEXTR(@"あの……もう少しだけこうしていてもいいですか？　/あと少しだけ……このままでいさせてください。")%%CNAME("主人")%」
		ENDIF
	CASE "朝六九式"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			PRINTFORML 「%TEXTR("んっ、れろっ、れろ……/んっ……んっ、ちゅっ、ちゅぅ……")%　おはようございます」
			PRINTFORMW 「%TEXTR(@"朝のご奉仕をしていたら%CNAME("一人称")%も湿的てきちゃいました……折角ですから互いにご奉仕しませんか？/中々起きてくれないのでご奉仕させていただきました……あっ、%CNAME("一人称")%の阴蒂酱、ダメェ。感じちゃいます")%」
		ENDIF
	CASE "朝口交"
		;恋慕
		IF TALENT:ARG:恋慕
			PRINTFORML 
			PRINTFORML 「%TEXTR("んっ、ちゅっ……/んぅ……んっ、ちゅぅ……")%　おはようございます」
			IF RAND:2 == 0
				CALL PRINT_STRW, @"「%CNAME("主人")%を起こし来了んですが、ぐっすり寝ておいでだったので……%TEXTR("悪戯しちゃいました/ご奉仕しちゃいました")%_HH_」"
			ELSE
				CALL PRINT_STRW, @"「酱と起きない悪い子は、んっ_HH_……%TEXTR("じゅるるっ、じゅぽっじゅぽっ/ジュルジュルっ、ジュルルっ、グポッグポッ")%、%TEXTR("もっとお仕置きしちゃいますよ/おしゃぶりの刑にしちゃいますよ")%？_HH_」"
			ENDIF
		ENDIF
	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:ARG:朝袭
	CASE "朝口交"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%, %CSTR:ARG:朝袭%
		[ENDIF]
	CASE "朝騎乗位"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%, %CSTR:ARG:朝袭%
		[ENDIF]
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%
		[ENDIF]
	ENDIF
CASE "一番乗り"
	[IF_DEBUG]
		PRINTFORMW KOJO_GREETING_KX1, %ARGS%
	[ENDIF]
CASE "ただいま"
	[IF_DEBUG]
		PRINTFORMW KOJO_GREETING_KX1, %ARGS%
	[ENDIF]
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%(夜)
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%(昼)
		[ENDIF]
	ENDIF

CASE "入室打招呼"
	IF CFLAG:ARG:呼び出された
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%
		[ENDIF]
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中")
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%
		[ENDIF]
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%(既に部員の大半集合済み)
		[ENDIF]
	ELSEIF CFLAG:ARG:呼び出された
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%
		[ENDIF]
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAMET(ARG, CSVNAME_ACTOR) で出来る
	CSVNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%(部室に老师がいる)
		[ENDIF]
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%(相手が現れるのが遅かった)
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_KX1, %ARGS%
		[ENDIF]
	ENDIF

CASE "居間に現れた"
	IF CFLAG:ARG:呼び出された
		PRINTFORMW 
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間", ARG)
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "便当"
	[IF_DEBUG]
		PRINTFORMW KOJO_GREETING_KX1, %ARGS%
	[ENDIF]
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"

ENDSELECT

