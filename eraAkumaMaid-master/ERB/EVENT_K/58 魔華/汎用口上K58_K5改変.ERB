		LOCAL = 2
	ELSE
		LOCAL = 1
	ENDIF
	;情緒が高い場合の追加口上
	SIF LOCAL >= RAND:4 && LOCAL >= 4
		LOCALS = %TEXTR("啾~/嗯啾~/嗯~/嗯嗯~")%%UNICODE(0x2661)%　%LOCALS%
