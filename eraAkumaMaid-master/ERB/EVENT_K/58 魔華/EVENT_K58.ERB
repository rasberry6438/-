	LOCALS  = びゅりゅびゅりゅ_びゅりゅびゅりゅ/びゅーびゅー_びゅーびゅー/どくどく_どくどく/ぴゅっぴゅ_ぴゅっぴゅ/びゅくびゅく_びゅくびゅく/ぴゅゆぴゅりゅ_ぴゅるぴゅる/
	LOCALS += "ビュリュビュリュ_びゅりゅびゅりゅ/ビュービュー_びゅーびゅー/ドクドク_どくどく/ピュッピュ_ぴゅっぴゅ/ビュクビュク_びゅくびゅく/ピュルピュル_ぴゅるぴゅる/"
CASE "子宮"
	LOCALS  = 子宮_しきゅう/子宮_しきゅう/子宮_しきゅう/お腹の奥_おなかのおく/メスの器官_めすのきかん/精液タンク_せいえきたんく/
	LOCALS += "子宮_しきゅう/子宮_しきゅう/子宮_しきゅう/お腹の奥_おなかのおく/"
	LOCALS += "精液捨場_ざーめんすてば/スペルマ廃棄所_すぺるま是きじょ/オス液投棄場_おすえきとうきば/排泄先_是せつさき/"
CASE "膣"
	LOCALS  = おまんこ_おまんこ/肉飞机杯_にくおなほ/チンポしごき穴_ちんぽしごきあな/精液絞り器_ざーめんしぼりき/シコシコ穴_しこしこあな/
	LOCALS += "オネダリ穴_おねだりあな/飞机杯_飞机杯/チンコキホール_ちんこきほーる/コキ捨て穴_こきすてあな"
CASE "陰茎形容"
	SELECTCASE SIZE("阴茎")
	;馬根、規格外
	CASE IS >= 3
			LOCALS = バケモノ_ばけもの/猛獣_もうじゅう/殺人_さつじん/人外_じんがい/
	;巨根
	CASE 2
			LOCALS = 凶悪_きょうあく/極悪_ごくあく/極太_ごくぶと/凶暴_きょうぼう/
	;普通
	CASE 1
			LOCALS = 精力超群_ぜつりん/変態_へんたい/ヤリチン_やりちん/つよつよ_つよつよ/
	;短小
	CASEELSE
			LOCALS = 可爱_可爱/キュートな_きゅーとな/
	ENDSELECT
;この下は三人称。
CASE "主人"
	LOCALS = 前輩_前輩
CASEELSE
	;名前を呼ぶ相手の角色番号をACTORに代入する
	SELECTCASE ARGS
	;相手が助手
	CASE "助手"
		ACTOR = ASSI
	;ここにはARGSに角色のCALLNAMEが入ったりしている場合(ARGS == "弗理希艾尔" など)に来る
	CASEELSE
		ACTOR = FIND_NO(ARGS)
	ENDSELECT

	;相手が教員
	IF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%老师_%CSTR:ACTOR:ふりがな%老师

	;自分も相手も学生なら学年比較
	ELSEIF COND("学生") && COND("学生", ACTOR)
		;相手が同学年
		IF COND("学年") == COND("学年", ACTOR)
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		;相手が下の学年
		ELSEIF COND("学年") > COND("学年", ACTOR)
			IF TALENT:ACTOR:男性
				LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
			ELSE
				LOCALS = %CALLNAME:ACTOR%酱_%CALLNAME:ACTOR%酱
			ENDIF
		;相手が上級生
		ELSE
			LOCALS = %CALLNAME:ACTOR%前輩_%CSTR:ACTOR:ふりがな%前輩
		ENDIF

	;それ以外
	ELSE
		LOCALS = %CALLNAME:ACTOR%桑_%CALLNAME:ACTOR%桑
	ENDIF

ENDSELECT

RETURNF LOCALS

