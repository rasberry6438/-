;イベントや台詞の都合上の問題でPLAYERに射精させたい場合は CALL SETFLAG, "強制射精", PLAYER とする事で强行射精させることができる
;-------------------------------------------------
@KOJO_MESSAGE_COM_K60
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSIPLAY
;	RETURN 0

;-------------------------------------------------
;愛撫
;-------------------------------------------------
IF SELECTCOM == 0
	;初めて
	IF FIRSTCOM(SELECTCOM) == 0
		IF CEVENT("恋慕：ささくれ")
			PRINTFORMW 「今更温柔的してやろうって？」
			PRINTFORMW 「とんでもなく安直で愚鈍だね、%CNAME("二人称")%」
		ELSE
			PRINTFORMW 「んふ♪ 基本のキってことかい？ 良いともサ」
			PRINTFORMW 「好きに触っておくれよ――あ、でも痛いのはイヤだよ？」
		ENDIF
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕
			IF CEVENT("恋慕：ささくれ")
				PRINTFORMW 「%TEXTR("他の子を当たってよ/嫌いだ/大嫌いだ/ちくちくする/苦しい/やめてよ/気持ち悪い/気持ち良くない/机の角の方がマシ/手汗が酷い")%」
			ELSE
				IF RAND:6 == 0
					CALL PRINT_STRW, "「ゆーびひょーらーい_HH_ あーん――ぱくっ_HH_」"
					CALL PRINT_STRW, "「ちゅぅ_HH_ちゅぅ_HH_ んっ_HH_ はまないよ、あいひょーふ_HH_」"
					CALL PRINT_STRW, "「んっ_HH_ んー_HH_　っ_HH_　ぷぁ――もう一回、ほしいなー_HH_」"
				ELSEIF RAND:5 == 0
					CALL PRINT_STRW, @"「ほぉら、手を握って_HH_ %CNAME("主人")%なら片手でも出来るだろう？」"
					CALL PRINT_STRW, "「ぁはっ_HH_ に、ひひっ_HH_ お擅長お擅長♪」"
				ELSEIF RAND:4 == 0
					CALL PRINT_STRW, "「んっ_HH_ ぁは、にひっ_HH_ 遠慮、するなよ？」"
					CALL PRINT_STRW, @"「%CNAME("一人称")%は%CNAME("二人称")%のペットだ――全部、%CNAME("主人")%のモノなんだから_HH_」"
				ELSEIF RAND:3 == 0
					PRINTFORMW 「%CNAME("主人")%は今までこうして%CNAME("一人称")%に触れた回数を覚えているかい？」
					PRINTFORMW 「%CNAME("一人称")%？ 覚えてるけないだろ。というか数えるヤツいるの？知らないけどさ」
					CALL PRINT_STRW, "「ま、何が言いたいかっていうと――数え切れないくらい構って貰ってペットは大満足ってことさ_HH_」"
				ELSEIF RAND:2
					CALL PRINT_STRW, @"「%TEXTR("ぁっ/んふっ/はぁ")%_HH_ %TEXTR("くひっ/んは――/んんっ")%_HH_　%TEXTR("わっ、/んぁっくぁ！？/ひぁ！？")% ふっふふっ！」"
					PRINTFORMW 「こっ、こらぁ、気持ちいいにくすぐったいを混ぜてくるのは条約違反だぞ！？」
				ELSE
					CALL PRINT_STRW, @"「%TEXTR("んふー/にひー")%_BH_ そうそう顎の辺りー_BH_ %TEXTR("ふぉー/うぉおー/んぉー/ひょあー")%_BH_　%TEXTR("ンきもちー/さいこー/あふぅー")%_BH_　%TEXTR("ﾌｧｰ/ﾓｯﾄﾓｯﾄｰ/ﾅﾃﾞﾅﾃﾞｼﾃｰ")%_BH_」"
					CALL PRINT_STRW, "「――ってこれ違うだろう。おい、普通にエッチな触り方を――ぁぅ_HH_」"
					PRINTFORMW 「ふっ雰囲気ぃ！ 大事にしてやれよぉ～！」
				ENDIF
			ENDIF
		;それ以外
		ELSE
			IF MARK:屈服刻印 >= 2
				IF RAND:3 == 0
					PRINTFORMW 「ンふっ♪――%TEXTR("ピリピリする/指が冷たい/ふわふわする/手懐ける為の手だ/食べちゃうぞ/小手調べ？/むかむかする/垂らし桑/その内爆発しちゃうかも")%」
				ELSEIF RAND:2
					PRINTFORMW 「ふぁ――ああ、気持ち良いサ」
					PRINTFORMW 「少し腹が立つくらいには、ネ」
				ELSE
					PRINTFORM 「%TEXTR(@"慣れた時程雑になりやすいもの。こういう前戯も精心――/もう%CNAME("二人称")%の愛撫にも慣れてしまったからネ、そう簡単には")%
					CALL PRINT_STRL, @"%TEXTR("んひゅっ/んっ/んぁっ/しりゅっ")%_HH_」"
					CALL PRINT_STRW, @"「%TEXTR("や、ははは、/や、ややや！")%%TEXTR(@"さっ流石は%CNAME("一人称")%の%CNAME("主人")%！ これはもうお腹を見せるしかないネ！脱帽！/弱点はお開阔处ってワケだ、流石は%CNAME("主人")%！")%」"
					PRINTFORMW 「――%TEXTR("なんだよ、笑うなよ。ちょっと変な声が出ただけだろう/あのね、今のは反則。ノーカウント。そうだろう？")%」
				ENDIF
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「艶のあるお肌だろ？ 吃飯にまで気を使ってるからネ！」
					PRINTFORMW 「おいどうした手が止まったネ。ほら、触れよ、ほら」
				ELSEIF RAND:2
					PRINTFORMW 「お気に召すまま触れて、喘がせてみせてヨ♪」
				ELSE
					PRINTFORMW 「あっちをこちょこちょこっちをさわさわ♪」
					CALL PRINT_STRW, "「んっふ_BH_ 癪に障った？ そりゃ失礼♪」"
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;-------------------------------------------------
;舐陰　面部騎乗(被逆推ているとき)
;-------------------------------------------------
IF SELECTCOM == 1
	;被逆推ているとき
	IF TEQUIP:推倒
		;初めて
		IF FIRSTCOM_REV(SELECTCOM) == 0
			IF CEVENT("恋慕：ささくれ")
				PRINTFORMW 「こういうこと、期待していたんだろう」
				PRINTFORMW 「やってやるから黙っていてよ。%CNAME("二人称")%の声、癇に障るんだ」
			ELSE
				PRINTFORMW 「んっふっふ、ペットの逆襲領帯ムだ♪」
			ENDIF
		;二回目以降
		ELSE
			;恋慕
			IF TALENT:恋慕
				IF CEVENT("恋慕：ささくれ")
					IF RAND:3 == 0
						PRINTFORMW 「黙ってくれ。そんなにおっ勃てて何言ったって馬鹿みたいだ」
					ELSEIF RAND:2
						PRINTFORMW 「自慰道具が口を零すなよ。黙れ」
					ELSE
						PRINTFORMW 「黙って。声を、聞かせないで。癇に障る」
					ENDIF
				ELSE
					IF RAND:6 == 0
						PRINTFORMW 「そーそー、精心ー精心ー――にひひっ♪」
					ELSEIF RAND:5 == 0
						CALL PRINT_STRW, "「ちょっと、いや、凄く興奮するねコレ_HH_」"
						CALL PRINT_STRW, "「いーい征服感だ_HH_」"
					ELSEIF RAND:4 == 0
						CALL PRINT_STRW, "「表面だけじゃだ～め_HH_ んっ――ふふ♪」"
						CALL PRINT_STRW, "「お擅長お擅長_HH_」"
					ELSEIF RAND:3 == 0
						CALL PRINT_STRW, "「はぁ――_HH_ ずっとコレしてたいな」"
						CALL PRINT_STRW, "「結構気に入っちゃった_HH_」"
					ELSEIF RAND:2 == 0
						PRINTFORMW 「重い？ 今重いって言ったか%CNAME("主人")%！！」
						PRINTFORMW 「よ～しならばもっとギシギシしてやるから重力を体感するが良い」
						PRINTFORMW 「重力だヨ、体重じゃないヨ！！」
					ELSE
						PRINTFORMW 「へ～いカウボーイ！ いやカウガール！ いぇ～い！」
						CALL PRINT_STRW, "「んっ_HH_ ちょ、待って！ 今遊んでるんだからそんなエッチな、あぁっ_HH_」"
					ENDIF
				ENDIF
			;それ以外
			ELSE
				IF MARK:屈服刻印 >= 2
					IF RAND:3 == 0
						PRINTFORMW 「んふ♪ おーにさーんこーちら、おーそそはこーこよってね♪」
					ELSEIF RAND:2
						PRINTFORMW 「食べる前に頂きますは言ったかい？ ま、言って無くても食べさせるんだけどネ♪」
					ELSE
						PRINTFORMW 「息継ぎ？ したい？ 是、吸ってー――どーん！ にひひっ♪」
					ENDIF
				ELSE
					IF RAND:3 == 0
						PRINTFORMW 「どうしてこんなことするかって――そりゃあ気分ですヨ♪」
					ELSEIF RAND:2
						PRINTFORMW 「ほーら餌だぞー%TEXTR("ポチ/タマ/ジョン")%ー」
					ELSE
						PRINTFORMW 「いやー、体は正直だね。こんなでも勃起するんだからサー」
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ELSE
		;初めて
		IF FIRSTCOM(SELECTCOM) == 0
			IF CEVENT("恋慕：ささくれ")
				PRINTFORMW 「舐めるって？ ――ははっ、どっちが犬だか」
			ELSE
				PRINTFORMW 「お？ にひっ♪ コッチに興味津々？ 良いとも良いとも」
				PRINTFORMW 「デリケートなトコロだ、綺麗にしておくれよ♪」
			ENDIF
		;二回目以降
		;清洁舔阴
		ELSEIF COND("清洁舔阴フラグ", PLAYER)
			;恋慕
			IF TALENT:恋慕
				IF CEVENT("恋慕：ささくれ")
					IF RAND:3 == 0
						PRINTFORMW 「引くよ正直。気持ち悪い」
					ELSEIF RAND:2
						PRINTFORMW 「弄脏的のが好きならゴミでも舐めてれば？」
					ELSE
						PRINTFORMW 「――不快だ」
					ENDIF
				ELSE
					IF RAND:3 == 0
						PRINTFORMW 「ん？ ――に、ひひっ♪ 綺麗にしてくれるの？」
					ELSEIF RAND:2
						PRINTFORMW 「あのね、多少は恥ずかしいから舐める音はミュートにして――」
						PRINTFORMW 「くれないんだもんなー！ むしろいつもより大音量なんだもんなー！！」
					ELSE
						PRINTFORMW 「良いね～倒錯的だ。そういうとこ好きだぜ♪」
						PRINTFORMW 「いつも以上に精心――%CNAME("二人称")%で汚してヨ♪」
					ENDIF
				ENDIF
			;それ以外
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「そうそう、ペットの処理は%CNAME("主人")%の義務ですヨ♪」
				ELSEIF RAND:2
					PRINTFORMW 「美味しい？ ねー、美味しい？ にひひっ♪」
				ELSE
					PRINTFORMW 「倒錯してるネ、良いね良いね♪」
				ENDIF
			ENDIF
		;それ以外
		ELSE
			;恋慕
			IF TALENT:恋慕
				IF CEVENT("恋慕：ささくれ")
					IF RAND:3 == 0
						CALL PRINT_STRW, @"「――っ_HH_ %TEXTR("くっ/はぁ/あぁ")%、%TEXTR("気持ち悪い/ざらつく/嘔吐感がする/最悪だ/床でも、舐めていたら？/やめてよ/下手くそ/馬鹿犬")%」"
					ELSEIF RAND:2
						PRINTFORMW 「どっちがペットなんだか。馬鹿らしい。%TEXTR("正直に言って気持ち悪いよ/反吐が出る")%」
					ELSE
						PRINTFORMW 「もうやめちゃえよ。こういうことは別のもっと可愛的反応をする子とやってくれ」
					ENDIF
				ELSE
					IF RAND:6 == 0
						CALL PRINT_STRW, "「に、ひひっ♪ つっかまーえた――_HH_」"
						PRINTFORMW 「もっと、もっと頂戴？ %CNAME("主人")%のペロペロ、もっと、欲しいナ♪」
						CALL PRINT_STRW, "「はぁ――いっ_HH_ ん――んぅ_HH_ ――い、いよっ_HH_ んっ_HH_」"
					ELSEIF RAND:5 == 0
						CALL PRINT_STRW, "「んふー♪ そーんなに美味しいのかーい？ ――い、いや、あの_HH_」"
						PRINTFORMW 「んなっ、真顔で頷かれると、て、照れるだろう！？」
					ELSEIF RAND:4 == 0
						CALL PRINT_STRW, "「だ、ダメだ、馬鹿_HH_ お豆ばっかりを――っ_HH_」"
					ELSEIF RAND:3 == 0
						CALL PRINT_STRW, @"「はっ、ぁ_HH_ 足、締めるなって言ったって、ネ」"
						CALL PRINT_STRW, @"「%CNAME("主人")%、%CNAME("二人称")%の、舌が、悪いんだよ_HH_」"
					ELSEIF RAND:2
						PRINTFORMW 「にひひっ♪ いや、本当にワンコみたいだナー♪」
						CALL PRINT_STRW, "「ほれ、お手、お手。ぎゅーって――ひゃぅ__HH_」"
						PRINTFORMW 「よ、弱い所を急に刺激するのは、勘弁しろい！」
					ELSE
						PRINTFORMW 「んっ――♪ む、なんだいなんだい、ちょっと気持ちよかっただけだろう！」
						PRINTFORMW 「変な声くらい大目に――え、嘘つくなって？」
						PRINTFORMW 「――あーそうさ！めっちゃくちゃ気持ちよかったさ！もっとやっておくれ！！」
					ENDIF
				ENDIF
			;それ以外
			ELSE
				IF MARK:屈服刻印 >= 2
					IF RAND:3 == 0
						PRINTFORMW 「躾けてるのか躾けられてるのか解らないネ～♪」
						CALL PRINT_STRW, "「っぁ_HH_ ――いや、こりゃあキチンと躾けられてるナ♪」"
					ELSEIF RAND:2
						PRINTFORMW 「ん？ んふ♪ %TEXTR("ほらほら、おそそはこちらよ～♪/かも～ん♪/舐めたい？舐めたいんだ？ にひっ♪ どうぞどうぞ♪")%」
					ELSE
						PRINTFORMW 「ni↘ひ！？――ちょ、待った、余裕ない、余裕がっ、舌っ、舌を、はなっ、やめっ！」
						CALL PRINT_STRW, @"「%TEXTR("んひゃっ/きゅっ/んはぁっ")%_HH_ そ、それ以上は%TEXTR("なんかも、漏れそうだからやめよう/やめないか/ギブ、ギブだってばっ")%！？」"
					ENDIF
				ELSE
					IF RAND:3 == 0
						PRINTFORMW 「ペットのお股舐めるのってどんな気分かナ♪」
					ELSEIF RAND:2
						PRINTFORMW 「ほーらこっちだよーワンコー――おっと、ワンコは%CNAME("一人称")%だ、わんわん♪」
					ELSE
						PRINTFORMW 「%TEXTR("くすぐった気持ちいいな/んんっ、ピリッときた/鼻息とかすぐ解るね、このスケベ桑！")%」
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF

;-------------------------------------------------
;肛門愛撫
;-------------------------------------------------
IF SELECTCOM == 2
	;初めて
	IF FIRSTCOM(SELECTCOM) == 0
		;気持ちいい
		IF CHECK("Ａ指挿入")
			IF TALENT:恋慕
				IF CEVENT("恋慕：ささくれ")
					CALL PRINT_STRW, "「――んひゃっ_HH_ っ、最悪、最悪っ！」"
					PRINTFORMW 「本当に、何を、何を考えてるんだよ、%CNAME("二人称")%は」
				ELSE
					CALL PRINT_STRW, "「おぉっ_HH_ お、お？ こ、コレ意外とイイね_HH_」"
					PRINTFORMW 「%CNAME("一人称")%にその素質があったのか、はたまた%CNAME("二人称")%の指が凄いのか――どっちかナ？」
				ENDIF
			ELSE
				PRINTFORMW 「んんっ！ ちょっと待った！ んふっ！ くぅっ、ま、待ってってば～！」
				PRINTFORMW 「いや、気持ち良いは気持ち良いけどっ、んっ、うぅ――！！」
			ENDIF
		;それ以外
		ELSE
			IF TALENT:恋慕
				IF CEVENT("恋慕：ささくれ")
					PRINTFORMW 「そこも触るんだ？ 良いけど、悪辣な趣味だね。怖気がする」
				ELSE
					PRINTFORMW 「んひゃ！？ こ、こらそこは違う穴だぞ！」
					PRINTFORMW 「え、間違ってない？ ――ほっほー、ナルホドね。ここでも気持ち良くして頂けると！」
				ENDIF
			ELSE
				PRINTFORMW 「はっはっは、穴を間違っているよ、茶目だなー%CNAME("主人")%！」
				PRINTFORMW 「なに、間違ってない？ ――マジかよ」
			ENDIF
		ENDIF
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕
			;Ａ振動棒使用中
			IF TEQUIP:肛用振動棒
				;Ｓ状結腸まで振動棒が侵入している
				IF CHECK_COM("結腸責め")
					IF CEVENT("恋慕：ささくれ")
						IF RAND:3 == 0
							PRINTFORMW 「ふー、ふー、奥まで犯して、満足かい？」
							PRINTFORMW 「満足したならさっさとコレ抜けよ、%TEXTR("気持ち悪くて/呼吸し辛くて")%しょうがない」
						ELSEIF RAND:2 == 0
							CALL PRINT_STRW, "「――ぉ゛っ_HH_ ――ぐぅっ！あぁっ！」"
							PRINTFORMW 「はぁ、はぁ、%TEXTR("最悪/ああ/本当に")%――%TEXTR("良い趣味だ/抜けよ/やめろよ")%、%TEXTR("気持ち悪い/嘔吐感がする/不快だから")%」
						ELSE
							PRINTFORMW 「は、ぁ――こんなモノで、奥まで滅茶苦茶にして満足？それは良かった」
							PRINTFORMW 「じゃあさっさと終わりにしてくれよ、不快でしょうがない」
						ENDIF
					ELSE
						IF RAND:3 == 0
							CALL PRINT_STRW, "「ぐふっ_HH_ くふふふ_HH_　あーあ、そんなっ_HH_ 奥までしちゃって_HH_」"
							CALL PRINT_STRW, @"「%CNAME("一人称")%のお腹もう全部ぐちゃぐちゃじゃないか――_HH_」"
						ELSEIF RAND:2
							CALL PRINT_STRW, "「あ゛ー_HH_ あ゛ぁ゛_HH_ もっと、もっとだ、もっと_HH_」"
							CALL PRINT_STRW, @"「もっとオクまで_HH_ %CNAME("主人")%に、コワしてほしいんだ_HH_」"
						ELSE
							CALL PRINT_STRW, "「ぎっひっ_HH_そうっ_HH_ ゴリゴリ_HH_　呼吸、止まっちゃいそう_HH_」"
						ENDIF
					ENDIF
				ELSE
					IF CEVENT("恋慕：ささくれ")
						IF RAND:3 == 0
							PRINTFORMW 「自分の手でする気概も無いんだね」
							PRINTFORMW 「ああ、でも、%CNAME("一人称")%はホッとしているよ。%CNAME("二人称")%に触られたくなんてない」
						ELSEIF RAND:2
							PRINTFORMW 「ぐっぅう――乱れる姿でも、見たかったのかい？ こんな玩具で？ それもお尻で？」
							PRINTFORMW 「空想はメモにでもしたためてお蔵入りにでもしておいてよ。%TEXTR("くだらない/気持ち悪い/馬鹿らしい")%」
						ELSE
							PRINTFORMW 「な、ん、だい――お尻から振動棒、生やしてる姿が、笑えるかい？」
							PRINTFORMW 「%CNAME("一人称")%も笑えるよ。触りもせずこんなもので喜ぶ%CNAME("二人称")%がいっそ哀れだ」
						ENDIF
					ELSE
						IF RAND:3 == 0
							CALL PRINT_STRW, "「あひっ_HH_ んっもう！ 振動棒動かすならきちんと許可を――んっ_HH_」"
							CALL PRINT_STRW, "「わかった、許可する、許可するから！ もっとゆっく――ぁ゛あっ_HH_」"
						ELSEIF RAND:2
							CALL PRINT_STRW, "「ち、力入らないんだから、そんなっ_HH_ ひっ_HH_ おぉっ_HH_」"
							CALL PRINT_STRW, "「く、くぅ～ん_HH_ ぅうおっ_HH_ ふ、ふざけるのも難しいなこれ！！」"
						ELSE
							CALL PRINT_STRW, "「にひひっ_HH_ 尾巴、いじってくれるんだ_HH_」"
							CALL PRINT_STRW, "「ほ～らふりふり_HH_ 可爱ワンコの尾巴い～っぱい弄って――んぉお゛っ_HH_」"
							CALL PRINT_STRW, @"「んぉ゛っ_HH_ ぉ゛_HH_ %TEXTR("お尻っ/尾巴でっ/いっぱいっ")%_HH_ 躾けられちゃう_HH_」"
						ENDIF
					ENDIF
				ENDIF
			ELSE
				IF CEVENT("恋慕：ささくれ")
					IF RAND:3 == 0
						PRINTFORMW 「気持ちよくする気があるのかい、下手くそ」
					ELSEIF RAND:2
