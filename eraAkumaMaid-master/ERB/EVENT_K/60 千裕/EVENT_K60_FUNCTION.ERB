;------------------------------
;捕食前フリ
;------------------------------
@PREDATION_T, ARGS
SELECTCASE ARGS
CASE "調教中つまみ食い"
	IF FLAG:二人きり == 0
		PRINTFORMW 「っちぇー、流石に人前じゃ食べられないナ、残念無念」
	ELSEIF CFLAG:食欲 < 1
		PRINTFORMW 「%TEXTR("でもお腹は満たされてるし――暴食はイケないな/うーん、でも食べ過ぎかナ、慎ましくいかないと")%」
	ELSEIF TALENT:恋慕 && CEVENT("今回はもう食べない") == 0
		CALL PRINT_STRL, @"「――%TEXTR("スキだらけだぜ_BH_/はぁ、美味しそう_BH_/ねー、食べてイイカナ？_BH_/据え膳喰わぬは女性の恥ってネ_BH_")%」"
		CALL PRINT_STRW, "「頂きます_BH_」"
		;この捕食で倒れないならぱっ君ちょ
		IF BASE:MASTER:魂 > CFLAG:食欲
			CALL PREDATION(ARGS)
		ELSE
			PRINTFORMW 「%TEXTR(@"なんてネ、驚いた？/おいおい、こんなに慎ましい%CNAME("一人称")%が鞭打つような真似すると思うかい？ 冗談だよ/冗談冗談、もうちょっと元気になって出直してもらおう/――――と思ったけどナー、もうちょっと食べ甲斐が出てから食べようっと")%」
			CALL SET_CEVENT, "今回はもう食べない"
		ENDIF
	ELSEIF CEVENT("恋慕：ささくれ") 
		SIF RAND:3 == 0
			PRINTFORMW 「――いいや、なんでもないよ。見ていただけさ、気にしないで」
	ELSE
		PRINTFORMW 「頂きます」
		CALL PREDATION(ARGS)
	ENDIF
	RETURN 0

CASE "調教後"
	IF TALENT:恋慕
		IF CEVENT("恋慕：ささくれ")
			PRINTFORMW 「――――なに？ もしかして食べて欲しいの？」
			PRINTFORMW 「%TEXTR(@"いやだね。まっぴらだ/他の子にでもお願いしたら？ %CNAME("一人称")%是やだ/お断りだよ。%CNAME("二人称")%は食べない")%」
			RETURN 0
		ELSE
			IF COND("満足")
				CALL PRINT_STRW, @"「%TEXTR(@"わんわんっ_HH_/ﾊｯﾊｯﾊｯﾊｯﾊッ_HH_/くぅ～んくぅ～ん_HH_")%」"
				PRINTFORMW 「%TEXTR(@"急に何だって、ほらあるだろう！ %CNAME("二人称")%と%CNAME("一人称")%の愛情表現が！/ほら、可愛いペットが鳴いてるぜ？/にひひっ――涎垂れて来ちゃった")%」
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("燃焼し切れなかった分食べちゃうゾ！！/っちぇーっちぇーっちぇっちぇっちぇー、こりゃあご心情取りが必要ですヨ？/埋め合わせはどうしてくれるのかナァ！！ ナァ！！")%」"
			ENDIF
		ENDIF
	ELSEIF MARK:屈服刻印 >= 2
		IF COND("満足")
			PRINTFORMW 「%TEXTR("それじゃあ慎ましいペットらしく、餌を頂こうか♪/お楽しみの後のオタノシミだ。頂けるかナ？/おっと、腹が鳴った。こりゃあ早めに召し上がらないとサ、失礼だろう、ネ？")%」
		ELSE
			PRINTFORMW 「%TEXTR(@"お残しされちゃったケド、%CNAME("一人称")%は遠慮なくぜーんぶ頂きたいネ♪/満たされなかった分、お腹を満たしたいトコロだけどナー？/ところで――お支支付がまだだぜお客様")%」
		ENDIF
	ELSE
		IF CFLAG:食欲 >= 50
			IF COND("満足")
				CALL PRINT_STRW, @"「%TEXTR("さ、満足した所で早く_BH_早く_BH_　美味しいお肉を頂戴ヨ_BH__BH__BH_/")%」"
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("食べてイイよね、ネ、ねっ！？/はぁ――_BH_ ねぇ、ねぇ、ねぇ、ヨシはまだ？ ネェってばぁ_BH_/待ては嫌だな聞きたくないナァ――ネ、飼い主桑ヨ？")%」"
			ENDIF
		ELSE
			IF COND("満足")
				PRINTFORMW 「――で、ヨ。まさか%CNAME("一人称")%がヤって満足して是終わりだと思ってる？」
				PRINTFORMW 「食欲の方も満たして欲しいんだけどナ～？」
			ELSE
				PRINTFORMW 「%TEXTR(@"それじゃ、%CNAME("一人称")%もマンゾクさせて貰おうかナ/運動した後のお肉は程よく柔らかくて美味しいんだよネェ♪/さ、お勘定しましょうや♪")%」
			ENDIF
		ENDIF
	ENDIF
CASE "日常"
	PRINTL 
	PRINTL 
	IF  CHARA_NUM("室内の邪魔者")
		IF TALENT:恋慕
			IF CEVENT("恋慕：ささくれ")
				PRINTFORMW 「%TEXTR("状況を見てものを言えないのかな。バレたくないのだけれど/人前で食べられたいのかい？ そんな趣味には付き合えないな")%」
			ELSE
				PRINTFORMW 「%TEXTR("うーん――申し出は嬉しいけどサ、二人きりの時にしようぜ/それを見せるなんてとんでもない！！")%」
			ENDIF
		ELSE
			PRINTFORMW 「%TEXTR("時と場合！主に人目とかサ！あるよネ！！/マジかよ、他の子にスプラッタ見せる気かい？")%」
		ENDIF
		PRINTFORMW どうやら人前で食べる気は無いようだ…
		RETURN 0
	ELSEIF CEVENT("恋慕：ささくれ")
		PRINTFORMW 「%TEXTR("食べさせてくれるって？ ははっ、要らないよ/ご心情取りのつもりなら結構だよ、食べない/嫌だね")%」
		PRINTFORMW %CALLNAME:TARGET%はぷいっとそっぽを向いてしまった…
		RETURN 0
	ELSE
		IF TALENT:恋慕
			IF CFLAG:食欲 >= 100
				PRINTFORMW 「%TEXTR(@"たべ、させて、くれる、の？ ほ、本当に？/ああ、良かった。幻覚が見えてたところなんだ。もうお腹ぺこぺこだ")%」
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("わんわん_BH_/ﾊｯﾊｯﾊｯﾊｯﾊッ_BH_/可愛い可愛いワンコ酱がお待ちかねだぜ？ くぅ～ん_BH_")%」"
			ENDIF
		ELSEIF MARK:屈服刻印 >= 2
			IF CFLAG:食欲 >= 100
				PRINTFORMW 「%TEXTR("マジかよ。ここで嘘っていうのは、勘弁してくれよ？/――――戯けてやろうかと思ったら浮かばなかったや。重症だ")%」
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("楽しみにしてたんだ。さぁさ、号令をどうぞ_BH_/ここまで来てお預けなんて、言わないよネ？/良い子にしてるからサァ、ご褒美～")%」"
			ENDIF
		ELSE
			PRINTFORMW 「%TEXTR(@"ヨシはまだかな、まだかナ～♪/これも躾の学习だぜ。さ、%CNAME("二人称")%の口から聞かせてよ/食べても良～い？")%」
		ENDIF
	ENDIF
ENDSELECT
PRINTFORML 
PRINTFORML 
PRINTFORM %CALLNAME:TARGET%的
IF CFLAG:食欲 >= 150
	CALL PRINT_STRW, "現在空腹度　_赤_【極高】_"
ELSEIF CFLAG:食欲 >= 100
	CALL PRINT_STRW, "現在空腹度　_赤_【高】_"
ELSEIF CFLAG:食欲 >= 50
	CALL PRINT_STRW, "現在空腹度　_黄色_【中】_"
ELSEIF CFLAG:食欲 >= 10
	PRINTFORMW 現在空腹度　【低】
ELSE
	PRINTFORMW 現在空腹度　【満腹】
ENDIF
PRINTFORML 
PRINTFORML %CALLNAME:TARGET%に自分を――――
PRINTFORML [0] 食べさせる（%CALLNAME:MASTER%：魂－１）
PRINTFORML [1] 食べさせない
CALL INPUT_SELECT, 2
;食べさせた
IF RESULT == 0
	PRINTFORML 
	IF TALENT:恋慕
		IF CEVENT("恋慕：ささくれ")
			IF CFLAG:食欲 >= 100
				PRINTFORMW 「%TEXTR("はぁ――はぁ――/い、良いの、かい？そ、それじゃあ――/ｺﾞｸｯ――")%%TEXTR("い、いただきますっ/頂き、ますっ")%」
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("にひひ――_HH_/んふ、有がとう_HH_/でっへっへ、それでは失礼して～/あぅ～ん_HH_/当店は注文の少ない料理店となっており")%%TEXTR("頂きます/いただきます/いっただきまーす")%%TEXTR("_BH_/♪/！//！！")%」"
			ENDIF
		ENDIF
	ELSE
		IF CFLAG:食欲 >= 100
			PRINTFORMW 「%TEXTR("にひっ――/ああ、やっと、やっとだ！/くひっ――/あはっ――")%%TEXTR("いただきますっ/頂きますっ")%！！」
		ELSE
			CALL PRINT_STRW, @"「%TEXTR("毎度どうも/わんわん/ﾊｯﾊｯﾊｯﾊ/あう～ん")%%TEXTR("_BH_/♪/！/")%頂きます%TEXTR("_BH_/♪/！/")%」"
		ENDIF
	ENDIF
	CALL PREDATION(ARGS)
;食べさせなかった
ELSE
	SELECTCASE ARGS
	CASE "調教後"
		IF TALENT:恋慕
			IF CFLAG:食欲 >= 50
				PRINTFORMW 「%TEXTR(@"食べたい、食べたい、食べたい、食べたい、食べたい――/――はは、はー、酷い%CNAME("主人")%だ/―――――――――は、ははは、あー")%」
				PRINTFORMW 「%TEXTR(@"%CNAME("二人称")%を、食べたい、なー、くそぅ/――最近、ずぅっと想像してるんだ。%CNAME("二人称")%の味を/ごめんなさい。お預けだよね――――大丈夫、大丈夫さ")%」
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("んふ――おっあっずっけかー_HH_/え～どうしようかな～♪ ふふっしょうがないにゃあ～_HH_/うーん、困っちゃったな")%」"
				CALL PRINT_STRW, @"「%TEXTR(@"ま、今の%CNAME("二人称")%の味を想像するのも楽しいものだからネェ♪/いやー本当に困ってるんだぜ、食べられなくてもなんだか気分が良いんだから_HH_/にひっ――_HH_ じゃあ、顔舐めて良い？ ちょっとだけ、ちょっとだけだからサ♪")%」"
			ENDIF
		ELSEIF MARK:屈服刻印 >= 2
			IF CFLAG:食欲 >= 50
				PRINTFORMW 「%TEXTR("――――あぁ、分かった、お預けだね/ははは――――/もしかしてまだ食べさせなくても大丈夫かもって思ってる？")%」
				PRINTFORMW 「%TEXTR(@"忠告しておこうか。結構限界だよ、%CNAME("一人称")%/我慢はするよ、命令だもの。でも限界は近いから覚えておいてね/――――大丈夫さ、我慢できる。半分嘘だけどね")%」
				PRINTL 
				PRINTFORMW %CALLNAME:TARGET%の恭順の珠；-500
				JUEL:恭順 -= 500
			ELSE
				PRINTFORMW 「%TEXTR("ちぇっ、待て、かァ。了解了解/我明白了よぅイケズ――――おっとっと今のナシ/くぅ～ん、ﾊｯﾊｯﾊ♪")%」
				PRINTFORMW 「%TEXTR(@"良い子にしてるから、次は酱と餌を頂戴ネ/大した忠犬っぷりだろ？ ご褒美で一口――――駄目かァ/%CNAME("三人称")%の言いつけならしょうがないネ。どうだいこの聞き分けの良さ、撫でてくれても良いんだぜ？")%」
				PRINTL 
				PRINTFORMW %CALLNAME:TARGET%の恭順の珠；-100
				JUEL:恭順 -= 100
			ENDIF
		ELSE
			IF CFLAG:食欲 >= 100
				IF CEVENT("千裕：反抗") == 0
					PRINTFORMW 「%CNAME("主人")%サ～」
					PRINTFORMW いつものへらへらとした声。
					PRINTFORMW 「何か勘違いしてないかい」
					PRINTFORMW それがドスの利いた声音变成了瞬間に天地がひっくり返った。
					PRINTFORMW 組み倒されている。そう理解した時にはもう遅い。
					PRINTFORMW 彼女の片足が%CALLNAME:MASTER%の胸に置かれて、起き上がろうにも身動ぎするので精一杯だ。
					PRINTFORMW 「じゅーどーとかあいきどーとか、一時期齧ってね。まぁこれは技術というより力任せな押さえつけなんだけれどサ」
					PRINTFORMW 「で、なんだっけ。この期に及んでご飯が頂けない、と。なるほど」
					PRINTFORMW 「ワンコの演技はご満足頂けなかったのかな？ それはたしかに%CNAME("一人称")%の努力不足だ」
					PRINTFORMW にこやかに口を開く間も、胸を押さえつける足は一定の力のまま緩まない。
					PRINTFORMW 「でも先に断っておこうか。このまま腹が減ってしまうと、%CNAME("二人称")%以外を食べないっていう約束も反故にしかねない」
					PRINTFORMW 「なんたってこっちは%CNAME("二人称")%と違ってちっぽけで慎ましい命が掛かってるから、ねっ」
					PRINTFORMW ――――ほんの一瞬、意識が飛んだ。同時に体全体が痺れて呼吸もままならず、無様に咳き込む。
					PRINTFORMW 「お帰り。効いたろ今の。俗に言う爱心ブレイクショットさ。気絶させるんだ」
					PRINTFORML 「%CNAME("一人称")%は出来るなら一口目は生きている状態が良いからネ、
					PRINTFORMW 　組伏せてから無力化する手段としてそれなりに使えるように練習したのサ」
					PRINTFORML 「簡単に言っているケド苦労したんだぜ。何せ気絶の原因の心室細動を叫醒衝撃ってのは就这様殺しかねないしサ」
					PRINTFORMW 「何よりタメが必要になるから密着状態じゃ使えないんだもの」
					PRINTFORMW 「だからほら、映画とか漫画でよくあるアレ、発勁をね、真似してみたんだ」
					PRINTFORMW 「最初は腕でやって、今やってるみたいに足でも練習してサ」
					PRINTFORMW 「と、まァ苦労話を聞いて貰ってる間も体感してもらってたけど――――」
					PRINTFORMW もう何度目かの覚醒を迎えて、%CALLNAME:MASTER%はもう動く気力すら無い。
					PRINTFORMW 「気分はどうかナ、%CNAME("主人")%ヨ」
					PRINTFORMW 脳を犯贴身んわりとした痺れは何かを考える余地を完全に奪っていた。
					PRINTFORMW ただいつものように单薄的笑みを貼り付けた彼女の顔がぼやけて浮かぶ。
					PRINTFORMW 「従順なペットの真似事をしてるから、ずっと食わせなくても大丈夫だって勘違いしたんだろう？」
					PRINTFORMW 茶化すような猫なで声が直に頭に響く。その声とは裏腹に、ボヤけた視界に映る彼女は一切笑着いない。
					PRINTFORMW 「残念無念、私はそんなに温柔的無いし、易しくも無い」
					PRINTFORMW そんな%CALLNAME:TARGET%の顔が、急によく見える距離まで近づいてきた。
					PRINTFORMW 腹のわずかな重みに、マウントポジションをとられていると知れる。
					PRINTFORMW 両手は%CALLNAME:MASTER%を逃がさないとばかりに、頭の両側で力強くシーツを掻いているのが解る。
					PRINTFORMW 「言ったろう、%CNAME("一人称")%を飼わないかって。飼い主の顔をしろよ」
					PRINTFORMW 「そんなだから、こうやって噛みつかれる」
					PRINTFORMW 鼻先が触れあう程の間近で、彼女は犬歯を剥き出しにした。
					PRINTFORMW 片刻后の沈黙にも、お互いに視線を取下ことはない。
					PRINTFORMW %CALLNAME:MASTER%は未だ体を動かせないし、何を考えているとも知れない彼女が退く气息もない。
					PRINTFORMW 解るのは、徐々に細くなる彼女の眼が、何か良からぬことを考えてると直感させるだけだ。
					PRINTFORMW 思考が円熟に至ったのか、彼女が口を開こうとしたその時。
					PRINTFORMW どこかで聞いた曲が鳴り響く。十数年前のヒットソングだ。
					PRINTFORMW 「――――おっとゴメンよ、私だね」
					PRINTFORMW 彼女は懐から手机を取り出すと、暫く弄った後に、单薄的笑いを浮かべた。
					PRINTFORMW 「にひっ――――予定が出来た。先にお闲しよう、ペットのジャレ合いは楽しんで貰えたかい」
					PRINTFORMW 彼女がテキパキと返回準備を終えるまでに、ようやく%CALLNAME:MASTER%は身を起こせる程度には回復した。
					PRINTFORMW 先程までの威圧感が無かったかのように、彼女は%CALLNAME:MASTER%にこやかに手を振る。
					PRINTFORMW 「回頭见、未熟な飼い主桑。今後の成長に期待するぜ」
					PRINTFORMW 扉は、余韻ひとつ残さず静かに閉められた…
					CALL SET_CEVENT, "千裕：反抗"
					CALL SET_CEVENT, "归途イベント禁止"
				ELSE
					PRINTFORMW 「%TEXTR("別に構わないぜ、それなりのことをするだけさ/へぇ、覚悟決まってんだネ。良いけれどサ/――――――にひっ♪ 好きにしろ、ってことネ")%」
				ENDIF
					PRINTFORML %CALLNAME:TARGET%の好感度：-200
					PRINTFORMW %CALLNAME:TARGET%の反感の珠：+10000
					CFLAG:好感度 -= 200
					JUEL:反感 += 10000
			ELSEIF CFLAG:食欲 >= 50
				PRINTFORMW 「%TEXTR("食べないでくださぁい――って？へー/――――――――――ふーん、そう、そうかい/あ゛？ ――おっと、失礼。スマイルスマイル♪")%」
				PRINTFORMW 「%TEXTR("我慢我慢、じぃっと我慢の子。どこではち切れるかはお楽しみに♪――――ってね/――はー、腹ぁ減ったなァ/そろそろ良いかなァ――――うーん？ こっちの話サ")%」
				PRINTL 
				PRINTFORML %CALLNAME:TARGET%の好感度：-50
				PRINTFORMW %CALLNAME:TARGET%の反感の珠：+2000
				CFLAG:好感度 -= 50
				JUEL:反感 += 2000
			ELSE
				PRINTFORMW 「%TEXTR("くぅ～ん、今回はお預けってコト？/そっかぁ、お預け。厳しい躾だネェ――――良いけどサ/食い逃げたぁ良い胆量じゃないカ！")%」
				PRINTFORMW 「%TEXTR("ま、ここは良い子に従いますよ、わんわん――ってね/ツケにした分、ご褒美が楽しみだネ？ ｸｽｸｽ――/次はお腹いっぱいにしてくれるかナ？ 飼い主桑ヨ♪")%」
				PRINTL 
				PRINTFORMW %CALLNAME:TARGET%の反感の珠：+500
				JUEL:反感 += 500
			ENDIF
		ENDIF
	CASE "日常"
		IF TALENT:恋慕
			IF CFLAG:食欲 >= 100
				PRINTFORMW 「%TEXTR("あー、そっかー――ゴメンよ、ちょっと戯ける元気もないや/くふ、くふふ――いやいや、大丈夫。大丈夫だよ/これも新手のPLAYか、愛されちゃって困っちゃうネ")%」
			ELSE
				PRINTFORMW 「%TEXTR(@"そんな～いけず～%CNAME("主人")%の馬鹿～おたんこなす～精力超群～/がっかりすると思った？忠犬の忍耐力舐めんなよ！あ～腹減った！！/スンスン、スンスン、いや食べられないから气味だけでも嗅いでるトコだ、邪魔しないでくれ")%」
			ENDIF
		ELSEIF MARK:屈服刻印 >= 2
			IF CFLAG:食欲 >= 100
				PRINTFORMW 「%TEXTR("ハーイ――で、さ。一応教えておくけどサ、限界は近いぜ/ちっちっちっ――ワカったよ。忍耐さ、出来るところまではネ/ふっふっふ、後悔するなよぉ～！")%」
			ELSE
				PRINTFORMW 「%TEXTR("間食はNG？ っちぇー、しっかりしてらぁ/こんなに良い子にしてるのに！ ヒドいや！/芸の一つでも見せれば一口くれない？ 駄目？ あ、そう")%」
			ENDIF
		ELSE
			IF CFLAG:食欲 >= 100
				PRINTFORMW 「%TEXTR(@"へぇ――良いぜ、それが%CNAME("二人称")%の判断なら、ネ/お腹がきゅんと鳴～る♪ 首がストンとな～る♪ ――ただのお歌ですヨ/ふぅん、そうかいそういかい♪ ――ちっちっちっ、さて、どうしてやろっかなァ")%」
			ELSE
				PRINTFORMW 「%TEXTR("っちぇー、ま、メインは今じゃないもの、引き下がりますヨ/あらら、おねだり失敗♪")%」
			ENDIF
		ENDIF
	ENDSELECT
ENDIF
RETURN 0



;------------------------------
;捕食関数
;------------------------------
@PREDATION,ARGS
#DIM MEMO_LINECOUNT

PRINTFORML 
SELECTCASE ARGS
CASE "調教中つまみ食い"
	IF TALENT:恋慕
		PRINTFORML %CALLNAME:MASTER%の魂：-%TEXTS("0.01",MAX((CFLAG:食欲 / 10) - ABL:信頼,1))%
		BASE:MASTER:魂 -= MAX((CFLAG:食欲 / 10) - ABL:信頼,1)
		PRINTFORMW %CALLNAME:MASTER%の精力：{MAX((CFLAG:食欲 / 10) - ABL:信頼,1)}
		BASE:精力 = MIN(BASE:精力 + MAX(CFLAG:食欲 / 10 - ABL:信頼,1), MAXBASE:精力)
	ELSE
		;魂・精力の加減処理
		PRINTFORML %CALLNAME:MASTER%の魂：-%TEXTS("0.01",(CFLAG:食欲 / 10))%
		BASE:MASTER:魂 -= (CFLAG:食欲 / 10)
		PRINTFORMW %CALLNAME:TARGET%の精力：+{(CFLAG:食欲 / 10) + 1}
		BASE:精力 = MIN(BASE:精力 + (CFLAG:食欲 / 10) + 1, MAXBASE:精力)
		;魂減算後復活と強制終了
		IF BASE:MASTER:魂 <= 0
			CALL PRINT_STR, "紅_＜复活！＞_W_"
			PRINTFORMW %CALLNAME:MASTER%用尽最后的力气复活了！
			BASE:MASTER:魂 = 1
			PRINTFORMW ★★★★结束床戏★★★★
			CALL ADD_STATE, MASTER, "撃沈"
			CALL BLANKLINE
			MEMO_LINECOUNT = LINECOUNT
			CALL KOJO_COUNTER, "強制終了"

			SIF LINECOUNT == MEMO_LINECOUNT
				CLEARLINE 1

			BEGIN AFTERTRAIN
		ENDIF
	ENDIF
	CFLAG:食欲 -= 5
CASEELSE
	IF TALENT:恋慕
		PRINTFORML %CALLNAME:MASTER%の魂：-0.5
		PRINTFORML %CALLNAME:TARGET%の好感度：+100
		PRINTFORML %CALLNAME:TARGET%の恭順の珠：+3000
		PRINTFORMW %CALLNAME:TARGET%の精力：+{MAXBASE:精力 / 2}
		BASE:MASTER:魂 -= 50
		CFLAG:好感度 += 100
		JUEL:恭順 += 3000
		BASE:精力 = MIN(BASE:精力 + MAXBASE:精力 / 2, MAXBASE:精力)
	ELSE
		PRINTFORML %CALLNAME:MASTER%の魂：-１
		SIF MARK:屈服刻印 >= 2
			PRINTFORML %CALLNAME:TARGET%の好感度：+100
		PRINTFORML %CALLNAME:TARGET%の恭順の珠：+500
		PRINTFORMW %CALLNAME:TARGET%の精力：+{MAXBASE:精力 / 2}
		BASE:MASTER:魂 -= 100
		SIF MARK:屈服刻印 >= 2
			CFLAG:好感度 += 100
		JUEL:恭順 += 500
		BASE:精力 = MIN(BASE:精力 + MAXBASE:精力 / 2, MAXBASE:精力)
		IF BASE:MASTER:魂 <= 0
			CALL PRINT_STR, "紅_＜复活！＞_W_"
			PRINTFORMW %CALLNAME:MASTER%用尽最后的力气复活了！
			BASE:MASTER:魂 = 1
		ENDIF
	ENDIF
	;捕食後台詞
	PRINTFORML 
	IF CFLAG:食欲 >= 100
		IF TALENT:恋慕
			CALL PRINT_STRW, @"「%TEXTR("佳肴、大人、でした/ご馳走、様/ご馳走様")%――」"
			IF RAND:3 == 0
				CALL PRINT_STRW, @"「%TEXTR("ぷはぁ/はー/あー")%！ %TEXTR("ワンコふっかーつ/戯ける元気が沸いてきたー/新しい顔だー元気百倍だー")%！」"
			ELSEIF RAND:2
				PRINTFORMW 「ふ、う――――にひひっ♪ 有がとう、%CNAME("主人")%」
				PRINTFORMW 「とても美味しかったよ♪」
			ELSE
				PRINTFORMW 「ｸﾞｽｯ――うぁ、な、なんでだろう。なんで泣いてるんだい、%CNAME("一人称")%は」
				PRINTFORMW 「美味しかった、美味しかったんだ。%CNAME("二人称")%が美味しくて――なんでか泣いてるんだ」
				PRINTFORMW 「へ、変だな。変だよ。お腹一杯で、幸せなのに――変だよ」
			ENDIF
		ELSE
			CALL PRINT_STRW, @"「%TEXTR("佳肴大人っ/ご馳走桑っ/ごっそさーんっ")%%TEXTR("_HH_/♪/！/！！")%」"
			CALL PRINT_STRW, @"「%TEXTR("や～っと戯ける元気も沸いてきたぜ！/いやー、空腹で死んじゃうかと思ったヨ/マジでここ最近で一番美味しかった！/ふ～生きてる～！人に良いと書いて食ってのはマジだネ！")%」"
		ENDIF
	ELSE
		CALL PRINT_STRW, @"「%TEXTR("佳肴大人/ご馳走様/ごっそ桑")%%TEXTR("_BH_/♪/！/！！")%」"
		IF TALENT:恋慕 && RAND:2
			IF RAND:2
				CALL PRINT_STRW, @"「%TEXTR("今日も美味しかったぜ/いつもすまないネー――っつってネ")%%TEXTR("_BH_/！/♪/！！")%」"
			ELSE
				PRINTFORMW 「%TEXTR("え、泣いてる？あ、ほんとだ/ん、なんか頬が熱いな――/む、あ、あれ、おかしいな――")%%TEXTR(@"――なんで泣いてるんだ、%CNAME("一人称")%/泣いてら/あ、はは何かで涙腺脆くなったかナ")%」
				PRINTFORMW 「%TEXTR(@"きっと%CNAME("二人称")%が美味しかったから、かナ。多分、そうだろうヨ/大丈夫、大丈夫。酱と%CNAME("二人称")%は美味しかったよ。有がとう")%」
			ENDIF
		ELSEIF RAND:3 == 0
			;ボジョレーヌーボー風
			;美味しかった
			PRINTFORM 「う～ん今日は
			IF RAND:2
				PRINTFORM %TEXTR("まさに１０年に一度の/最初の一口を超える/史上稀に見る/前回と同等の/厳しい条件の中洗練された/エレガントで味わい深く、平衡のよい")%
				PRINTFORM %TEXTR("味わい/出来/旨味/仕上がり/至高の舌触り")%
				PRINTFORMW ！」
				PRINTFORMW 「つまり美味しかった！いつもこの状態を維持したいネ！」
			;不味かった
			ELSE
				PRINTFORM %TEXTR("過去最悪/地の底で燻っている/最後まで広がり切らない/厳しさに敗けきった/口にするのも憚られる")%
				PRINTFORMW %TEXTR("味わい/出来/旨味/仕上がり/最低の喉越し")%」
				PRINTFORMW 「よし、大姐姐と生活習慣を見直そう」
			ENDIF
		ELSEIF RAND:2
			;食レポ風
			IF RAND:3
				PRINTFORMW 「%TEXTR("う～んこれは星を取れる味！！/舌が唸る！お腹が喜ぶ！心が起伏着的！/料理漫画だったらお半脱するところだったネ！/お金取れるぜこれ、開業する？")%」
			ELSEIF RAND:2
				PRINTFORMW 「%TEXTR(@"めっちゃぷりぷりしてた、今もお腹で踊ってる/なんか今日無敵になれそう/舌とのシンクロ率１００％かよ/五臓六腑に帯污迹的渡るぅ！")%」
			ELSE
				PRINTFORMW 「%TEXTR(@"このお肉が%CNAME("一人称")%を狂わせる、反省しろよ/もう少し美味さを控えめにしてくれ、壊す気か、%CNAME("一人称")%を/舌がまだ求めてやがるぜ、へっへっへ")%」
			ENDIF
		ELSE
			;健康診断系
			PRINTFORMW 「%TEXTR("野菜酱と取ってる？/よく食べよく寝る！実践できてるネ！偉いぞ～/お肉もっと食べなさい/ちょっとハリが悪いぜ/吃飯平衡は良いナ、よしよし/明日は油もの控えよう/運動は酱としてるネ、結構結構")%」
			PRINTFORMW 「健全なお肉は健全な生活習慣からだからネ！」
		ENDIF
	ENDIF

	CFLAG:食欲 = 0
	CALL SET_CEVENT("今日の吃飯")
ENDSELECT

RETURN 0
