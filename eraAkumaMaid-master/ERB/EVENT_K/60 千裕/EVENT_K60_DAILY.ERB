;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K60, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STRW, @"「ん、ふふ、おはよう%CNAME("主人")%_BH_」"
	PRINTFORMW 「動けないかい？ それは結構。そうなってくれないと困るからネ」
	PRINTFORMW 「大丈夫大丈夫、取って食ったりはしないサ」
	CALL PRINT_STRW, "「ごーめん、嘘だ_BH_ いただきまーす_BH_」"
;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
;	CASE "朝逆正常位"
;		PRINTFORMW 
;	CASE "朝舐陰"
;		PRINTFORMW 
	CASE "朝足交"
		PRINTFORMW 「いやーまさか刚睡醒から足でされて元気だなんてネー」
		PRINTFORMW 「しかもまだビンビンですヨ。これはもっと罰を与えなきゃネー前輩としてはサ！」
		PRINTFORMW 「ほれほれうりうり♪ わんこのアンヨでどぴゅっとお出し♪」
	CASE "朝の新妻PLAY"
		PRINTFORMW 
	CASE "朝乳交"
		PRINTFORMW 「駆け付け一番、胸部はどうだい？」
		PRINTFORMW 「幸せそーな顔だねー♪ じゃ、一発イっとこうか♪」
	CASE "朝騎乗位"
		IF TALENT:恋慕 && RAND:2
			CALL PRINT_STRW, "「動かないのも意外と良い、な……ふふ_HH_ はっは、もう、情けない位媚びてるの解るなぁ……」"
			IF RAND:5 == 0
				CALL PRINT_STRW, @"「…………好きだよ。 %CNAME("一人称")%は、好き、%CNAME("二人称")%が、好き。大好き_HH_」"
			ELSEIF RAND:4 == 0
				CALL PRINT_STRW, "「……食べたい_HH_ でも、食べたくない……食べて、欲しくなる事もある。変だ。やっぱり好きって、変だな……それでも、好きだ_HH_」"
			ELSEIF RAND:3 == 0
				CALL PRINT_STRW, "「……本当はね、愛してるって言いたい……けど、愛とかよく解らないんだ。だから好き。大、好き……はぁ、兒童っぽいなぁ」"
			ELSEIF RAND:2
				CALL PRINT_STRW, @"「……こんなに元気にして。仕方の無い人…_HH_ それは%CNAME("一人称")%もか…全く、我ながら……莫迦だ」"
			ELSE
				CALL PRINT_STRW, "「…………ごめんね、こんな化け物で。食べるのも忍耐から。一杯、するから。だから…だから……」"
			ENDIF

			CALL PRINT_STRW, "「――――あっ、あらァ～？ 起きてんでやんの。のっ覗きはぁ良くないナ！ え、襲った側の台詞かって？ そりゃご尤も！！」"
			PRINTFORMW 「で、ですヨ――――あのーですネ！ バレずに頂く予定だったんですヨ？ なんでさっきのはですネ。いや、あの、ですネ」
			CALL PRINT_STRW, "「――――とりあえず一発ヌいてからにしよう！ おら腰振り始め！！ そぉおおおいぞぉおおおおおおい！ あ、是、黙ります」"
		ELSE
			CALL PRINT_STRW, "「んっ_HH_ わっ、起きちゃった？」"
			CALL PRINT_STRW, "「にひっ_HH_ 忠犬ですから？ んぁっ_HH_ こうして健気にもご奉仕をっni↘っ_HH_」"
			CALL PRINT_STRW, "「あっ_HH_ こ、こらっ_HH_ 急に動くのは反則だろうっっ_HH_」"
		ENDIF
	CASE "朝Ａ騎乗位"
		CALL PRINT_STRW, "「おはよ_HH_ 朝からお尻で絞られる気分はどうかナ_HH_」"
		CALL PRINT_STRW, @"「にひひひ――_HH_ おちんちんは%CNAME("一人称")%のお尻が大好你たいだネー_HH_」"
	CASE "朝接吻"
		PRINTFORMW 「有ゃ、ごめんごめん。起こしちゃったナ」
		PRINTFORMW 「食べようとしてたんじゃないんだぜ、ほんとほんと」
	CASE "朝添い寝"
		PRINTFORMW 「ん、ふぁああ――――にひひっ♪」
		IF TALENT:恋慕 && RAND:2
			PRINTFORMW 「汗臭い？ そりゃ失敬。筋トレの後なもんでネ」
			PRINTFORMW 「こうやって%CNAME("二人称")%に汗の气味を覚えさせて――――いやぁん冗談ですヨ♪」
		ELSE
			PRINTFORMW 「つまみ食いしてやろうかナーなんて思ったんだけど、気持ちよさそうに寝てたもんだからつい一緒に寝ちゃった」
		ENDIF
	CASE "朝口交"
		IF TALENT:恋慕 && RAND:2
			CALL PRINT_STRW, "「んもっ――ん？ ――んふっ_HH_」"
			PRINTFORMW %CALLNAME:MASTER%が目を覚ました事に気付いた%CALLNAME:TARGET%は口の動きをより複雑にさせていく。
			CALL PRINT_STRW, "「んっふ_HH_ んーっふ_HH_ んーっふ_HH_ んーっふ_HH_」"
			PRINTFORMW 何を言っているのか解らない筈なのに煽られている事だけはよく解った…
		ELSE
			PRINTFORMW 「んっ？ んも、おふぇぉ！ じゅる――んじゅ――くちゅ――じゅ――」
			PRINTFORMW 「んっんっ――じゅじゅっ――じゅぼじゅぼじゅぼじゅぼじゅぼじゅぼ♪」
		ENDIF
	CASE "朝六九式"
		CALL PRINT_STRW, "「んむっ_HH_ んももっ！？ もっ！ もんっ！」"
		CALL PRINT_STRW, "「んんん～っ_HH_ じゅぼじゅるんじゅっ_HH_ じゅるるっ_HH_」"
	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝添い寝"
		IF CFLAG:食欲 >= 50
			PRINTFORMW 「ところでサ、添い寝しながらずっと良い气味がしててさー」
			PRINTFORMW 「もう辛抱堪らないってトコなのサ。ネ、解るよネ？ オアズケは聞いてあげられないナ♪」
			PRINTL 
			PRINTFORMW 返事をする間もなく%CALLNAME:TARGET%は%CALLNAME:MASTER%に"齧り"ついた…
			CALL PREDATION
		ELSE
			PRINTFORMW 「ふぁあ、まだ少し眠いナ。起きなきゃいけないのが辛いトコロだ」
			IF TALENT:恋慕
				PRINTFORMW 「もう少し一緒に寝ちゃうかい？ ず～っと二人、誰にも邪魔されないで、なんなら今日一日、さ」
				PRINTFORMW 「――――にひっ♪ 大姐姐の冗談を真に受けちゃいけないナ？ さっさとおっきして顔洗ってこ～い！」
			ELSE
				PRINTFORMW 「ん～っヨシ！！ そ～ら起きた起きた！ おはようの時間だぜ」
			ENDIF
		ENDIF
	CASE "朝口交","朝乳交"
		IF CFLAG:食欲 >= 50
			PRINTFORMW 「朝の一番搾りを頂いたところでー」
			CALL PRINT_STRW, "「――――朝食を味わいたいナ_BH_」"
			PRINTL 
			PRINTFORMW 返事をする間もなく%CALLNAME:TARGET%は%CALLNAME:MASTER%に"齧り"ついた…
			CALL PREDATION
		ELSE
			PRINTFORMW 「ひゃ～顔から何からもうドロドロだ。まったく朝から本当に元気じゃないか」
			IF TALENT:恋慕
				PRINTFORMW 「にひっ♪ このまんまおはようの接吻でもしてやろうかと思ったけどや～めた♪」
				PRINTFORMW 「感謝してくれよ、衝動のままザー接吻しなかった自制の効くペットにサ♪」
			ELSE
				PRINTFORMW 「ついでに汗も流してきましょうかネ」
				PRINTFORMW 「おっと、覗いても良いけど代金は頂くぜ？ 身体でサ♪」
			ENDIF
		ENDIF
	CASE "朝騎乗位","朝Ａ騎乗位"
		IF CFLAG:食欲 >= 50
			PRINTFORMW 「はぁ～すっきり！ %CNAME("二人称")%もご苦労！」
			PRINTFORMW 「じゃ、朝食の用意と洒落込もうか？ 準備は良いよね」
			PRINTL 
			PRINTFORMW 返事をする間もなく%CALLNAME:TARGET%は%CALLNAME:MASTER%に"齧り"ついた…
			CALL PREDATION
		ELSE
			IF TALENT:恋慕
				IF RAND:2
					PRINTFORMW 「もう終わり？ おはようするかい？ そ・れ・と・も？」
					PRINTFORMW 「――――にひっ♪ やめたやめた。さっさと淋浴浴びて他の子に顔出してこいよ、後始末位ペットに任せてサ♪」
				ELSE
					CALL PRINT_STRW, "「にひひ_HH_ いや～悪いね_HH_ ほんとーに悪い事しちゃったと思ってるんですヨ_HH_」"
					CALL PRINT_STRW, @"「今日、誰に幾ら愛を囁いたって最初に抱いたのは可愛いペットの%CALLNAME:TARGET%酱なんだぜ_HH_」"
				ENDIF
			ELSE
				PRINTFORMW 「朝の運動終わり！ いやいや、散歩の代わりに丁度良いじゃないか♪」
				PRINTFORMW 「あ、そうそう。汗くらい流してこいよ、結構汗臭くて美味しそうな气味がプンプンしてるぜ」
			ENDIF
		ENDIF
	CASE "朝六九式"
		IF CFLAG:食欲 >= 50
			PRINTFORMW 「早起きのトク――いただきます♪」
			PRINTL 
			PRINTFORMW 返事をする間もなく%CALLNAME:TARGET%は%CALLNAME:MASTER%に"齧り"ついた…
			CALL PREDATION
		ELSE
			IF TALENT:恋慕
				PRINTFORMW 「あ～クッソ、%CNAME("一人称")%まで気持ち良くする必要は無いんじゃないかい？」
				PRINTFORMW 「べっつにー！ 怒ってないです～嘘です～怒ってまーす！」
				PRINTFORMW 「%CNAME("二人称")%だけを気持ち良くしたいの、可愛い顔見たいんですヨ！」
				PRINTFORMW 「わかる？このオトメゴコロ！ %CNAME("一人称")%は分からんけど！」
				PRINTFORMW 「はーーーー！ もう！ バカ！ ちんちん！ 知らん！ わんわん！ さっさと支度してこーい！」
			ELSE
				PRINTFORMW 「なんだか納得いかないナー、狩る側が一緒に狩られたというか、共倒れと言いますかネ」
				PRINTFORMW 「んー、まあいっか！ 叫醒つもりだったしネ、ほら、さっさと始末して顔洗ってきなさいナ♪」
			ENDIF
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF CEVENT("恋慕：ささくれ")
		PRINTFORMW 「%TEXTR("――――ああ、おはよう/起きたんだ、そっか")%」
	ELSEIF COND("体育")
		IF TALENT:恋慕
			PRINTFORMW 「今日は体育の日だろ。起き抜けから酱と気合入れなきゃあね」
		ELSE
			PRINTFORMW 「今日は体育の日だってなんで教えてくれなかったんだ！」
			PRINTFORMW 「%TEXTR("えーっと、運動の時間はこの位の時だね？ よしよし、一番良い肉質になるのは、っと/肉質が変わっちゃうだろう！？ 今日は硬めの気分だったのに！")%」
		ENDIF
	ELSE
		PRINTFORMW 「おっはよ！ %CNAME("主人")%♪」
		PRINTFORMW 自然に齧りついて来ようとする%CALLNAME:TARGET%を%CALLNAME:MASTER%はギリギリで躱す。
		PRINTFORMW 「っちぇー、先っちょだけ、先っちょ位いいだろ？ネ？ネ？」
	ENDIF
CASE "登校"
	IF CEVENT("恋慕：ささくれ")
		PRINTFORMW %CALLNAME:TARGET%は何も言わず%CALLNAME:MASTER%と一定の距離を取って歩いている…
	ELSEIF DAYEV("試験当日")
		IF RAND:3 == 0
			PRINTFORMW 「おーどうしたんだい。気のせいか顔が青いぞ」
			IF CFLAG:MASTER:勉強時間 >= 8
				PRINTFORMW 「そう心配するなよ。やるコトはやったんだろう？」
				PRINTFORMW 「%TEXTR("おろおろするより胸を張れよ。そうすれば少しはマシに解けるさ/後は力を出し切るだけ、そうしたら結果は後から着いてくるものサ")%」
				PRINTFORMW 「%TEXTR("おー今すっごい良いコト言った。知らんけど/――お、今の良いね。何だか前輩みたいだ！")%」
			ELSE
				PRINTFORMW 「ははぁん、さては勉強あまりして来なかったクチだ。ご愁傷サマ♪」
			ENDIF
		ELSEIF RAND:2
			PRINTFORMW 「――――あー、ごめんごめん。今日のテスト範囲を頭の中でお浚いしていたんだ」
			PRINTFORMW 「ギリギリまで予行演習は欠かさない主義なのサ」
		ELSE
			PRINTFORMW 「テスト勉強は捗ったかい？」
			PRINTFORMW 「もっと泣きついてくれても良かったんだぜーこの頼れる前輩にさァ！」
		ENDIF
	ELSEIF DAYEV("試験返却日")
		PRINTFORMW 「おいおい、不景気な顔だね%CNAME("主人")%」
		PRINTFORMW 「%TEXTR("さあさ、努力の成果の確認日だぜ？/俯いたって返されるものは返されるんだからサ")%」
		PRINTFORMW 「もっと足取り轻轻地行こうじゃないか♪」
	ELSEIF COND("朝袭相手")
		IF TALENT:恋慕
			IF RAND:2
				PRINTFORMW 「――くくっ♪ イケナイなー今朝の噛み跡が見えてるじゃあないか」
				CALL PRINT_STRW, "「登校中にそんな破廉恥を見せつけるなんて悪い子だネ_BH_」"
			ELSE
				PRINTFORMW 「今朝の元気はどうしたー」
				PRINTFORMW 「もっとシャキシャキッと歩けるだろ～まったく最近の青年は」
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORMW 「ほら、気にしない気にしなーい」
				PRINTFORMW 「ちゃーんと見えない所食べたんだから、噛み跡は見えないって」
			ELSE
				PRINTFORMW 「いやー朝からお腹が満たされてると気分が良い。足取りも軽やかになろうもんだよ」
			ENDIF
		ENDIF
	ELSEIF COND("体育")
		IF TALENT:恋慕
			IF RAND:2
				PRINTFORMW 「実の所体育は余り好きじゃないんだ」
				PRINTFORMW 「苦手科目というワケでも無いんだけどねー、ほら、わかるだろう？」
				PRINTFORMW 「プリプリのお肉が踊るのを見てると――――どーもお腹がネ」
			ELSE
				PRINTFORMW 「そう言えば今朝見た夢の話なんだけど」
				PRINTFORMW 「体育で良い汗を掻いた%CNAME("二人称")%のお肉にかぶりつくっていう夢を見たんだ」
				PRINTFORMW 「いやー、良い夢だったな。凄くいい夢だった。いい夢だったんだよ」
				PRINTFORMW 「――――露骨に目を逸らされるともっとおねだりしちゃうぞ？」
			ENDIF
		ELSE
			IF RAND:2 == 0
				PRINTFORMW 「お肉の質に気を使って手を拔出とかさー、ダメ？」
				PRINTFORMW 「はー体育でめちゃくちゃ本気出すとか小学生男子か%CNAME("二人称")%は」
			ELSE
				PRINTFORMW 「是是、登校中にまでごちゃごちゃ言いませんよー忠犬でございますから」
				PRINTFORMW 「――――ところでさー、体育の後汗の帯污迹的た毛巾とか持ってきてくれないカナー」
				PRINTFORMW 「何に使うかって？ ほら、焼肉店の通気孔から漏れる風味で味気ない胖次を食べるとかあるだろう、ネ？」
			ENDIF
		ENDIF
	ELSE
		IF TALENT:恋慕
			IF RAND:3 == 0
				PRINTFORMW 「お、もしかして車道側を歩いてくれるのかい？」
				CALL PRINT_STRW, @"「クスクス♪ ヤサシイなーさっすが%CNAME("主人")%_BH_」"
			ELSEIF RAND:2
				PRINTFORMW 「ふわぁ――とっと、見なかったことにしておくれよ」
				PRINTFORMW 「%TEXTR(@"レアなんだ。安くないんだぜ、%CNAME("一人称")%の哈欠はサ♪/大哈欠なんて同班同学に見られたら目を丸くされちゃうからね")%」
			ELSE
				PRINTFORMW 「あっ、あー、まぁ、良いか」
				PRINTFORMW 「同班同学がぎょっとした顔でこっち見ていたんだよ」
				CALL PRINT_STRW, "「見せつけてやるのも面白そうかななんてネ_BH_」"
				PRINTFORMW 「%TEXTR("ほら、腕組もうぜ、腕/いっちょ胸とか揉んどく？/剪刀手とかしてみようか、へいへーい")%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「%TEXTR("ちゃーんとリードは引っ張っておいてね/よそ見是けないねー")%、%CNAME("主人")%」
				PRINTFORMW 「朝は皆警戒心单薄的のさ、ついうっかり小腹が空いてパックンチョ――――大変でしょう？」
			ELSEIF RAND:2
				PRINTFORMW 「眠たそうな顔だねー。ひとカジリしたら目が覚めるかな？」
				PRINTFORMW 「なーんて、冗談なんだけど。どう？ おひねりついでに一口良い？ ダメ。あ、そう。くぅ～ん」
			ELSE
				PRINTFORMW 「お、少し出口るよ？」
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の元を出口ると同級生らしき生徒の元へと嫋やかな仕草で頭を下げて話を始めた。
				PRINTFORMW 物腰柔らかな所作は普段からは想像も出来ないような優等生らしさがにじみ出ている…
			ENDIF
		ENDIF
	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
	IF TALENT:恋慕
		IF CEVENT("恋慕：ささくれ")
			PRINTFORMW 「……二人きりだからって何か期待してるわけじゃあ無いよね」
		ELSE
			PRINTFORML 「おっ――――にひひっ♪」
			PRINTFORMW 「都合もよろしく二人きりじゃあないか、何しようか？ナニをシようか！？」
		ENDIF
	ELSE
		PRINTFORML 「へいへーい、誰も居ないならアッチで前輩とイイコトしない？」
		PRINTFORMW 「喰わなければ？ そんなご無体な」
	ENDIF
CASE "ただいま"
	IF CEVENT("恋慕：ささくれ")
		PRINTFORMW 「……ただいま」
	ELSEIF RAND:2
		PRINTFORMW 「ただいまもどりましたよーっと」
	ELSE
		PRINTFORML 「ただい――――ん？ なんだいその顔」
		PRINTFORML 「あー、ちょっと家に寄ってつまみ食いしたから、その痕かな」
		PRINTFORMW 「にひひっ、先に着替えてこようかナ」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:恋慕
			IF CEVENT("恋慕：ささくれ")
				PRINTFORMW 「あっ！――――ん、おかえりなさい」
			ELSE
				PRINTFORMW 「待ってたぜお帰りっ！」
			ENDIF
		ELSE
			PRINTFORMW 「欢迎回来」
			PRINTFORMW 「で、%CNAME("二人称")%が居なくても良い子にしてた%CNAME("一人称")%にご褒美は無いのかナ？」
		ENDIF
	ELSE
		IF TALENT:恋慕
			IF CEVENT("恋慕：ささくれ")
				PRINTFORMW %CALLNAME:TARGET%はこちらを一瞥して、小さく鼻を鳴らした…
			ELSE
				PRINTFORMW 「おっ、来た来た♪ 遅いぜ%CNAME("主人")%」
			ENDIF
		ELSE
			PRINTFORMW 「中々来ないから心配しちゃったヨ。まったく、監視管理は大事だぜ？」
			PRINTFORMW 「何の、とは言わないけどネェ――♪」
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		IF CEVENT("恋慕：ささくれ")
			PRINTFORML 「呼ばれて来てやったよ」
			PRINTFORMW 「ホント、よく躾けられてるだろ。笑っちゃうよね」
		ELSE
			PRINTFORML 「呼び出しなんて%CNAME("主人")%たらダ・イ・タ・ン♪」
