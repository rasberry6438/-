;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K48, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	LOCALS = 你_你
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 部长_部长
CASE "助手"
	IF NO:PLAYER == 34
		LOCALS = 欧内酱_欧内酱
	ELSEIF NO:PLAYER == 35
		LOCALS = 欧尼酱_欧尼酱
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部"
			IF TALENT:男性
				LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
			ENDIF
		CASE "１年生", "２年生", "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = %CALLNAME:PLAYER%老师_%CALLNAME:PLAYER%老师
			ELSE
				LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

