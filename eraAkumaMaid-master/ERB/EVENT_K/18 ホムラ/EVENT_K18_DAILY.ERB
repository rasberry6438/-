;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K18, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝六九式"
		IF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, "「――嗯……早上好~~從清早就開始H、感覺很好呢」_W_"
			PRINTFORMW 「早上的服務已經結束啦。趕快起床、把你那舒服的不像樣的臉洗一洗呦~~」
		ELSE
			PRINTFORMW 「――早上好呦~嚇了一跳麼？」
			PRINTFORMW 「本来只是想叫你起床的、但是看到这么雄伟的……嗯~让人难耐啊」
			PRINTFORML %CALLNAME:TARGET%在交談的同時、下面堅挺的帳篷仍舊沒有平靜
			CALL PRINT_STR, "「嗯~_H_嗯~~~_H_」情愛的吻像雨水一樣密集落下……_W_"
			PRINTFORMW 「……哎呀呀、都這麼晚了呢。真可惜啊、我們之後再·繼·續·呦~……嗯哼哼~~~」
		ENDIF
	CASE "朝乳交", "朝騎乗位", "朝足交"
		IF EXP_GAIN("射精経験", PLAYER)
			PRINTFORMW 「……那麼、早上那一份的種汁榨出來了呢~、%CNAME("一人称")%要不要洗個澡呢？」
			PRINTFORMW 「那麼、%CNAME("二人称")%趕快起床、把你那舒服得不像樣的臉洗一洗呦~~」
		ELSE
			PRINTFORMW 「都這麼晚了呢。對不起呀、我們以後再繼續舒服的事好不好呀~」
			PRINTFORMW 「那麼、%CNAME("二人称")%趕快起床、把你那舒服得不像樣的臉洗一洗呦~~」
		ENDIF
	CASE "朝の新妻PLAY"
		IF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, "「唔…唔~~早上好_H_、清晨最濃的種汁~唔~咕嘟、咕嘟~吞進~哈~肚子裏、黏糊糊的_H_,_W_"
		ELSE
			CALL PRINT_STR, @"「唔、唔~早上好呦_H_真是的、一大早就很精神啊%CNAME("二人称")%……_H_」_W_"
		ENDIF
		PRINTFORMW 「早餐马上就做好了、%CNAME("二人称")%要记得洗脸呦~」
	CASE "朝添い寝"
		PRINTFORMW 「――呼~啊~嗯……早上好~嗯……」
		PRINTFORMW 「……已經這麼晚了啊、必須起床、我知道啦～」
		PRINTFORML %CALLNAME:TARGET%带着一副困倦的样子从床上爬起来、
		PRINTFORMW 在数次尝试之后、就像放弃了一样、再次将%CALLNAME:MASTER%的頭埋抱在自己的雙峰之間。
		PRINTFORMW 「……不行啦…抱著太舒服了……好像抱着一個魔法抱枕啊……完全抵抗不了這種魔法呢~~」
		PRINTFORMW 「所以……%CNAME("一人称")%不要起床啦~来陪我繼續睡覺吧」
		PRINTFORML 隨便找了個藉口、又一次和%CALLNAME:TARGET%睡在了一起。
		PRINTFORMW %CALLNAME:MASTER%在窒息于柔軟的雙乳之前全力掙脫了出來……
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1
	
	PRINTFORMW 「嗯…早上好。今天也是一個清爽的早晨啊」
	IF COND("体育")
		PRINTFORMW 「說起來、今天%CNAME("二人称")%的班上有體育課吧」
		PRINTFORMW 「%CNAME("一人称")%不要因為老師是我就偷懶啊？」
	ELSEIF COND("水泳")
		PRINTFORMW 「這麼說來、今天是上游泳課的日子啊」
		PRINTFORMW 「不要被泳装女孩子迷住而溺水了哦。作为%CNAME("一人称")%我职责的一部分会在旁邊一直监督你的呦~」
		PRINTFORMW 「――怎麼了？　%CNAME("一人称")%很期待我穿著泳装的樣子？」
		PRINTFORMW 「……說什麼呢……快去準備！真是的……」
		PRINTFORMW 「…………說起來……體毛的處理、要好好地做一下了……」
	ENDIF
CASE "一番乗り"
	IF COND("同伴中")
		PRINTFORMW 「嗯？什么、只有%CNAME("一人称")%到了吗。来得稍微早了一些啊」
	ELSE
		PRINTFORMW 「呦！辛苦了、年轻人。午休期间不睡觉而在认真学习着呢？　嗯？」
		PRINTFORMW %CALLNAME:TARGET%用右手挽住%CALLNAME:MASTER%的頭、就这样抱着。
		PRINTFORMW ……她那富有弹性、硕大膨胀的胸部、就这么强行贴在%CALLNAME:MASTER%的脸颊上。
	ENDIF
CASE "ただいま"
	;弗理希艾尔も同居中？
	IF RAND:3 == 0 && FINDCHARA(NO, 1) > 0 && COND("同居中", FINDCHARA(NO, 1) )
		PRINTFORMW 「对不起、没想到回来晚了。稍等一下啊、现在就准备晚饭――」
		PRINTFORMW 「――是这样吗。%CALLNAME:FINDCHARA(NO, 1)%有些累了吗……看来还没习惯这种生活啊」
	ELSEIF RAND:2 == 0
		PRINTFORMW 「我回来了。哈~今天工作也很顺利」
	ELSE
		PRINTFORMW 「呼、我回来了。职工会议拖得太久、有点晚了」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		;弗理希艾尔も同居中？
		IF RAND:3 == 0 && FINDCHARA(NO, 1) > 0 && COND("同居中", FINDCHARA(NO, 1) )
			IF RAND:2 == 0
				PRINTFORMW 「你好、回来了吗～今天一整天也要过去了啊～」
				PRINTFORMW ……%CALLNAME:TARGET%一手拿着啤酒罐、微醺躺在客厅的沙发上……
				PRINTFORMW 「今天的家务活轮到%CALLNAME:FINDCHARA(NO, 1)%了呢、稍稍放松了些哦。呵呵~」
				PRINTFORMW 「……咕嘟、咕嘟……哈～！下班后在家喝啤酒~最棒了！」
				PRINTFORMW 「我老公在的时候、可绝对不会做这种事。真是的、有女仆的生活真是太棒了！」
				PRINTFORMW ……正在后面厨房准备晚餐的%CALLNAME:FINDCHARA(NO, 1)%、有一点点尴尬的样子。
				PRINTFORML 我向忠诚的女仆致以感谢的目光。
				PRINTFORMW %CALLNAME:MASTER%从%CALLNAME:TARGET%一旁拿起啤酒、慢吞吞地走近了。
			ELSE
				PRINTFORMW 「欢迎回家。很快就能吃晚饭了、洗洗手等待一下」
				PRINTFORMW 穿着围裙形象%CALLNAME:TARGET%、在厨房里以精妙的手法准备着晚饭。
				PRINTFORMW 「什么都让%CALLNAME:FINDCHARA(NO, 1)%做的话、我家务活的本事要荒废了呢。让我也来帮忙吧」
			ENDIF
		ELSEIF RAND:2 == 0
			CALL SETFLAG, "衣装全脱ぎ", TARGET
			CALL SET_COORDINATE, "内衣", TARGET
			CALL REPRINT_MAIDFACE
			PRINTFORMW 「欢迎回家。回来得相当晚啊」
			PRINTFORMW ……明显是刚刚洗过澡、只穿着内衣的%CALLNAME:TARGET%站在走廊里……
			PRINTFORMW 还在轻微冒着蒸汽的小麦色肌肤、以及难以描述的美艳身姿……
			PRINTFORMW 「嗯？……啊、我先把澡洗了」
			PRINTFORMW 「现在没有人使用浴室、%CNAME("二人称")%干脆也进去把澡洗了吧」
			PRINTFORMW %CALLNAME:TARGET%这么说着走回了客厅、在走路的时候被内衣包裹的丰满山丘上下跃动着……
		ELSE
			PRINTFORMW 「嗯……你回来了」
			PRINTFORMW %CALLNAME:TARGET%坐在餐厅、一脸认真地面对着笔记本电脑。
			PRINTFORMW ……戴着平时不戴的眼镜、给人一种比平时更严肃的印象。
			PRINTFORMW 「……啊、对不起。工作只剩下一点、马上就要结束了……」
			PRINTFORMW 说完之后%CALLNAME:TARGET%又沉默了、继续工作着……
			PRINTFORML
			PRINTFORMW 「——呼。好、终于结束了！嗯~~~好累啊……”」
			PRINTFORMW 好像暂时告一段落了、%CALLNAME:TARGET%把背靠在椅子上、用力向后舒展着身体。
			PRINTFORMW ……她丰满的胸部、像要撑開衣服似的挺起、美丽的曲线尽显无疑……
			PRINTFORMW 「……嗯？怎么了、还站在那里啊。%CNAME("一人称")%有什么事吗？」
			CALL SET_OTHER, "眼鏡", TARGET
		ENDIF
	ELSE
		IF COND("水泳") && CHECK_CLO("泳装姿") && CHECK_BODYS("競泳泳装")
			IF CEVENT("泳装お披露目")
				PRINTFORMW 「哦、辛苦了。打扰你了」
				PRINTFORMW 在%CALLNAME:MASTER%对面的%CALLNAME:TARGET%、理所当然是泳装的姿态……
				IF RAND:3 == 0
					PRINTFORML 穿着竞赛泳衣的%CALLNAME:TARGET%趴倒在沙发上。
					PRINTFORMW 完美的臀部曲线、就那么向上展露着……
				ELSEIF RAND:2 == 0
					PRINTFORML PRINTFORML 穿着竞赛泳衣的%CALLNAME:TARGET%躺倒在沙发上。
					PRINTFORMW 丰满的山丘、丝毫没有下垂地挺立着……
				ELSE
					PRINTFORML 竞赛泳衣非常地合身、显得很苗条。
					PRINTFORMW 琥珀色的美腿恰到好处地展露出来……
				ENDIF
			ELSE
				PRINTFORMW 「哦、辛苦了。打扰你了」
				PRINTFORMW ……这样说着、在部室的沙发上放松的%CALLNAME:TARGET%、不知为何是穿着泳衣……
				PRINTFORMW 「怎么了、突然站在那里……嗯？这身打扮吗？」
				PRINTFORMW 「上游泳课的日子穿泳衣是这个部室的规则……不对吗？」
				PRINTFORML 竞赛泳衣非常地合身、显得苗条而且恰到好处地展示了肉体。
				PRINTFORMW 优雅地交叉美腿躺下的身姿、真是让人“烦恼”的耀眼光景啊……
				PRINTFORML 在心中向我忠诚的女仆送出最大程度的称赞、
				PRINTFORMW %CALLNAME:MASTER%向%CALLNAME:TARGET%强有力地竖起大拇指。
				PRINTFORMW 「……？？？」
				CALL SET_CEVENT, "泳装お披露目"
			ENDIF
		ELSE
			PRINTFORMW 「哦、辛苦了。打扰你了」
		ENDIF
	ENDIF

CASE "入室打招呼"
	SELECTCASE CFLAG:MASTER:PREV_DAILYCATE
		;Talk
		CASE 1
			SELECTCASE CSTR:MASTER:PREV_DAILYCOM
				CASE "甜言蜜语"
					PRINTFORMW 「啊……对不起。打扰了、呐」
					PRINTFORMW %CALLNAME:TARGET%看到正在说话的%CALLNAME:MASTER%、不好意思地挠了挠頭。
				CASE "打招呼舐陰", "打招呼口交"
					PRINTFORMW 「好吧。打扰……了……哦——」
					PRINTFORMW 「……啊、那个、什么啊。好像打扰你了」
					PRINTFORMW 「……对不起。我这就走」
					PRINTFORMW %CALLNAME:MASTER%苦笑地挽留住%CALLNAME:TARGET%、催促她进入了房间……
			ENDSELECT
		;互动
		CASE 2
			
		;卖个破绽
		CASE 3
			SELECTCASE CSTR:MASTER:PREV_DAILYCOM
				CASE "膝枕", "抱住", "膝枕"
					IF SAVESTR:現在位置 == "部室"
						PRINTFORMW 「哈哈……真是的。在学校内让女人这么服侍你、%CNAME("二人称")%是什么身份呐」
						PRINTFORMW %CALLNAME:TARGET%露出惊讶的表情叹了一口气……
					ENDIF
				CASE "展示内褲", "摩擦股間", "手淫", "摩擦股間", "口淫", "接吻", "把胸部压了上去"
					PRINTFORMW 「好吧。打扰……了……哦——」
					PRINTFORMW 「……啊、那个、什么啊。好像打扰你了」
					PRINTFORMW 「……对不起。我这就走」
					PRINTFORMW %CALLNAME:MASTER%苦笑地挽留住%CALLNAME:TARGET%、催促她进入了房间……
			ENDSELECT
		;胖次
		CASE 4
			PRINTFORMW 「はぁー……まったく。なに馬鹿なことをやってるんだ%CNAME("二人称")%は……」
			PRINTFORMW %CALLNAME:TARGET%露出惊讶的表情叹了一口气……
		;性処理
		CASE 6
			PRINTFORMW 「好吧。打扰……了……哦——」
			PRINTFORMW 「……啊、那个、什么啊。好像打扰你了」
			PRINTFORMW 「……对不起。我这就走」
			PRINTFORMW %CALLNAME:MASTER%苦笑地挽留住%CALLNAME:TARGET%、催促她进入了房间……
	ENDSELECT
	
	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("Ｈ的いちゃつき中", MASTER)
		PRINTFORMW 「はぁー……まったく。なに馬鹿なことをやってるんだ%CNAME("二人称")%は……」
		PRINTFORMW %CALLNAME:TARGET%露出惊讶的表情叹了一口气……
		RETURN 1
	ENDIF

	IF COND("水泳")
		IF CEVENT("泳装お披露目")
			PRINTFORMW 「哦、辛苦了。打扰你了哦」
			PRINTFORMW %CALLNAME:TARGET%一进部室就理所当然地换上了泳装……
		ELSE
			PRINTFORMW 「哦、辛苦了。打扰你了哦」
			PRINTFORML ……说着话的%CALLNAME:TARGET%进入房间后、突然在房间的角落里開始脱衣服、
			PRINTFORMW 一眨眼间、就成了游泳课上穿着泳衣的样子……
			PRINTFORMW 「呼、这样就好了……嗯、你用呆呆的表情看着什么啊？」
			PRINTFORMW 「听说游泳的日子就要在这个房间换泳衣的……不对吗？」
			PRINTFORML 在心中向我忠诚的女仆送出最大程度的称赞、
			PRINTFORMW %CALLNAME:MASTER%向%CALLNAME:TARGET%强有力地竖起大拇指。
			PRINTFORMW 「……？？？」
			CALL SET_CEVENT, "泳装お披露目"
		ENDIF
	ELSE
		PRINTFORMW 「哦、辛苦了。打扰你了哦」
	ENDIF
CASE "抜け出された"
	IF RAND:2
		PRINTFORMW 「什么嘛、今天没事了吗？　……啊、没什么、不要在意不要在意」
		PRINTFORMW 「我就自己在这里随便放松一下、你也没必要在意我、自己去干自己想做的事就行了」
	ELSE
		PRINTFORMW 「嗯？　今天就到这里吗。嘛、倒也没什么问题就是了……」
	ENDIF
CASE "便当"
	IF CEVENT("初めての便当") == 0
		PRINTFORMW 「看……按照约定我做了便当了」
		SIF COND("同居中") == 0
			PRINTFORMW 「在不被丈夫发现的情况下多做一人份的便当、相当辛苦啊」
		PRINTFORML 打開盒饭的盖子一看、里面的菜色鲜艳、种类丰富。
		PRINTFORMW 摆放精致的便当、非常能引起人的食欲……
		PRINTFORMW 「什么啊、那意外的眼神。别看我这样、料理还是很拿手的啊」
		PRINTFORMW 「别发呆啦、快点吃吧。%CNAME("一人称")%的午休时间也是很短暂的啊」
		PRINTFORMW 在%CALLNAME:TARGET%的催促下、%CALLNAME:MASTER%開始吃便当了。
		PRINTFORMW ……便当有着不逊色于亮丽外观的美味、%CALLNAME:MASTER%非常满足……
		CALL SET_CEVENT, "初めての便当"
	ELSEIF RAND:4 == 0 && COND("同居中") == 0
		PRINTFORMW 「啊！这道菜我第一次挑战。怎么样……味道怎么样啊？」
		PRINTFORMW ……美味啊。%CALLNAME:MASTER%满足地向%CALLNAME:TARGET%点了点頭。
		PRINTFORMW 「PRINTFORMW 「哇……呵呵、太好了……」
		PRINTFORMW 「其实那个、是用了丈夫的便当没有放的材料、专門给%CNAME("二人称")%做的哦」
		PRINTFORMW 「因为我老公很挑食、我们总是只吃固定几样菜……」
		PRINTFORMW 「相反、%CNAME("二人称")%什么都会高兴地吃啊。想挑战一下各种各样的新菜呢……」
		PRINTFORMW 「托您的福、最近制作便当变得很開心……你要负起这个责任哦、呵呵」
	ELSEIF RAND:3 == 0
		PRINTFORMW 菜肴完全命中了个人的喜好、%CALLNAME:MASTER%的筷子完全停不下来……！
		PRINTFORMW 「喂喂。不用吃得那么急、又没人和你抢~」
		PRINTFORMW 「……不过嘛、能吃到美味的料理、果然还是会很開心吧……呵呵呵」
	ELSEIF RAND:2 == 0
		PRINTFORMW 「怎么样、好吃吗？不用客气、尽情享用吧」
		PRINTFORMW 「……哇、嘴边还沾着饭粒呢……啊、不是那边啦」
		PRINTFORMW 「已经……你看、我来吧、在这边啊」
		PRINTFORMW %CALLNAME:TARGET%用手擦去%CALLNAME:MASTER%嘴边的米粒。
		PRINTFORMW 就这样把米粒放进自己的嘴里、用舌頭舔着……
		PRINTFORMW 「唔……嗯、这样就好了。……嗯？怎么了――、啊啊」
		PRINTFORMW 「对不起、不知不觉就和在老公身边一样了。惹你讨厌了吗？」
	ELSE
		PRINTFORMW 「……无论%CNAME("二人称")%做什么、都能吃得很香呢」
		PRINTFORMW 实际上、%CALLNAME:TARGET%做的便当总是很好吃。
		PRINTFORMW %CALLNAME:MASTER%坦率地说出了这句话。
		PRINTFORMW 「……真的、%CNAME("二人称")%是个好孩子啊。哼哼哼……♪」
		PRINTFORML %CALLNAME:MASTER%被带着放松表情的%CALLNAME:TARGET%紧紧抱住、
		PRINTFORMW 把你的脸埋进她丰满的谷间、轻轻地抚摸着你的頭。
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K18, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「……没想到、竟然要寄宿在这样的一间的房里啊」
	PRINTFORML 「嘛、从今天開始、要和%CNAME("一人称")%住在一起咯？……请多関照」
CASE "浑身是汗"
	PRINTFORML 「呼～……运动之后、非常地热啊……」
	PRINTFORML %CALLNAME:TARGET%啪嗒一声拉開T恤的领口。
	PRINTFORML 浮现着汗水的深谷若隐若现……
CASE "刚睡醒"
	PRINTFORML 「哼……哈……嗯~睡得好香」
	PRINTFORML 睡醒了的%CALLNAME:TARGET%伸了一个大大的懒腰、丰满的双乳好像都要把前胸的衣服撑破了……
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K18
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 
CASE "契約成立"
	PRINTFORML 
CASE "摆脱"
	SELECTCASE SAVESTR:現在位置
