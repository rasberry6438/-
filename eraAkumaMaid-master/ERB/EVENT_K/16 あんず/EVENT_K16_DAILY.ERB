@KOJO_GREETING_K16, ARGS
#DIM MEMO_LINECOUNT
;打招呼
#DIMS GREETING
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT


SELECTCASE ARGS
CASE "朝拘束"
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「哟～早上好呀！ 话不多说赶紧来一起快活吧～～♪」
			CASE 1
				PRINTFORMW 「嘿嘿～～♪ 就是因为%CNAME("二人称")%如此大意才会被捉住的哦～♪」
			CASEELSE
				PRINTFORMW 「摆出这么一副傻脸在那里呼呼大睡的～……是在诱惑%CNAME("一人称")%吗～？！」
		ENDSELECT
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「啊、起来了？ 那就给我打起精神！%CNAME("二人称")%也要帮%CNAME("一人称")%舔到舒服为止！」
					PRINTFORMW 「这种回礼精神可是很重要的哦～♪」
				CASE 1
					PRINTFORMW 「哎呀呀～一大早的就湿成了这样！ 这下怕不是得在下面铺层吸水垫会比较好吧？」
				CASEELSE
					PRINTFORMW 「嗯～！在无意识当中也有好好地在当一个女孩子呢，佩服佩服！」
				ENDSELECT
		;それ以外
		ELSE
			PRINTFORMW 「好嘞、就稍微借%CNAME("一人称")%用来练习一下前阵子在网上看到的技巧吧～♪」
		ENDIF

	CASE "朝足交"
		SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 「嘿嘿嘿～感觉怎么样啊～…喂…不准动不准动！ %CNAME("一人称")%现在可是正单腿站在床上啊！」
				PRINTFORMW 「踩上去了踩上去了！ 要踩到蛋蛋上了！」
			CASE 1
				PRINTFORMW 「嚼嚼嚼……啊、%CNAME("一人称")%现在正在吃早餐呢」
				PRINTFORMW 「反正对付%CNAME("二人称")%用一条腿就足够了吧♪」
			CASE 2
				PRINTFORMW 「耶～～ 早上好啊～～ %CNAME("二人称")%的肉棒现在已经被%CNAME("一人称")%给完全压制了！」
				PRINTFORMW 「想要解放%CNAME("二人称")%的肉棒的话，就%TEXTR("把作业的答案给我抄/下次带我出去玩/帮我给游戏练级/说10次杏珠真可爱")%～！」
			CASE 3
				PRINTFORMW 「嘿嘿～♪ 不稍微惩罚一下的话，这根坏心眼肉棒又会马上去做那些坏事了呢～！」
			CASEELSE
				PRINTFORMW 「嘿呀嘿呀♪ 感觉咋样啊～？」
		ENDSELECT
	CASE "朝の新妻PLAY"

	CASE "朝乳交"
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「呼…呼… 哈啊～这么弄起来意外的累人啊～～！」
					PRINTFORMW 「早知道就不去想用乳交来叫%CNAME("二人称")%起来的主意了～！」
				CASE 1
					PRINTFORMW 「看招看招、这种事可没别人会帮%CNAME("二人称")%弄了哦～！ 尽管给我感激零涕吧～♪」
				CASEELSE
					PRINTFORMW 「嘿嘿～ 怎么样啊～%CNAME("一人称")%这美好的欧派天国！」
					PRINTFORMW 「不论是睡着还是醒着时都仿佛在梦中一般吧～～♪」
				ENDSELECT
		;それ以外
		ELSE
			PRINTFORMW 「把这个…这么弄……啊咧～ 没法像视频里一样顺利啊～」
			PRINTFORMW 「……啊、你醒啦？ 早安～」
		ENDIF
	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「啊、起来了？ 那就给我打起精神！%CNAME("二人称")%也要帮%CNAME("一人称")%舔到舒服为止！」
					PRINTFORMW 「这种回礼精神可是很重要的哦～♪」
				CASE 1
					PRINTFORMW 「看招看招♪ 一起来就能享受到%CNAME("一人称")%的口交服务会很高兴吧～？」
				CASEELSE
					PRINTFORMW 「真是的～不在一早上就稍微榨点出来就会立马积攒得满满的呢～」
				ENDSELECT
		;それ以外
		ELSE
			PRINTFORMW 「好嘞、就稍微借%CNAME("一人称")%用来练习一下前阵子在网上看到的技巧吧～♪」
		ENDIF
	ENDSELECT

CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝六九式"
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「嘿嘿嘿～ 怎样啊～惊喜吗～？」
			CASE 1
				PRINTFORMW 「哎呀，当瞟到内裤下面露出的那玩意时就不禁想到『必须上了』！什么的♪」
			CASEELSE
				PRINTFORMW 「啊咧？！ 明明都弄出了这么大动静竟然还睡得那么死？ ……早上好！」
		ENDSELECT
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「唔，好像今天有体育课啊……好麻烦啊～」
			CASE 1
				PRINTFORMW 「啊咧…体操服好像昨天拿去洗了啊……算了不管了」
			CASE 2
				PRINTFORMW 「啊～啊～～有体育课的日子真是让人抑郁啊～」
			CASEELSE
				PRINTFORMW 「啊啊～！ 不想非得在上课时去运动啊～」
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 「呼啊～～好困啊～～」
			CASE 1
				PRINTFORMW 「啊～啊～就没有陨石什么的落下来让学校休课吗～」
			CASE 2
				PRINTFORMW 「早～～安～～……呼啊…」
			CASE 3
				PRINTFORMW 「奇怪、鞋子放哪去了」
			CASEELSE
				PRINTFORMW 「厕所里面的是谁～～？ 赶快出来～～」
		ENDSELECT
	ENDIF
CASE "一番乗り"
	IF TALENT:相思相愛
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「哈！！堂堂亮相……等下这不是谁也不在嘛」
				PRINTFORMW 「难得有机会在開門前两个人一起摆好姿势了，这样不就显得%CNAME("一人称")%们像个笨蛋一样吗？」
			CASE 1
				PRINTFORMW 「哎呀呀，搞什么嘛怎么谁都不在」
				PRINTFORMW 「大家就不能稍微学习一下%CNAME("一人称")%们两人的这股干劲嘛～」
			CASEELSE
				PRINTFORMW 「哎呀呀，难得有机会本来还想着卖点狗粮给大家看一下呢」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「哦、%CNAME("二人称")%动作真快呢」
			CASE 1
				PRINTFORMW 「嘿嘿～ %CNAME("二人称")%翘掉值日过来了♪」
			CASE 2
				PRINTFORMW 「哦～？ 挺早的嘛！　真有干劲呢，佩服佩服」
			CASEELSE
				PRINTFORMW 「哦♪ 趁着人少时抢占自己喜欢的座位咯！」
		ENDSELECT
	ENDIF
	
CASE "ただいま"
;周六日も講義主上ゃないですかやだぁ～～～っ！！
;でもそのまんまでいいや
	IF TEXTS("曜日") == "金"
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「这周大家也辛苦了！　明天就是休息日了！　好耶！」
			CASE 1
				PRINTFORMW 「%CNAME("一人称")%又撑过了一周！」
				PRINTFORMW 「然后终于！ 期待已久的周末就近在眼前了！」
			CASE 2
				PRINTFORMW 「耶耶耶♪ 周末周末～～♪」
				PRINTFORMW 「大玩特玩到把学校的事全部抛到脑后的程度吧～～」
			CASEELSE
				PRINTFORMW 「我回来啦～～♪」
				PRINTFORMW 「知道吗？知道吗？ 其实明天就是双休日了哦？」
		ENDSELECT

	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「好～～嘞」
			CASE 1
				PRINTFORMW 「我回来啦～～♪」
			CASE 2
				PRINTFORMW 「糟了糟了！ 要错过那个节目了！！」
			CASEELSE
				PRINTFORMW 「啊～～已经快累死了～～！」
		ENDSELECT
	
	ENDIF

CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「哎呀？%CNAME("二人称")%现在才回来？」
			CASE 1
				PRINTFORMW 「欢迎回来～　%CNAME("二人称")%又绕路去哪了？　有带礼物回来给%CNAME("一人称")%吗？？」
			CASE 2
				PRINTFORMW 「啊～早知道就联系一下了，有想要%CNAME("一人称")%回来路上帮忙买的东西啊～」
			CASEELSE
				PRINTFORMW 「欢迎回来～～」
		ENDSELECT
	ELSE
		IF COND("同伴中", MASTER)
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「哎呀、%CNAME("partner")%也在一起啊」
			CASE 1
				PRINTFORML 「%CNAME("partner")%好啊～～～」
				PRINTFORMW 「顺便%CNAME("二人称")%也好啊♪」
			CASEELSE
				PRINTFORMW 「%CNAME("partner")%你和这家伙一起出去没问题吗～？」
			ENDSELECT
		ELSE
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「好慢！这已经算是迟到了哦～～ 」
			CASE 1
				PRINTFORMW 「真是的！ 你这家伙是不是有些懒散过頭了？」
			CASE 2
				PRINTFORMW 「啊、终于回来了吗！」
			CASEELSE
				PRINTFORMW 「既然都回来迟了，至少就不能拿点什么有趣的玩意回来吗？ 真是不行啊～」
			ENDSELECT
		ENDIF
	ENDIF

CASE "入室打招呼"
			SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 「各位好呀～～！！ 」
			CASE 1
				PRINTFORMW 「大家好～～！」
			CASE 2
				PRINTFORMW 「耶～耶～ 你好你好～！」
			CASE 3
				PRINTFORMW 「我是警察！ 所有人都不准动！ ……開玩笑的！」
			CASEELSE
				PRINTFORMW 「当当当～～！！ 」
			ENDSELECT
			
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			SELECTCASE RAND:6
			CASE 0
				PRINTFORMW 「各位好呀～～！！ 」
			CASE 1
				PRINTFORMW 「大家好～～！」
			CASE 2
				PRINTFORMW 「哦～～！ 大家都已经集中了嘛～～！」
			CASE 3
				PRINTFORMW 「哦、今天人还挺齐的嘛！ 那么要来玩点什么吗？！」
			CASE 4
				PRINTFORMW 「大家稍微听我说哦～～！ 刚才在来之前啊……」
			CASEELSE
				PRINTFORMW 「哎呀～似乎迟到了呢　抱歉抱歉♪」
			ENDSELECT
	ELSE
			SELECTCASE RAND:6
			CASE 0
				PRINTFORMW 「各位好呀～～！！ 」
			CASE 1
				PRINTFORMW 「大家好～～！」
			CASE 2
				PRINTFORMW 「哦、今天就这些成员吗？」
			CASE 3
				PRINTFORMW 「哦～～？ 看来%CNAME("一人称")%还不是最后一个！」
			CASE 4
				PRINTFORMW 「哦，这就已经来了？ 佩服佩服♪」
			CASEELSE
				PRINTFORMW 「哦、正好热闹着呢各位！」
			ENDSELECT
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		;老师を発言者として選び直す
		ACTOR = FIND_COND("教員", "入室打招呼中")
		CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%
			SELECTCASE RAND:4
			CASE 0
				CALL EMOTION_PLUS, "惊嚇", TARGET
				PRINTFORMW 『呃！ 老师来了！ 不妙不妙！ 快帮忙把这个藏起来！』
			CASE 1
				PRINTFORMW 『才没有在偷懒呢～都有在好好干哦～』
			CASE 2
				PRINTFORMW 『您～好～！』
			CASEELSE
					IF RELATION:(NO:ACTOR) >= 150
						PRINTFORMW 『哎呀～老师也已经完全习惯这里的氛围了呢～』
					ELSE
						PRINTFORMW 『呃，老师也来了么。 真难放開啊～……』
					ENDIF
			ENDSELECT
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 『好啊！』
			CASE 1
				PRINTFORMW 『大家～～好呀！』
			CASE 2
				PRINTFORMW 『哦，社团教室也逐渐变得狭窄起来了啊』
			CASE 3
				PRINTFORMW 『好啦好啦、那边再稍微挤一下！ 给人让个位子！』
			CASEELSE
				PRINTFORMW 『嘿嘿～～不早一点来可是抢不到好位子的哟～～♪』
			ENDSELECT
	ELSE
			SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 『好啊！』
			CASE 1
				PRINTFORMW 『大家～～好呀！』
			CASE 2
				PRINTFORMW 『好、既然%CNAME(CALLNAME_ACTOR)%也来了那就玩点什么吧！』
			CASE 3
				PRINTFORMW 『哼哼、怎么样啊？被%CNAME("一人称")%抢先到达的感觉！』
			CASEELSE
				PRINTFORMW 『哦、来得真巧啊！ 听我说听我说啊～其实刚才啊……』
			ENDSELECT
	ENDIF
CASE "居間に現れた"
	IF CFLAG:呼び出された
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「咋了～？」
			CASE 1
				PRINTFORMW 「哦？ 是有谁送零食小礼包过来了吗？」
			CASEELSE
				PRINTFORMW 「干嘛啦～明明%CNAME("一人称")%正打到高潮局面呢～」
			ENDSELECT
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「你原来还在啊！」
	ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「啊…肚子饿扁了啦～有什么吃的东西吗？」
			CASE 1
				PRINTFORMW 「那啥～%TEXTR("纸巾、纸巾哪去了/延长线、延长线去哪了/新的灯管放在哪里？/这里有图钉之类的玩意吗～？")%」
			CASEELSE
				
			ENDSELECT
	ENDIF
CASE "全裸化への反応"
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「啊啦啦～真放得開啊～」
			CASE 1
				PRINTFORMW 「呀啊啊！！ 突然之间開始脱衣服是要干什么呀～？！」
			CASE 2
				PRINTFORMW 「哇、为啥要脱衣服？！」
			CASEELSE
				PRINTFORMW 「哦！ 所以？ 所以呢？ 准备干什么？ 为啥要全裸？！」
			ENDSELECT

CASE "抜け出された"
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「好好、请吧～想去哪里都行，慢走～」
			CASE 1
				PRINTFORMW 「诶？ 这就解散了？ 不会太快了吗？！」
			CASE 2
				PRINTFORMW 「咕～～～～！ 搞什么嘛那帮家伙！！」
			CASEELSE
				PRINTFORMW 「哇哦，完全没有把%CNAME("一人称")%这边的事放在眼里呢」
			ENDSELECT
CASE "便当"
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「来，难得%CNAME("一人称")%早起给%CNAME("二人称")%做的，赶紧给我吃了你个走运的小混蛋」
			CASE 1
				PRINTFORMW 「外面沾着柴鱼花的是柴鱼花饭团，没有的就是昆布味的呢」
			CASE 2
				PRINTFORMW 「这可是%CNAME("一人称")%倾注心血做出来的哦～虽然全都是速冻食品就是了」
			CASEELSE
				PRINTFORMW 「偶尔换成%CNAME("二人称")%来做便当也是完全没问题的哦？」
			ENDSELECT
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
