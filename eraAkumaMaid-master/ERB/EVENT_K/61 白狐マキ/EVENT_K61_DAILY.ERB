;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K61, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
	CASE "朝袭"
		SELECTCASE CSTR:朝袭
			CASE "朝口交"
				CALL PRINT_STRW, @"「%TEXTR("ん、おはおー/んんんー/んっ、んっ_HH_")%」"
				RETURN 1
			CASE "朝騎乗位"
				CALL PRINT_STRW, @"「%TEXTR("ん、おはよ_HH_/んー/んっ、んっ_HH_/あ_HH_、おきた？")%」"
				DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
				RETURN 1
			CASE "朝足交"
				CALL PRINT_STRW, @"「%TEXTR("ん、おはよ/ん、おきた？")%」"
				RETURN 1
			CASE "朝の新妻PLAY"
				CALL PRINT_STRW, @"「%TEXTR("ん……/んっ")%」"
				RETURN 1
			CASEELSE
				DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, 朝袭 %CSTR:朝袭%
		ENDSELECT
	CASE "打招呼"
		;朝のＨイベント終了後にする打招呼
		SELECTCASE CSTR:朝袭
			CASE "朝足交"
				CALL PRINT_STRW, @"「%TEXTR("ちんちんよかった？/きもちかった_HH_？/もいっかいする？")%」"
				RETURN 1
			CASE "朝口交"
				CALL PRINT_STRW, @"「%TEXTR("ちんちんよかった？/きもちかった_HH_？/もいっかいする？/もちょっとでそう？/ん_HH_、ちんちんくさい_HH_/ん、しょっぱい_HH_")%」"
				RETURN 1
			CASE "朝の新妻PLAY"
				CALL PRINT_STRW, @"「%TEXTR("んふーっ_HH_……/だいどころ、あぶないよ？")%」"
				RETURN 1
			CASEELSE
				DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, 朝袭 %CSTR:朝袭%
		ENDSELECT

		PRINTDATAW
			DATA 「ん」
			DATA 「んー」
			DATA 「おはー」
			DATA 「おはよ」
			DATA 「おはおは」
			DATA 「おはござます」
			DATA 「ん、おはよ」
		ENDDATA
		RETURN 1
	CASE "登校"
		IF DAYEV("試験当日")
			PRINTDATAW
				DATA 「てすとやだ……」
				DATA 「てすとめんどくさい……」
				DATA 「……ん」
				DATA 「ん……」
			ENDDATA
			RETURN 1

		ELSEIF DAYEV("試験返却日")

		ELSEIF COND("朝袭相手")
			CALL PRINT_STRW, @"「%TEXTR("またおひるにね/おひるにまたする_UH_？")%」"
			RETURN 1

		ELSEIF COND("体育")

		ELSEIF COND("水泳")

		ELSE
			PRINTDATAW
				DATA 「がっこー」
				DATA 「がっこがっこー」
			ENDDATA
			RETURN 1

		ENDIF
	CASE "一番乗り"
		;この角色と一緒に部室に入った際に誰も他にいない場合
		PRINTDATAW
			DATA 「ん」
			DATA 「んー」
			DATA 「んっんー」
			DATA 「へいらっしぇー」
			DATA 「たのもー」
			DATA 「かんねー、いちばんのり」
		ENDDATA
		RETURN 1
	CASE "ただいま"
		PRINTDATAW
			DATA 「ん」
			DATA 「んー」
			DATA 「おうち」
			DATA 「おうちー」
			DATA 「ただま」
			DATA 「ただまー」
			DATA 「たでまー」
			DATA 「あいむほーむ」
			DATA 「あいむほむほーむ」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "既に入室"
		;家で出迎えてくれる時
		IF COND("夜")
			PRINTDATAW
				DATA 「ん」
				DATA 「んー」
				DATA 「ん、おかえり」
				DATA 「おかえりー」
				DATA 「おかえぃー」
				DATA 「へいらっしぇー」
				DATA 「らっしゃっせー」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSE
			PRINTDATAW
				DATA 「ん」
				DATA 「んー」
				DATA 「こんちー」
				DATA 「こんちーわ」
				DATA 「んちわー」
				DATA 「ちわー」
				DATA 「わー」
				DATA 「ちわ」
				DATA 「ちわわ」
				DATA 「へいらっしゃい」
				DATA 「へいらっしぇー」
				DATA 「らっしゃっせー」

				;便利店の入店音
				DATA 「てれてれ てれーん てれれれ れーん」
			ENDDATA
			RETURN 1
		ENDIF

	CASE "入室打招呼"
		IF CFLAG:呼び出された
			PRINTDATAW
				DATA 「ん」
				DATA 「んー」
				DATA 「なにごとだー」
				DATA 「よばれてとびでてー」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSEIF COND("Ｈ的いちゃつき中", MASTER)
			;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
			IF (TALENT:淫乱 && !RAND:2)
				PRINTDATAW
					DATALIST
						DATA 「ん……」
						DATAFORM %CALLNAME:PLAYER%たちに気が付いた%CALLNAME:TARGET%は物欲しそうな目をしている。
					ENDLIST
				ENDDATA
				DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
				RETURN 1
			ENDIF

			PRINTDATAW
				DATALIST
					DATA 「ん……ふーん」
					DATAFORM %CALLNAME:PLAYER%たちを不躾に眺めている……。
				ENDLIST
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSE
			PRINTDATAW
				DATA 「たのもー」
				DATA 「んー」
				DATA 「んっんー」
				DATA 「まだやってる？」

				;入ってきたのは你だ
				DATA 「へいらっしぇー」
			ENDDATA
			RETURN 1
		ENDIF
	CASE "簡易入室打招呼"
		;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
		IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			;遅れた時のセリフ（既に部員の４分の３が集合済）
			PRINTDATAW
				DATA 「ん」
				DATA 「んー」
				DATA 「まだやってる？」
				DATA 「おくれちった」
				DATA 「おそくなった」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSEIF CFLAG:呼び出された
			PRINTDATAW
				DATA 「ん」
				DATA 「んー」
				DATA 「なにごとだー」
				DATA 「よばれてとびでてー」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSE
			PRINTDATAW
				DATA 「たのもー」
				DATA 「んー」
				DATA 「んっんー」

				;入ってきたのは你だ
				DATA 「へいらっしぇー」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ENDIF
	CASE "打招呼返し"
		;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
		;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
		;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる

		;打招呼してきた人の中から一人選ぶ
		ACTOR = FIND_COND("入室打招呼中")
		;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
		CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

		;ここでは括弧に『』を使う

		;相手が現れるのが遅かった（既に部員の４分の３が集合済）
		IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			PRINTDATAW
				DATA 『ん』
				DATA 『んー』
				DATA 『らっしゃー』
				DATA 『いらっしゃー』
				DATA 『いらっしゃいまー』
				DATA 『らっしゃいまー』
				DATA 『おつとめごくろー大人ま』
			ENDDATA
			RETURN 1
		ELSE
			IF CHARA_NUM("入室打招呼中") > 1
				PRINTDATAW
					DATA 『ん』
					DATA 『んー』
					DATA 『らっしゃー』
					DATA 『らっしゃいまー』
					DATA 『だんたい大人のおなーりー』
				ENDDATA
				RETURN 1
			ELSE
				PRINTDATAW
					DATA 『ん』
					DATA 『んー』
					DATA 『ちわー』
					DATA 『ちわわー』
					DATA 『ちくわー』
					DATA 『らっしゃー』
					DATA 『いらっしゃー』
					DATA 『いらっしゃいまー』
					DATA 『らっしゃいまー』
				ENDDATA
				RETURN 1
			ENDIF
		ENDIF
	CASE "便当"
		PRINTDATAW
			DATA 「んまんま」
			DATA 「んふー」
			DATA 「ん、おいしい？」
			DATA 「どう？」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "場所を変える"
		PRINTDATAL
			DATA 「どしたの？」
			DATA 「どしたー？」
			DATA 「ん？」
			DATA 「んー？」
			DATA 「どこいくの？」
		ENDDATA
		RETURN 1
	CASEELSE
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
ENDSELECT
RETURN 0

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;动画ーションの関係上、１ターンに何度も呼ばれたりするのでフラグ操作をする場合には
;IF CFLAG:KOJO_DAILY済 == 0
;で分岐させるのが良いでしょう
;-------------------------------------------------
@KOJO_DAILY_K61, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
	CASE "初回台詞"
		;初回のセリフ
		PRINTFORML 「んふー」
		PRINTFORML %CALLNAME:TARGET%は、すでに我が物顔でくつろいでいる。
		RETURN 1
	CASE "浑身是汗"
		PRINTDATAL
			DATA 「……ん」
			DATA 「……べとべとん」
			DATA 「……へっくし」
			DATA 「……あつ」
		ENDDATA
		RETURN 1
	CASE "刚睡醒"
		PRINTDATAL
			DATALIST
				DATAFORM 「……%TEXTR("んぅ/あさ/ひる/よる")%？」
				DATAFORM %CALLNAME:TARGET%は困倦ているようだ。
			ENDLIST
			DATALIST
				DATAFORM 「……%TEXTR("ぁふ/んわ/ねむ")%」
				DATAFORM %CALLNAME:TARGET%は哈欠をしている。
			ENDLIST
		ENDDATA
		RETURN 1
	CASE "点心の感想"
		;表記ゆれ対応
		IF K61_INCLUDES(SAVESTR:点心, CSTR:讨厌的东西+"/巧克力") != ""
			;ものをもらっておいてなんて野郎だ
			PRINTFORML 「むぅ……これにがて」
			PRINTFORML %CALLNAME:TARGET%は%K61_INCLUDES(SAVESTR:点心, CSTR:讨厌的东西 + "/巧克力")%があまり好きではないらしい。
		ELSEIF COND("点心当番")
			;今日の点心を用意したのは自分
			PRINTFORML 「んまんま」
			PRINTFORML %CALLNAME:TARGET%は%TEXTS("点心")%をもりもり食べている。
		ELSE
			;点心が亲手自制でない
			PRINTFORML 「んふー」
			PRINTFORML %CALLNAME:TARGET%は%TEXTS("点心")%をもりもり食べている。
		ENDIF
	CASE "睡眠"
		;沉眠著
		;梦话
		CALL KOJO_SLEEP_K61
		;梦话用の汎用地の文呼出
		CALL DAILY_MESSAGE, "梦话"
	CASE ""
		;nothing
	CASEELSE
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
	CASE "愛の告白"
		CALL PRINT_STRL, "「……ん_H_」"
		PRINTFORML %CALLNAME:TARGET%は頬を染めて有点害臊的うつむいている。
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "主人公的房間に連れ込まれた"
		;你と一緒に、主人公的房間に移動した
		PRINTDATAL
			;他人的房間を物色するデリカシーのなさ
			DATAFORM %CALLNAME:TARGET%は所在なさげに、きょろきょろとあたりを見回している。
			DATAFORM %CALLNAME:TARGET%はすんすんと鼻を鳴らしている。
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "自室に招待"
		;你と一緒に、partner的房間に移動した
		PRINTDATAL
			;でもかたづけない
			DATA 「ちらかってるけど」

			DATA 「ん、いらっしゃ」
			DATA 「ん、どしたの？」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "露脐装超短裙胖次チラ"
		PRINTDATAL
			DATA 「ん？」
			DATA 「ん、どしたの？」
			DATA 「なにかみつけた？」
			DATA 「なにかあった？」
			DATA 「……みえた？」
		ENDDATA
		RETURN 1
	CASE "掀起裙子"
		PRINTDATAL
			DATA 「へんたい……」
			DATA 「すかーとめくりまん……」
		ENDDATA
		RETURN 1
	CASE "撫摸股間"
		CALL PRINT_STRL, @"「%TEXTR("ん、へんたい……_HH_/もちょっとする？/もちょっといいよ？")%」"
		RETURN 1
	CASE "拜托処理性欲"
		PRINTDATAL
			DATA 「きもちかった？」
			DATA 「きもちよかった？」
			DATA 「……もっかいする？」
			DATA 「……もちょっとする？」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "检查内褲"
		IF COND("合意：检查内褲")
			PRINTDATAL
				DATA 「ん、へんたい……」
				DATA 「もうちょっとみる？」
				DATA 「もうちょっとさわる？」
			ENDDATA
			RETURN 1
		ELSE
			PRINTDATAL
				DATA 「へんたいちゅういほうはつれーちゅう」
				DATA 「へんたい……」
			ENDDATA
			RETURN 1
		ENDIF
	CASE "检查阴部"
		IF COND("合意：检查阴部")
			PRINTDATAL
				DATA 「ん、へんたい……」
				DATA 「もうちょっとみる？」
			ENDDATA
			RETURN 1
		ELSE
			PRINTDATAL
				DATA 「へんたいちゅういほうはつれーちゅう」
				DATA 「へんたい……」
			ENDDATA
			RETURN 1
		ENDIF
	CASE "打招呼舐陰"
		PRINTDATAL
			DATA 「へんたい……」
			DATA 「すきなの？」
		ENDDATA
		RETURN 1
	CASE "股間撮影", "阴部摄影", "裙下撮影", "内褲摄影"
		PRINTDATAL
			DATA 「へんたい……」
			DATA 「すきなの？」
			DATA 「きれいにとれた？」
			DATA 「それどうするの？」
		ENDDATA
		RETURN 1
	CASE "沏茶"
		PRINTDATAL
			DATALIST
				DATAFORM ｽﾞｽﾞ……
				DATAFORM 「ほふぅ」
				DATAFORM %CALLNAME:TARGET%は満足そうに%SAVESTR:茶%をすすっている、好きなのだろうか。
			ENDLIST
		ENDDATA
		RETURN 1
	CASE "泡茶"
		IF K61_INCLUDES(SAVESTR:点心, CSTR:讨厌的东西) != ""
			PRINTFORML %CALLNAME:TARGET%は%TEXTS("点心")%を睨みながら%SAVESTR:茶%をすすっている。
		ELSE
			PRINTFORML %CALLNAME:TARGET%は茶菓子をつまんでいる。
		ENDIF
		RETURN 1
	CASE "膝枕"
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%はうとうととしている。
			DATAFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%の膝の上でごろごろしている。
		ENDDATA
		RETURN 1
	CASE "抚摸尾巴"
		;嫌ではないが気になるので他のことはできない。
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%はうとうととしている。
			DATAFORM %CALLNAME:TARGET%はぼーっとしている。
			DATAFORM %CALLNAME:TARGET%は抚摸尾巴%CALLNAME:PLAYER%の手の動きが気になるようだ。
		ENDDATA
		RETURN 1
	CASE "胖次交易"
		PRINTDATAL
			DATA 「まいど有ー」
			DATA 「そんなにほしかったの？」
			DATA 「それどうするの？」
			;本人は返してもらうつもり
			DATA 「酱とかえしてね？」
		ENDDATA
		RETURN 1
	CASE "隐秘摄影失敗"
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%は照相机に向かってダブル剪刀手を決めている。
			DATAFORM %CALLNAME:TARGET%のドヤ顔が照相机にドアップになっている。
		ENDDATA
		RETURN 1
	CASE "一起学习"
		;REF 教材の参考 https://happylilac.net/mu1605121851.html
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%は定規をペンで弾き飛ばしている。
			DATAFORM %CALLNAME:TARGET%はくるくるとペンを回して……あっ、落とした。
			DATAFORM %CALLNAME:TARGET%はノートの隅に落書きをしている。
			DATAFORM %CALLNAME:TARGET%は分度器をじっと看了过来……。
			DATAFORM %CALLNAME:TARGET%はコンパスをくるくると回している。
			;摩擦で抜けなくなるやつ
			DATAFORM %CALLNAME:TARGET%は二冊の教科書の页を互い違いに挟み込んでいる。
			DATA 「にとーへん僕桑かっけい」
			DATA 「えいかく、どんかく、ちょっかーく」
			DATA 「3と、4と、5で、ちょっかく」
			;正三角形を二等分した30°, 60°, 90°の三角定規の各辺の長さの比は3:4:5じゃないので裏切られた気がしている。
			DATA 「3と4と5じゃなかった……」
			DATA 「球のてんかいずって、どんなの？」
			;地図記号
			DATA 「こうじょうに手がはえると、はつでんしょ」
		ENDDATA
		RETURN 1
	CASE "甜言蜜语"
		IF CFLAG:甜言蜜语
			;成功
			PRINTDATAL
				DATAFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%の腕に尾巴を絡ませている。
				DATAFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%に抱き着いている。
			ENDDATA
			RETURN 1
		ELSE
			;nothing			
		ENDIF
	CASE "膝枕"
		IF !RAND:3
			PRINTDATAL
				DATAFORM %CALLNAME:TARGET%は構ってほしそうに%CALLNAME:PLAYER%を看了过来。
			ENDDATA
			RETURN 1
		ENDIF
	CASE "亲脸頬"
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%の隣に座り、もじもじとしている。
			DATAFORM %CALLNAME:TARGET%は構ってほしそうに%CALLNAME:PLAYER%を看了过来。
			DATAFORM %CALLNAME:TARGET%は構ってほしそうにすり寄ってきている。
		ENDDATA
		RETURN 1
	CASE "抱住"
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%に抱き着いたまま、尾巴をぱたぱたと振っている。
			DATAFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%に抱き着いたまま、もじもじと身じろぎをしている。
			DATAFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%に抱き着いたまま、もじもじと身体を擦り付けている。
			DATAFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%の隣に座り、もじもじとしている。
