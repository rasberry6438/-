;约会の際のセリフ
;システム側の地の文や反応を出力させたくない場合には TFLAG:地の文スキップ = 1 としてください
;约会回数は EXP:约会経験 が+1ずつされます。初回のセリフは IF EXP:约会経験 == 0 で分岐するのが良いでしょう
;素質で分岐させる場合にはTALENT:恋慕での分岐がおすすめです
;-------------------------------------------------
@KOJO_DATE_K61, ARGS

SELECTCASE ARGS
	CASE "约会のお誘い"
		;你を週末约会に誘った
		PRINTDATAW
			DATA 「こんど、ひま？」
			DATA 「こんしゅう、ひま？」
			DATA 「じかんある？」

			;ん、じゃわからんが
			DATA 「ん」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, %STR:日常イベント名%, %STR:PLAY内容%
		RETURN 1
	CASE "约会に誘われた","邀请去约会"
		;你から週末约会に誘われた
		;自発的に约会の做便当给よ！って場合はCALL SET_CEVENT, "约会の便当"としてください。
		PRINTDATAW
			DATA 「わかった」
			DATA 「ん」
			DATA 「んー」
			DATA 「いくー」
			DATA 「なにするの？」
			DATA 「どこいく？」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, %STR:日常イベント名%, %STR:PLAY内容%
		RETURN 1
	CASE "约会本番"
		;地の文が用意されているので、轻轻地１行程度の反応で良いと思います
		SELECTCASE SAVESTR:约会
			CASEELSE
				DEBUGPRINTFORML KOJO_DATE_K61 约会本番, %SAVESTR:约会%
		ENDSELECT

	CASE "约会中の買食物"
		;地の文を自己書きたい場合にはコメントアウトを外してください
		;TFLAG:地の文スキップ = 1
		SELECTCASE SAVESTR:约会
			CASEELSE
				DEBUGPRINTFORML KOJO_DATE_K61 约会本番, %SAVESTR:约会%, %STR:選択したもの%
		ENDSELECT
	CASE "泳装決定"
		;泳装を買いに行って、結果的に選んだ泳装に対するセリフです
		;もしも去年も泳装を選んでいたなら、CSTR:今年の泳装 に記録されているので比較する事が出来る
		SELECTCASE SWIM(STR:選んだ泳装, "種類")
			CASEELSE
				DEBUGPRINTFORML KOJO_DATE_K61 泳装決定, %SWIM(STR:選んだ泳装, "種類")%
		ENDSELECT

	CASE "泳装に着替えた"
		;泳池などで泳装姿に着替え、待ち合わせ場所に集合した時です
		;どの泳装に着替えたか？
		SELECTCASE SWIM(CSTR:今年の泳装, "種類")
			CASEELSE
				DEBUGPRINTFORML KOJO_DATE_K61 泳装に着替えた, %SWIM(CSTR:今年の泳装, "種類")%
		ENDSELECT
	CASE "帰り際"
		PRINTDATAW
			DATA 「んふー」
			;こなみ
			DATA 「たのしかった」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, %STR:日常イベント名%, %STR:PLAY内容%
		RETURN 1

	CASE "约会終了"
		;エッチをする場合
		IF TFLAG:床イン
			CALL PRINT_STRW, @"「%CNAME("性行為")%する？　ん_HH_」"
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, %STR:日常イベント名%, %STR:PLAY内容%
			RETURN 1
		ELSE
			PRINTDATAW
				DATA 「またあそぼうね」
				DATA 「またこようね」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, %STR:日常イベント名%, %STR:PLAY内容%
			RETURN 1
		ENDIF
	CASEELSE
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
ENDSELECT
RETURN 0

