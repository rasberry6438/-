;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K41, ARGS

#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = %NAME:MASTER%_%CSTR:MASTER:ふりがな%
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "２年生", "１年生", "初等部", "中等部"
			LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
		CASE "３年生", "研究生", "ＯＧ"
			LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
		CASE "不法侵入", "備品"
			LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
		CASEELSE
			IF COND("教員", PLAYER)
				LOCALS = %CALLNAME:PLAYER%老师_%CALLNAME:PLAYER%老师
			ELSE
				LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = あの人_あのひと
CASE "助手"
	SELECTCASE CSTR:PLAYER:学年
	CASE "２年生", "１年生", "初等部", "中等部"
		LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
	CASE "３年生", "研究生" ,"ＯＧ"
		LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
	CASE "不法侵入", "備品"
		LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
	CASEELSE
		IF COND("教員", ASSI)
			LOCALS = %CALLNAME:PLAYER%老师_%CALLNAME:PLAYER%老师
		ELSE
			LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
		ENDIF
	ENDSELECT
ENDSELECT
RETURNF LOCALS

