;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K26, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
	CASE "一人称"
		LOCALS = 我_我
	CASE "二人称"
		;相手が主人
		IF PLAYER == MASTER
			LOCALS = 前辈_学长
		;相手が主人ではない
		ELSE
			IF STRCOUNTS(CSTR:PLAYER:学年, "２年生","３年生","研究生")
				LOCALS = %CALLNAME:PLAYER%前輩_%CSTR:PLAYER:ふりがな%前輩
			ELSEIF STRCOUNTS(CSTR:PLAYER:学年, "教師","司書")
				LOCALS = 老师_老师
			ELSEIF CSTR:PLAYER:称号 == "実母"
				LOCALS = 母上大人_母上大人
			ELSEIF CSTR:PLAYER:称号 != "１年生" && !TALENT:娇小 && TALENT:PLAYER:娇小
				LOCALS = %CALLNAME:PLAYER%酱_%CSTR:PLAYER:ふりがな%酱
			ELSE
				LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
			ENDIF
		ENDIF
	CASE "主人"
		LOCALS = 前辈_学长
	CASE "助手"
		IF STRCOUNTS(CSTR:ASSI:学年, "２年生","３年生","研究生")
			LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
		ELSEIF STRCOUNTS(CSTR:ASSI:学年, "教師","司書")
			LOCALS = 老师_老师
		ELSEIF CSTR:ASSI:称号 == "実母"
			LOCALS = 母上大人_母上大人
		ELSEIF CSTR:ASSI:称号 != "１年生" && !TALENT:娇小 && TALENT:ASSI:娇小
			IF TALENT:ASSI:男性
				LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:ASSI%酱_%CSTR:ASSI:ふりがな%酱
			ENDIF
		ELSE
			LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
		ENDIF
	CASE "阴茎"
		IF TALENT:淫乱
			IF RAND:4 ;大体こっち
				LOCALS = 鸡鸡_鸡巴
			ELSE
				LOCALS = 丁丁_鸡巴
			ENDIF
		ELSE
			LOCALS = 小鸡鸡_小鸡鸡
		ENDIF
ENDSELECT
RETURNF LOCALS

