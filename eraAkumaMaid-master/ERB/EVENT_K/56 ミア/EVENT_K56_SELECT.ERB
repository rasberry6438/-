			CALL PRINT_STRW, @"沉眠著にも関わらず、米娅の%CNAME("直腸_地文")%は%CNAME("阴茎_地文")%を締め付け、本能的に%CNAME("精液_地文")%を搾り取った"
	ENDSELECT
	;肛門後背位1回分の経験を獲得
	CALL SIMULATE_TRAIN_COM, 72, 1
	EXP:睡眠姦経験 += 1
	PRINTL
	PRINTFORML 眠ってる米娅の%CNAME("肛門_地文")%%TEXTR("の奥/")%に{EX:Ａ中出し量}mlの%CNAME("精液_地文")%を%CNAME("中出し_地文2")%
	PRINTL
	PRINTFORML 睡眠姦経験+1
	PRINTFORML +肛門後背位1回分の経験
;睡眠中の悪戯はここまで

ENDSELECT

