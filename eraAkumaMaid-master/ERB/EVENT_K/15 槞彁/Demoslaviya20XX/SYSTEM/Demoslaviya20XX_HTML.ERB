;;;RESULT_DATA = 作成したHTMLを入れる所だよー これを戻り値として使うわよ！
;;;X_View = 描画範囲内左隅X座標
;;;Y_View = 描画範囲Y座標（要するにMAP全体で見たときのY座標　データ取得用）
;;;ARG:2 = 描画中Y座標行目（こっちは、表示画面内のY座標　表示するときの描画Y座標移動用）
;;;=========================
@D20XX_GenerateHTML_AttackRange(X_View,Y_View,Y_Line,RESULT_DATA)
	#DIMS DYNAMIC UnitType
	#DIM DYNAMIC WeaponRange
	#DIM DYNAMIC X_Now
	#DIM DYNAMIC Y_Now

	#DIM DYNAMIC X_Count
	#DIM DYNAMIC X_View
	#DIM DYNAMIC Y_View
	#DIM DYNAMIC Y_Line

	#DIMS REF RESULT_DATA
	[IF_DEBUG]
		PRINTFORML 【Debug!】 D20XX_GenerateHTML_AttackRange X_View = {X_View}, Y_View = {Y_View}, Y_Line = {Y_Line}, 
	[ENDIF]
	IF D20XX_SelectUnit != 0
		UnitType = Demolaviya20XX_Unit:D20XX_SelectUnit:1
		WeaponRange = TOINT(D20XX_GET_WEAPONDATA(D20XX_SelectWeapon,"射程"))
		CALL D20XX_UnitSearch(D20XX_SelectUnit)
		IF RESULT:0 == 1
			Y_Now = RESULT:1
			X_Now = RESULT:2
			CALL D20XX_AttackRangeCalc(D20XX_SelectWeapon,WeaponRange,Y_Now,X_Now)
			;いつもの
			FOR X_Count, X_View, X_View+38 ;表示画面は横幅12だよ！でもヘクス領帯ル方式なので数値が二倍になるのだ！
				SIF X_Count >= 200
					BREAK ;描画範囲突破してしまった場合はBreakするよ
				SIF Y_Line >= 200
					BREAK ;上に同じく
				SIF Y_VIEW >= 200
					BREAK
				IF X_Count % 2 == Y_View % 2
					IF D20XX_AttackRangeMap:Y_View:X_Count >= 0
						IF Y_VIEW % 2 == 0
							RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + (X_Count - X_View)*D20XX_HexSize/2}'>"
							RESULT_DATA += @"<img src='AttackHex' height='{D20XX_HexSize}' ypos='{Y_LINE*75 +50}'>"
							RESULT_DATA += @"</nonbutton>"
						ELSE 
							RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + D20XX_HexHalfSize + ((X_COUNT - X_View)-1)*D20XX_HexSize/2}'>"
							RESULT_DATA += @"<img src='AttackHex' height='{D20XX_HexSize}' ypos='{Y_LINE*75 +50}'>"
							RESULT_DATA += @"</nonbutton>"
						ENDIF
					ENDIF
				ENDIF
			NEXT
		ENDIF
	ENDIF
	RETURN


