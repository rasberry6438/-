@D20XX_GenerateHTML_Hex(ARG,ARG:1,ARG:2,RESULT_DATA)
	#DIM DYNAMIC X_VIEW
	#DIM DYNAMIC Y_VIEW
	#DIM DYNAMIC X_COUNT
	#DIM DYNAMIC Y_LINE
	#DIMS DYNAMIC HEX_NAME
	#DIMS DYNAMIC TITLE_DATA
	#DIMS DYNAMIC SRC_DATA

	#DIMS REF RESULT_DATA

	;#region 初期化
		X_VIEW = ARG
		Y_VIEW = ARG:1
		Y_LINE = ARG:2
		RESULT_DATA = 
	;#endregion 
	FOR X_COUNT, X_VIEW, X_VIEW+38 ;表示画面は横幅12だよ！でもヘクス領帯ル方式なので数値が二倍になるのだ！
		SIF X_COUNT >= 200
			BREAK ;描画範囲突破してしまった場合はBreakするよ
		SIF Y_LINE >= 200
			BREAK ;上に同じく
		SIF Y_VIEW >= 200
			BREAK

		IF X_COUNT % 2 == Y_VIEW % 2
			HEX_NAME = %D20XX_GET_HEXDATA(D20XX_MAP:Y_VIEW:X_COUNT:0,"ファイル名")%
			TITLE_DATA = %D20XX_Generate_Hex_Comment(HEX_NAME)%
			SRC_DATA = %D20XX_GET_HEXDATA(HEX_NAME, "ファイル名")%
			
			IF HEX_NAME != "Error"
				IF Y_VIEW % 2 == 0
					RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + (X_COUNT-X_VIEW)*D20XX_HexSize/2}'>"
					RESULT_DATA += @"<img src='%HEX_NAME%' height='{D20XX_HexSize}' ypos='{Y_LINE*75 +50}'>"
					RESULT_DATA += @"</nonbutton>"
				ELSE 
					RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + D20XX_HexHalfSize + (X_COUNT-X_VIEW-1)*D20XX_HexSize/2}'>"
					RESULT_DATA += @"<img src='%HEX_NAME%' height='{D20XX_HexSize}' ypos='{Y_LINE*75 +50}'>"
					RESULT_DATA += @"</nonbutton>"
				ENDIF
			ENDIF
		ENDIF
	NEXT
	RETURN 

;;;=========================
;;;ユ针织描画HTML命令作成メソッド
;;;ARG = 描画範囲内左隅X座標
;;;ARG:1 = 描画範囲Y座標（要するにMAP全体で見たときのY座標　データ取得用）
;;;ARG:2 = 描画中Y座標行目（こっちは、表示画面内のY座標　表示するときの描画Y座標移動用）
;;;=========================
@D20XX_GenerateHTML_Unit(ARG,ARG:1,ARG:2,RESULT_DATA)
	#DIM DYNAMIC X_VIEW
	#DIM DYNAMIC Y_VIEW
	#DIM DYNAMIC X_COUNT
	#DIM DYNAMIC Y_LINE
	#DIM DYNAMIC UnitID
	#DIMS DYNAMIC UnitName
	#DIMS DYNAMIC UnitIFF
	#DIMS DYNAMIC UnitColor
	#DIMS DYNAMIC IsUnitMoved

	#DIMS REF RESULT_DATA

	;#region 初期化
		X_VIEW = ARG
		Y_VIEW = ARG:1
		Y_LINE = ARG:2
		RESULT_DATA = 
	;#endregion 
	FOR X_COUNT, X_VIEW, X_VIEW+38 ;表示画面は横幅12だよ！でもヘクス領帯ル方式なので数値が二倍になるのだ！
		SIF X_COUNT >= 200
			BREAK ;描画範囲突破してしまった場合はBreakするよ
		SIF Y_LINE >= 200
			BREAK ;上に同じく
		SIF Y_VIEW >= 200
			BREAK
		IF X_COUNT % 2 == Y_VIEW % 2
			UnitID = TOINT(D20XX_Map:Y_VIEW:X_COUNT:1)
			UnitName = %D20XX_Unit:UnitID:1%
			UnitIFF = %D20XX_Unit:UnitID:0%

			IF UnitName != "None" && UnitName != ""
				IF UnitIFF == "敵"
					UnitColor = Enemy
				ELSEIF UnitIFF == "自軍"
					IF D20XX_Unit:UnitID:3 < D20XX_GET_UNITDATA(UnitName,"行動力")
						UnitColor = Moved
					ELSE
						UnitColor = Wait
					ENDIF
				ELSEIF UnitIFF == "友軍"
					UnitColor = Friend
				ENDIF
				IF Y_VIEW % 2 == 0
					RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + (X_COUNT - X_VIEW)*D20XX_HexSize/2 + 50}'>"
					RESULT_DATA += @"<img src='%UnitColor%' height='{D20XX_UnitSize}' ypos='{Y_LINE*75 +100}'>"
					RESULT_DATA += @"</nonbutton>"
					RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + (X_COUNT - X_VIEW)*D20XX_HexSize/2 +50}'>"
					RESULT_DATA += @"<img src='%D20XX_GET_UNITDATA(UnitName,"ファイル名")%' height='{D20XX_UnitSize}' ypos='{Y_LINE*75 +100}'>"
					RESULT_DATA += @"</nonbutton>"
				ELSE 
					RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + D20XX_HexHalfSize + ((X_COUNT - X_VIEW)-1)*D20XX_HexSize/2 + 50}'>"
					RESULT_DATA += @"<img src='%UnitColor%' height='{D20XX_UnitSize}' ypos='{Y_LINE*75 +100}'>"
					RESULT_DATA += @"</nonbutton>"
					RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + D20XX_HexHalfSize + ((X_COUNT - X_VIEW)-1)*D20XX_HexSize/2 +50}'>"
					RESULT_DATA += @"<img src='%D20XX_GET_UNITDATA(UnitName,"ファイル名")%' height='{D20XX_UnitSize}' ypos='{Y_LINE*75 +100}'>"
					RESULT_DATA += @"</nonbutton>"
				ENDIF
			ENDIF
		ENDIF
	NEXT
	RETURN 
;;;=========================
;;;移動可能範囲表示HTML作成メソッド
;;;RESULT_DATA = 作成したHTMLを入れる所だよー これを戻り値として使うわよ！
;;;ARG:0 = 描画範囲内左隅X座標
;;;ARG:1 = 描画範囲Y座標（要するにMAP全体で見たときのY座標　データ取得用）
;;;ARG:2 = 描画中Y座標行目（こっちは、表示画面内のY座標　表示するときの描画Y座標移動用）
;;;=========================
@D20XX_GenerateHTML_MovableRange(ARG:0,ARG:1,ARG:2,RESULT_DATA)
	#DIMS DYNAMIC UnitType
	#DIM DYNAMIC UnitNowMovePower
	#DIM DYNAMIC X_Now
	#DIM DYNAMIC Y_Now

	#DIM DYNAMIC X_Count
	#DIM DYNAMIC X_View
	#DIM DYNAMIC Y_View
	#DIM DYNAMIC Y_Line

	#DIMS REF RESULT_DATA

	;#region 初期化
		X_View = ARG
		Y_View = ARG:1
		Y_Line = ARG:2
		UnitNowMovePower = 0
		RESULT_DATA = 
	;#endregion 

	IF D20XX_SelectUnit != 0
		UnitType = %D20XX_Unit:D20XX_SelectUnit:1%
		UnitNowMovePower = TOINT(D20XX_Unit:D20XX_SelectUnit:3)
		CALL D20XX_UnitSearch(D20XX_SelectUnit)
		IF RESULT:0 == 1
			Y_Now = RESULT:1
			X_Now = RESULT:2
			D20XX_MoveCostMap:Y_Now:X_Now = TOINT(D20XX_Unit:D20XX_SelectUnit:3)
			CALL D20XX_MoveCostCalc(UnitType,UnitNowMovePower,Y_Now,X_Now)
			;いつもの
			FOR X_Count, X_View, X_View+38 ;表示画面は横幅12だよ！でもヘクス領帯ル方式なので数値が二倍になるのだ！
				SIF X_Count >= 200
					BREAK ;描画範囲突破してしまった場合はBreakするよ
				SIF Y_Line >= 200
					BREAK ;上に同じく
				SIF Y_VIEW >= 200
					BREAK
				IF X_Count % 2 == Y_View % 2
					IF D20XX_MoveCostMap:Y_View:X_Count >= 0
						IF Y_VIEW % 2 == 0
							RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + (X_COUNT - X_View)*D20XX_HexSize/2}'>"
							RESULT_DATA += @"<img src='MoveHex' height='{D20XX_HexSize}' ypos='{Y_LINE*75 +50}'>"
							RESULT_DATA += @"</nonbutton>"
						ELSE 
							RESULT_DATA += @"<nonbutton pos='{D20XX_LeftSpase + D20XX_HexHalfSize + ((X_COUNT - X_View)-1)*D20XX_HexSize/2}'>"
							RESULT_DATA += @"<img src='MoveHex' height='{D20XX_HexSize}' ypos='{Y_LINE*75 +50}'>"
							RESULT_DATA += @"</nonbutton>"
						ENDIF
					ENDIF
				ENDIF
			NEXT
		ENDIF
	ENDIF
	RETURN

