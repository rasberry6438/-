;;;=========================
;;;Hex&Unitボタンバインディング用HTML作成メソッド
;;;ARG = 描画範囲内左隅X座標
;;;ARG:1 = 描画範囲Y座標
;;;ARG:2 = 描画中Y座標行目
;;;=========================
@D20XX_GenerateHTML_MapButton(ARG:0,ARG:1,ARG:2,RESULT_DATA)
	#DIM DYNAMIC X_VIEW
	#DIM DYNAMIC Y_View
	#DIM DYNAMIC X_Count
	#DIM DYNAMIC Y_LINE
	
	#DIMS DYNAMIC HexName
	#DIMS DYNAMIC HexText
	#DIM DYNAMIC HexButtonValue

	#DIMS DYNAMIC UnitID
	#DIMS DYNAMIC UnitName
	#DIMS DYNAMIC UnitText
	#DIMS DYNAMIC UnitDefaultSrc
	#DIM DYNAMIC UnitButtonValue


	#DIMS REF RESULT_DATA

	;#region 初期化
		X_VIEW = ARG
		Y_View = ARG:1
		Y_LINE = ARG:2
		RESULT_DATA = 
	;#endregion 

	FOR X_Count , X_VIEW , X_VIEW + 38
		SIF X_Count >= 200
			BREAK ;描画範囲突破してしまった場合はBreakするよ
		SIF Y_LINE >= 200
			BREAK ;上に同じく
		SIF Y_VIEW >= 200
			BREAK

		IF X_Count % 2 == Y_View % 2
			HexName = %D20XX_MAP:Y_View:X_Count:0%
			HexButtonValue = 1000000 + Y_View * 1000 + X_Count
			IF D20XX_GET_HEXDATA(D20XX_MAP:Y_View:X_Count:0,"領帯ル名") != "Error" && D20XX_GET_HEXDATA(D20XX_MAP:Y_View:X_Count:0,"領帯ル名") != "虚無" && D20XX_GET_HEXDATA(D20XX_MAP:Y_View:X_Count:0,"領帯ル名") != ""
				IF Y_View % 2 == 1
					RESULT_DATA += @"<button value='{HexButtonValue}' title='%D20XX_GET_HEXDATA(D20XX_MAP:Y_View:X_Count:0,"解説")%\n デバッグ ={D20XX_AttackRangeMap:Y_View:X_Count}' pos='{D20XX_LeftSpase + (X_COUNT-X_VIEW)*D20XX_HexSize/2}'>"
					RESULT_DATA += @"<img src = 'NoneHex' srcb = 'CursorHex' ypos = '{Y_LINE * 75 +50}' height = '{D20XX_HexSize}'>"
					RESULT_DATA += @"</button>"
				ELSE
					RESULT_DATA += @"<button value='{HexButtonValue}' title='%D20XX_GET_HEXDATA(D20XX_MAP:Y_View:X_Count:0,"解説")% \n デバッグ ={D20XX_AttackRangeMap:Y_View:X_Count}' pos='{D20XX_LeftSpase + (X_COUNT-X_VIEW)*D20XX_HexSize/2}'>"
					RESULT_DATA += @"<img src = 'NoneHex' srcb = 'CursorHex' ypos = '{Y_LINE * 75 +50}' height = '{D20XX_HexSize}'>"
					RESULT_DATA += @"</button>"
				ENDIF
			ENDIF

			UnitID = %D20XX_MAP:Y_View:X_Count:1%

			IF ISNUMERIC(UnitID) == 1
				UnitName = %D20XX_Unit:(TOINT(UnitID)):1%
				
				IF TOINT(UnitID) == D20XX_SelectUnit
					UnitDefaultSrc = SelectUnit
				ELSE
					UnitDefaultSrc = NoneUnit
				ENDIF

				UnitButtonValue = 2000000 + Y_View * 1000 + X_Count
				IF Y_View % 2 == 1
					RESULT_DATA += @"<button value='{UnitButtonValue}' title='%D20XX_GenerateUnitComment(TOINT(UnitID))%' pos='{D20XX_LeftSpase + D20XX_HexHalfSize + (X_COUNT-X_VIEW - 1)*D20XX_HexSize/2 + 50}'>"
					RESULT_DATA += @"<img src = '%UnitDefaultSrc%' srcb = 'CursorUnit' ypos = '{Y_LINE * 75 +100}' height = '{D20XX_UnitSize}'>"
					RESULT_DATA += @"</button>"
				ELSE
					RESULT_DATA += @"<button value='{UnitButtonValue}' title='%D20XX_GenerateUnitComment(TOINT(UnitID))%' pos='{D20XX_LeftSpase + D20XX_HexHalfSize + (X_COUNT-X_VIEW -1)*D20XX_HexSize/2 + 50}'>"
					RESULT_DATA += @"<img src = '%UnitDefaultSrc%' srcb = 'CursorUnit' ypos = '{Y_LINE * 75 +100}'  height = '{D20XX_UnitSize}'>"
					RESULT_DATA += @"</button>"
				ENDIF
			ENDIF
		ENDIF
	NEXT
	RETURN
