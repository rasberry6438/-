;=========================
;通常画面HTML統合メソッド
;ARG:0 = 描画範囲内左隅X座標
;ARG:1 = 描画範囲Y座標
;=========================
@D20XX_GenerateHTML(X_ViewPoint,Y_ViewPoint)
	#DIM DYNAMIC LCOUNT
	#DIM DYNAMIC X_ViewPoint
	#DIM DYNAMIC Y_ViewPoint
	#DIMS DYNAMIC HEX_HTML
	#DIMS DYNAMIC UNIT_HTML
	#DIMS DYNAMIC MovableRangeHTML
	#DIMS DYNAMIC AttackRangeHTML
	#DIMS DYNAMIC AstarRouteHTML
	#DIMS DYNAMIC MapButtonHTML
	#DIMS DYNAMIC CommandBttonHTML

	[IF_NDEBUG]
		CLEARLINE LINECOUNT
	[ENDIF]
	;初期化するよ
	CALL D20XX_HTML_Constructor
	CALL D20XX_MoveCostMap_Constructor
	CALL D20XX_MoveRoute_Constructor
	CALL D20XX_AttackRangeMap_Constructor
	CALL D20XX_AttackRoute_Constructor
	
	FOR LCOUNT, 0, 16
		CALL D20XX_GenerateHTML_Hex(X_ViewPoint,(Y_ViewPoint + LCOUNT),LCOUNT,HEX_HTML)
		CALL D20XX_GenerateHTML_Unit(X_ViewPoint,(Y_ViewPoint + LCOUNT),LCOUNT,UNIT_HTML)
		CALL D20XX_GenerateHTML_MapButton(X_ViewPoint,(Y_ViewPoint + LCOUNT),LCOUNT,MapButtonHTML)
		MovableRangeHTML = 
		AttackRangeHTML = 
		AstarRouteHTML = 
		CALL D20XX_GenerateHTML_CommandButton(LCOUNT,CommandBttonHTML)
		IF D20XX_SelectCommandMode == "移動" && TOINT(D20XX_Unit:D20XX_SelectUnit:3) > 0
		;	IF Demoslaviya20SXX_EnemyTargetMode == "True"
		;		PRINTFORML 【Debug】GHTML31
		;		CALL D20XX_GenerateHTML_EnemyMoveHexSelect(ARG:0,(ARG:1 + LCOUNT),LCOUNT,MovableRangeHTML)
		;	ELSE
		;		PRINTFORML 【Debug】GHTML34
				CALL D20XX_GenerateHTML_MovableRange(X_ViewPoint,(Y_ViewPoint + LCOUNT),LCOUNT,MovableRangeHTML)
		;	ENDIF
			D20XX_HTML:LCOUNT += @"<nobr>%HEX_HTML%%UNIT_HTML%%MovableRangeHTML%%MapButtonHTML%%CommandBttonHTML% </nobr>"
		ELSEIF D20XX_SelectCommandMode == "最短経路計算"
			;PRINTFORMW 【Debug！】GHTML40 X_ViewPoint ={X_ViewPoint} Y_ViewPoint = {Y_ViewPoint}
			CALL D20XX_GenerateHTML_MovableRange(X_ViewPoint,(Y_ViewPoint + LCOUNT),LCOUNT,MovableRangeHTML)
			CALL D20XX_GenerateHTML_AstarHexSelect(X_ViewPoint,(Y_ViewPoint + LCOUNT),LCOUNT,AstarRouteHTML)
			D20XX_HTML:LCOUNT += @"<nobr>%HEX_HTML%%UNIT_HTML%%MovableRangeHTML%%AstarRouteHTML%%MapButtonHTML%%CommandBttonHTML% </nobr>"
		ELSEIF D20XX_SelectCommandMode == "攻撃" && TOINT(D20XX_Unit:D20XX_SelectUnit:3) > 0
			;IF Demoslaviya20SXX_EnemyTargetMode == "True"
		;		CALL D20XX_GenerateHTML_EnemyTargetHexSelect(ARG:0,(ARG:1 + LCOUNT),LCOUNT,AttackRangeHTML)
		;	ELSE
				CALL D20XX_GenerateHTML_AttackRange(X_ViewPoint,(Y_ViewPoint + LCOUNT),LCOUNT,AttackRangeHTML)
		;	ENDIF
			D20XX_HTML:LCOUNT += @"<nobr>%HEX_HTML%%UNIT_HTML%%AttackRangeHTML%%MapButtonHTML%%CommandBttonHTML% </nobr>"
		ELSE
			D20XX_HTML:LCOUNT += @"<nobr>%HEX_HTML%%UNIT_HTML%%MapButtonHTML%%CommandBttonHTML% </nobr>"
		ENDIF

		;D20XX_HTML:LCOUNT += @"<nobr>%HEX_HTML%%UNIT_HTML%%MovableRangeHTML%%AstarRouteHTML%%AttackRangeHTML%%MapButtonHTML%%CommandBttonHTML%</nobr>"
		;PRINTFORML %D20XX_HTML:LCOUNT%
	NEXT
	;ここは余白なので何も入れたらだめなのだ！気を付けるのだ！
	FOR LCOUNT,16,28
		CALL D20XX_GenerateHTML_CommandButton(LCOUNT,CommandBttonHTML)
		D20XX_HTML:LCOUNT += @"<nobr>%CommandBttonHTML% </nobr><br>"
	NEXT

