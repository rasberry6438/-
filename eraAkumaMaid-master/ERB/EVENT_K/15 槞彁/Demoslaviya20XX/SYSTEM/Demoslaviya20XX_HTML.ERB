						RESULT_DATA += @"</nonbutton>"
					ENDIF
				ENDIF
			;ENDIF
		NEXT
	ENDIF
	;PRINTFORML D20XX_GenerateHTML_AstarHexSelect %RESULT_DATA%
	RETURN



;;;=========================
