	IF D20XX_SelectUnit != 0
		;PRINTFORM D20XX_SelectUnit = {D20XX_SelectUnit} X_View = {X_View}  Y_View = {Y_View} Y_Line = {Y_Line}
		;いつもの
		FOR X_Count, X_View, X_View+38 ;表示画面は横幅12だよ！でもヘクス領帯ル方式なので数値が二倍になるのだ！
			SIF X_Count >= 200
				BREAK ;描画範囲突破してしまった場合はBreakするよ
			SIF Y_Line >= 200
				BREAK ;上に同じく
			SIF Y_VIEW >= 200
				BREAK
			;IF X_Count % 2 == Y_View % 2
