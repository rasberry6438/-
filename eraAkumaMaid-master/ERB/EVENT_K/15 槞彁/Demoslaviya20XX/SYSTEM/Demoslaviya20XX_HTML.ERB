@D20XX_NO_NOBR(ARGS)
	#DIM LCOUNT
	#DIMS ResultHtml
	#DIM TagCount
	#DIMS DYNAMIC MEMO_RESULTS,100
	#DIM MEMO_REDRAW

	MEMO_REDRAW = CURRENTREDRAW()

	REDRAW 0
	ResultHtml = 
	;HTML_PRINT CONVERTSTR(ARGS)
	;HTML_PRINT ARGS
	;LOCALS = %HTML_GETPRINTEDSTR(0)%
	;CLEARLINE 1
	HTML_TAGSPLIT ARGS
	TagCount = RESULT
	
	FOR LCOUNT, 0, TagCount
		MEMO_RESULTS:LCOUNT = %RESULTS:LCOUNT%
	NEXT
	FOR LCOUNT, 0, TagCount
		IF SUBSTRING(MEMO_RESULTS:LCOUNT, 0, 5) == "<nobr"
		ELSEIF SUBSTRING(MEMO_RESULTS:LCOUNT, 0, 6) == "</nobr"
		ELSEIF SUBSTRING(RESULTS:LCOUNT, 0, 2) == "<p"
		ELSEIF SUBSTRING(RESULTS:LCOUNT, 0, 3) == "</p"
		ELSEIF SUBSTRING(MEMO_RESULTS:LCOUNT, 0, 7) == "<button"
		ELSEIF SUBSTRING(MEMO_RESULTS:LCOUNT, 0, 8) == "</button"
		ELSEIF SUBSTRING(MEMO_RESULTS:LCOUNT, 0, 10) == "<nonbutton"
		ELSEIF SUBSTRING(MEMO_RESULTS:LCOUNT, 0, 11) == "</nonbutton"
		ELSE
			ResultHtml += @"%MEMO_RESULTS:LCOUNT%"
		ENDIF
	NEXT
	RESULTS = %ResultHtml%
	REDRAW MEMO_REDRAW
;
