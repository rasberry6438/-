		ENDIF
	CASE "朝接吻"
		;恋慕
		IF TALENT:恋慕 && RAND:2
			PRINTFORMW %CALLNAME:TARGET%像在做爱的时候一样、紧紧纠缠着%CALLNAME:MASTER%的舌頭、蹂躏了好一阵

			CALL SIMULATE_SHOW_NEWDAY
			CALL AFTERTEXT_NEWDAY
			PRINTL 
			CALL EMOTION_PLUS, "甜言蜜语", TARGET

			CALL PRINT_STR, "「早上好_HH_」_W"
			CALL PRINT_STR, @"「呐、今天要不要和%CNAME("一人称")%一直这样做？_H_」_W"
			PRINTFORMW 「忘记彼此之外的一切、只是一味地亲吻、享受着彼此的身体」
