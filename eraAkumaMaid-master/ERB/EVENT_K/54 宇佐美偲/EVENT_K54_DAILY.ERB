;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K54, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	PRINTFORMW 「あーあ、起きちゃった……ま、天所のシミでも数えててくださいね」
;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「あっ、起きちゃいましたか…」
				PRINTFORMW 「寝てる内に済まそうと考えてましたが起きたなら、もう関係ないですね、」
				PRINTFORMW そう言うと%CALLNAME:TARGET%は朝から%TEXTR("荒々し/容赦な")%く腰を振っている
			CASE 1
				PRINTFORMW 「おや、起きましたね…」
				PRINTL 
				CALL PRINT_SELECT, "寝てるのに勝手にしないで欲しい…"
				IF RESULT == 0
					PRINTFORMW 「えぇ？起こし来了のにビチョビチョに濡らしてた%CNAME("二人称")%が悪いでしょ」
					PRINTFORMW 「こっちも、あんなの見せられて勃たせないのも失礼なので、しょうがなく交尾してるんですよ」
				ENDIF
			ENDSELECT
	CASE "朝舐陰"
	CASE "朝足交"

	CASE "朝の新妻PLAY"
			CALL SET_SHIRT, "工作围裙"
			CALL EMOTION_PLUS, "重置/微笑２/赤面２/对话框爱心"
			SELECTCASE RAND:2
				CASE 0
					CALL PRINT_STRW, @"「あんっ_H_もう、料理中は危ないからダメですよ」"
					CALL PRINT_STRW, @"「あっ_H_激烈地しちゃ、んっ_HH_ダメっなのni↘、んっん」"
					CALL PRINT_STRW, @"「はー、いっぱいだせて気持ち良くなれましかたか_H_」"
				CASE 1
					PRINTFORMW 「朝ご飯はご飯と玉子焼きでいいですよね？」
					PRINTFORMW 廚房で調理する%CALLNAME:TARGET%は圍裙一枚という煽欲的な恰好をしている
					CALL PRINT_STRW, @"「%CNAME("二人称")%どうしたんですか、冷めちゃいますよ_BH_」"
					PRINTFORML
					PRINTFORMW 食卓に皿は一つも盛り付けられていない…。つまりそういうことだろう…
					CALL PRINT_STRW, @"「朝から元気ですね_BH_出し終わってから、一緒に食べましょうか_BBHH_」"
			ENDSELECT
			
	CASE "朝乳交"

	CASE "朝面部騎乗"
		
	CASE "朝騎乗位"
		IF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STRW, @"「あ！、おはようございまーす_H_」"
			CALL PRINT_STRW, @"「我慢できなかったので_HH_　%TEXTR("襲って/勝手に")%交尾してまーす_BH_　好きな時に出してくださ～ぅ、おっ……_HH_」"
		ELSE
			CALL PRINT_STRW, @"「%TEXTR("あぅ/おぅ")%っ_H_　う、うぅ、あっ_H_」"
			CALL PRINT_STRW,  "「あ……っ_H_　ちんちん_H_　すごいよぉ…_BH_」"
		ENDIF

	CASE "朝接吻"

	CASE "朝添い寝"
			CALL EMOTION_PLUS, "重置/微笑/红晕/对话框爱心"
			PRINTFORMW 「んー、やっぱり%CNAME("二人称")%の腕に抱かれるのは気持ちいいですね」
			PRINTFORMW 「今日是い朝になりそうです」
	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		PRINTFORMW 「勝手に啜ってるんでお気になさらず…」
	CASE "朝騎乗位"
		CALL EMOTION_PLUS, "重置/微笑/赤面２/对话框爱心"
		CALL PRINT_STRW, @"「これなら%CNAME("二人称")%の%TEXTR("赤酱/兒童")%_H_　ちゃーんと孕めそう……_H_」"
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 
	ELSE
		PRINTFORMW 「おはようございまーす」
		PRINTFORMW 「カワイイ後輩が起こしにきたんですから、さっさと起きてください」
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")
		CALL EMOTION_PLUS, "重置/微笑/4/对话框兴奋/挎包"
		PRINTFORMW 「おはようございます！今日は定期試験です、酱と勉強しましたか？」
		PRINTFORMW 「%CNAME("一人称")%は、すっごい勉強したので多分１００点です！」
		PRINTFORMW 試験だというのに%NAME:TARGET%はご心情に駆けていく
	ELSEIF DAYEV("試験返却日")
		CALL EMOTION_PLUS, "重置/ガビーン/对话框乱/挎包"
		PRINTFORMW 「おはようございま～す、手ごたえがぁないでーす」
		PRINTFORMW 「はぁ～～、きっとダメだぁ～」
		PRINTFORMW 試験の結果が悪そうというだけで、%NAME:TARGET%は世界が終わったような表情でトボトボと歩いている
	ELSEIF COND("朝袭相手")
		CALL PRINT_STRW, @"「あっ…、_H_改めておはようございます…」"
		CALL PRINT_STRW, @"「じゃ、また部活で…_H_」"
	ELSEIF COND("体育")

	ELSEIF COND("水泳")
		CALL EMOTION_PLUS, "大口/通常眉/对话框兴奋/挎包"
		PRINTFORMW 「うわ～い、泳池！泳池！」
		PRINTFORMW どうやらこの上なくはしゃいでいる。
	ELSE

	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
	SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「いぇ～い、いっちばーん！！」
		CASE 1
			PRINTFORMW 「あれ、誰也不在ですね……少し、寂しいですね」
	ENDSELECT
CASE "ただいま"
		PRINTFORMW 「ただいまー！晩御飯はなんですかー！」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
			CALL EMOTION_PLUS, "ほんわか/微笑３/紅潮２/对话框爱心/布"		
		IF TALENT:恋慕 && RAND:2 == 0
			PRINTFORML 「あっ……おかえりです…」
			PRINTFORML 「その…、時間があったら交尾、しませんか？」
		ELSE
			PRINTFORMW 「おかえりなさ～い」
			PRINTFORMW 「お浴室洗っておいたので一緒に是りませんか？」
		ENDIF
	ELSEIF COND("昼")
		IF RAND:2 == 0
			PRINTFORMW 「あっ！お先にくつろがせてもらってまーす」
			PRINTFORMW 「交尾したくなったら呼んでくださーい」
		ELSE
			PRINTFORMW 「こっちは濡らしてあるので入れたくなったらいつでも言ってください」
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORMW 「どうも～、で%CNAME("一人称")%になんの用です？」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		PRINTFORMW 「わ～、あとで、%CNAME("一人称")%にもお願いしますね」
	ELSE
		PRINTFORMW 「あ～、ずるい！%CNAME("一人称")%も交尾した～い」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「うわっ、皆もういる！すみません遅れました～」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「どうもで～す」
	ELSE
		PRINTFORMW 「今日も交尾ですか～？」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW （えぇ…老师来るの、この部活！？）
		PRINTFORMW %NAME:TARGET%は何かに受惊了的様子
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「お先にくつろがせてもらってま～す」
	ELSE
		PRINTFORMW 「あれ、もしかして早く来過ぎましたかね…」
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「夜くらいゆっくりしたいんですけどね～」
		PRINTFORMW 「仕方ないので来てあげました」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「も～、何です？%CNAME("一人称")%寝たいんですけど、」
	ELSE
		CALL EMOTION_PLUS, "ほんわか/叹气/对话框ねぼけ"
		PRINTFORMW 「ふぁ～～、ねっむ…」
	ENDIF
CASE "抜け出された"
		CALL EMOTION_PLUS, "困扰眉/涙目〇〇/左〇〇/对话框乱/一文字"
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「あっ、……行っちゃった……」
				PRINTFORMW %CALLNAME:TARGET%は肩を落とししょげている
			CASE 1
				PRINTFORMW 「えぇ～せっかく来たのに放置ですか…」
				PRINTFORMW 「しかも別の子と尾巴りするって…はぁ～」
		ENDSELECT
CASE "便当"
		PRINTFORMW 「味はどうです？その、頑張って用意したのでほめてください」
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
		CALL EMOTION_PLUS, "号泣２/ω/斜眉"
		PRINTFORMW 「おあずけくらうこっちの気持ちも考えてくださいよ～！」
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;动画ーションの関係上、１ターンに何度も呼ばれたりするのでフラグ操作をする場合には
;IF CFLAG:KOJO_DAILY済 == 0
;で分岐させるのが良いでしょう
;-------------------------------------------------
@KOJO_DAILY_K54, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
		CALL EMOTEFACE, "重置/小口/赤面２"
		PRINTFORML 「うわ～、ここに居る人を何人も孕ませたんですね、すっご」
CASE "浑身是汗"
	IF RAND:2 == 0
		PRINTFORML 「ふぃ～、あっついですね～、嫌になっちゃいますね」
	ELSE
		CALL EMOTEFACE, "重置/ほんわか/婴儿红晕/对话框兴奋/冰"
		PRINTFORML 「冰うま～、%CNAME("二人称")%もどうです、冰？」
	ENDIF
CASE "刚睡醒"
	IF CHOSE_DEVENT("睡眠姦Ｖ中出し")
		CALL EMOTEFACE, "重置/害羞/对话框困倦///"
		CALL PRINT_STRL, @"「………あのですね_／／／_するなとは言ってないこっちも悪いですけど」"
		PRINTFORML 「知らない内にことが進んでるとこっちも困るので、起きてる内に手をだしてください」
		CALL PRINT_STRL, @"「気持ち良かったですよ……_H__／／／_」"
	ELSE
		PRINTFORML 「んん～、おはようです。ちょっと顔洗ってきます」
		PRINTFORML 宇佐美は伸びをしながら洗面台の方に向かった
	ENDIF
CASE "Cosplay中"
	PRINTFORML 「こんな格好させて何かないんですか？」
CASE "点心の感想"
	;今日の点心を用意したのは自分
	IF COND("点心当番")
		PRINTFORML 「どうですか？りんご綺麗に切れてるでしょ？」
		PRINTFORML 「ほめてください！ほめてください！」
	;今日の点心は誰かの亲手自制
	ELSEIF COND("亲手自制点心")
		;今日の点心当番の人の亲手自制を初めて食べた
		IF DEVENT(@"%TEXTS("点心当番")%の亲手自制") == 0
			;点心当番が[料理擅長]
			IF TALENT:恋慕
				PRINTFORML 「すっごいおいしいですよこれ！今度このお菓」
				CALL PRINT_STRL, @"「子作りしませんか_H__BH_」"
			ELSE
				CALL EMOTEFACE, "重置/对话框開心/斜眉/红晕/半開３"
				PRINTFORML 「ん～、もぐもぐ。」
				PRINTFORML 「おいひい～」
			ENDIF
		ELSE
			PRINTFORML 
		ENDIF
	;点心が亲手自制でない
	ELSE
		CALL EMOTEFACE, "重置/对话框開心/斜眉/红晕/半開３"
		PRINTFORML 「ん～、もぐもぐ。」
		PRINTFORML 「おいひい～」
	ENDIF
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K54
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「再度確認なんですけど、本当にいいんですよね！？」
	PRINTFORML どうやら、%NAME:TARGET%の言う街道相当扱いが難しそうだ
CASE "约会的约定"
	CALL EMOTEFACE, "重置/へ字２/紅潮２/闭眼２/对话框開心"
	PRINTFORML 「いっつも、交尾に付き合ってもらってるのでその礼物です」
;これは悪魔や天使や驱魔人のみ
CASE "契約成立"
	PRINTFORML 「いや、%CNAME("一人称")%弗理希艾尔桑みたいな」
	PRINTFORMW 「魂の取引はできませんよ、他を当たってください」」
;你にお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	PRINTFORML 
CASE "摆脱"
	PRINTFORML 「人に見られるようなとこで、できないことをするんですね…」
	PRINTFORML 「どこに行きますか？いっそのこと情人旅館にでも行ってしまいましょうか…」
;你と一緒に、主人公的房間に移動した
CASE "主人公的房間に連れ込まれた"
	PRINTFORML 「へ～、思った以上に整ってますね」
	PRINTFORML 「で、これから何をするんですか?」
;你と一緒に、partner的房間に移動した
CASE "自室に招待"
	PRINTFORML 「あんまり掃除できてないですけど、それなりにくつろいでください」
;partnerが既に居る部屋に、你が移動した
CASE "自室に訪問された"
	;指导者を伴っている場合
	IF FIND_COND("現在存在", "助手")
		PRINTFORML 「%CNAME("助手")%も大変ですね～、%CNAME("二人称")%の相手難しそうでしたら、」
		PRINTFORML 「%CNAME("一人称")%が代わりますから、言ってくださいね！」
	ELSE
		PRINTFORML 「うわぁ、惊嚇した～」
		PRINTFORML 「来るなら手机使って教えてくださいよー」
	ENDIF
CASE "指导者部屋訪問"
	PRINTFORML 
;自宅移動の後に你に連れられて居間に回来了場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 「うゎ、帰ってきた！」
CASE "オナバレ"
	PRINTFORML 「あっ、……忘れてくださーい…」
	PRINTFORML 消え入りそうな声で戸を閉じた
CASE "露脐装超短裙胖次チラ"
	IF TALENT:恋慕
		CALL EMOTEFACE,"重置/斜眉/对话框爱心/紅潮/ڡ"
		PRINTFORML 「ふふっ、見せてるんですよ。察してください」
		PRINTFORML 「どうしました？興奮してきましたか？」
		CALL PRINT_STRL, @"「こっちは隠さず見せつけてるんですから、そっちも隠さないでくださいよ…_H_」"
	ELSE
		CALL EMOTEFACE,"重置/こまり眉/对话框！？/半開２"
		PRINTFORML 「ん？どうかしましたか、変な顔して…」
		PRINTFORML 「もしかして、顔に何かついてますか！？」
		PRINTFORML 手で顔をペタペタと触り、おかしい所がないか探している
		PRINTFORML どうやら、こちらの視線の先に気づいていないようだ
	ENDIF
CASE "低腰臀圍"
	PRINTFORML 
CASE "掀起裙子"
		CALL EMOTEFACE,"重置/笑顔２/对话框爱心/赤面２"
		PRINTFORML 「も～、H～」
		PRINTFORML %CALLNAME:TARGET%の瞳はどことなく淫靡だ
CASE "撫摸股間"
		CALL EMOTEFACE, "重置/赤面/ヮ３"
		PRINTFORML 「触ってその気にさせたから、ちゃーんと最後までしてくださいよ」
		PRINTFORML %CALLNAME:TARGET%は股間を触る手を両腕で離さないようにして耳元で囁いてくる
CASE "检查内褲"
	IF COND("合意：检查内褲")
		PRINTFORML 「にしても、%CNAME("二人称")%ってそんなに胖次が見たいんですか～」
		PRINTFORML 「変態桑ですね、お返しに子種注いでくださいよ」
		PRINTFORML ぶつぶつ言いながらも、下摆を上げて見やすくしてくれている
	ENDIF
CASE "检查阴部"
	IF COND("合意：检查阴部")
		PRINTFORML 「その綺麗にして有ますから早くちんちん入れてください…」
	ENDIF
CASE "拜托処理性欲"
	IF RAND:2 == 0
		PRINTFORML 「ねぇ、これみてくださいよ～」
		PRINTFORML 「さっき出した、ほかほかの精液で胖次が汚れちゃいました」
		CALL EMOTEFACE, "重置/赤面/胖次花绳变化/通常眉/笑顔３"
	ELSE
		PRINTFORML 「中に出した分が溢れちゃって胖次汚れちゃいました…」
		PRINTFORML 「これ、見てくださいよ、溢れた分だけでこ～んなに精液が」
		CALL EMOTEFACE, "重置/赤面/胖次花绳变化/通常眉/笑顔３"
	ENDIF
CASE "进行性処理"
	PRINTFORML 「じゃあ、こっちも作業続けてるんで他の娘にでも相手してもらってください」
	PRINTFORML %CALLNAME:TARGET%は平静を保ちながら足を開くものの耳の先まで真っ赤になっていた
CASE "帮忙消除性欲"
	PRINTFORML %CALLNAME:TARGET%はぐったりとした様子で股間から%CALLNAME:MASTER%の精液を垂らしている
CASE "清洁口交"
	CALL EMOTEFACE, "重置/赤面/精液左頬/唇边手"
	PRINTFORML 「んっ、んっ、味はよくないですね。まぁ仕方のないことですが」
	PRINTFORML 「さぁ、綺麗变成了から交尾しましょうね」
CASE "買東西"
