;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K21, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝騎乗位"
		SIF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, "「啊啊……_H_」_W"
	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1
	
	IF COND("発情期")
			PRINTFORML 「……您好。」
			PRINTFORMW %CALLNAME:TARGET%尾巴上的火焰摇曳闪烁、她低下頭弱弱地向你打招呼。
	ELSEIF COND("体育")		
		PRINTFORML 「%CNAME("二人称")%的班级好像要上体育课呢。」
		IF RAND:2
			PRINTFORMW 「待会儿我给你倒杯茶吧？」
		ELSE
			PRINTFORMW 「我会从教室看着你的、就让我看看。%CNAME("二人称")%帅气的样子吧。呼呼♪」
		ENDIF
	ELSE
		IF RAND:3 == 0 && TEXTS("曜日") != "日"
			PRINTFORML 「你看起来都要睡着了、要来拉住我的手吗？」
			PRINTFORMW 「被凭依了之后我好像变得更快了。比平时走路要快很多哦。」
		ELSEIF RAND:2
			PRINTFORMW 「早上好！%CNAME("二人称")%、今天也是好天气呢。」
		ELSE
			PRINTFORML 「好像还很困呢。我来帮你清醒一下。」
			PRINTFORML %CALLNAME:TARGET%从包中取出喷雾器、直接喷在满脸困意的%CALLNAME:MASTER%脸上。
			PRINTFORMW 「啊、之后还要擦干呢、先不要动哦。」
		ENDIF
	ENDIF
CASE "一番乗り"
		IF COND("発情期")
			PRINTFORML 「啊、%CNAME("二人称")%。」
			PRINTFORML %CALLNAME:TARGET%湿润的眼瞳看向了%CALLNAME:MASTER%、尾巴上的火焰因情欲高涨而更加热烈、但她很快又沉默着低下頭去。
		ELSE
			PRINTFORML 「看起来、是%CNAME("一人称")%第一个到呢。」
			PRINTFORMW 「呼呼、要独占%CNAME("二人称")%了呢。」
		ENDIF
CASE "ただいま"
		IF COND("発情期")
			PRINTFORML 「……我回来了。」
			PRINTFORMW 摇晃着燃烧的尾巴、%CALLNAME:TARGET%声如蚊蚋地打着招呼。
		ELSE
			PRINTFORML 「我回来了。」
			PRINTFORMW %CALLNAME:TARGET%爽朗地声音响彻了整个房间。
		ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF COND("発情期")
			PRINTFORML 「……欢迎回来、%CNAME("二人称")%。」
			PRINTFORML %CALLNAME:TARGET%抱住了自己的尾巴规规矩矩地打着招呼。
			PRINTFORMW 喘息却无法抑制地粗重起来、于尾巴上燃烧的火焰中弥散出粉色的烟雾。
		ELSE
			PRINTFORML 「来了、欢迎回来、%CNAME("二人称")%♪」
			PRINTFORML 「现在、正在准备料理呢、稍等一下哦。」
			PRINTFORMW 正在做饭的%CALLNAME:TARGET%穿着围裙迎接了%CALLNAME:MASTER%。
		ENDIF
	;部室で出迎えてくれる時
	ELSE
		IF COND("発情期")
			PRINTFORML 「那个、%CNAME("二人称")%……不、什么都没有。」
			PRINTFORMW 坐在椅子上的%CALLNAME:TARGET%紧抓着裙边好像在忍耐着什么。
		ELSE
			PRINTFORML 「太晚了啊。%CNAME("二人称")%」
			PRINTFORMW 「今天也要開心地进行社团活动哦。」
		ENDIF
	ENDIF

CASE "入室打招呼"
PRINTFORMW 「抱歉、我迟到了。」

;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「对不起、我迟到了！早上好。」
	ELSE
		PRINTFORMW 「早上好。」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		;老师を発言者として選び直す
		ACTOR = FIND_COND("教員", "入室打招呼中")
		CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%
		IF RAND:2
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%老师。早上好。』
		ELSE
			PRINTFORMW 『啊、%CNAME(CALLNAME_ACTOR)%老师。这是昨天寄存在我这的打印件。』
		ENDIF
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF COND("易出汗", ACTOR) && RAND:2
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%。出了好多汗呢。发生什么了吗？』
		ELSE
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%。早上好。』
		ENDIF
	ELSEIF CFLAG:茶 == 0 && RAND:2 && (COND("弗理希艾尔", ACTOR) || COND("卡莉斯塔", ACTOR) )
		PRINTFORML 『%CNAME(CALLNAME_ACTOR)%。早上好。』
		PRINTFORMW 『是要沏茶对吗？我来帮忙吧。』
		CALL SETFLAG_DAILY, "茶"
	ELSE
		PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%。早上好。』
	ENDIF
	
CASE "抜け出された"
	IF COND("発情期")
		PRINTFORML 「……还是没能……说出口吗……。」
		PRINTFORMW %CALLNAME:TARGET%望着自己尾巴上发狂的火焰、叹了口气。
	ELSE
		PRINTFORML 「……那是去了吧。完全暴露了啊。那两个人。」
		PRINTFORMW 「即使这样……好羡慕啊。」
	ENDIF

CASE "便当"
	IF RAND:5 == 0
		PRINTFORML 「今天我用水壶装了些蛤蜊味增汤哦。」
		PRINTFORMW 「呼、呼、好了、%CNAME("二人称")%。趁着还没凉快尝尝吧。」
	ELSEIF RAND:4 == 0
		PRINTFORML 「偶尔用苹果当点心也不错吧？」
		PRINTFORML %CALLNAME:TARGET%从粉色的便当盒中取出切好的苹果、递到了%CALLNAME:MASTER%的嘴边。
		PRINTFORMW 「来、%CNAME("二人称")%、啊～～～嗯♪」
	ELSEIF RAND:3 == 0
		PRINTFORML 「还没有吃饱吗？那就交给%CNAME("一人称")%吧。」
		PRINTFORMW %CALLNAME:TARGET%的手中托着两块烟熏乳酪。
		PRINTFORML 「没能填饱%CNAME("二人称")%的肚子是%CNAME("一人称")%的责任、所以请用吧！」
		PRINTFORML 「啊、但是、要是把这个交出去了、%CNAME("一人称")%的下午茶就……。」
		PRINTFORML %CALLNAME:TARGET%烦恼了好一阵、还是把其中一块递给了%CALLNAME:TARGET%。
		PRINTFORMW 「呜呜、再见了、%CNAME("一人称")%的烟熏乳酪。」
	ELSEIF RAND:2
		PRINTFORML 「怎么样？好吃吗？」
		PRINTFORML 看到%CALLNAME:TARGET%充满期许的目光、%CALLNAME:MASTER%嗯嗯地点了点頭。
		PRINTFORML 新鲜的番茄和生菜、味道浓郁的乳酪和火腿、再加上松软的面包、确实是非常美味的三明治、
		PRINTFORML 虽然很好吃、但就算加上鸡蛋、便当盒中的颜色也明显偏黄。
		PRINTFORMW 「真的吗？如果喜欢的话我还有准备芝士粉哦、不用客气尽情品尝吧♪」
	ELSE
		PRINTFORML 「像这样两个人一起吃便当、好像没什么问题呢。」
		PRINTFORML %CALLNAME:TARGET%望着燃烧的尾巴叹了口气。
		PRINTFORML 大概是正对没有发生变化的现实感到无奈吧。
		PRINTFORMW 这样想着的%CALLNAME:MASTER%搂住了%CALLNAME:TARGET%的肩膀。
		PRINTFORML 「啊、%CNAME("二人称")%总是支撑着%CNAME("一人称")%呢。」
		PRINTFORMW 「明明还想由%CNAME("一人称")%来成为你的支柱呢。」
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K21, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「各位部员。请多関照！」
CASE "浑身是汗"
	PRINTFORML 「今天真热呢、好想去洗澡啊。」
CASE "刚睡醒"
	PRINTFORML 「对不起、还是、好困……zzz」
	PRINTFORML %CALLNAME:TARGET%捂着嘴打了个哈欠、伸着懒腰。
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K21
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML (%CNAME("二人称")%的话语还在我的心中回响。谢谢你。%CNAME("二人称")%)
CASE "契約成立"
	PRINTFORML 
CASE "摆脱"
	PRINTFORML 「居然瞒着大家、两个人出門独处。呼呼♪」
	PRINTFORML %CALLNAME:TARGET%笑眯眯地跟在%CALLNAME:MASTER%的身后。
CASE "露脐装超短裙胖次チラ"
	PRINTFORML 「难道说、看到了吗？」
	PRINTFORML 「喂、你那样遮遮掩掩的、本来能蒙混过去的事也会暴露的吧。真是的。」
	PRINTFORML 但%CALLNAME:TARGET%看起来似乎并不怎么在意。
CASE "掀起裙子"
	PRINTFORML 「呀啊、这～样～！不行的哦！」
CASE "買東西"
	;買ってきた道具などで分岐
	;IF COND("巨大振動棒購入")

	;	CALL SET_DEVENT, "巨大振動棒感想"
	;ELSE
		;単純にいっぱい買ったかどうかで分岐
		SELECTCASE COND("買上品数")
		CASE IS >= 6
			PRINTFORML 「这个就在那边、那个放在这里。」
			PRINTFORML 「呼、终于结束了。买了那么多。整理起来也很麻烦呢。」
		CASEELSE
			PRINTFORML (又变得躁动不安了、是因为在那家店里逛太久了吗？)
		ENDSELECT
	;ENDIF
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;標語の反応。書いたらコメントアウトを外してください
IF CFLAG:標語実践
	SELECTCASE SAVESTR:標語
	CASE "注意口渴"
		IF DEVENT("標語茶") == 0
			PRINTFORML 「您有需求吗？茶包里还有运动饮料、碳酸饮料和梅干哦」
			PRINTFORM 「%CNAME("弗理希艾尔")%
		SIF COND("現在存在", FINDCHARA(NO, 20))
			PRINTFORM 或 %CNAME("卡莉斯塔")%
			PRINTFORML 应该对茶和茶点已经有所准备、不过这也是为了以防万一嘛。」
			PRINTFORML %CALLNAME:TARGET%似乎考虑了很多。
			CALL SET_DEVENT, "標語茶"
		ENDIF
	CASE "体操服はやっぱり布鲁马"
		IF DEVENT("標語布鲁马") == 0
			PRINTFORML 「因为只有比较小尺寸的体操服、穿起来不是很合适啊。」
			PRINTFORML 小小的布鲁玛勒紧%CALLNAME:TARGET%的臀部、强调着大腿的肉感。
			CALL SET_DEVENT, "標語布鲁马"
		ENDIF
	CASE "学生服は露脐装超短裙に限る"
		IF DEVENT("標語露脐装超短裙") == 0
			PRINTFORML 「%CNAME("一人称")%还以为迷你裙会是怎样的、呼呼、感觉还不错嘛。」
			PRINTFORML %CALLNAME:TARGET%高兴地在镜子前摆起了姿势。
			CALL SET_DEVENT, "標語露脐装超短裙"
		ENDIF
	ENDSELECT
ENDIF

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;イベント台詞。書いたらコメントアウトを外してください
;CFLAG:日常事件回数が10増えるたびに一つ叫醒ぐらいで良いでしょう
IF CFLAG:日常事件回数 >= 5 && DEVENT("質問１") == 0 && FIND_COND("現在存在", "弗理希艾尔")
	PRINTFORML 『真没想到会有人相信那种不知廉耻的传闻而找到这里来呢。』
	PRINTFORML 「%CNAME("弗理希艾尔")%、是你散播的传闻吧。难以置信。果然是恶魔啊。」
	PRINTFORML 『说的是呢、本来就是恶魔啦。不过也多亏了传闻才能把你钓到手了呢。』
	PRINTFORML 『流泻而出的灵魂之汁即是甘露、在淫靡溃烂缠身的尽頭、你将獲得救赎。』
	PRINTFORML 『再这样拖延下去会被吸引来的可就不仅仅是下级恶魔了哦。』
	PRINTFORML 「欸、只是、这样？%CNAME("一人称")%听到的传闻明明要更加下流……。」
	PRINTFORML 『诶~、那是怎样的传闻呢？』
	PRINTFORML 看到露出玩味笑容向她提问的弗里希艾尔、才注意到自己失言了的%CALLNAME:TARGET%满脸通红地低下頭去。
	CALL SET_DEVENT, "質問１"
ENDIF
IF CFLAG:日常事件回数 >= 15 && DEVENT("質問２") == 0
	PRINTFORML 「诶、我的尾巴？很不可思议吧、这个。」
	PRINTFORML %CALLNAME:TARGET%为了让%CALLNAME:MASTER%看得更清楚、把尾巴伸了过来。
	PRINTFORML 在尾巴末端摇曳燃烧着的红色火焰、正在彰显着自己的存在感。
	PRINTFORML 被勾起兴趣的%CALLNAME:MASTER%试着去触摸那火焰。
	PRINTFORML 那火焰随着%CALLNAME:MASTER%的动作而改变形状、但却几乎没有什么温度。
	PRINTFORML 「好像不会影响到别的事物呢。被子也没有被点着过。」
	PRINTFORML 感受着火焰摇摇晃晃的触感、忽然升起了一种想要破坏什么的情绪。
	PRINTFORML 「嗯唔、%CNAME("二人称")%。总感觉眼神好恐怖啊？」
	PRINTFORML 当%CALLNAME:TARGET%把尾巴一把收回去的时候、%CALLNAME:MASTER%那破坏性的冲动就瞬间烟消云散了。
	PRINTFORML 「唔、究竟是什么情况啊？这条尾巴。」
	CALL SET_DEVENT, "質問２"
ENDIF


;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;他の人といちゃついた後にpartner変更
IF COND("いちゃいちゃ") == 0 && FIND_COND("Ｈ的いちゃつき中")
;	PRINTFORML 
ENDIF

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;時間帯と場所と関係で変化有些
SIF RAND:2
	Call TaiTaChara_K21

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;以下は随机台詞など
IF COND("発情期")
	IF RAND:2
		PRINTFORML 发情中的%CALLNAME:TARGET%两眼无神地发着呆、一味地舔着奶酪。
	ELSE
		PRINTFORML 「烟、冒出来了。%CNAME("一人称")%、就要这样消失了啊……。」
		PRINTFORML %CALLNAME:TARGET%一边看着自己的尾巴一边自言自语。
	ENDIF
ELSEIF COND("夜") || PLACE("自宅")
	IF RAND:4 == 0 && (FIND_COND("弗理希艾尔", "現在存在") || FIND_COND("卡莉斯塔", "現在存在") )
		PRINTFORML 「如果做家务有什么忙不过来的地方、请不要客气地来找我哦。」
		PRINTFORML 「一直给您添麻烦真的很抱歉。」
	ELSEIF RAND:3 == 0
		PRINTFORML %CALLNAME:TARGET%在书桌上展開%TEXTR("国語/日本史/数学/化学/英語")%的参考书和笔记本、開始学习。
	ELSEIF RAND:2 == 0
		PRINTFORML 「厨房借我用一下哦～。」
		PRINTFORML %CALLNAME:TARGET%拿着%TEXTR("奶油乳酪/薄力粉/黄油/鸡蛋/洋酒")%走进了厨房。
	ELSE
		PRINTFORML 「单抽卷还有5枚、石頭还剩5000。怎样、会出吗！……啊。」
		PRINTFORML %CALLNAME:TARGET%似乎正在用手机玩社交游戏。
		;足りません
	ENDIF
ELSEIF PLACE("部室")
	IF RAND:2
		PRINTFORML %CALLNAME:TARGET%正在房间里%TEXTR("更换垃圾袋/擦拭桌子/擦拭窗户/清扫地板/给花瓶换水")%
	ELSE
		PRINTFORML 「大家、我做了些%TEXTR("芝士蛋糕/裱花蛋糕/巧克力蛋糕/瑞士卷/冰淇淋")%。可以的话请尝一尝吧。」
		PRINTFORML %CALLNAME:TARGET%从装有充足保冷剂的冷藏盒中取出了%TEXTR("芝士蛋糕/裱花蛋糕/巧克力蛋糕/瑞士卷/冰淇淋")%、并分给了部员们。
	ENDIF
ELSE
	PRINTFORML 「单抽卷还有5枚、石頭还剩5000。怎样、会出吗！……啊。」
	PRINTFORML %CALLNAME:TARGET%似乎正在用手机玩社交游戏。
ENDIF


;-------------------------------------------------
;梦话。随意むにゃむにゃ言わせてください
;-------------------------------------------------
@KOJO_SLEEP_K21
;上から順に発言頻度が高いようにする(== 0の付け忘れではない)
IF RAND:3
	IF RAND:2
		PRINTFORML 「唔~嗯……呜喵呜喵……」
	ELSE
		PRINTFORML 「ZZZ……」
	ENDIF
ELSEIF RAND:2
	IF RAND:2
		PRINTFORML 「书……不还的话……」
	ELSE
		PRINTFORML 「欸嘿嘿……好大……这样的话……才不会……输给大家」
	ENDIF
ELSE
;ネズミなので狐や犬や猫に食べられる夢
	IF RAND:5 && FIND_COND("六華")
		PRINTFORML 「好黑……好害怕……会融化的……我我会、做个好孩子的……」
	ELSEIF RAND:4 && FIND_COND("哈娜")
		PRINTFORML 「呜呜……%CALLNAME:FIND_COND("哈娜")%小姐……饶了我吧……不要咬啊……」
