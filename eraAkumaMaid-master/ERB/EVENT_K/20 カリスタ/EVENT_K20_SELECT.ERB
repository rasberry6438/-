	PRINTL 
	PRINTFORMW 「そうですね、そちらの方が似合うとお思いであれば……かしこまりました」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%は明日から男性用の制服を着て登校するよう成为了
	CALL SET_CEVENT, "卡莉斯塔：制服男装"


CASE "眼鏡外して"
	PRINTFORMW %CALLNAME:MASTER%は明日から眼鏡を外してもらえるように%CALLNAME:TARGET%にお願いした
	PRINTL 
	PRINTFORMW 「まぁ、字を読む時以外は生活に支障が無いので……かしこまりました」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%は明日から眼鏡を外して登校するよう成为了
	CALL DEL_CEVENT, "卡莉斯塔：眼鏡着用"

CASE "眼鏡付けて"
	PRINTFORMW %CALLNAME:MASTER%は明日から眼鏡を付けてもらえるように%CALLNAME:TARGET%にお願いした
	PRINTL 
	PRINTFORMW 「かしこまりました、次から戻しますね」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%は明日から眼鏡を付けて登校するよう成为了
	CALL SET_CEVENT, "卡莉斯塔：眼鏡着用"


CASE "家では執事服"
	PRINTFORMW %CALLNAME:MASTER%は明日から家で執事服を着るように%CALLNAME:TARGET%にお願いした
	PRINTL
	PRINTFORMW 「かしこまりました、次から是つもの服を着てきますね」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%は明日から家で執事服を着るよう成为了
	CALL DEL_CEVENT, "卡莉斯塔：女仆装"

CASE "家では女仆装"
	PRINTFORMW %CALLNAME:MASTER%は明日から家で女仆装を着るように%CALLNAME:TARGET%にお願いした
	PRINTL 
	PRINTFORMW 「女仆装ですね？……かしこまりました」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%は明日から家で女仆装を着るよう成为了
	CALL SET_CEVENT, "卡莉斯塔：女仆装"


ENDSELECT




