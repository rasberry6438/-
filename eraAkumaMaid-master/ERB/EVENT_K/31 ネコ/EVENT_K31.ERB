;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K31, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我

CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF TALENT:PLAYER:男性
			LOCALS = 哥哥_哥哥
		ELSE
			LOCALS = 姐姐_姐姐
		ENDIF
	;特定個人 EVENT_COMMでイベント踏んだら呼び名砕けるとかしたい。
	;フェリ酱桑
	ELSEIF NO:PLAYER == 1
		LOCALS = 女仆大姐姐_女仆姐姐
	;六華桑
	ELSEIF NO:PLAYER == 3
		LOCALS = 蛇神_蛇神
	;哈娜酱
	;ネコの中の人もたまにワンコと錯覚する
	ELSEIF NO:PLAYER == 7
		IF CEVENT("ネコイベント：哈娜初会話")
			LOCALS = 狼酱_狼酱
		ELSE
			LOCALS = 汪酱_汪酱
		ENDIF
	;アイリ酱
	;猫歴の前輩だからねこ前輩。今後誰か後輩角色にねこ前輩って呼ばれても「それはあっち」って言う。
	ELSEIF NO:PLAYER == 13
		LOCALS = 猫前辈_猫猫前辈
	;祥子前輩
	;ぽっぽぽぽぽとか歌う歌手とは関係ない
	ELSEIF NO:PLAYER == 21
		LOCALS = 老鼠前辈_老鼠前辈
	;琉娜前輩
	;ネタが暴走した結果。マジに琉娜おねー桑、略してマリオ。うーん下らない、すき
	ELSEIF NO:PLAYER == 22
		IF CEVENT("ネコイベント：琉娜初会話")
			LOCALS = 真龙姐姐_真龙姐姐
		ELSE
			LOCALS = 传说一般的姐姐_传说一般的姐姐
		ENDIF
	;相手が主人ではない
	ELSE
		;相手が学生だった場合
		SELECTCASE CSTR:PLAYER:学年
		;学生年下
		CASE "１年生", "初等部", "中等部"
			IF TALENT:PLAYER:男性
				LOCALS = 后辈君_后辈君
			ELSE
				LOCALS = 后辈酱_后辈酱
			ENDIF
		CASE "２年生"
			IF TALENT:PLAYER:男性
				LOCALS = 哥哥_哥哥
			ELSE
				LOCALS = 姐姐_姐姐
			ENDIF
		;学生年上
		CASE "３年生", "研究生"
			LOCALS = %CALLNAME:PLAYER%前輩_%CSTR:PLAYER:ふりがな%前輩
		;相手が同学年、ないし学生では無く教職。
		CASEELSE
			IF STRCOUNTS(CSTR:PLAYER:学年, "教師")
					LOCALS = %CALLNAME:PLAYER%老师_%CSTR:PLAYER:ふりがな%老师
			ELSEIF STRCOUNTS(CSTR:PLAYER:学年,  "司書")
					LOCALS = %CALLNAME:PLAYER%同学_%CALLNAME:PLAYER%同学
			;其他
			ELSE
				IF TALENT:PLAYER:男性
					LOCALS = 哥哥_哥哥
				ELSE
					LOCALS = 姐姐_姐姐
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
CASE "主人"
	IF TALENT:PLAYER:男性
		LOCALS = 哥哥_哥哥
	ELSE
		LOCALS = 姐姐_姐姐
	ENDIF
CASE "助手"
	SELECTCASE CSTR:ASSI:学年
	;学生年下
	CASE "１年生", "初等部", "中等部"
		IF TALENT:ASSI:男性
			LOCALS = 后辈君_后辈君
		ELSE
			LOCALS = 后辈酱_后辈酱
		ENDIF
	CASE "２年生"
		IF TALENT:ASSI:男性
			LOCALS = 哥哥_哥哥
		ELSE
			LOCALS = 姐姐_姐姐
		ENDIF
	;学生年上
	CASE "３年生", "研究生"
		LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
	;相手が同学年、ないし学生では無く教職。
	CASEELSE
		IF STRCOUNTS(CSTR:ASSI:学年, "教師")
			LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
		ELSEIF STRCOUNTS(CSTR:ASSI:学年,  "司書")
			LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
		ELSE
			IF TALENT:ASSI:男性
				LOCALS = 哥哥_哥哥
			ELSE
				LOCALS = 姐姐_姐姐
			ENDIF
		ENDIF
	ENDSELECT
ENDSELECT
RETURNF LOCALS

