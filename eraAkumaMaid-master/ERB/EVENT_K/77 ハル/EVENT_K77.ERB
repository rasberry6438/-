@IS_SINGLE_ENDING_K77
;エンディングを書いたら、下のRETURN 1のコメントアウト;を消してください
RETURN 1

@MENU_SINGLE_ENDING_K77
;エンディングを書いたら、下のRETURN 0を消してください
;RETURN 0

;ENDを選択する際の表示。下で満たしたEND条件に応じて全角21文字以内で好きに変えて良い
STR:ＥＮＤ标题 = ハルの日誌を読む

;魂の量によるEND
;SIF BASE:魂 >= 300
;	ENDの番号_END名 となっている
;	番号は1～60の範囲(でもできれば１から順に詰めて使用)の半角数字で、ＥＮＤ名は喜欢的东西に変更してください
;	STR:エンディング += "1_ＥＮＤ名１/"
;其他の条件(例えば淫乱)によるEND
;SIF TALENT:淫乱
;	STR:エンディング += "2_ＥＮＤ名２/"
;魂の量と其他の条件(例えば相思相愛)によるEND
;SIF BASE:魂 >= 300 && TALENT:相思相愛
;	STR:エンディング += "3_ＥＮＤ名３/"



SIF CFLAG:ハルは日誌を付けているようです == 15
	STR:エンディング += "1_日誌の表紙をめくる/"
SIF TALENT:恋慕
	STR:エンディング += "2_過去の記録１/"
SIF TALENT:恋慕
	STR:エンディング += "4_過去の記録２/"
SIF TALENT:恋慕 && (CFLAG:ハル－１００ == 2 || CFLAG:ハル－１０００ == 5)
	STR:エンディング += "3_過去の記録３/"
SIF TALENT:恋慕 && CFLAG:ハル告白断った
	STR:エンディング += "5_最近の記録/"
SIF TALENT:恋慕 && CEVENT("春") + CEVENT("夏") + CEVENT("秋") + CEVENT("冬") + CEVENT("初夏") + CEVENT("初秋") + CEVENT("晩秋")
	STR:エンディング += "6_你との記録１/"
SIF TALENT:相思相愛 && CFLAG:ハルケンカ >= 1
	STR:エンディング += "7_你との記録２/"


RETURN 1


;閲覧していないイベントがある場合などに選択肢と色を変える事も出来る。これ以外でも好きな条件で変えて良い
;IF COND("未読ＥＮＤ選択可能")
;	STR:ＥＮＤ标题 = 终末之始
;	;これを指定しない場合には本体側で STR:ＥＮＤカラー = 爱心粉紅 となる
;	STR:ＥＮＤカラー = 爱心粉紅
;ENDIF


;ARGSには@MENU_SINGLE_ENDING_K1で設定したEND名のどれかが入っている
;見たENDはCEVENTで記録される
;例）ＥＮＤ名１を閲覧した場合は CEVENT("ＥＮＤ名１") = 1 となる
@SINGLE_ENDING_K77, ARGS
SELECTCASE ARGS
CASE "日誌の表紙をめくる"
	IF TALENT:恋慕 == 0 
		CALL PRINT_STRW, @"黄色_※このエンディングを見ると%CALLNAME:TARGET%は恋慕を取得します"
		PRINTFORML 
		PRINTFORMW 日誌を読みますか？
	ENDIF
	PRINTFORML 
	SIF CFLAG:ハル告白断った
		PRINTFORML [0] 日誌を開く
	PRINTFORML [1] 日誌を読む
	PRINTFORML [999] 返回
	PRINTL 

	CALL INPUT_SELECT, 2, 999

	IF RESULT == 0
		CALL K77_HaLL5c68a32_MyMORIMOTO
	ELSEIF RESULT == 1
		CALL K77_HaLL5c68a32_FALLINLOVE
		TALENT:恋慕 = 1
		PRINTFORML 
		SIF TALENT:恋慕 == 0
			CALL PRINT_STRW, @"%CALLNAME:TARGET%は%CALLNAME:MASTER%への_爱心粉紅_恋慕_を得た"
		CALL DEL_CEVENT, "合意禁止：接吻"
		CALL DEL_CEVENT, "合意禁止：肛門性交"
		CALL DEL_CEVENT, "合意禁止：叫出来Ｈ"
		CALL DEL_CEVENT, "放課後Ｈ的邀请禁止"
		CALL DEL_CEVENT, "朝袭禁止"
		CFLAG:ハル告白断った = 1
;初期の禁止事項↓ ★全開放するには、相思相愛になる必要がある★
;合意禁止：胖次交易/放課後Ｈ的邀请禁止/合意禁止：接吻/合意禁止：掀起裙子/合意禁止：摆POSE/合意禁止：检查内褲/合意禁止：内衣写真/合意禁止：检查阴部/合意禁止：Ｈ的写真/合意禁止：精飲/合意禁止：飲尿PLAY/合意禁止：肛門性交/合意禁止：身体穿環/合意禁止：露出PLAY/合意禁止：ＡＶ撮影/合意禁止：叫出来Ｈ/放課後Ｈ禁止/Ｈ的调戏禁止/"晒痕禁止/朝袭禁止/夜袭禁止/"

	ELSE
		PRINTFORMW 閲覧終了
	ENDIF
CASE "過去の記録１"
	PRINTFORMW ここで読めるのは%CALLNAME:TARGET%が起動してから、学校に行くまでの記録です。
	PRINTFORMW どれを読みますか？
	PRINTFORML 
	PRINTFORML [0] 日誌その１
	PRINTFORML [1] 日誌之２
	PRINTFORML [2] 日誌その３
	PRINTFORML [3] 日誌その４
	PRINTFORML [4] 日誌その５
	PRINTFORML [5] 日誌その６
	PRINTFORML [6] 日誌その７
	PRINTFORML [7] 日誌その８
	PRINTFORML [8] 日誌その９
	PRINTFORML [9] 日誌その１０
	PRINTFORML [999] 返回
	PRINTL 

	CALL INPUT_SELECT, 10, 999

	;日誌その１から順番に‥‥ちょっと多い？
	IF RESULT == 0
		CALL K77_HaLL5c68a32_DAYs
	ELSEIF RESULT == 1
		CALL K77_HaLL5c68a32_is_angry
	ELSEIF RESULT == 2
		CALL K77_HaLL5c68a32_want_to_goto_school_1
	ELSEIF RESULT == 3
		CALL K77_HaLL5c68a32_want_to_goto_school_2
	ELSEIF RESULT == 4
		CALL K77_HaLL5c68a32_want_to_goto_school_3
	ELSEIF RESULT == 5
		CALL K77_HaLL5c68a32_want_to_goto_school_4
	ELSEIF RESULT == 6
		CALL K77_HaLL5c68a32_want_to_goto_school_5
	ELSEIF RESULT == 7
		CALL K77_HaLL5c68a32_want_to_goto_school_6
	ELSEIF RESULT == 8
		CALL K77_HaLL5c68a32_want_to_goto_school_7
	ELSEIF RESULT == 9
		CALL K77_HaLL5c68a32_goto_school
	ELSE
		PRINTFORMW 閲覧終了
	ENDIF
CASE "過去の記録２"
	PRINTFORMW %CALLNAME:TARGET%が学校へ行きだしてから今日までの、%CALLNAME:MASTER%との記録です。
	PRINTFORMW どれを読みますか？
	PRINTFORML 
	PRINTFORML [0] 日誌その１
	PRINTFORML [1] 日誌之２
	PRINTFORML [2] 日誌その３
	PRINTFORML [3] 日誌その４
	SIF CEVENT("手抜きの弁当")
		PRINTFORML [4] 日誌その５
	PRINTFORML [999] 返回
	PRINTL 

	$INPUT_LOOP_過去の記録２
	INPUT

	IF RESULT == 0
		CALL K77_HaLL5c68a32_BOOK
	ELSEIF RESULT == 1
		CALL K77_HaLL5c68a32_STOPBY_CONVENIENCESTORE
	ELSEIF RESULT == 2
		CALL K77_HaLL5c68a32_ROUGHINPOINTS
	ELSEIF RESULT == 3
		CALL K77_HaLL5c68a32_STADYTIME
	ELSEIF RESULT == 4 
		CALL K77_HaLL5c68a32_scholldays
	ELSEIF RESULT == 999
		PRINTFORMW 閲覧終了
	ELSE
		GOTO INPUT_LOOP_過去の記録２
	ENDIF
CASE "過去の記録３"
	PRINTFORMW %CALLNAME:MASTER%が%CALLNAME:TARGET%に嫌われていた頃の記録です。
	PRINTFORMW どれを読みますか？
	PRINTFORML 
	PRINTFORML [0] 日誌その１
	IF CFLAG:ハル－１０００ == 5
		PRINTFORML [1] 日誌之２
		PRINTFORML [2] 日誌その３
		PRINTFORML [3] 日誌その４
	ENDIF
	PRINTFORML [999] 返回
	PRINTL 

	$INPUT_LOOP_過去の記録３
	INPUT

	IF RESULT == 0
		CALL K77_HaLL5c68a32_BEAUTIFUL_SCENERY
	ELSEIF RESULT == 1
		CALL K77_HaLL5c68a32_OF_ANXIETY
	ELSEIF RESULT == 2
		CALL K77_HaLL5c68a32_STRAYDOG
	ELSEIF RESULT == 3
		CALL K77_HaLL5c68a32_BADDRUNKENNESS
	ELSEIF RESULT == 999
		PRINTFORMW 閲覧終了
	ELSE
		GOTO INPUT_LOOP_過去の記録３
	ENDIF
CASE "最近の記録"
	PRINTFORMW 最近%CALLNAME:TARGET%の頭を占めている出来事についての記録です。
	PRINTFORMW どれを読みますか？
	PRINTFORML 
	PRINTFORML [0] 葛藤
	SIF CFLAG:ハル葛藤 >= 1
		PRINTFORML [1] 懊悩
	SIF CFLAG:ハル葛藤 >= 2
		PRINTFORML [2] 解凍
	SIF TALENT:相思相愛
		PRINTFORML [3] 始動
	PRINTFORML [999] 返回
	PRINTL 

	$INPUT_LOOP_最近の記録
	INPUT

	IF RESULT == 0
		CALL K77_HaLL5c68a32_CONFICT
		CFLAG:ハル葛藤 = 1
	ELSEIF RESULT == 1 
		CALL K77_HaLL5c68a32_ANGUISH
		CFLAG:ハル葛藤 += 1
	ELSEIF RESULT == 2 
		CALL K77_HaLL5c68a32_START
		CFLAG:ハル葛藤 += 1
	ELSEIF RESULT == 3 && TALENT:相思相愛
		CALL K77_HaLL5c68a32_LOVE_LOVER_LOVE
		CFLAG:ハル葛藤 += 1
	ELSEIF RESULT == 999
		PRINTFORMW 閲覧終了
	ELSE
		GOTO INPUT_LOOP_最近の記録
	ENDIF
CASE "你との記録１"
	PRINTFORMW %CALLNAME:MASTER%と%CALLNAME:TARGET%の些細な思い出のいくつかです。
	PRINTFORMW どれを読みますか？
	PRINTFORML 
	SIF CEVENT("春")
		PRINTFORML [0] 桜が開心の木の下で
	SIF CEVENT("初夏")
		PRINTFORML [1] 新緑に丘に座って草団子
	SIF CEVENT("夏")
		PRINTFORML [2] 蒸し暑い真夏の夜の夢に
	SIF CEVENT("初秋")
		PRINTFORMW [3] 採れたてだからしょうがない
	SIF CEVENT("秋")
		PRINTFORML [4] 大イチョウが色づくある日
	SIF CEVENT("晩秋")
		PRINTFORMW [5] 亲手自制で芸術を楽しむ
	SIF CEVENT("冬")
		PRINTFORML [6] ウィンター・ウォーカー
	PRINTFORML [999] 返回
	PRINTL 

	$INPUT_LOOP_你との記録
	INPUT

	IF RESULT == 0 
		CALL K77_HaLL5c68a32_SPRING_WARKING
	ELSEIF RESULT == 1 
		CALL K77_HaLL5c68a32_KUSA_DANGO
	ELSEIF RESULT == 2 
		CALL K77_HaLL5c68a32_MIDSUMMER_NIGHT_DREAM
	ELSEIF RESULT == 3 
		CALL K77_HaLL5c68a32_BUDOU_GARI
	ELSEIF RESULT == 4 
		CALL K77_HaLL5c68a32_AUTMN_WALKING
	ELSEIF RESULT == 5 
		CALL K77_HaLL5c68a32_KUMIHIMO_KYOUSHISTU
	ELSEIF RESULT == 6 
		CALL K77_HaLL5c68a32_WINTER_WALKER
	ELSEIF RESULT == 999
		PRINTFORMW 閲覧終了
	ELSE
		GOTO INPUT_LOOP_你との記録
	ENDIF
CASE "你との記録２"
	PRINTFORMW %CALLNAME:TARGET%と%CALLNAME:MASTER%のケンカ中の%CALLNAME:TARGET%の心境です。
	PRINTFORML 
	PRINTFORMW ※少々重い内容となりますので、自己責任でお読み下さい。
	PRINTFORML 
	PRINTFORML [0] 放置されると悲しくなったりする
	PRINTFORML [999] 読むのをやめる

	$INPUT_LOOP_你との記録２
	INPUT

	IF RESULT == 0
		CALL K77_HaLL5c68a32_GATIGIRE_angry
	ELSEIF RESULT == 999
		PRINTFORMW 閲覧終了
	ELSE
		GOTO INPUT_LOOP_你との記録２
	ENDIF
	
ENDSELECT
RETURN 1
