;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K77, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位", "朝舐陰", "朝騎乗位"
	CASE "朝足交"
		CALL SETFLAG, "变成内衣姿态", TARGET
		CALL SET_SHIRT, "你の恤"
		CALL SETFLAG, "地の文スキップ"
	CASE "朝の新妻PLAY"
		CALL SETFLAG, "变成内衣姿态", TARGET
		CALL SET_SHIRT, "你の恤"
		CALL SETFLAG, "地の文スキップ"
	CASE "朝乳交"
		CALL SETFLAG, "变成内衣姿态", TARGET
		CALL SET_SHIRT, "你の恤"
		CALL SETFLAG, "地の文スキップ"
	CASE "朝面部騎乗"
		CALL SETFLAG, "变成内衣姿态", TARGET
		CALL SET_SHIRT, "你の恤"
		CALL SETFLAG, "地の文スキップ"
	
	CASE "朝Ａ騎乗位"
		CALL SETFLAG, "变成内衣姿态", TARGET
		CALL SET_SHIRT, "你の恤"
	CASE "朝接吻"
		CALL SETFLAG, "地の文スキップ"
		CALL SETFLAG, "变成内衣姿态", TARGET
		CALL SET_SHIRT, "你の恤"
		CALL EMOTION_PLUS, "重置/憤怒眉/半目/红晕/嘟嘴２", TARGET
		PRINTFORMW 「おい、起きろ%CNAME("二人称")%、朝だぞ」　　　ちゅ
		PRINTFORML 
		PRINTFORMW これがセクサロイドの役目だとでも思ってるのか、%CALLNAME:TARGET%が%CALLNAME:MASTER%を起こしに来る時是つも、
		PRINTFORMW 起床を促すぞんざいな言葉とはうらはらな、ほっぺへの打招呼の接吻が付いてくる。
	CASE "朝添い寝"
		CALL SETFLAG, "地の文スキップ"
		CALL SETFLAG, "变成内衣姿态", TARGET
		CALL EMOTION_PLUS, "重置/睡眠", TARGET
		PRINTFORMW %CALLNAME:MASTER%が睁開眼睛と、隣には%CALLNAME:MASTER%の被窝にくるまって寝ている%CALLNAME:TARGET%がいた。
		PRINTFORML 
		PRINTFORMW 「‥‥すぴー… ん…？ んぁ…？ ‥‥‥‥にゅ‥‥‥‥‥‥はよ、%CNAME("二人称")%‥‥ん～…」
		CALL EMOTION_PLUS, "斜眼４/小口３/通常眉３/紅潮/对话框困倦", TARGET
		PRINTFORMW %CALLNAME:MASTER%が揺すって叫醒と、まだ眠そうな目を開けて%CALLNAME:TARGET%は朝の打招呼をしてきた。
		PRINTFORML 
	CASE "朝口交", "朝六九式"
		CALL SETFLAG, "变成内衣姿态", TARGET
	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		PRINTFORMW 「なんだよ、もう起きたのか%CNAME("二人称")%」
	CASE "朝騎乗位"
		PRINTFORMW 「えへ。ちょっとした恶作剧のつもりだったのに、起こしちまったみてえだな」
		PRINTFORMW 「おはよ、%CNAME("二人称")%」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 「今日は体育があったよな？ 準備してあっから忘れんなよ？」
	ELSE
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 「おはよ、%CNAME("二人称")%」
	ENDIF
CASE "登校"
	IF CFLAG:心情差
		CALL EMOTION_PLUS, "重置/半目/飘忽/憤怒眉/嘟嘴", TARGET
		PRINTFORMW 「‥‥‥はよ」
	ELSE
		IF DAYEV("試験当日")
			CALL EMOTION_PLUS, "重置/通常", TARGET
			PRINTFORML 「今日ってテストがあるんだっけ？」
			IF BASE:学力 >= 70
				PRINTFORMW 「%CNAME("一人称")%、そこそこ勉強したけど、テストはどうかな～？」
			ELSEIF BASE:学力 <= 30
				PRINTFORMW 「今回全然勉強しなかったし、%CNAME("一人称")%テストはヤベえかも‥‥‥」
			ENDIF
		ELSEIF DAYEV("試験返却日")

		ELSEIF COND("朝袭相手")

		ELSEIF COND("体育")
			CALL EMOTION_PLUS, "重置/", TARGET
			PRINTFORMW 「おはよ%CNAME("二人称")%。今日は体育があったよな」
			PRINTFORMW 「酱と体操服と运动衫、持ってきただろうな？」
		ELSEIF COND("水泳")
			CALL EMOTION_PLUS, "重置/", TARGET
			PRINTFORMW 「おはよ%CNAME("二人称")%。今日は水泳があったよな」
			PRINTFORMW 「酱と泳装、忘れてねえだろうな？」
		ELSE

		ENDIF
	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
	CALL EMOTION_PLUS, "重置/通常", TARGET
	PRINTFORMW 「おっと、今日は%CNAME("一人称")%が一番乗りみてえだな」
CASE "ただいま"
	IF TALENT:叛逆 || CFLAG:ハル心情差
		CALL EMOTION_PLUS, "重置/半目/憤怒眉/△", TARGET
		PRINTFORMW 「‥‥‥ただいま～」
	ELSE
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 「今戻った。ん？ 何だよ%CNAME("二人称")%」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:叛逆 || CFLAG:ハル心情差
			CALL EMOTION_PLUS, "重置/半目/憤怒眉/△", TARGET
			PRINTFORMW 「‥‥おかえり」
		ELSEIF RAND:2 == 0
			CALL EMOTION_PLUS, "重置/通常", TARGET
			PRINTFORMW 「おかえり、%CNAME("二人称")%。今%CSTR:茶%淹れようと思ってたとこなんだけど、%CNAME("二人称")%も飲む？」
		ELSE
			CALL EMOTION_PLUS, "重置/通常", TARGET
			PRINTFORMW 「%CNAME("二人称")%おかえり」
			IF COND("冬") && TALENT:叛逆 == 0
				PRINTFORMW 「外、寒くなかったか？ %CNAME("二人称")%もこたつ入んなよ」
			ENDIF
		ENDIF
	ELSE
		IF TALENT:叛逆 || CFLAG:ハル心情差
			CALL EMOTION_PLUS, "重置/半目/憤怒眉/△", TARGET
			PRINTFORMW 「‥‥よっす%CNAME("二人称")%」
		ELSE
			CALL EMOTION_PLUS, "重置/红晕", TARGET
			PRINTFORMW 「よっす%CNAME("二人称")%」
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		IF TALENT:叛逆 || CFLAG:ハル心情差
			CALL EMOTION_PLUS, "重置/半目/憤怒眉/△", TARGET
			PRINTFORMW 「‥‥何？」
		ELSE
			IF TEXTS("日期対応イベント") == "即将到来文化祭"
				CALL EMOTION_PLUS, "重置/憤怒眉/△/红晕", TARGET
				PRINTFORMW 「%CNAME("一人称")%、クラスの文化祭準備で忙しいんだけど、なんか用だった？」
			ELSE
				CALL EMOTION_PLUS, "重置/通常", TARGET
				PRINTFORMW 「%CNAME("二人称")%、呼ばれたから来てみたんだけど、なんか用？」
			ENDIF
		ENDIF
	;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
	ELSEIF TALENT:叛逆 || COND("Ｈ的いちゃつき中", MASTER)
		CALL EMOTION_PLUS, "重置/半目/憤怒眉/△", TARGET
		PRINTFORMW 「なんだよ、お取込み中ってやつ？ %CNAME("二人称")%はお盛んだな」
		CFLAG:ハル心情差 += 1
		CALL PRINT_STRW, @"暗灰色_【心情：-{CFLAG:ハル心情差}】"
	ELSE
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 「よっす%CNAME("二人称")%」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 「悪い、ちょっと遅れたっ」
		PRINTFORML 「何？ 今日みんなヒマだったの？」
	ELSEIF CFLAG:呼び出された && CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4
		IF TALENT:叛逆 || CFLAG:ハル心情差
			CALL EMOTION_PLUS, "重置/半目/憤怒眉/△", TARGET
			PRINTFORMW 「‥‥‥‥‥何？」
		ELSE
			CALL EMOTION_PLUS, "重置/通常", TARGET
			PRINTFORMW 「何だよ～、他にもいるのに、何も%CNAME("一人称")%呼び出さなくてもいいじゃんか～」
		ENDIF
	ELSE
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 「こんちゃーっ」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORML 『あれ？ %CNAME(CALLNAME:ACTOR)%じゃん。こんなとこで仕事サボってんの？』
		PRINTFORMW 『悪い老师だなぁ～』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 『よっす遅かったな、%CNAME(CALLNAME:ACTOR)%。何か用事でもあったのか？』
	ELSE
		CALL EMOTION_PLUS, "重置", TARGET
		PRINTFORMW 『こんちゃ、%CNAME(CALLNAME:ACTOR)%。今日もまったりやってこうぜ』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		IF TALENT:叛逆 || CFLAG:ハル心情差
			CALL EMOTION_PLUS, "重置/半目/憤怒眉/△", TARGET
			PRINTFORMW 「‥‥何？」
		ELSE
			CALL EMOTION_PLUS, "重置/通常", TARGET
			PRINTFORMW 「%CNAME("二人称")%、呼んだ？ なんか用？」
		ENDIF
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 「ふぅ‥‥」
	ELSE
		CALL EMOTION_PLUS, "重置/通常", TARGET
		PRINTFORMW 「我回来了——」
	ENDIF
;你と一緒に摆脱人はPARTNER()で参照できる。名前はCALLNAME:PARTNER()です
CASE "抜け出された"
	CALL EMOTION_PLUS, "重置/憤怒眉", TARGET
	PRINTFORMW 「あそ。行ってら～%CNAME("二人称")%。%CNAME("partner")%も腰には気を付けるんだぞ～」
	CFLAG:ハル心情差 += 1
	CALL PRINT_STRW, @"暗灰色_【心情：-{CFLAG:ハル心情差}】"
CASE "便当"
	IF CFLAG:ハル心情差
		CALL EMOTEFACE, "重置/半目/飘忽/憤怒眉/嘟嘴", TARGET
		PRINTFORMW 「%CNAME("一人称")%、今日は干劲出なかったし、適当メシな」
		PRINTFORML 
		PRINTFORMW %CALLNAME:TARGET%の言うとおり、いつもよりメニューがかなり適当だ‥‥
		PRINTFORMW どうやら%CALLNAME:TARGET%の心情を損ねると、弁当のメニューが貧しくなるらしい。
		PRINTFORML 
	ELSEIF TALENT:料理擅長
		IF RAND:5 == 0
			SAVESTR:現在位置 = 野外
			CALL EMOTION_PLUS, "重置/通常", TARGET
			PRINTFORMW 「%CNAME("二人称")%、今日は天気もいいし、たまには外行って食わねえ？」
			PRINTFORMW %CALLNAME:TARGET%の提案で今日は%TEXTR("校舎の房顶/中庭/グラウンド脇")%にやってきた。
			PRINTFORML 
		ELSE
			CALL EMOTION_PLUS, "重置/通常", TARGET
			PRINTFORMW 「よっす%CNAME("二人称")%、待った？ さっそく食おうぜっ」
			PRINTFORMW %CALLNAME:TARGET%と待ち合わせて、いつものように２人で弁当を食べる。
			PRINTFORMW この時間もなんだか馴染んだものになってきた。
			PRINTFORML 
		ENDIF
		IF RAND:10 == 0
			PRINTFORMW 「じゃーん、今日は気が向いたから、甜点まであるんだぞっ」
			PRINTFORML 
			PRINTFORMW そう言って%CALLNAME:TARGET%が取り出したのは、%TEXTR("うさぎに剥いたりんご/ヨーグルト/スイートポテト/冷凍餡餅ナップル/冷凍橘子")%だった。
			PRINTFORML 
		ELSEIF RAND:5 == 0
			PRINTFORMW 「今日はなんか、加減忘れて作ったせいで、ちょっと量が多いから、いっぱい食え」
			PRINTFORML 
			PRINTFORMW そういえば%CALLNAME:TARGET%の言う街道、今日の弁当箱是つものより一回り大きいものだ。
		ELSEIF RAND:3 == 0
			PRINTFORMW 「ついでだから今日は、ポットに汁物も持ってきたぞ。嬉しいだろ？」
			PRINTFORML 
			PRINTFORMW %CALLNAME:TARGET%はそう言い的同时、%TEXTR("味噌汤/コーンスープ/オニオンスープ/ピリ辛中華スープ/わかめスープ")%の入ったポットを差し出してきた。
			PRINTFORML 
		ENDIF
		PRINTFORMW さてさて、それでは肝心の%CALLNAME:TARGET%の弁当の中身は‥‥‥
		PRINTFORML 
		IF RAND:4 == 0
			PRINTFORMW なんと、豪快に%TEXTR("牛丼/焼肉丼/ステーキ丼/ガーリックチキン丼/すきやき丼")%弁当だった。
			CFLAG:ハル弁当指数 += 1
		ELSEIF RAND:3 == 0
			PRINTFORM %TEXTR("飯団/三明治/おにぎらず/ピラフ/チャーハン炊き込みご飯/チキン番茄酱ライス")%と、
			PRINTFORM %TEXTR("アスパラのベーコン巻き/豚の梅しそ巻き油炸/エビ阴蒂ームコロッケ/レンコン入り漢堡肉/エビフライ/とんかつ")%と、
			PRINTFORM %TEXTR("玉子の巻き焼/野菜入りの開放型オムレツ/プレーンオムレツ/ポテトサラダ/スクランブルエッグ")%と、
			PRINTFORM %TEXTR("ラタトュユ/番茄マリネ/番茄の乳酪焼き/焼き番茄/ガーリック番茄煮")%と、
			PRINTFORM %TEXTR("ブロッコリーのドレッシング漬け/水菜と油炸の煮びたし/チンゲン菜とツナの炒め合え/きゅうりのピリ辛漬け/ほうれん草の芝麻和え")%の弁当で、
			PRINTFORMW なにやらとても美味そうだ。
			CFLAG:ハル弁当指数 += 1
		ELSEIF RAND:2 == 0
			PRINTFORM 白飯に%TEXTR("たくあん/うめぼし/しば漬け/塩こぶ/ゆかり/ふりかけ")%と、
			PRINTFORM %TEXTR("から油炸/一口漢堡肉/シイタケのひき肉詰め/豚の姜汁焼肉/茄子のはさみ油炸")%と、
			PRINTFORM %TEXTR("ゆで卵/フライドポテト/玉子の巻き焼/スクランブルエッグ/プレーンオムレツ")%と、
			PRINTFORM %TEXTR("ほうれん草の涼拌菜/きゅうりとわかめの酢の物/温野菜サラダ/無限捲心菜/オクラの梅しそ和え")%で、
			PRINTFORMW そこそこがんばって作ってくれたらしい。
			CFLAG:ハル弁当指数 += 1
		ELSE
			PRINTFORM 得意料理らしいメインの%TEXTR("鶏つくねの照り焼き/豚のロール巻き/さばのゆず塩焼き/イカのコロッケ/メンチカツ")%と、
			PRINTFORM 色味に気を使った黄色の%TEXTR("かぼちゃのサラダ/乳酪スクランブルエッグ/マカロニのカレー風味サラダ/さつま芋の清爽煮/厚焼き玉子")%、
			PRINTFORM 赤の%TEXTR("にんじんのきんぴら/番茄のマリネ/ラディッシュのハム巻き/にんじんごまナムル")%、
			PRINTFORMW 緑の%TEXTR("ブロッコリーの醤油炒め/ピーマンのオイスター焼き/小松菜のツナ和え/アスパラのみそマヨ和え/きゅうりのごま焼き")%だった。
			PRINTFORMW 最近料理の腕を上げたのか、とても美味そうな弁当だと、２人で並んで手を合わせていただく。
			CFLAG:ハル弁当指数 += 1
		ENDIF

		PRINTFORML 
		PRINTFORML そんな%CALLNAME:TARGET%の弁当を%CALLNAME:MASTER%が‥‥
		IF RAND:3 == 0
			CALL PRINT_SELECT, "いつも頑張ってくれてて嬉しいと言う/どれもすごく美味しそうだと夸奖/とりあえずちょっとケチをつける"
			IF RESULT == 0
				CALL EMOTION_PLUS, "重置/笑颜２/大口/红晕", TARGET
				PRINTFORMW 「あーうん。%CNAME("二人称")%が嬉しいと思ってんなら、%CNAME("一人称")%も作った甲斐もあるよ」
				PRINTFORMW そう言って坦率に開心地した。
			ELSEIF RESULT == 1
				CALL EMOTION_PLUS, "重置/△/紅潮", TARGET
				PRINTFORMW 「え‥‥？ ぁ、うめえ？ あそ‥‥ふーん‥‥‥‥」
				PRINTFORMW 分かりにくいけど、弁当を褒められた事が嬉しいらしい。
				PRINTFORMW 少しそっぽ向いた%CALLNAME:TARGET%の頬に薄く朱がさしているのに気づいた。
				CFLAG:ハル弁当指数 += 1
			ELSE
				CALL EMOTION_PLUS, "重置/半目/憤怒眉/△", TARGET
				PRINTFORMW 「なんだよ、作ってもらっといてそれかよ‥‥」
				PRINTFORMW 見ていて明らかなほど、%CALLNAME:TARGET%の心情が悪くなった。
				PRINTFORMW 好感度-20
				CFLAG:好感度 -= 20
				CFLAG:ハル心情差 += 1
				CALL PRINT_STRW, @"暗灰色_【心情：-{CFLAG:ハル心情差}】"
			ENDIF
		ELSEIF RAND:2
			CALL PRINT_SELECT, "いつも有がとうと感謝する/今日のはすごく美味そうだなと喜ぶ/今日はなんか手抜きしてない？とケチをつける"
			IF RESULT == 0
				CALL EMOTION_PLUS, "重置/笑颜２/大口/红晕", TARGET
				PRINTFORMW 「あーうん。%CNAME("二人称")%が嬉しいと思ってんなら、%CNAME("一人称")%も作った甲斐もあるよ」
				PRINTFORMW そう言って坦率に開心地した。
			ELSEIF RESULT == 1
				CALL EMOTION_PLUS, "重置/△/紅潮", TARGET
