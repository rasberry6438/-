;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K66, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		;[IF_DEBUG]
		;	PRINTFORMW KOJO_GREETING_K66, %ARGS%, %CSTR:朝袭%
		;[ENDIF]
	CASE "朝騎乗位"
		;[IF_DEBUG]
		;	PRINTFORMW KOJO_GREETING_K66, %ARGS%, %CSTR:朝袭%
		;[ENDIF]
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 
	ELSE
		;[IF_DEBUG]
		;	PRINTFORMW KOJO_GREETING_K66, %ARGS%
		;[ENDIF]
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")

	ELSEIF DAYEV("試験返却日")

	ELSEIF COND("朝袭相手")

	ELSEIF COND("体育")

	ELSEIF COND("水泳")

	ELSE

	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
	STR:1 = 「む……今日は……%CNAME("一人称")%が……最初、か……」#F重置/斜眉/無口系
	IF TALENT:恋慕
		STR:2 = きょろきょろと落ち着かない様に視線をうろうろさせる%CALLNAME:TARGET%にどうしたのかと聞くと#F重置/斜眼４/飘忽/困扰眉/紅潮２/頬手
		STR:3 = 「な、何でもない……ただ……%CNAME("二人称")%と……二人きり……なんだな……と、思った……だけ……」#F重置/困扰眉/小口２/紅潮２/对话框汗２/頬手
	ELSE
		STR:2 = 「何時も……思う、のだが……」#F重置/半目/困扰眉/小口２
		STR:3 = 「部外者が……学び舎に……入るのは……良いの……だろう、か……？」#F重置/闭眼２/困扰眉/へ字３
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET
CASE "ただいま"
	IF TALENT:恋慕
		IF CEVENT("満ちた月")
			STR:1 = 「ん……ただいま、%CALLNAME:MASTER%……」#F重置/斜眉/微笑３
			IF RAND:3 ==  0
				STR:2 = 「ふふっ……♪」#F重置/笑颜４/斜眉/微笑３
				STR:3 = 「何でもない……ただ……少し、嬉しく……思った……だけ……」#F重置/半目/斜眉/微笑３
			ENDIF
		ELSE
			STR:1 = 「戻った……」#F重置/半目/困扰眉/小口２
		ENDIF
	ELSE
		STR:1 = 「戻った……」#F重置/闭眼２/小口２
		IF RAND:10 == 0
			STR:2 = 「今日は……仕事があった……」#F重置/半目/困扰眉/小口２
			STR:3 = 「はふ……疲惫……」#F重置/闭眼２/困扰眉/小口２/叹气
		ELSE
			STR:2 = 「少し……散步に……出ていた……だけ……」#F重置/半目/小口２
		ENDIF
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:恋慕
			IF CEVENT("満ちた月")
				STR:1 = 「おかえり、%CALLNAME:MASTER%……」#F重置/半目/斜眉/微笑３
				IF FLAG:二人きり && RAND:3 == 0
					STR:2 = 「帰って……くるのが……少し、待ち遠しかった……」#F重置/笑颜４/斜眉/微笑３/紅潮/頬手
				ENDIF
			ELSE
				STR:1 = 「あ、ああ……おかえり……%CNAME("二人称")%……」#F重置/困扰眉/小口２
			ENDIF
		ELSE
			STR:1 = 「ん……%CNAME("二人称")%、か……」#F重置/半目/斜眉/無口系
		ENDIF
	ELSE
		IF TALENT:恋慕 && CEVENT("満ちた月")
			IF FLAG:二人きり
				STR:1 = 「ぁ、いらっしゃい……%CALLNAME:MASTER%……横、どう……？」#F重置/半目/斜眉/微笑３/布
				STR:2 = そう言って%CALLNAME:TARGET%が手招きするから彼女の横に座った。
				STR:3 = _暗灰色_「ふふっ……♪Corculum……♪」#F重置/笑颜４/斜眉/微笑３/紅潮
				;かー！二人きりで人目がないからって「愛しい人」とか言って卑しい女ばい！
			ELSE
				STR:1 = 「ん……先に……寛がせて……貰っている、ぞ……」#F重置/笑颜４/斜眉/微笑３
			ENDIF
		ELSE
			STR:1 = 「%CNAME("二人称")%か……先に……寛がせて……貰っている、ぞ……」#F重置/闭眼２/小口２
		ENDIF
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET

CASE "入室打招呼"
	IF CFLAG:呼び出された
		IF TALENT:恋慕 && CEVENT("満ちた月")
			STR:1 = 「ん……%CNAME("二人称")%……%CNAME("一人称")%に……何用……？」#F重置/半目/斜眉/微笑３
		ELSE
			STR:1 = 「%CNAME("二人称")%……%CNAME("一人称")%に……何用……？」#F重置/半目/困扰眉/小口２
		ENDIF
	;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF TALENT:恋慕
			IF CEVENT("満ちた月")
				IF PLACE("部室")
					STR:1 = 「%CNAME("二人称")%……学び舎、では……程々に……」#F重置/闭眼２/困扰眉/小口２
					STR:2 = 呆れた顔をしながら%CALLNAME:TARGET%に注意をされた……
				ELSE
					STR:1 = 「………」#F重置/半目/对话框・・・
					STR:2 = 部屋に入って来た%CALLNAME:TARGET%が白けた瞳で見つめてくる……
					IF RAND:3 == 0
						STR:3 = と、思ったら街道すがりに脛を蹴られた……#F重置/半目/嘟嘴
					ENDIF
				ENDIF
			ELSE
				STR:1 = 「%CNAME("一人称")%……いや……何でもない……」#F重置/半目/困扰眉/小口２
				STR:2 = 何かを言おうとして止めた後に%CALLNAME:TARGET%はソファーに腰かけた……#F重置/陰/困扰眉/無口系
			ENDIF
		ELSE
			STR:1 = 「%CNAME("二人称")%が……どこで……催そうが……良い、けれど……」
			STR:2 = 「人目は……気にした方が……良い……」
		ENDIF
	ELSE
		STR:1 = 「失礼、する……」#F重置/闭眼２/小口２
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		STR:1 = 「む……遅れた……よう、だな……すまない……」#F重置/闭眼２/困扰眉/小口２
	ELSEIF CFLAG:呼び出された
		STR:1 = 「ん……こんにちは、だ……%CNAME("二人称")%、%CNAME("一人称")%に……何用……？」#F重置/半目/小口２
	ELSE
		STR:1 = 「ん……失礼、する……」#F重置/闭眼２/小口２
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		STR:1 = 『%CSVCALLNAME(NO:ACTOR)%教諭……こんにちは、だ……』#F重置/半目/小口２
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		STR:1 = 『%CSVCALLNAME(NO:ACTOR)%殿……こんにちは、だ……』#F重置/半目/小口２
	ELSE
		STR:1 = 『%CSVCALLNAME(NO:ACTOR)%殿……こんにちは、だ……』#F重置/半目/小口２
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET

CASE "居間に現れた"
	IF CFLAG:呼び出された
		IF TALENT:恋慕 && CEVENT("満ちた月")
			STR:1 = 「ん……%CNAME("二人称")%……%CNAME("一人称")%に……何用……？」#F重置/半目/斜眉/微笑３
		ELSE
			STR:1 = 「%CNAME("二人称")%……%CNAME("一人称")%に……何用……？」#F重置/半目/困扰眉/小口２
		ENDIF
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		
		STR:1 = 「戻った……」#F重置/闭眼２/小口２
	ELSE
		STR:1 = 「ん……」#F重置/闭眼２/無口系
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET

CASE "抜け出された"
	IF TALENT:恋慕
		IF CEVENT("満ちた月")
			STR:1 = 「Te velle me eligere...」#F重置/半目/困扰眉/嘟嘴３
		ELSE
			STR:1 = 「いって、らっしゃい……」#F重置/陰/困扰眉/小口２
		ENDIF
	ELSE
		STR:1 = 「はぁ……盛ん……」#F重置/闭眼２/困扰眉/小口２　
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET

CASE "便当"
	IF TALENT:恋慕
		STR:1 = 「ん……%CNAME("二人称")%……頼まれていた……便当……」#F重置/半目/斜眉/微笑３
	ELSE
		STR:1 = 「ん……%CNAME("二人称")%……頼まれていた……モノ……」#F重置/闭眼２/無口系
	ENDIF
	STR:2 = そう言って%CALLNAME:TARGET%が便当を%CALLNAME:MASTER%に差し出す。
	IF DEVENT("共に吃飯を") == 0
		IF TALENT:恋慕
			STR:3 = 「その……一緒に……どう……？」#F重置/斜眼４飘忽/困扰眉/小口２/紅潮/頬手
			STR:4 = もう一つ便当箱を取り出して、期待するような瞳で見てくる%CALLNAME:TARGET%に良いぞと答えると。#F重置/半目/困扰眉/無口系/紅潮/頬手
			CALL DIALOGUE_FRAME_NAMEW, TARGET
			STR:1 = 「ぁ……ふふっ……有がとう……」#F重置/笑颜４/斜眉/微笑３/对话框開心
			STR:2 = 露出笑容開心地する%CALLNAME:TARGET%を横に座らせ、一緒にお昼ご飯を食べた……
		ELSE
			STR:3 = 「では……失礼、する……」#F重置/闭眼２/小口２
			STR:4 = すっと立ち去ろうとする%CALLNAME:TARGET%を引き留めて一緒に食べないか？と提案してみると。#F重置/無口系
			CALL DIALOGUE_FRAME_NAMEW, TARGET
			STR:1 = 「む……%CNAME("一人称")%も……か……？」#F重置/半目/小口２/对话框？
			STR:2 = %CALLNAME:MASTER%の提案に彼女は少し考える様な素振りをした後に#F重置/闭眼２/無口系/对话框・・・/托下巴
			STR:3 = 「すまない……実は……先に……済ませた……」#F重置/半目/困扰眉/小口２
			STR:4 = 先に済ましていたのは仕方がない、言っていなかった自分が悪いと%CALLNAME:MASTER%は言い。#F重置/闭眼２/困扰眉/無口系
			STR:5 = なら、お昼が終わるまで少し話さないかと改めて提案し直した。
			STR:6 = 「ふむ……%CNAME("二人称")%が……望むの、であれば……そうしよう……」#F重置/半目/小口２
			STR:7 = それから暫く%CALLNAME:TARGET%と会話しながら彼女の作ってくれた便当を楽しんだ……#F重置/闭眼２/無口系
		ENDIF
		CALL SET_DEVENT, "共に吃飯を"
	ELSE
		IF TALENT:恋慕
			IF RAND:5 == 0
				STR:3 = 「今日も……一緒に……食べて……良い……？」#F重置/半目/斜眉/微笑３
				STR:4 = 用意していた自分の弁当を飘忽と見せ、怯生生地聞いてくる%CALLNAME:TARGET%の姿に
				STR:5 = 「？」#F重置/笑颜４/斜眉/微笑３/对话框？
				STR:6 = 一瞬、幼子を見る目变成了がぐっと堪えて、一緒に食べようかと返した。
			ELSE
				STR:3 = 「今日も……一緒に……食べる……？」#F重置/半目/斜眉/微笑３
				STR:4 = 飘忽と自分用の便当を見せて、%CALLNAME:TARGET%が聞いてくるので一緒に食べようかと返して一緒に昼食を始めた。#F重置/笑颜４/斜眉/微笑３
			ENDIF
			CALL DIALOGUE_FRAME_NAMEW, TARGET
			IF RAND:3 == 0
				STR:1 = 「今日、のは……旬の……食材を、使ってみた……の、だけれど……どう……？」#F重置/半目/斜眉/小口２/对话框？
				STR:2 = ふむ、%CALLNAME:TARGET%の言葉に釣られて普段より味わって食べてみる。#F重置/半目/斜眉/無口系
				STR:3 = ……確かに今の季節以外で食べるよりも美味しさが違く感じる。#F重置/半目/斜眉/微笑３
				STR:4 = 「ふふっ……目利きも……頑張ったから……少し、自信作……」#F重置/笑颜４/斜眉/小口２
				STR:5 = 胸を張り、少し得意げな顔をする%CALLNAME:TARGET%に微笑ましいモノを感じ的同时、彼女との昼食を楽しんだ……#F重置/半目/斜眉/微笑３
			ELSEIF RAND:2 == 0
				STR:1 = 「ぁ……口の端に……%TEXTR(@"おかず/ご飯")%、着いてる……ん……取れた……」#F重置/半目/斜眉/微笑３
				STR:2 = そう言って%CALLNAME:TARGET%が口の端を拭ってくれた。
				STR:3 = 「美味しそう、に……食べて、くれるのは……嬉しい、けれど……注意、しないと……ダメ……」#F重置/笑颜４/困扰眉/微笑３/頬手
				STR:4 = 圆圈で幼子の様な扱いだが、これはこれで良いなと思う%CALLNAME:MASTER%だった。
			ELSE
				STR:1 = 「今日の……便当は……どう……？」#F重置/斜眉/小口２/对话框？
				STR:2 = 味の感想を聞いてきた%CALLNAME:TARGET%に箸の進みで察してほしいと返すと
				STR:3 = 「ん……%CALLNAME:MASTER%の……舌に、合って……良かった……」#F重置/笑颜４/斜眉/微笑３
				STR:4 = 「美味しく、なかったら……言って、ね……？練習、するから……」#F重置/半目/左笑颜４/斜眉/小口２/頬手
				STR:5 = 果たして、彼女のご飯が美味しくないと言う日は来るのだろうか？#F重置/笑颜４/斜眉/微笑３/頬手
				STR:6 = 何て思いながら%CALLNAME:TARGET%との昼食を楽しんだ……
			ENDIF
		ELSE
			STR:3 = 「ん……では……食べよう……か……」
			STR:4 = そう言って自分用に用意した、便当を広げ%CALLNAME:MASTER%の隣に座って一緒に昼食を始めた……
			CALL DIALOGUE_FRAME_NAMEW, TARGET
			IF RAND:3 == 0
				STR:1 = 「今日は……%TEXTR(@"肉/野菜/おかず")%を……很多に……した……」#F重置/闭眼２/小口２
				STR:2 = 言われて見てみると確かに前より比率が少し寄っているのが分かった。#F重置/闭眼２/無口系
				STR:3 = 「この間……%CNAME("二人称")%の……箸が……よく、進んでいた……から……」#F重置/半目/小口２
				STR:4 = 細かな心遣いだが実際にされると少し嬉しいモノだ……
			ELSEIF RAND:2 == 0
				STR:1 = 「%CNAME("二人称")%……口の端に……着いてる……ん……取れた……」#F重置/半目/無口系
				STR:2 = そう言って%CALLNAME:TARGET%が口の端を拭ってくれた。
				STR:3 = 「そうして……食べるのは……善いけれど……少しは……気を、付けよ……」#F重置/闭眼２/斜眉/小口２
				STR:4 = 少しだけ呆れた様な顔をしながらも口調は穏やかだった……
			ELSE
				STR:1 = 「今日、のは……%CNAME("二人称")%の口に……合って、いるか……？」#F重置/半目/斜眉/小口２/对话框？
				STR:2 = %CALLNAME:TARGET%の便当を食べていると、味はどうだと聞かれたので美味しいと返すと。
				STR:3 = 「そう、か……%CNAME("二人称")%の口に……合って、いるなら……良い……」#F重置/闭眼２/斜眉/小口２
				STR:4 = 少しだけ開心地、僅かに口角を上げて%CALLNAME:TARGET%が微笑む……#F重置/闭眼２/斜眉/微笑３
			ENDIF
		ENDIF
	ENDIF
	CALL DIALOGUE_FRAME_NAMEW, TARGET
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"

ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;动画ーションの関係上、１ターンに何度も呼ばれたりするのでフラグ操作をする場合には
;IF CFLAG:KOJO_DAILY済 == 0
;で分岐させるのが良いでしょう
;-------------------------------------------------
@KOJO_DAILY_K66, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「ん……%CNAME("二人称")%……これから……よろしく……」
CASE "浑身是汗"
	CALL EMOTEFACE, "重置/半目/左闭眼２/蹙眉/無口系/对话框乱/頬汗"
	IF RAND:2 == 0
		PRINTFORML 「やはり……この国の……夏は……蒸し、暑い……」
	ELSE
