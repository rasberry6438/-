;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K36, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;見ないと思ったら约会的约定がない状態で周日限定なのか。そりゃ出ないわ。
CASE "朝拘束"
	IF ABL:MASTER:受虐属性 >= 3 && ABL:施虐属性
		CALL EMOTION_PLUS, "躁动难忍", TARGET
		PRINTFORMW 「好机会，今天我就把你彻底榨干」
		PRINTFORMW 「直到你一干二净之前是不会放过你的、做好觉悟吧……♪」
	ELSE
		CALL EMOTION_PLUS, "心情好", TARGET
		PRINTFORMW 「早上好！情况理解了吗？」
		PRINTFORMW 「既然眼睛都睁開了、那么接下来就要来正式的了？」
	ENDIF
;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください


CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"
	CASE "朝乳交"

	CASE "朝騎乗位"
		IF EXP_GAIN("射精経験", PLAYER) && CEVENT("初回朝袭")
			CALL EMOTION_PLUS, "躁动难忍", TARGET
			PRINTFORMW 「%TEXTR("啊哈……♪　起来了……♪/嗯……♪　快要出来了……？/呵呵、可以射出来了……♪")%」
		ENDIF
	CASE "朝Ａ騎乗位"

	CASE "朝接吻"
		PRINTFORMW 「%TEXTR("＿/ちゅ（亲吻声）")%……♪」
	CASE "朝添い寝"
	IF RAND:2 && CEVENT("初回朝袭")
		CALL SETFLAG, "地の文スキップ"
		IF RAND:3 && CFLAG:MASTER:约会的约定 == 0 && TEXTS("曜日") == "日" && COND("同居中")
			PRINTFORMW 早上，突然睁開眼的时候，%CALLNAME:TARGET%正在自己的被窝里呼呼大睡。
			PRINTFORMW 好像是来叫醒%CALLNAME:MASTER%的，但可能是休息日的缘故，精神松懈了，结果跟着一起睡着了。
			CALL EMOTION_PLUS, "睡眠", TARGET
			PRINTFORMW 「すぅすぅ……」（鼾声）
			PRINTFORMW 茫然地望着平和的%CALLNAME:TARGET%的睡脸，自己也想睡回笼觉了……。
		ELSEIF RAND:2 && COND("同居中")
			PRINTFORMW 早上，被%TEXTR("香喷喷的/刺激食欲的/看起来很好吃的")%的气味所唤醒、%CALLNAME:TARGET%坐在敞開的窗框上吃着面包和%TEXTR("荷包蛋/煎蛋/培根/香肠")%
			CALL EMOTION_PLUS, "心情好", TARGET
			PRINTFORMW 「もぐもぐ（咀嚼声）……。%CNAME("二人称")%的份也带过来了。一起来吃吧！」
		ELSEIF RAND:2
			CALL EMOTION_PLUS, "心情好", TARGET
			PRINTFORMW 「%TEXTR("起床！ /天亮了！")%」
			PRINTFORMW %CALLNAME:TARGET%痛快地打開了房间的窗帘、%TEXTR("沉睡/贪睡")%的%CALLNAME:MASTER%的脸上被%TEXTR("炫目的/清爽的/清爽的")%的朝阳直接照射。
			IF RAND:3 == 0 && COND("同居中")
				PRINTFORMW 「嘿、早上好！」
				PRINTFORMW 「早饭做好了哦。一起来吃吧！」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「早上好、%CALLNAME:MASTER%！　今天也请多関照！」
			ELSEIF RAND:2
				PRINTFORMW 「嘿！　%CALLNAME:TARGET%小姐来叫你起床了！」
			ELSE
				PRINTFORMW 「呵呵，今天也是好天气哦！」
			ENDIF
		ELSE
			PRINTFORMW 早上、%CALLNAME:MASTER%正%TEXTR("睡着呢/沉睡着")%、忽然有人在%TEXTR("腋下/足底/脖子")%上轻轻搔痒，意识突然清醒了。
			PRINTFORMW %TEXTR("揉着睡眼坐起来/吓得跳起来")%、%CALLNAME:TARGET%挂着%TEXTR("恶作剧/笑眯眯/愉快")%的笑容打了招呼。
			CALL EMOTION_PLUS, "心情好", TARGET
			IF RAND:3 == 0 && COND("同居中")
				PRINTFORMW 「啊哈哈！你醒了吗？」
				PRINTFORMW 「早饭做好了哦。一起来吃吧！」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「早上好、%CALLNAME:MASTER%！　今天也请多関照！」
			ELSEIF RAND:2
				PRINTFORMW 「嘿！　%CALLNAME:TARGET%小姐来叫你起床了！」
			ELSE
				PRINTFORMW 「啊哈哈，是怎么叫都叫不醒的那一边的不对哦？」
			ENDIF
		ENDIF
	ENDIF
	CASE "朝口交"
		SIF EXP_GAIN("射精経験", PLAYER) && CEVENT("初回朝袭")
			PRINTFORMW 「呵呵，起来了吗？就这样给你吸出一发吧……♪」

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
;あんまり唐突過ぎるので、一個会話を入れる。
;OPによると趁睡袭击番とかなんだかよく分からんもんがあるらしいが、まぁよく分からんし……
	IF CEVENT("初回朝袭") == 0
		CALL EMOTION_PLUS, "心情好", TARGET
		PRINTFORMW 「嘿嘿，早上好！醒了吗？」
		PRINTFORMW 「我说%CNAME("二人称")%啊。好像每天都是弗理希艾尔小姐来叫你起床的吧？」
		PRINTFORMW 「%CNAME("一人称")%也决定心情好的时候来做、稍微期待一下吧~♪」
		CALL SET_CEVENT, "初回朝袭", TARGET
		RETURN 1
	ENDIF
;约会前。同居人数が少ない場合は選ばれる率が上がるのでちょっと頻度落とす。
IF CFLAG:MASTER:约会的约定 && CFLAG:约会的约定 && TEXTS("曜日") == "日" && (RAND:4 || CHARA_NUM("同居中")  >= 3)
		CALL EMOTION_PLUS, "心情好", TARGET
		PRINTFORMW 「早上好！　%TEXTR("睡醒了吗？ /已经是早上了！/早饭做好了！")%」
		PRINTFORMW 「今天要一起%TEXTR("去玩/去约会")%不是吗？不要呼呼大睡了，赶快去%TEXTR("洗脸吧！/刷牙吧！/洗澡吧！")%」
;さらに4分の1の確率で下の汎用打招呼に流す。
ELSEIF RAND:4
	SELECTCASE CSTR:朝袭
	CASE "朝乳交"
		IF EXP_GAIN("射精経験", PLAYER) && RAND:3
			CALL PRINT_STR, "「もう、出し過ぎだって。ベタベタになっちゃったじゃん……♪」_W"
		ELSE
			PRINTFORMW 「怎么样？　看到%CNAME("一人称")%的胸部打起精神来了吗？」
		ENDIF
	CASE "朝口交", "朝六九式"
		IF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, @"「早上好ー。%TEXTR("朝から濃いなぁ/朝からすごい量/今日も元気そうだな")%……_HH_」_W"
		ELSE
			PRINTFORMW 「早上好！」
			PRINTFORMW 「……那么，这个怎么办？还有时间，就这样拔了吗？」
		ENDIF
	CASE "朝騎乗位","朝Ａ騎乗位"
		IF EXP_GAIN("射精経験", PLAYER) && RAND:3
			CALL PRINT_STR, "「呼……_HH_　早上好♪」_W"
		ELSE
			PRINTFORMW 「呼，出了一身好汗！一起冲个澡吧！」
		ENDIF
	CASE "朝接吻"
		CALL EMOTION_PLUS, "心情好", TARGET
		IF RAND:3 == 0 && TALENT:相思相愛 == 0 && COND("同居中")
			PRINTFORMW 「啊哈哈。一般来说，唤醒的吻都是你这一边来做吧」
			PRINTFORMW 「下次、%CNAME("二人称")%这一边来这样叫醒我吧？　怎么样？」
		ELSEIF RAND:2
			PRINTFORMW 「啊哈哈，什么嘛，不用害羞也可以啊？」
		ELSE
			PRINTFORMW 「快，起来起来，再不起来，我就吻你了？」
		ENDIF
	CASE "朝足交"
			IF RAND:3 == 0 && EXP:足交経験 >= 30 && EXP_GAIN("射精経験", PLAYER) 
				PRINTFORMW 「呵呵。%CALLNAME:TARGET%小姐的脚法怎么样？　变得熟练起来了吧？」
			ELSEIF RAND:2 && TEXTS("曜日") != "日"
				PRINTFORMW 「啊哈哈，对快要迟到的睡懒觉的人这样哦♪」
			ELSE
				PRINTFORMW 「醒了吗？不早点起来的话，要继续ふみふみ（fumifumi形容踩踏的拟声词）喽？」
			ENDIF
	CASE "朝の新妻PLAY"
			CALL EMOTION_PLUS, "心情好", TARGET
			IF RAND:2 && TEXTS("曜日") != "日"
				PRINTFORMW 「呵呵，怎么样？今天也准备好%TEXTR("加油/努力")%了吗？」
			ELSE
				PRINTFORMW 「哈啊、很舒服……♪」
				PRINTFORMW 「那么，接下来吃早饭吧！不，在那之前淋浴比较好吧……」
			ENDIF
	ENDSELECT
ENDIF

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1
	CALL EMOTION_PLUS, "心情好", TARGET
	IF RAND:3 == 0 && COND("同居中")
			PRINTFORMW 「嘿、早上好！」
		IF COND("水泳")
			IF RAND:3 == 0 && COND("夏")
				PRINTFORMW 「今天看上去也挺热的！　不错的游泳的日子呢！」
			ELSE
				PRINTFORMW 「泳衣和毛巾就放在这里，别忘了哦？」
			ENDIF
		ELSEIF COND("体育")
			IF RAND:5 == 0
				PRINTFORMW 「天气总觉得怪怪的。要是上体育课之前不下雨就好了……」
			ELSEIF RAND:3
				PRINTFORMW 「今天的体育，女子是%TEXTR("足球/排球/篮球/羽毛球/垒球")%！　很期待！」
			ELSE
				PRINTFORMW 「我把体操服放在这里，你可别忘了？」
			ENDIF
		ENDIF
	ELSE
		IF RAND:3 == 0 && COND("同居中")
			PRINTFORMW 「早上好！」
			PRINTFORMW 「早饭做好了哦。一起来吃吧！」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「早上好、%CALLNAME:MASTER%！　今天也请多関照！」
		ELSEIF RAND:2
			PRINTFORMW 「%TEXTR("哦/唔姆")%っ。早上好！」
		ELSE
			PRINTFORMW 「醒了吗？吃饭前我要去冲个澡！」
		ENDIF
	ENDIF

CASE "一番乗り"
	CALL EMOTION_PLUS, "心情好", TARGET
	IF COND("同伴中")
			IF RAND:3 == 0 && COND("Ｈ的サプライズ可能") && COND("欲求不満") >= 2
				CALL EMOTION_PLUS, "躁动难忍", TARGET
				PRINTFORMW 「呐、%CALLNAME:MASTER%。来得正好、今天来和%CNAME("一人称")%做吧？」
				PRINTFORMW 「现在、很想要和%CNAME("二人称")%一起做。……可以吧？」
			ELSE
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				PRINTFORMW 「嗯、稍微有点早吧？」
				PRINTFORMW 「算了。%CNAME("二人称")%就在那里坐着。我来泡茶吧」
			ENDIF
	ELSE
		IF COND("同伴中", MASTER) && RAND:2
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				CALL EMOTION_PLUS, "心情好", TARGET
				PRINTFORMW 「欢迎光临！喝茶吗？」
		ELSE
				;一番乗りの時はすでに淋浴を浴びてる。
				CFLAG:淋浴済 = 1
				CFLAG:汗 = 0
				CALL RESET_STAIN_EX, "老廃物以外全部", TARGET
				;この領帯ミングで欲求不満気味or淫乱な時は紧身短褲をキャストオフする
				IF (COND("欲求不満") && CEVENT("調教開始：Ｈに慣れた")) || CEVENT("調教開始：淫乱")
					SIF CHECK_BOTTOM("紧身短褲")
						CALL SET_BOTTOM, "", TARGET
				ENDIF
			IF COND("浑身是汗", MASTER)
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				CALL EMOTION_PLUS, "心情好", TARGET
				PRINTFORMW 「哇啊、%CNAME("二人称")%。汗流浃背哦？」
				PRINTFORMW 「来这里，我帮你擦。。come on♪」
				PRINTFORMW %CALLNAME:TARGET%把毛巾拉出来，向汗流浃背的%CALLNAME:MASTER%招了招手。
				CFLAG:MASTER:汗 = 0
			ELSEIF COND("夏") && RAND:2
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				SAVESTR:茶 = 麦茶
				PRINTFORMW 「%CNAME("二人称")%也喝麦茶吗？」
				PRINTFORMW 「好了好了。那我去拿杯子」
			ELSEIF COND("冬") && RAND:2
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				PRINTFORMW 「喝点热的吗？想喝点什么？」
			ELSEIF RAND:2
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				PRINTFORMW 「哦，今天来的够早的！我给你泡杯茶犒劳一下吧」
			ELSE
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				PRINTFORMW 「哦，今天%CNAME("二人称")%穿了两件啊！　我去泡茶稍等一下」
			ENDIF
		ENDIF
	ENDIF
CASE "ただいま"
	IF COND("冬")
		IF RAND:3 == 0
			PRINTFORMW 「我回来了」
			PRINTFORMW 「……话说、得先去洗手」
		ELSE
			PRINTFORMW 「我回来了。啊、好冷好冷……」
		ENDIF
	ELSEIF TEXTS("季節詳細") == "晩秋" && RAND:2
		PRINTFORMW 「我回来了，天黑得越来越早了」
	ELSEIF COND("夏") && RAND:2
		CALL EMOTION_PLUS, "心情好", TARGET
		PRINTFORMW 「啊，空调開得好凉快啊……♪」
	ELSEIF RAND:2
		CALL EMOTION_PLUS, "心情好", TARGET
		PRINTFORMW 「嗨！　我回来了！」
	ELSE
		PRINTFORMW 「我回来了」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TEXTS("曜日") == "土"
			CALL EMOTION_PLUS, "心情好", TARGET
			PRINTFORMW 「欢迎回来、%CALLNAME:MASTER%！　这周也%TEXTR("辛苦了！/辛苦了！")%」
		ELSE
			PRINTFORM 「
			SIF COND("同伴中", MASTER)
				PRINTFORM 你们两个、
			PRINTFORMW %TEXTR("欢迎回来/欢迎回来！/欢迎回来♪/辛苦了！")%」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR("好！/辛苦了！/嗨！")%」
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "心情好", TARGET
		;点心当番が料理擅長だとこう。フェリ桑作なら確率上げてもいい
		IF COND("点心の作り手が料理擅長") && COND("亲手自制点心") && RAND:2
			PRINTFORMW 「我听说今天有亲手做的点心！　%CNAME("一人称")%、%TEXTR("飒爽地/华丽地")%%TEXTR("参上/登場")%！」
		ELSE
			PRINTFORMW 「%TEXTR(@"久等了！/%CNAME("一人称")%、参上！/哦、久等了！")%」
		ENDIF
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF RAND:2
			CALL EMOTION_PLUS, "躁动难忍", TARGET
			PRINTFORMW 「哦，今天也很丰盛啊…………」
		ELSE
			CALL EMOTION_PLUS, "心情好", TARGET
			PRINTFORMW 「啊哈哈，你在干什么啊」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR(@"好！/%CNAME("一人称")%、参上！/哈罗！")%」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR(@"啊嘞，大家都在啊。今天有什么事吗？/抱歉。有点来晚啦！/好！")%」
	ELSE
		PRINTFORMW 「%TEXTR(@"好！/%CNAME("一人称")%、参上！/哈罗！")%」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		IF RAND:2
			PRINTFORMW 『您好』
		ELSE
			PRINTFORMW 『辛苦了』
		ENDIF
	;絡みやすそうな人たち(独断)
	ELSEIF (CALLNAME_ACTOR == "哈娜" || CALLNAME_ACTOR == "杏珠" || CALLNAME_ACTOR == "織") && COND("相性○", ACTOR)
		CALL EMOTION_PLUS, "心情好", TARGET
		PRINTFORMW 『啊、不是%CNAME(CALLNAME_ACTOR)%嘛！　%TEXTR(@"好！/哈喽！/这边这边！/等着你呢！")%』
	;同級生以下(三十路は弾く)はよっす！
	ELSEIF TALENT:ACTOR:外見年齢 <= 3 && COND("学生", ACTOR) && COND("学年") >= COND("学年", ACTOR)
			PRINTFORMW 『%TEXTR(@"好！ /辛苦了/辛苦了")%』
	;前輩とか目上相手は敬語。
	ELSE
		IF RAND:2
			PRINTFORMW 『您好』
		ELSE
			PRINTFORMW 『辛苦了』
		ENDIF
	ENDIF


CASE "居間に現れた"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "心情好", TARGET
		PRINTFORMW 「%TEXTR(@"叫我了吗？/%CNAME("一人称")%、参上！/有什么事？")%」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		IF STATE("困倦",MASTER) && RAND:3
			PRINTFORMW 「总觉得很困吧？差不多该回房间了吧？」
		ELSEIF STATE("酔酒",MASTER)
			CALL EMOTION_PLUS, "心情好", TARGET
			PRINTFORMW 「啊哈哈！你还在喝酒啊」
		ELSE
			PRINTFORMW 「哼哼……」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR(@"好/嘿！")%」
	ENDIF
CASE "全裸化への反応"
	IF ABL:欲望 >= 3 && ABL:露出癖 >= 3
		IF TALENT:伪娘
			CALL EMOTION_PLUS, "躁动难忍", TARGET
			PRINTFORMW （啊，脱了衣服还真有一种小伙子的感觉……）
		ELSEIF TALENT:淫乱 && (CADV("调教师", TARGET) || TALENT:好色+TALENT:小悪魔) && CFLAG:躁动难忍
			CALL EMOTION_PLUS, "躁动难忍", TARGET
			PRINTFORMW （哇啊，好色情啊。连我都觉得有种奇怪的感觉了……）
		ELSEIF TALENT:巨乳 >= 2
			CALL EMOTION_PLUS, "躁动难忍", TARGET
			PRINTFORMW （好大啊！　这就是我的短处啊……）
		ENDIF
	ENDIF
