;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K30, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝接吻"
		CALL EMOTION_PLUS, "重置/左闭眼/半目/小口/紅潮２"
		PRINTFORML 「ん、ちゅっ…%TEXTR("早上好う/む、起きたか")%、%CNAME("二人称")%」
		IF TEXTS("曜日") == "日"
			PRINTFORMW 「今日は休日だが%TEXTR("何かする予定はあるか？/どうやって過ごす？")%%TEXTR(@"%CNAME("一人称")%は少し出かけるが…何ならついてきても良いのだぞ？/何も無いならむやみに出かけず家で過ごすのも良いだろうな/%CNAME("一人称")%はゆっくり過ごさせてもらうぞ")%」
		ELSE
			PRINTFORMW 「%TEXTR(@"今日も一日張り切っていこうではないか/目覚めの接吻はどうだ、%CNAME("二人称")%よ/接吻だけで半勃ちか…朝から元気そうで何よりだな")%」
		ENDIF
	CASE "朝口交", "朝六九式"
		CALL EMOTION_PLUS, "重置/半目/小口/紅潮２"
		PRINTFORMW 「んっ…んぐ…ん…ちゅぱっ、ちゅぽっ………む、ようやく起きたか、%CNAME("二人称")%」
		PRINTFORMW 「…残念だが朝はこれだけだぞ？後でたっぷり相手してやる」
	CASE "朝騎乗位"
		CALL EMOTION_PLUS, "重置/ワ/紅潮２/对话框爱心"
		CALL PRINT_STRW, "「んぁっ、良い、良いぞっ…くっ、ああぁッ_H_」"
		CALL PRINT_STRW, @"「はっ、はぁ…早上好う、%CNAME("二人称")%よ。ククッ、朝一の騎乗位は効くな…ぁンッ_H_」"
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育があるようだな。ほれ、体育着だ」
	ELSE
		IF RAND:2
			PRINTFORMW 「起きろ、この寝坊助め」
			PRINTFORMW 「まだ朝食を取る時間だけはあるぞ。早いところ食べて準備しろ」
		ELSE
			PRINTFORMW 「起きたか、%CNAME("二人称")%よ」
			IF FINDCHARA(NO, 1) > 0
				PRINTFORMW 「もう%CALLNAME:FINDCHARA(NO, 1)%が朝食来招待，待っているぞ」
			ENDIF
		ENDIF
	ENDIF
CASE "一番乗り"
	PRINTFORMW 「邪魔するぞ…って何だ、誰也不在のか」
CASE "ただいま"
	PRINTFORMW 「帰ったぞ、%CNAME("二人称")%」
	PRINTFORMW 「%TEXTR(@"隅で見ていたが、今日も一日頑張っていたな。どれ、頭を撫でてやろう/今日の夕食は何だ？腹が減ってしょうがない/今、浴室は誰も入ってないか…%CNAME("一人称")%が先に入っても良いか？")%」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「帰ったか、%CNAME("二人称")%よ」
		PRINTFORMW 「%TEXTR("隅で見ていたが、今日も一日頑張っていたな。どれ、頭を撫でてやろう/浴室が沸いているぞ。先に入って来てしまえ/疲惫時には甜物に限る。ほれ、アメをやろう")%」
	ELSE
		PRINTFORMW 「来たか、%CNAME("二人称")%よ」
		PRINTFORMW 「%TEXTR(@"授業中に居睡眠するのは感心せんな。%CNAME("一人称")%が隠れて起こしてなければどうなっていたか…/今日はしっかり勉学に励んでいたな。午後もその調子で頑張るのだぞ/悪いが先に寛がせてもらっていた。…茶でも飲むか？")%」
	ENDIF

CASE "入室打招呼"
	SELECTCASE CFLAG:MASTER:PREV_DAILYCATE
	;Talk
	CASE 1
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "甜言蜜语"
			PRINTFORMW 「…%TEXTR("ふむ/ほう")%、奉承の最中だったか」
		CASE "打招呼舐陰", "打招呼口交"
			PRINTFORMW 「…%TEXTR("ふむ/ほう")%、日も高いうちからお盛んだな、えぇ？」
		ENDSELECT
		PRINTFORMW %CALLNAME:TARGET%正在认真地读着标题为%CALLNAME:MASTER%達をニヤニヤしながら興味深げに見ている。
	;互动
	CASE 2
	;卖个破绽
	CASE 3
	;胖次
	CASE 4
		IF RAND:2
			CALL EMOTION_PLUS, "重置/斜眼２/斜眉"
			PRINTFORMW 「…ああいや、%CNAME("一人称")%の事は気にせず続けるが良い」
			PRINTFORMW 「それとも少し席を外した方が良いか？」
		ELSE
			CALL EMOTION_PLUS, "重置/斜眼２/斜眉/無口系/对话框・・・"
			PRINTFORMW 「あーその、%CNAME("二人称")%よ…」
			PRINTFORMW 「…まぁ良いか。ここでやってるうちは誰も咎めんしな」
		ENDIF
	CASE 6
		IF RAND:2
			CALL EMOTION_PLUS, "重置/斜眼２/斜眉/無口系/对话框・・・"
			PRINTFORMW 「…ああいや、%CNAME("一人称")%の事は気にせず続けるが良い」
			IF TALENT:恋慕
				PRINTFORMW 「無論、後で%CNAME("一人称")%も相手してくれるのだろうな？」
			ELSE
				PRINTFORMW 「それとも少し席を外した方が良いか？」
			ENDIF
		ELSE
			CALL EMOTION_PLUS, "重置/斜眼２/△"
			PRINTFORMW 「あーその、%CNAME("二人称")%よ…」
			IF TALENT:恋慕
				PRINTFORMW 「%CNAME("一人称")%もその…そういう事してやっても良いのだぞ？」
			ELSE
				PRINTFORMW 「…まぁ良いか。ここでやってるうちは誰も咎めんしな」
			ENDIF
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF CFLAG:呼び出された
		;IF COND("夜")
			
		;ELSE
			PRINTFORML 「%TEXTR("どうした/何だ/呼んだか/何用だ/只今馳せ参じたぞ")%、%CNAME("二人称")%」
			IF RAND:2
				PRINTFORML 「…%TEXTR("ククッ/全く/はぁ")%、そんなに驚かなくても良いだろうに…%CNAME("二人称")%は可愛いな」
				PRINTFORMW %TEXTR(@"死角から/%CALLNAME:MASTER%の後ろに/扉も窓も通らずに")%音も無く現れた%CALLNAME:TARGET%正在认真地读着标题为クスクスと愉快そうに笑着いる…。
			ELSE
				PRINTFORML 「%CNAME("一人称")%%TEXTR("の事が恋しくなったか？ククッ♪/に何か頼みでもあるのか？/の力が必要と見たぞ/はすぐに駆けつけるぞ、大事な契約者の危機かもしれないからな")%」
				PRINTFORMW 呼び出された%CALLNAME:TARGET%正在认真地读着标题为得意げに微笑んでいる。
			ENDIF
		;ENDIF
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR("…出遅れたか/むぅ、既に皆揃っておるようだな")%」
	ELSE
		PRINTFORMW 「邪魔するぞ」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『…授業もこっちもで、教師は大変だな？』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『遅かったな。皆大体出揃っているぞ』
	ELSE
		PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%か。先に寛がせてもらってるぞ』
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "便当"
	IF CEVENT("初めての便当") == 0
		CALL EMOTION_PLUS, "重置/飘忽/困扰眉/無口系"
		PRINTFORMW 「さて、約束街道弁当来招待，きたぞ…。%CNAME("二人称")%の口に合えば良いのだが」
		PRINTFORMW %CALLNAME:TARGET%が自信無さげに手渡してきた弁当は…フルーツ三明治だった。
		PRINTFORMW 一口食べてみると、阴蒂ームのほど良い甘さと果物の酸味が絶妙で美味しかった…。
		CALL EMOTION_PLUS, "重置/笑颜４/ヮ"
		PRINTFORMW 「…その反応から察するに、食べられる代物だったと言うことだな」
		PRINTFORMW 「誰かに自分の作ったものを美味しく食べてもらえる…中々嬉しいものだな」
		PRINTFORMW どこか照れくさそうにはにかむ%CALLNAME:TARGET%をおかずに、弁当を食べきった…。
		CALL SET_CEVENT, "初めての便当", TARGET
	ELSE
		CALL EMOTION_PLUS, "重置/笑颜４"
		IF RAND:3 == 0
			PRINTFORMW 「今日の弁当は%TEXTR("アップル餡餅/イチジクのタルト/バタースコッチシナモン餡餅")%だ」
			PRINTFORMW 「…甜点じゃないかと思ったろう？すまない、急に食べたくなってしまったのだ…」
		ELSEIF RAND:2
			PRINTFORMW 「今日の弁当は%TEXTR("カレー風味の/乳酪入りチキン/海鮮ピラフ風")%ライスコロッケだ」
			PRINTFORMW 「父が人类界で一番美味い食べ物だともてはやして、一時期こればかり料理人に作らせていてな…」
		ELSE
			PRINTFORMW 「今日の弁当は%TEXTR("カツ三明治/フルーツ三明治/タマゴ三明治/ＢＬＴ三明治")%だぞ」
			PRINTFORMW そう言って%CALLNAME:TARGET%正在认真地读着标题为弁当箱を３つほど差し出してきた…。
			PRINTFORMW 「…張り切って作り過ぎてしまってな。%CNAME("一人称")%は間食もコレになりそうだ」
		ENDIF
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K30, ARGS
#DIM MEMO_LINECOUNT
#DIM ACTOR
MEMO_LINECOUNT = LINECOUNT


SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「悪魔の%CALLNAME:TARGET%だ。请多関照」
	PRINTFORML %CALLNAME:TARGET%正在认真地读着标题为部員達に握手して回っている。
CASE "浑身是汗"
	CALL EMOTEFACE, "重置/半目/蹙眉/へ字２/对话框汗２/頬汗"
	IF RAND:2
		PRINTFORML 「呼、好热…。衣服都被汗浸透了」
		PRINTFORML %CALLNAME:TARGET%一边不停的擦汗、一边用团扇向胸口扇风。
	ELSE
		PRINTFORML 「%CNAME("一人称")%还不是容易出汗的人…」
		PRINTFORML 筋疲力尽的%CALLNAME:TARGET%无力地擦着汗…。
	ENDIF
CASE "刚睡醒"
	IF CHOSE_DEVENT("睡眠姦接吻")
		CALL EMOTEFACE, "重置/左闭眼２/無口系/紅潮２/对话框爱心/托下巴"
		PRINTFORML 「%TEXTR(@"呜呣…王子的吻…吗/嗯…觉醒地比我想象地要快/%CNAME("二人称")%啊。%CNAME("一人称")%地嘴上怎么有我的味道？库库ッ♪")%」
	ELSEIF CHOSE_DEVENT("睡眠姦口交")
		CALL EMOTEFACE, "重置/斜眼２/困扰眉/ڡ/紅潮２"
		IF RAND:2
			PRINTFORML 「嗯、ぅ…ッ、你的精液真浓…咳咳…」
			IF ABL:精液中毒
				PRINTFORML %CALLNAME:TARGET%在那里抱怨着、脸却放松下来、舌頭好像在品味精液般慢慢搅动…。
			ELSE
				PRINTFORML %CALLNAME:TARGET%熏红了脸轻轻地咳嗽…。
			ENDIF
		ELSE
			PRINTFORML 「…%CNAME("一人称")%睡觉时的嘴巴舒服吗？」
			IF ABL:精液中毒
				PRINTFORML %CALLNAME:TARGET%舔着嘴唇，用火热的目光注视着%CALLNAME:MASTER%…。
			ELSE
				PRINTFORML %CALLNAME:TARGET%用呆滞的目光看着%CALLNAME:MASTER%…。
			ENDIF
		ENDIF
	ELSEIF CHOSE_DEVENT("睡眠姦Ｖ中出し")
		CALL EMOTEFACE, "重置/左闭眼２/微笑６/紅潮２/对话框爱心"
		IF RAND:3 == 0 && ABL:信頼 >= 5 && CEVENT("両親の夢")
			PRINTFORML 「…我梦到自己在%TEXTR(@"父亲死的那天晚上/母亲的葬礼之后")%，被%CNAME("二人称")%抱在怀中」
			PRINTFORML 「梦中的%CNAME("二人称")%呢…一边温柔地安慰哭泣的%CNAME("一人称")%、一边尽力我身体里注入精液…？」
			PRINTFORML 用手指擦掉眼角的泪痕、%CALLNAME:TARGET%一脸得意地靠近%CALLNAME:MASTER%…。
		ELSEIF RAND:2
			PRINTFORML 「嗯…谁的精子啊，感觉子宫好重…。%CNAME("二人称")%、你知道是谁吗？」
			PRINTFORML %CALLNAME:TARGET%故意揉了揉下腹问道…。
		ELSE
			PRINTFORML 「呣、这个是…内裤上地是精液吗」
			PRINTFORML 「库库、%CNAME("一人称")%睡觉时地小穴很舒服吧…%CNAME("二人称")%觉得呢」
			PRINTFORML %CALLNAME:TARGET%优雅地用手指搅动上面精液、向%CALLNAME:MASTER%露出威胁的笑容…。
		ENDIF
	ELSE
		;奥丝卡が夢でうなされているのを一度でも見ると追加
		IF RAND:3 == 0 && CEVENT("両親の夢") + CEVENT("悪夢")
			CALL EMOTEFACE, "重置/左涙目＞＜/蹙眉/哈欠大"
			PRINTFORML 「%TEXTR(@"夢啊…夢、原来是夢…/…啧、又是那个梦吗…/…怎么了%CNAME("二人称")%。%CNAME("一人称")%的脸上有什么东西吗？")%」
			PRINTFORML %CALLNAME:TARGET%的脸颊与眼角，出现了淡淡的泪痕…。
		ELSEIF RAND:2
			CALL EMOTEFACE, "重置/左闭眼２/哈欠小"
			PRINTFORML 「嗯嗯嗯恩…！呃啊啊啊…」
			PRINTFORML 醒来的%CALLNAME:TARGET%挺起了脊背、舒展的身体强调着丰满的胸部。
		ELSE
			CALL EMOTEFACE, "重置/左闭眼２/半目/～/对话框困倦/唇边手"
			PRINTFORML 「呼…呼哇～…咕噜咕噜…」
			PRINTFORML %CALLNAME:TARGET%优雅地用手遮住嘴巴打了个哈欠。
		ENDIF
	ENDIF
CASE "Cosplay中"
	PRINTFORML 
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K30
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML （父亲…母亲…我……会幸福的…!）
	PRINTFORML 眼睛里含着泪的%CALLNAME:TARGET%握着%CALLNAME:MASTER%的手祈祷道…。
CASE "约会的约定"
	PRINTFORML （好了，现在開始得准备衣服了…）
;これは悪魔のみ
CASE "契約成立"
	IF RAND:3 == 0
		CALL EMOTEFACE, "重置/小口/托下巴"
		PRINTFORML 「不过、一天就能再生一人还多的魂…」
		PRINTFORML 「…你真幸运没被那些人渣注意到」
	ELSEIF RAND:2
		CALL EMOTEFACE, "重置/左笑颜４/对话框開心"
		PRINTFORML 「库库、这场交易真不错♪　下次继续」
		PRINTFORML %CALLNAME:TARGET%看起来非常高兴，对%CALLNAME:MASTER%露出了笑容。
	ELSE
		CALL EMOTEFACE, "重置/左闭眼２/半目/ڡ２"
		PRINTFORML 「嗯哼、真是优质的魂。那么马上…啊、嗯…」
		IF RAND:2
			PRINTFORML %CALLNAME:MASTER%只见白色的魂白色摇曳的灵魂缠绕在%CALLNAME:TARGET%艳丽的舌頭上被慢慢吞下…。
			PRINTFORML 「…着是什么表情。只是美美地享用了%CNAME("二人称")%交易的魂而已…库库库♪」
		ELSE
			PRINTFORML 「…呼呼、感觉得到吗…%CNAME("二人称")%的魂喝%CNAME("一人称")%融为一体了…」
			PRINTFORML %CALLNAME:TARGET%把双手放在乳沟上，心满意足地呼出了口气…。
		ENDIF
	ENDIF
CASE "摆脱"
	PRINTFORML 
CASE "主人公的房間に連れ込まれた"
	IF RAND:2
		PRINTFORML 「どうした、わざわざ%CNAME("一人称")%を連れ込んだりして」
		PRINTFORML 「…ナニをするつもりか気になるなぁ？ククッ♪」
		SIF BASE:欲求不満 >= 75
			PRINTFORML 冗談のように喉を鳴らして笑う%CALLNAME:TARGET%だが、%CALLNAME:MASTER%を捉える眼光は期待に満ち満ちている…。
	ELSE
		PRINTFORML 「スンスン…部屋の前を通るたび感じていたが、やはり%CNAME("二人称")%の气味が濃いな」
		PRINTFORML 「青臭い、精力に満ちた若い雄の气味だ…。ククッ、少なくとも%CNAME("一人称")%は好きな气味だぞ？」
		SIF BASE:欲求不満 >= 75
			PRINTFORML %CALLNAME:TARGET%正在认真地读着标题为%CALLNAME:MASTER%にしなだれかかり、マーキングするように腰を纠缠着擦り付けてくる…。
	ENDIF
CASE "自室に招待"
	IF RAND:2
		PRINTFORML 「%CNAME("二人称")%よ。闲ならこのまま%CNAME("一人称")%的房間で過ご桑か？」
		PRINTFORML 「%TEXTR("恥ずかしい話だが、寂しくて/何というか、无聊で")%な…。一人で過ごすのは慣れているはずだったのだが…」
	ELSE
		PRINTFORML 「わざわざ来てもらってすまないな、%CNAME("二人称")%」
		PRINTFORML 「いやなに、単純に%CNAME("二人称")%と二人っきりになりたかっただけだ」
		SIF BASE:欲求不満 >= 75
			PRINTFORML そう言う%CALLNAME:TARGET%正在认真地读着标题为%CALLNAME:MASTER%に身を寄せ、艶めかしく%CALLNAME:MASTER%の胸や下腹部を撫で続けている…。
	ENDIF
CASE "自室に訪問された"
