;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K38, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	IF CHARA_NUM("室内の邪魔者") == 0 && CEVENT("名没有の呼び出し")
		LOCALS = 我_我
	ELSE
		IF RAND:2
			LOCALS = 老师_老师
		ELSE
			LOCALS = 我_我
		ENDIF
	ENDIF
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF CHARA_NUM("室内の邪魔者") == 0 && CEVENT("名没有の呼び出し")
			LOCALS = 你_你
		ELSE
			LOCALS = 部長桑_部長桑
		ENDIF
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 部長桑_部長桑
CASE "助手"
	LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
ENDSELECT
RETURNF LOCALS

