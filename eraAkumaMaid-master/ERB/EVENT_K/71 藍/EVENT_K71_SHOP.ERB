;-------------------------------------------------
;购物での台詞
;-------------------------------------------------
@KOJO_SHOP_K71, ARGS

SELECTCASE ARGS
CASE "一緒に出発"
	;初回
	IF SEVENT(ARGS) == 0
		PRINTFORMW 「えっ、%CNAME("一人称")%と购物に行きたいの？」
		PRINTFORMW 「何買いに行こっかなー！新作も欲しいんだ！」
	;着替えてもらう
	ELSEIF CHECK_CLO("裸体")
		IF ABL:露出癖 >= 3
			CALL PRINT_STR, @"「わ、%CNAME("一人称")%……いろんな人に見られちゃうんだ……_BH_」"
		ELSE
			PRINTFORMW 「すっぽんぽんで行くの！？流石に風邪ひいちゃうよ……？！」
		ENDIF
	ELSEIF CHECK_CLO("泳装姿")
		PRINTFORMW 「海とか泳池じゃないのに泳装ってなんだか変な感じー」
	ELSEIF CHECK_CLO("体操服姿")
		PRINTFORMW 「んー、別に運動するわけじゃないんだけどなぁ」
	ELSEIF CHECK_CLO("内衣姿")
		PRINTFORMW 「ちょっと寒いかも……服着ないー？」
	ELSE
		PRINTFORMW 「よーし、それじゃあれっつごー！」
	ENDIF
CASE "目送"
		PRINTFORMW 「%CNAME("二人称")%、いってらっしゃーい！」
CASE "没収胖次出発"
	;初回
	IF SEVENT(ARGS) == 0
		IF ABL:露出癖 >= 3
			CALL PRINT_STRW, @"「胖次履いてないと……ちょっと、気持ちいいかも……_BH_」"
		ELSE
			PRINTFORMW 「えっ、なんで胖次持ってかれたんだろ……」
		ENDIF
	;既に没穿内褲
	ELSEIF EQUIP:胖次 == 0
		PRINTFORMW 「もー、最初から履いてないよー？」
	ELSE
		IF ABL:露出癖 >= 3
			CALL PRINT_STRW, @"「誰かに見られちゃったら……えへへ_BH_」"
		ELSE
			PRINTFORMW 「また胖次无かぁ、いいんだけどー」
		ENDIF
	ENDIF
CASE "保持现在的装扮出発"
	IF CHECK_CLO("泳装姿")
		PRINTFORMW 「なんだか悪いことしちゃってるみたいだねー」
	ELSEIF CHECK_CLO("体操服姿")
		PRINTFORMW 「買い物競争だー！」
	ELSEIF CHECK_CLO("内衣姿") || CHECK_CLO("裸体")
		IF ABL:露出癖 >= 3
			CALL PRINT_STRW, @"「もー、見られちゃったらどうするのー？……気持ちいいからいいけどぉ_BH_」"
		ELSE
			PRINTFORMW 「就这様出てくの？服くらい着よー？」
		ENDIF
	ENDIF
CASE "会計交代"
	;初回
	IF SEVENT(ARGS) == 0 || SEVENT("商品購入") == 0
		PRINTFORMW 「これ買ってくればいいの？わかったー！」
	ELSE
		PRINTFORMW 「すぐ買ってくるねー！」
	ENDIF
;ここはwaitを使わないようにしてください(PRINTFORMW の代わりに PRINTFORML を使う)
CASE "询问有什么想要的东西"
	;PRINTFORML 
CASE "商品購入"
	;今買った物
	SELECTCASE STR:購入道具
	;ギリギリ通常商品の範疇or你用
	CASE "撮影機材"
		PRINTFORMW 「照相机？もしかして録画するの？」
		PRINTFORMW 「下手なPLAYング出来ないじゃん！」
	CASE "潤滑液", "媚薬", "飞机杯"
		PRINTFORMW 「えっちに使うやつ？そっかー、玩遊戲のに必要なら買ってこなくちゃ！」
	CASE "避孕套"
		;この商品を初めて買わされた
		IF SEVENT(STR:購入道具) == 0
			PRINTFORMW 「こんなの必要なのー？まぁいいけどー」
		ELSE
			PRINTFORMW 「またこれかー、いらないと思うんだけどなぁ」
		ENDIF
	;えっちな商品
	CASEELSE
		PRINTFORMW 「……えっと、なんだかすごい目で見られてる気がするけど、本当にいいの？」
	ENDSELECT
CASE "店内待機"
	;とりあえず何か一個買った時or33％で喋る
	SIF COND("買上品数") != 1 && RAND:3
		RETURN 0
	;胖次没収中
	IF COND("買い物胖次没収中")
		PRINTFORMW 「いい感じの風を感じる気がするー」
	ELSE
		PRINTFORMW 「まだー？」
	ENDIF
CASE "购物終了"
	;胖次没収中
	IF COND("買い物胖次没収中")
		PRINTFORMW 「うーん……ちょっと癖になっちゃいそうだねー」
	;なにも買わなかった
	ELSEIF COND("買上品数") == 0
		PRINTFORMW 「結局何も買わなかったんだー、次は何か買お？」
	ELSEIF CHECK_CLO("泳装姿") || CHECK_CLO("体操服姿")
		PRINTFORMW 「ちょっと新鮮で楽しかったなー！」
	ENDIF
ENDSELECT

