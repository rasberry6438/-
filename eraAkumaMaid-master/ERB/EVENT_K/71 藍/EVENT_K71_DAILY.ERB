;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K71, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		CALL PRINT_STRW, @"「おっはよー！えへへ、%CNAME("二人称")%の中……気持ちいいよぉ_BH_」"
		PRINTFORMW 「ほんとは、もうちょっと……楽しみたい、けど……」
		CALL PRINT_STRW, @"「もう、我慢できない……いっぱい中に出しちゃうね_BH_」"
	CASE "朝舐陰"
		CALL PRINT_STRW, @"「ちゅっ……ん_BH_ じゅるっ」"
		PRINTFORMW 「……ふぁっ、もう起きちゃった？」
		PRINTFORMW 「もーちょっとだけまってねー！ いまいいとこ……ちゅうっ」
	CASE "朝足交"
		PRINTFORMW 「んー……あ、おっはよー！」
		PRINTFORMW 「今日はね、足の練習してるんだけど……」
		PRINTFORMW 「きもちいい？ 痛かったら言ってね？」
	CASE "朝の新妻PLAY"
		PRINTFORMW 「おはよー！爸爸！」
		PRINTFORMW 「もうすぐごはんが……ふぇっ！？」
		CALL PRINT_STRW, @"「んぅ…_BH_ も、もー……焦げちゃうから、ちょっとだけだよー？」"
	CASE "朝乳交"
		PRINTFORMW 「んーしょ、んーしょ……あ、ごめんねー！」
		PRINTFORMW 「頑張ってるつもりだけど、ちょっと難しくて……」
		PRINTFORMW 「でもせーいっぱいご奉仕するからね！」
	CASE "朝面部騎乗"
		PRINTFORMW 「起きたー？おはよー！」
		PRINTFORMW 「%CNAME("二人称")%の大好きなおまた、朝から見れたらうれしいかなって！」
		CALL PRINT_STRW, @"「ひゃっ_BH_ あ、あんまりなめちゃダメだよぉ_BH_」"
	CASE "朝騎乗位", "朝Ａ騎乗位"
		IF CANTRAIN("騎乗位") && CANTRAIN("肛門騎乗位")
			CALL SETFLAG, "地の文スキップ"
			PRINTFORMW 腹周りの圧迫感で睁開眼睛と、%CALLNAME:TARGET%が一心不乱に腰を振っている
			PRINTL 
			CALL PRINT_STRW, @"「ぅ……ふぁっ_BH_……あっ、おはよー、%CNAME("二人称")% _BH_」"
			PRINTFORMW 「えへへ、どう？今日はね、いい感じの游戏を見つけてきたんだ！」
			PRINTFORMW 「ほら、見ての街道裙で隠れちゃってるけど……」
			PRINTFORMW 「おまたとおしり、どっちに入ってるでしょー？」
			CALL PRINT_STRW, @"「もし間違えたら……どうしよっかなぁ_BH_」"
			CALL SIMULATE_SHOW_NEWDAY
		ELSE
			CALL PRINT_STRW, @"「んぁっ_BH_ おはよっ_BH_ %CNAME("二人称")% _BH_」"
			CALL PRINT_STRW, @"「まってねっ、すぐ……ヌくからぁ_BH_」"
		ENDIF
	CASE "朝接吻"
		PRINTFORMW 「ん……ちゅっ」
		PRINTFORMW 「……えへへ、おはよー、%CNAME("二人称")%っ」
	CASE "朝添い寝"
		PRINTFORMW 「んぅ……ふぁ……」
		PRINTFORMW 「……みゅぅ……もうごふん……」
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%のぬくもりに安心を感じて二度寝してしまった。
	CASE "朝口交", "朝六九式"
		CALL PRINT_STRW, @"「じゅっ……じゅぷじゅぷ、じゅるるっ_BH_」"
		CALL PRINT_STRW, @"「……んっ、ほふぁほー……ふぉーひょっほふぁっへへぇ……じゅぞぞぞっ_BH_」"
	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		CALL PRINT_STRW, @"「んぅ……ふぁ……すっごく濃い_BH_」"
		CALL PRINT_STRW, @"「んっ、んくっ……えへへ_BH_ とってもおいしかったよ、%CNAME("二人称")%_BH_」"
	CASE "朝騎乗位"
		CALL PRINT_STRW, @"「えへへ……_BH_ いっぱい出たね_BH_」"
		PRINTFORMW 「また抜きに来るから、まっててねっ！」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育の日かぁ」
		PRINTFORMW 「いっぱい遊べるスポーツだったらいいな！」
	ELSE
		PRINTFORMW 「%CNAME("二人称")%！はやくいこっ！」
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")
		PRINTFORMW 「あっそうだ！テストだー！よくわかんないけど、とりあえずどーんといこー！」
	ELSEIF DAYEV("試験返却日")
		PRINTFORMW 「えっ、テスト帰ってくるの？」
		IF CALCF("試験の予想点数") >= 90
			PRINTFORMW 「大丈夫！今回のはEZってとこだから！」
		ELSEIF CALCF("試験の予想点数") >= 70 
			PRINTFORMW 「えーっと、酱と游戏で復習したから大丈夫だよー！」
		ELSEIF CALCF("試験の予想点数") >= 50 
			PRINTFORMW 「そ、そんなことより、%CNAME("一人称")%、早く游戏がやりたいな！」
		ELSE
			PRINTFORMW 「うそっ、しんじられなーい！」
		ENDIF
	ELSEIF COND("朝袭相手")
		PRINTFORMW 「あっ、みんなには内緒だよー！」
	ELSEIF COND("体育")
		PRINTFORMW 「体操服、涼しくていいやー」
	ELSEIF COND("水泳")
		PRINTFORMW 「酱と泳装来てるから大丈夫だね！」
		PRINTFORMW 「……胖次？あれ、持ってきてたっけ……」
	ELSE
		SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「游戏！游戏！游戏がやりたい！」
				PRINTFORMW 「あ、%CNAME("二人称")%の分も用意してるからね！」
			CASE 0
				PRINTFORMW 「今日は誰と玩遊戲の？」
				PRINTFORMW 「%CNAME("一人称")%？%CNAME("一人称")%はねー……紗南と玩耍たいな！」
			CASEELSE
				PRINTFORMW 「最近ガチャの運が悪くても大丈夫だよー」
				PRINTFORMW 「次はきっといいことだらけだから！」
		ENDSELECT
	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
	PRINTFORMW 「板根藍いちばんのりー！」
	IF CEVENT("調教開始：初調教") == 0 || (TALENT:恋慕 == 0 && TALENT:淫乱 == 0)
		PRINTFORMW 「なにするー？游戏？それともべんきょー？」
	ELSE
		PRINTFORMW 「なにするー？游戏？それともえっち？」
	ENDIF
CASE "ただいま"
	PRINTFORMW 「ただいまーっ！」
	PRINTFORMW 「新しい游戏やらなくちゃ！」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「おかえりー！今日も夜更かしだー！」
	ELSE
		PRINTFORMW 「おかえりー！ちょっと遅かったねー」
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORMW 「呼ばれて飛び出てじゃーん！」
		PRINTFORMW 「それでなにするの？」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF TALENT:恋慕 || TALENT:淫乱 || TALENT:相思相愛
			PRINTFORMW 「あー！ずるーい！%CNAME("一人称")%にもやってー！」
		ELSE
			PRINTFORMW 「もう遊んでるじゃん！ なにしよっかなぁ」
		ENDIF
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「こんにちはー！もう大体集まってるねー」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「%CNAME("二人称")%っ！きたよー！」
	ELSE
		PRINTFORMW 「やっほー！うん、まだ广阔ね！」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『あ！こんにちは！老师！』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『遅かったねー！茶いる？』
	ELSE
		PRINTFORMW 『こんにちはー！』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「よんだー？今いいとこだったんだけどー」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「戻ったよー！何かやってる？」
	ELSE
		PRINTFORMW 「喉乾いたー！あとお菓子あるー？」
	ENDIF
;你と一緒に摆脱人はPARTNER()で参照できる。名前はCALLNAME:PARTNER()です
CASE "抜け出された"
	IF TALENT:恋慕 || TALENT:淫乱 || TALENT:相思相愛
		PRINTFORMW 「えーっ！おいてくのー！？ %CNAME("一人称")%も連れてってよー！」
	ELSE
		PRINTFORMW 「いってらっしゃーい！さー、今日も練習だー！」
	ENDIF
CASE "便当"
	PRINTFORMW 「是！便当だよ！酱とできてたらいいけど……」
	SIF CFLAG:料理経験 <= 10
		PRINTFORMW 「……あれ、本当に大丈夫かな？米は洗剤じゃなくて水で洗ったし、塩を砂糖でごまかさなかったし……」
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	IF TALENT:恋慕 || TALENT:淫乱 || TALENT:相思相愛
		PRINTFORMW 「いいなー！%CNAME("一人称")%もえっちしたかったー！」
	ELSE
		PRINTFORMW 「あ、それじゃあ游戏してくるねー！」
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;动画ーションの関係上、１ターンに何度も呼ばれたりするのでフラグ操作をする場合には
;IF CFLAG:KOJO_DAILY済 == 0
;で分岐させるのが良いでしょう
;-------------------------------------------------
@KOJO_DAILY_K71, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「ここが部室かー、いいお部屋だね！」
	PRINTFORML 「あ、お菓子がある！しかも美味しいやつだこれ！」
	PRINTFORML 「いたれりつくせり……今日からここが%CNAME("一人称")%のキャンプ地だよー！」
CASE "浑身是汗"
	PRINTFORML 「あっつーい……干からびるぅ……」
	IF ABL:露出癖 >= 1
		PRINTFORML 「もう脱いじゃおっかなー……涼しい方が良いし……」
	ELSE
		PRINTFORML 「冰どこー……？ないのー？信じられなーい……！」
	ENDIF
CASE "刚睡醒"
	PRINTFORML 「うみゅ……げーむとらないでぇ……」
	PRINTFORML %CALLNAME:TARGET%は困倦ながら手を伸ばしてくる。
	PRINTFORML 悄悄的游戏ローリズを手元に置いてあげた。
CASE "Cosplay中"
	SELECTCASE SHIRT(TARGET)
		CASE "女教師"
			PRINTFORML 「こほん、授業を始めるよー！」
			PRINTFORML %CALLNAME:TARGET%は眼鏡をくいっと上げて指示棒を持つ
			PRINTFORML 「布丁が2つと冰阴蒂ームが1つ、全部で何個でしょー！」
			PRINTFORML 「……ねー、聞いてるー？」
		CASE "和服"
			PRINTFORML 「七五三とか正月とかに着たやつだね！」
			PRINTFORML %CALLNAME:TARGET%は何気无に袖をふりふりしている。
			PRINTFORML 「んー……なんだか参拜したくなってきちゃった」
			PRINTFORML 「ねー%CNAME("二人称")%！……どこだっけ、蓮台野？行こ！」
		CASE "女仆装"
			PRINTFORML 「じゃーん、女仆装だよ！」
			PRINTFORML 「どう？可愛いよね？似合うでしょ？」
			PRINTFORML %CALLNAME:TARGET%はモップをもって床を掃除マネをしている。
			IF TALENT:恋慕
				PRINTFORML 「えへへ、%CNAME("二人称")%だけの女仆桑だよ！」
			ELSE
				PRINTFORML 「えへへ、本物の女仆桑变成了みたい！」
			ENDIF
		CASE "体操服"
			PRINTFORML 「体育しないのにこれがいいの？」
			PRINTFORML %CALLNAME:TARGET%は首をかしげながらこちらを見てくる。
			PRINTFORML 「たまに見てるのにへんなのー」
			PRINTFORML 「……動きやすいのはそうだけどさー、もっと主上ゃん！ね？」
		CASE "护士服"
			PRINTFORML 「救護の時間だよー！」
			PRINTFORML %CALLNAME:TARGET%はどこからともなく大号的注射器を持ってきた。
			PRINTFORML 「え？これ？保健室にあった！何に使うんだろね？」
			PRINTFORML 「そんなことより、早く横になってー！」
			IF TALENT:恋慕
				PRINTFORML 「……どこか知らないところで倒れても、すぐ駆けつけるからね？」
			ELSE
				PRINTFORML 「さー、調子悪いところはどこかなー？」
			ENDIF
		CASE "旗袍"
			PRINTFORML 「あいやー！スピニング……なんとか！」
			PRINTFORML %CALLNAME:TARGET%は開心的格ゲーごっこをしている。
			PRINTFORML 「とっても動きやすくていいね！気に入っちゃった！」
			PRINTFORML 「……でもなんで前後に布が垂れてるんだろ？」
		CASE "水手服"
			PRINTFORML 「いつものと違う制服もいいねー」
			PRINTFORML %CALLNAME:TARGET%はぱたぱたと袖を振っている。
			PRINTFORML 「でもちょっと袖が長いかなぁ」
			PRINTFORML 「えっとー……えへへ、幽霊桑だよー！」
		CASE "巫女"
			PRINTFORML 「はらたまきよたまー」
			PRINTFORML %CALLNAME:TARGET%は大幣をわさわさと振っている
			PRINTFORML 「ぐっどらっく！抜き征く%CNAME("二人称")%に敬礼を！」
			PRINTFORML 「本物の巫女桑にはなれないけど、祈祷くらいはできるよ！」
		CASE "情趣内衣"
			PRINTFORML 「なにこれー、とってもスケスケじゃん！」
			PRINTFORML %CALLNAME:TARGET%は訝しげにこちらを見てくる。
			PRINTFORML 「えっ、これも睡衣の一種なの？」
			PRINTFORML 「へぇー、変わった睡衣もあるんだねー」
		CASE "婚纱礼服"
			PRINTFORML 「わぁ……とってもきれいだね」
			PRINTFORML %CALLNAME:TARGET%は身体を動かして服を眺めている。
			IF TALENT:相思相愛
				PRINTFORML 「……これならいつでも式を挙げられるね」
			ELSE
				PRINTFORML 「えへへ、今日はお嫁桑だよー！」
			ENDIF
		CASE "哥特蘿莉礼服"
			PRINTFORML 「わー！とってもひらひらだー！」
			PRINTFORML %CALLNAME:TARGET%は鏡の前でふりふりと下摆を回している。
			PRINTFORML 「こんな衣装もあるんだね！面白色よー！」
			PRINTFORML 「あ！そうだ！」
			PRINTFORML くるりと振り返って、下摆を持ち上げて一礼する。
			PRINTFORML 「ほら、なんだか大小姐みたいでしょ！どう？いい感じだよね！」
		CASE "啦啦队制服"
			PRINTFORML 「あ、これって応援団の服じゃん！」
			PRINTFORML %CALLNAME:TARGET%はポンポンを持ち的同时、わさわさと揺らしている。
			PRINTFORML 「えっと、どうやるんだっけ……」
			PRINTFORML 「フレッ！フレッ！ がんばれっ！がんばれっ！ ……あってる？」
		CASE "睡衣"
			PRINTFORML 「この睡衣いいねー！柄も好きだよ！」
			PRINTFORML 「それに……寝ながら游戏するのにちょうどいいかも！」
			PRINTFORML %CALLNAME:TARGET%は何かを思いついたように手を叩いた。
			PRINTFORML 「そうだ！二人横になって游戏でもする？」
			PRINTFORML 「おんなじところで並ぶときっと楽しいよ！」
		CASE "怪獣睡衣"
			PRINTFORML 「ぎゃおー！たーべちゃーうぞー！」
			PRINTFORML 「えへへ、とってもかわいくてふかふかでいいやつだよこれ！」
			PRINTFORML %CALLNAME:TARGET%は袖を握りながらフードをパカパカしている。
			PRINTFORML 「怠け者じゃないけど、一緒に寝よー？」
		CASEELSE
			PRINTFORML 「どう？似合ってるー？」
			PRINTFORML %CALLNAME:TARGET%は腕を振ったり一回転したりしながらアピールをしている。
			PRINTFORML 「%CNAME("一人称")%だってジョブチェンジの一つくらいできるんだからね！」
		ENDSELECT
CASE "点心の感想"
	;今日の点心を用意したのは自分
	IF COND("点心当番")
		PRINTFORML 「折角だから頑張って作ってきたよー！ ほら！食べて食べて！」
	;今日の点心は誰かの亲手自制
	ELSEIF COND("亲手自制点心")
		;今日の点心当番の人の亲手自制を初めて食べた
		IF DEVENT(@"%TEXTS("点心当番")%の亲手自制") == 0
			;点心当番が[料理擅長]
			IF COND("点心の作り手が料理擅長")
				PRINTFORML 「美味しー！いくらでも食べられそう！」
				PRINTFORML 「もっと食べたいなー……ダメ？」
				PRINTFORML ……%CALLNAME:TARGET%がこちらの分も食べたそうに何度も見てくる。
					SIF TALENT:淫乱
						PRINTFORML 「それとも……%CNAME("二人称")%のおちんちん、食べさせてくれるの？」
			ELSE
				PRINTFORML 「美味しいお菓子は游戏の源、だよ！」
			ENDIF
		ELSE
			IF COND("点心の作り手が料理擅長")
				PRINTFORML 「やっぱりおいしいなぁ……おかわりー！」
			ELSE
				PRINTFORML 「こんなにタダでお菓子食べて大丈夫かなぁ？」
			ENDIF
		ENDIF
	;点心が亲手自制でない
	ELSE
		PRINTFORML 「あ、お菓子はっけーん！食べちゃうね、%CNAME("二人称")%！」
	ENDIF
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K71
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
