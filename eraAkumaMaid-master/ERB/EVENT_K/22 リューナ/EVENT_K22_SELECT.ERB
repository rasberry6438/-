;22は角色番号に置換してください
;-------------------------------------------------
;お願い・取引での選択肢を追加。番号は80～89を使ってよい
@WISH_K22_ADDSELECT
IF CEVENT("新しい服")
	IF CEVENT("美式校服化：琉娜")
		STR:選択肢 = %STR:選択肢% [80] %TEXT_LJ("美式校服着用を止めて", 16)% … %CALLNAME:TARGET%に美式校服着用を止めてもらいます/
	ELSE
		STR:選択肢 = %STR:選択肢% [80] %TEXT_LJ("美式校服を着て", 16)% … %CALLNAME:TARGET%に美式校服を着て部活に着てもらいます/
	ENDIF
ENDIF
SIF CEVENT("襲来")
	STR:選択肢 = %STR:選択肢% [81] %TEXT_LJ("ラディーネの来訪頻度を変えて", 16)% … %CALLNAME:TARGET%を通じてラディーネにお願いします/

;お願い・取引で増やした選択肢の地の文と台詞
@KOJO_WISH_K22_ADDSELECT, ARGS
SELECTCASE ARGS
CASE "美式校服着用を止めて"
	PRINTFORMW %CALLNAME:MASTER%决定听从%CALLNAME:TARGET%的%ARGS%建议。
	PRINTL 
	PRINTFORML 「诶～？不换了吗？」
	PRINTFORMW 「明白了、下次就穿着平时的衣服吧」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%不再穿着情趣水手服了。
	CALL DEL_CEVENT, "美式校服化：琉娜"
CASE "美式校服を着て"
	PRINTFORMW %CALLNAME:MASTER%决定听从%CALLNAME:TARGET%的%ARGS%建议。
	PRINTL 
	PRINTFORML 「嗯、希望我下次穿着情趣水手服来？」
	PRINTFORMW 「好的、下次我会穿来的」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%開始穿着情趣水手服了。
	CALL SET_CEVENT, "美式校服化：琉娜"
	;フラグを弄る場合はそれもここで行う
CASE "ラディーネの来訪頻度を変えて"
	PRINTFORMW %CALLNAME:MASTER%决定听从%CALLNAME:TARGET%的%ARGS%建议。
	PRINTL 
	PRINTFORML 「おばあ酱の来る日？」
	PRINTFORM 「今は
	IF CFLAG:3200 == 0
		PRINTFORML 土日だけ来るけど…毎日来てもらうようにする？」
	ELSE
		PRINTFORML 毎日来るけど…土日にだけ来てもらうようにする？」
	ENDIF
	CALL PRINT_SELECT, "そうする/やっぱり还是算了"
	IF RESULT == 0
		PRINTFORMW 「了解、おばあ酱に伝えとくね」
		IF CFLAG:3200 == 0
			CFLAG:3200 = 1
		ELSE
			CFLAG:3200 = 0
		ENDIF
	ELSE
		PRINTFORMW 「还是算了の？うん、分かった」
	ENDIF
	;フラグを弄る場合はそれもここで行う
ENDSELECT

