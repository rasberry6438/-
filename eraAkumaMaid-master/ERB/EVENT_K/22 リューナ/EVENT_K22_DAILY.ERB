;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K22, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STR, @"「%TEXTR("呼呼/……")%…_H_」_W_"
	CALL PRINT_STR, "「今天会一直陪着你哦…_H_　做好觉悟吧_H_」_W_"
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝の新妻PLAY"
		;恋慕
		IF TALENT:恋慕
			CALL PRINT_STRW, "「♪～～～_H_　♪～♪～～_H_　♪～♪♪～～～_H_　…啊、早上好♪」"
			PRINTFORMW 「马上就要吃早饭了、去洗下手吧♪」
			PRINTFORML 迎接被早餐的香味钓起来的%CALLNAME:PLAYER%的是%CALLNAME:TARGET%满面笑容的问候、
			PRINTFORMW 穿着裸体围裙、一边哼着小曲、一边兴高采烈地准备早餐。
			PRINTFORML %CALLNAME:TARGET%每动一下、臀部和尾巴就像要引诱别人那样晃动起来、看到这副景象股间变得灼热起来的%CALLNAME:PLAYER%、
			PRINTFORM 从背后悄悄接近
			IF RAND:5 == 0
				PRINTFORMW 把手伸进围裙的缝隙、紧紧地抓住胸部。
			ELSEIF RAND:4 == 0
				PRINTFORMW 肉棒激烈地磨蹭着秘裂和阴蒂。
			ELSEIF RAND:3 == 0
				PRINTFORMW 温柔地抚摸尾巴的根部和屁股。
			ELSEIF RAND:2 == 0
				PRINTFORMW 紧紧地抱住、夺走了嘴唇。
			ELSE
				PRINTFORMW 轻咬舔弄着脖颈和翅膀的根部。
			ENDIF
			CALL PRINT_STR, "「嗯、啊啊…！_H_　…呼呼嗯、果然…_H_　我就觉得穿成这样你一定会过来_H_L_"
			CALL PRINT_STR, "　但是、实际上试着引诱对方来的话、果然还是很害羞啊…_／／／_」_W_"
			PRINTFORML 被充满欲情的%CALLNAME:PLAYER%从背后抱住的%CALLNAME:TARGET%发出了甜蜜的声音、
			PRINTFORMW 害羞地回过頭、用魅惑的眼神微笑着。看来是真的在引诱你了。
			CALL PRINT_STR, @"「想在吃早饭之前先吃%CNAME("一人称")%吧？_H_　…嗯、好啊_H_」_W_"
			PRINTFORML 半兴奋半羞耻的%CALLNAME:TARGET%向%CALLNAME:PLAYER%挺起了屁股、
			CALL PRINT_STR, @"将秘处扩展到连阴道口都张開、好像有发出『咕啪_H_』声音一样、粘稠的爱液滴落下来…。_W_"
			CALL PRINT_STR, @"「%CNAME("一人称")%的这里已经准备好了…_H__L_"
			CALL PRINT_STR, @"　所以、让我多尝尝%CNAME("二人称")%的肉棒、呃、呃咿——————！？_HH_」_W_"
			PRINTFORML 看着%CALLNAME:TARGET%涨红了脸撒着娇的痴态、已经无法忍受的%CALLNAME:PLAYER%、
			PRINTFORMW 一口气将肉棒顶入小穴深处、和%CALLNAME:TARGET%反复激烈地缠绵着、连早饭都忘了…。
		;それ以外
		ELSE
			PRINTFORMW 「呀啊！？　有点危险吧！　现在正在用%TEXTR("火/菜刀")%哦！」
			PRINTFORML 悄悄地来到穿着落体围裙的%CALLNAME:TARGET%的身后、冲动地从背后抱住的%CALLNAME:PLAYER%、
			PRINTFORMW 被罕见得发出可爱悲鸣的%CALLNAME:TARGET%的尾巴打倒了…。
			PRINTFORML 但%CALLNAME:PLAYER%立刻站了起来、一边向瞪着这边的%CALLNAME:TARGET%道歉一边温柔地抱住她、
			PRINTFORMW 缓慢地爱抚着围裙下柔软的肉体、慵懒得用肉棒的前端在秘裂中进出。
			CALL PRINT_STR, "「啊、嗯、有点…就算是温柔的也不行的啊………啊啊、真是没办法…_H_」_W_"
			PRINTFORML 虽然%CALLNAME:TARGET%很困扰、但继续做下去之后、就一边叹着气一边停下来正在做菜的手
			PRINTFORMW 红着脸自己挺起腰部、小穴的深处也做好了接受%CALLNAME:PLAYER%的准备。
			CALL PRINT_STR, "「咕、嗯啊、啊啊…！_H_　看、插进来了哦…射精之后就赶快结束吧？」_W_"
			PRINTFORML %CALLNAME:PLAYER%回应了%CALLNAME:TARGET%的请求紧紧地抓住了她的腰部和尾巴、
			CALL PRINT_STR, @"早饭的时间和%CALLNAME:TARGET%的「这样没法做饭啦_H_」的抗议消失之前尽情地侵犯了…。_L_"
		ENDIF
	CASE "朝騎乗位"
		;発情期
		IF COND("発情期")
			CALL EMOTION_PLUS, "重置/爱心眼/赤面/对话框爱心大量"
			CALL PRINT_STR, @"「啊啊_H_　…喂、%CALLNAME:PLAYER%、起床啦_H_起、床、啦_H_　嗯啊啊_H_」_W_"
			PRINTFORML 甜美的娇声和黏着的水声混合在一起的呼唤、以及下半身传来的快感唤醒了%CALLNAME:PLAYER%。
			PRINTFORMW 映入眼帘的是失却理智的微笑、和%CALLNAME:TARGET%被快乐染红的脸庞。
			IF RAND:3 == 0 && TALENT:恋慕
				CALL PRINT_STR, "「诶嘿嘿、早上好_H_　小鸡鸡可是很早就已经起来了哦_H_」_W_"
				CALL PRINT_STR, @"「%CALLNAME:PLAYER%ってば、寝てるのに濃いのを何度も種付けするから、%CNAME("一人称")%の子宮、もう%TEXTR("たぷたぷ/満タン")%なんだぁ…_H_」_W_"
				PRINTFORM 正如%CALLNAME:TARGET%所说、她的
				SIF MARK:淫紋
					PRINTFORM 刻上了淫纹
				PRINTFORML 光滑的小腹、
				PRINTFORMW 因为%CALLNAME:PLAYER%睡着时射出的数发浓厚的精液而膨胀起来。
				CALL PRINT_STR, @"「恩呼呼…%CALLNAME:PLAYER%的、又勃起了哦…？_H_」_W_"
				CALL PRINT_STR, @"「_H_」_W_"
				PRINTFORML 眼前的光景和子宫口吸附着的感触让%CALLNAME:PLAYER%的肉棒又变得精神起来了、
				PRINTFORMW 之后就像%CALLNAME:TARGET%寻求的那样、数次向子宫注入追加的精液…。
	;			PRINTFORML 
	;			PRINTFORML …その後%CALLNAME:PLAYER%が身支度をして朝食をとりに行き、部屋に残った%CALLNAME:TARGET%正在认真地读着标题为床に横たわり、
	;			PRINTFORMW 多幸感に満ちた表情で、%CALLNAME:PLAYER%の精液で膨らんだ下腹部をゆっくり愛おしそうに擦っていた…。
	;			CALL PRINT_STR, @"（…%CALLNAME:PLAYER%と%CNAME("一人称")%の兒童、出来るかな…　出来たら良いなぁ…_H_）_W_"
			ELSEIF RAND:2
				CALL PRINT_STR, "「啊哇、终——于起来了_H_　这个懒觉老师_H_」_W_"
				PRINTFORML %CALLNAME:TARGET%一边这么说着、一边将小穴紧紧吸附住的肉棒抽离身体、
				PRINTFORMW 堵住小穴的肉棒被抽出、用手指捞起精液和爱液的混合液、放入口中舔舐品味起来…。
				CALL PRINT_STR, @"「诶嘿嘿、我開动了_H_　啾噜、啾啵、咕嘟…_H_」_W_"
				CALL PRINT_STR, @"「嗯~、%CNAME("二人称")%和%CNAME("一人称")%的爱液、非常地色情、很美味哦…_H_」_W_"
			ELSE
				PRINTFORMW 大概是过了很长一段时间、两人的股间互相分泌出来的体液起了白色泡沫、弥漫着淫靡的%TEXTS("气味")%。
				PRINTFORML %CALLNAME:TARGET%恋恋不舍得抬起腰、从不断開合痉挛着的秘处那里、
				PRINTFORMW 精液和爱液混合而成的黏液伴着一些下流的声音溢出来，在床单上留下了污渍。
				CALL PRINT_STR, @"发情之后思考完全宕机的%CALLNAME:TARGET%一边说着「讨厌、太可惜了_H_」、_L_"
				PRINTFORMW 用手指把流出来的精液放回穴内、用自慰的动作咕啾咕啾地送入小穴深处…。
			ENDIF
		;それ以外
		ELSE
			CALL EMOTION_PLUS, "目陰/微笑/赤面/对话框爱心"
			CALL PRINT_STR, "「啊嗯、哈啊……_H_　呜、啊~嗯…！_H_」_W_"
			PRINTFORML 被抑制住的喘息声以及腰部传来的重量和快感唤醒的%CALLNAME:PLAYER%抬起頭来、
			PRINTFORMW 和涨红着脸跨坐在%CALLNAME:PLAYER%的身上挺动腰肢的%CALLNAME:TARGET%视线交汇。
			IF TALENT:恋慕
				CALL EMOTION_PLUS, "重置/笑顔/赤面/对话框爱心"
				CALL PRINT_STR, "「嗯、早上好………呼呼、睡得好吗？_H_」_W_"
				PRINTFORMW 注意到%CALLNAME:PLAYER%已经醒来的%CALLNAME:TARGET%、没有停下腰部的动作微笑着问好。
				CALL PRINT_STR, @"「%CNAME("二人称")%的小鸡鸡、硬硬地勃起着、看起来很痛苦哦？_H_」_W_"
				CALL PRINT_STR, @"「嗯…现在就让鸡鸡放松下来、睡吧、就这样交给%CNAME("一人称")%吧_H_」_W_"
				PRINTFORML %CALLNAME:TARGET%这样说着用手制止了想要起身的%CALLNAME:PLAYER%、
				PRINTFORMW 啪叽、爱液四散飞溅、腰部的活塞运动越来越激烈。
				CALL PRINT_STR, @"「啊哈、小鸡鸡、在颤抖着…_H_　呼啊啊嗯~、差不多要、射出来了？_H_」_W_"
				PRINTFORML 察觉到%CALLNAME:PLAYER%即将射精的%CALLNAME:TARGET%露出了淫荡的笑容、
				PRINTFORML 就这样俯下身来紧贴着%CALLNAME:PLAYER%、两人的手掌交叠紧紧相握、
				PRINTFORMW 一边用子宫口吞下肉棒的前端、一边在耳畔喃喃细语…。
				CALL PRINT_STR, @"「从早上開始就很想要%TEXTR("最喜欢/爱着/坚强")%的雄性的%TEXTR("精液/子种")%、就跑到床上来了…_H_」_W_"
				CALL PRINT_STR, @"「向这个自己动着腰的%TEXTR("欲求不满/变态/色情/好色/下流")%的母龙的子宫里…_H_」_W_"
				CALL PRINT_STR, @"「%TEXTR("biu~biu~/咕嘟咕嘟")%地_H_施舍你%TEXTR("热热的/黏黏的/浓厚的")%%TEXTR("精液/子种")%吧…～～～ッ_HH_」_W_"
				PRINTFORML 到达极限的%CALLNAME:PLAYER%射出了早上最热最浓的精液、射精持续了一分钟都没有结束、
				PRINTFORMW 伏在身上的%CALLNAME:TARGET%也一副幸福的表情高声娇叫到达高潮。
				PRINTFORML 彼此的高潮平息下来之后、%CALLNAME:TARGET%恋恋不舍地起身、
				PRINTFORMW 没能进到小穴深处、像果冻一样浓厚的白浊从结合处黏稠地挤出来…。
			ELSE
				CALL EMOTION_PLUS, "害羞/呀哒２/赤面/对话框！"
				PRINTFORMW 「啊………早、早上好………嗯、这、这个是…那个………」
				PRINTFORML 和醒了过来的%CALLNAME:PLAYER%视线交汇的%CALLNAME:TARGET%、
				PRINTFORMW 跨坐在%CALLNAME:PLAYER%的身上、语无伦次…。
				PRINTFORML %CALLNAME:PLAYER%像是催促沉默的%CALLNAME:TARGET%继续做下去一样、向上挺了挺腰、
				PRINTFORMW %CALLNAME:TARGET%紧紧得勒紧阴道、一边上下摆动腰部一边用甜美的声音辩解道…。
				CALL PRINT_STR, @"「啊、我只是来叫醒%CNAME("二人称")%的哦…啊_H_」_W_"
				CALL PRINT_STR, @"「哦啊、可是%CNAME("二人称")%的小鸡鸡、啊、变得非常有精神_H_」_W_"
				CALL PRINT_STR, @"「就、就是因为这样、%CNAME("一人称")%、才变得忍耐不住了啊！_H_」_W_"
				PRINTFORML 在%CALLNAME:TARGET%辩解到最后的那一瞬间、%CALLNAME:PLAYER%用力地向上挺腰射精、
				PRINTFORMW %CALLNAME:TARGET%同时仰起身到达高潮、随着肉棒的脉动、身体和尾巴不断颤抖…。
			ENDIF
		ENDIF
	CASE "朝口交", "朝六九式"
		IF COND("発情期")
			CALL EMOTION_PLUS, "重置/半目/斜眉/ڡ２/爱心眼/赤面/对话框爱心"
			CALL PRINT_STR, "「……嗯呜、咕嘟…呼哈、哈啊啊………啊、早、早上好…_H_」_W_"
			CALL PRINT_STR, "「本来想来叫你起床…那个…非常地有精神、雄性的气味很强烈…不知不觉就…_H_」_W_"
			IF RAND:4 == 0 && TALENT:恋慕
				PRINTFORML 发情的%CALLNAME:TARGET%一边用一只手去抓起飞散的精液舔舐品尝、
				PRINTFORMW 另一只手咕啾咕啾地抚弄着秘处、故意发出很大的水声…。
				CALL PRINT_STR, "「舔了这么浓厚的东西、身体也越来越燥热了…_H_」_W_"
				CALL PRINT_STR, "「已经没什么时间了、现在我只能忍耐了…之后下面的嘴也能拜托你吗？_H_」_W_"
			ELSEIF RAND:3 == 0
				CALL PRINT_STR, "「呼呼、现在就帮你『清理』哦_H_　不用客气、看……啊~嗯…_H_」_W_"
				PRINTFORML 瞳孔荡漾的%CALLNAME:TARGET%一边这么说着一边用手臂用力地按住%CALLNAME:PLAYER%的腰、
				PRINTFORMW 舌頭缠绕着被精液和唾液濡湿的肉棒、%TEXTR("含到喉咙深处/含在口中紧紧吸附着")%…！
				CALL PRINT_STR, "「嗯呼、啾噜噜…呼啊、嗯咕_H_　啾啵~啾啵~啾啵！_H_」_W_"
				PRINTFORML %CALLNAME:PLAYER%忍受不住%CALLNAME:TARGET%那明显超出『清理』范畴的口交、
				PRINTFORML 大量地射出精液后、%CALLNAME:TARGET%的喉咙鼓动着将精液一饮而尽…。
			ELSEIF RAND:2
				CALL PRINT_STR, "「现在帮你清理干净、不要动哦………pr……_H_」_W_"
				PRINTFORMW %CALLNAME:TARGET%这样说着、舌頭在被精液和唾液濡湿的肉棒前端缠绕着
				PRINTFORML 就这样深深地含入口中、各个角落都认真地清理了…。
				CALL PRINT_STR, "「嗯………啾噜……啾啵_H_　哈啊…看、清理干净了哦_H_」_W_"
			ELSE
				PRINTFORML 看着%CALLNAME:TARGET%嘴角沾着精液满脸羞涩的样子、
				PRINTFORMW 股间再次充血、又变得硬了起来。
				CALL PRINT_STR, "「嗯、又变大了……呼呼、没办法、必须要让它平静下来呢…_H_」_W_"
				PRINTFORML %CALLNAME:TARGET%舔舔嘴唇然后大大地张開嘴来、
				PRINTFORML 将肉棒含入口中、用细致的口技再次引导射精…。
			ENDIF
		ELSE
			CALL EMOTION_PLUS, "重置/半目/斜眉/ڡ２/赤面"
			PRINTFORMW 「啊、早上好…吓到你了吗？」
			PRINTFORMW 「呃、本来是想叫你起床的…可是那个…变得很精神…」
			PRINTFORMW 脸涨得通红的%CALLNAME:TARGET%这样辩解着、而进入了临战状态的坚挺的肉棒
			IF RAND:2
				PRINTFORM 正被满怀爱意地亲吻着
			ELSE
				PRINTFORM 正被舌頭缠绕舔弄、赋予甜美的刺激
			ENDIF
			PRINTFORMW 然后、她有点恋恋不舍地离開了%CALLNAME:PLAYER%的身体。
			PRINTFORMW 「诶、继续？　…早饭要凉了哦、之后再继续吧…」
		ENDIF
	ENDSELECT
CASE "打招呼"
	SELECTCASE CSTR:朝袭
		CASE "朝接吻"
			CALL EMOTION_PLUS, "害羞/赤面"
			PRINTFORML 「啾…哦、%TEXTR("早上好/起来了呢")%」
			IF TEXTS("曜日") == "日"
				PRINTFORML 「今天是
				IF CFLAG:约会的约定
					PRINTFORML 约会的日子呢。要是能享受很多就好了」
				ELSE
					PRINTFORML %TEXTR("有预定吗/有空吗")%？　%TEXTR("如果什么事都没有的话、就这样磨磨蹭蹭也不错啊/要不一起出門吧？/不管怎样、总之先吃早饭吧")%」
				ENDIF
			ELSE
				PRINTFORML 「%TEXTR(@"呼呼、醒了吗？　没什么哦♪/今天也要精神饱满地努力吧？/喏、要是醒了的话就赶快起床哦！")%」
			ENDIF
		CASE "朝口交", "朝六九式"
			CALL EMOTION_PLUS, "重置/半目/ڡ２/赤面"
			IF TEXTS("曜日") == "日"
				PRINTFORML 「呼呼、好像变得很清爽呢」
				PRINTFORM 「继续？　嗯、
				IF CFLAG:约会的约定
					PRINTFORML 约会%TEXTR("回来/之后")%再继续做吧♪」
				ELSE
					PRINTFORML %TEXTR("吃过早饭之后/到了晚上")%再继续吧♪」
				ENDIF
			ELSE
				PRINTFORML 「喂、已经很清爽了、快起来快起来！」
				PRINTFORML 「%TEXTR("到学校/之后")%再继续做吧…好吗？」
			ENDIF
		CASE "朝騎乗位"
			CALL EMOTION_PLUS, "重置/半目/斜眉/微笑３/赤面/对话框爱心"
			CALL PRINT_STR, @"「%TEXTR("啾、啾~…/嗯呼、呜啊啊…")%_H_　%TEXTR("再次…/呼呼、")%早上好_H_」_L"
			IF RAND:4 && TEXTS("曜日") == "日"
				CALL PRINT_STR, @"「今天是休息日…还要继续做吗？_H_」_L"
			ELSE
				CALL PRINT_STR, @"「%TEXTR(@"刚起来就射出来这么多黏糊糊的、%CNAME("一人称")%的本能都要觉醒了…没什么哦/抱歉哦、一早上就袭击你…可是一看到晨勃就忍耐不住了/想要、继续做…？　呼呼、早饭要凉了、之后再做吧…")%_H_」_L"
			ENDIF
		CASE "朝の新妻PLAY"
				CALL EMOTION_PLUS, "重置/半目/蹙眉/小口/对话框乱"
				PRINTFORML 「啊、真是的、%TEXTR("稍微做点饭/好不容易做的饭")%都凉了啊…」
				IF TALENT:恋慕
					PRINTFORM 「嘛、没能阻止你的%CNAME("一人称")%也有责任呢…
				ELSE
					PRINTFORM 「
				ENDIF
				PRINTFORML 现在重新加热、坐在那里等一下哦」
		CASE "朝添い寝"
			CALL EMOTION_PLUS, "重置/眯眯眼/蹙眉/哈欠大/对话框困倦"
			IF RAND:3 == 0
				PRINTFORMW 「嗯~呜…？　已经早上了吗………」
			ELSEIF RAND:2
				PRINTFORMW 「呼啊～………呜～嗯………早上好………」
			ELSE
