;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K13, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	IF COND("発情中")
		CALL EMOTION_PLUS, "重置/小悪魔", TARGET
		CALL PRINT_STR, "「啊，终于醒来了吗…_」_W"
		PRINTFORMW 「那么，可以了吧？哈…哈…（色气地喘气）现在開始也没有问题吧…？」
	ELSE
		CALL EMOTION_PLUS, "重置/躁动难忍", TARGET
		PRINTFORMW 「那、那个…即使不在发情期也偷偷溜进来了…」
		PRINTFORMW 「总、总之！现在请对%CNAME("一人称")%好好负起责任来…！」
	ENDIF
CASE "朝袭"

CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝乳交", "朝足交", "朝六九式"
		SIF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, "「嗯唔……_H_」_W"
		PRINTFORMW 「啊…\@ COND("発情中") ? 起来了吗…？ # 弄醒你了吗… \@早安」
	CASE "朝騎乗位"
		CALL EMOTION_PLUS, "重置/小悪魔", TARGET
		SIF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, "「呼啊啊……_H_」_W"
		PRINTFORMW 「…\@ COND("発情中") ? fufu……♪ # 呼…… \@终于起来了吗…真是的…」
	CASE "朝の新妻PLAY"
		CALL EMOTION_PLUS, "重置/小悪魔", TARGET
		IF COND("発情中")
			CALL PRINT_STR, "「呼~……_H_呼~……_H_」_W"
			IF RAND:2
				CALL PRINT_STR, "「…呀…早上開始那个就这么有精神……_H_」_W"
			ELSE
				CALL PRINT_STR, "「…那个东西、这么有精神……_H_」_W"
			ENDIF
		ELSE
			CALL EMOTION_PLUS, "重置/憤怒", TARGET
			IF RAND:2  && TEXTS("曜日") != "日"
				PRINTFORMW 「真是的、迟到了该怎么办呀……这样的理由、不觉得很羞耻吗…」
			ELSE
				PRINTFORMW 「はぁっ…はぁっ……一点都没有变呢、不愧是%TEXTR("笨蛋/变态")%呢…」
			ENDIF
		ENDIF
	CASE "朝接吻"
		IF COND("発情中") && RAND:2
			CALL EMOTION_PLUS, "重置/小悪魔", TARGET
			PRINTFORMW 「あ、起きた？」
			PRINTFORMW 「…全然起きないから、ちょっと心配だったのよ？」
			CALL PRINT_STRW, @"「さっきから%CNAME("一人称")%、何回も接吻してたのに…_H_」"
		ELSEIF !COND("発情中") && RAND:2
			CALL EMOTION_PLUS, "重置/紅潮", TARGET
			PRINTFORMW 「……早上好」
			PRINTFORMW 「はぁ……朝から何やってるのかしらね、%CNAME("一人称")%達……」
		ELSE
			PRINTFORMW 「\@ COND("発情中") ? 呼呼♪怎么样…？ # 那个…稍、 \@稍微、清醒了一些吗…？」
		ENDIF
	CASE "朝添い寝"
		IF COND("発情期") || RAND:4 == 0
			CALL EMOTION_PLUS, "重置/躁动难忍", TARGET
			CALL PRINT_STR, "「…那个、唔…唔…早上好…_／／／_」_W"
			PRINTFORMW 「抱、抱歉…现在就从床上下来……！」
		ELSEIF RAND:3 == 0 && ABL:信頼 >= 5
			CALL EMOTION_PLUS, "重置/刚睡醒", TARGET
			PRINTFORMW 「んにゃ…」
			CALL EMOTEFACE, "重置/紅潮", TARGET
			PRINTFORMW 「…………あ」
			CALL EMOTEFACE, "憤怒眉/吊眼飘忽/小口/紅潮", TARGET
			PRINTFORMW 「ここが一番、良く寝れると思ったから、つい…」
		ELSEIF RAND:2 && COND("冬")
			IF RAND:2
				CALL EMOTION_PLUS, "重置/斜眼/通常眉/無口系/对话框困倦", TARGET
				PRINTFORMW 「寒いから…出口ないで…」
				CALL EMOTEFACE, "闭眼/通常眉/無口系/对话框困倦", TARGET
				PRINTFORMW 「あと5分だけ、保持这个样子……」
			ELSE
				CALL EMOTION_PLUS, "重置/吊眼飘忽/紅潮", TARGET
				PRINTFORMW 「……これは、その」
				PRINTFORMW 「寒いでしょ、最近…風邪ひいたら困るし、お互いに…」
			ENDIF
		ELSE
			CALL EMOTION_PLUS, "重置/斜眼/通常眉/無口系/对话框困倦", TARGET
			PRINTFORMW 「糟糕、不小心就睡着了…」
			CALL EMOTEFACE, "憤怒/紅潮", TARGET
			PRINTFORMW 「才、才不是！为什么非要偷看%CNAME("二人称")%的睡颜啊！」
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1
	CALL EMOTION_PLUS, "重置", TARGET
	PRINTFORMW 「……早上好」
	IF COND("同居中") == 0 && RAND:4 == 0
		PRINTFORMW 「快点起来哦、难得今天特意过来叫你起床」
	ELSEIF RAND:3 == 0 && TEXTS("曜日") != "日"
		PRINTFORML 「…さ、快点去学校、唔…诶？是要一起去学校吗？」
		PRINTFORMW 「唔…要是产生了谣言的话该怎么办啊…？」
	ELSEIF RAND:2
		PRINTFORML 「…不好的睡眠习惯暴露出来了哟、看、那里…」
		PRINTFORMW 「现在就帮%CNAME("二人称")%治好，不要乱动哦…」
	ELSE
		PRINTFORMW 「…来一起吃早饭吧…不吃的话一整天都会不舒服的哦？」
	ENDIF

CASE "登校"
	IF DAYEV("試験当日")
		PRINTFORMW 「そういえば…試験、今日だっけ？」
		IF COND("朝袭相手") && CALCF("試験の予想点数") <= 50 && RAND:2
			PRINTFORMW 「……あんな事してないで今朝も勉強してればよかったかな…」
		ELSEIF CFLAG:勉強時間 >= 5
			PRINTFORMW 「今回に関しては酱と準備してるし…ま、余裕よね」
		ELSEIF RAND:2
			PRINTFORMW 「……ま、今更焦ってもどうしようもないわよね」
			PRINTFORMW %CALLNAME:TARGET%はここにきて悟りを開いてしまったようだ…
		ELSE
			PRINTFORMW 「今日に限って凄く直感が冴えたりしないかなあ…」
			PRINTFORMW %CALLNAME:TARGET%は現実逃避をしている…
		ENDIF
	ELSEIF DAYEV("試験返却日")
		PRINTFORMW 「試験の結果、今日帰ってくるのよね…」
		IF COND("Ｈ的約定")
			IF CFLAG:Ｈ的約定 == 90
				PRINTFORMW 「まあ、妙な約束もしちゃってたけど？あの点数なんて取れるわけないしね…」
				SIF CALCF("試験の予想点数", MASTER) >= 90
					PRINTFORMW 「……ちょっと待って、何？その自信有げな顔は…」
			ELSE
				IF BASE:MASTER:学力 >= 70 && RAND:2
					PRINTFORMW 「……えっと」
					PRINTFORMW 「…あんまり変なこと、命令しないでよ？」
				ELSE
					PRINTFORMW 「気が重いなあ…%CNAME("二人称")%の点数も%CNAME("一人称")%の運命に関わってるわけだし…」
				ENDIF
			ENDIF
		ELSE
			IF CALCF("試験の予想点数") >= 90 
				PRINTFORMW 「今回は結構自信あるの…ふふんっ」
			ELSEIF CALCF("試験の予想点数") >= 70 
				PRINTFORMW 「ま、平均は堅いんじゃないかしら」
			ELSEIF CALCF("試験の予想点数") >= 50 
				PRINTFORMW 「悪くはない、と思うんだけど」
			ELSE
				PRINTFORMW 「心の準備、しないと…」
			ENDIF
		ENDIF
	ELSEIF COND("朝袭相手")
		IF COND("発情中") && TALENT:恋慕
			IF RAND:2
				PRINTFORMW 「……んふふ」
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の腕にしがみつきながら歩いている…
				PRINTFORMW 「ね…あとでさっきの続き、してくれる？」
			ELSE
				PRINTFORMW 「今朝はごめん…どうしても我慢できなくって」
				PRINTFORMW 「もう一つ謝らなきゃいけないんだけど…%CNAME("一人称")%、まだまだ、満足出来てないの…」
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORMW 「ちょっと、腕貸してくれる？」
				PRINTFORMW 「…いろいろあって疲れてるの！ホントにそれだけだから！」
				PRINTFORMW %CALLNAME:MASTER%は%CALLNAME:TARGET%と腕を組みながら登校した…
			ELSE
				PRINTFORMW 「あんまりべたべたしないでよ…」
				PRINTFORMW 「……誰かに見られたら言い訳できないじゃない」
			ENDIF
		ENDIF
	ELSEIF COND("体育")
		PRINTFORMW 「…今天有体育课呢…%TEXTR("如果可以的话，真想去参观一下啊。/如果是球技比试的话，我可不会输的")%…」
	ELSEIF COND("水泳")
		PRINTFORMW 「…今天有游泳课呢…说实话不是很擅长游泳啊…」
	ELSEIF RAND:3 == 0 && TALENT:相思相愛
		PRINTFORMW 「そ、その……手、繋いで行きたい、かな…って」
		IF RAND:2
			PRINTFORMW 「誰かに見られても…誤解とかではないでしょ」
			SIF RAND:2
				PRINTFORMW 「……ないわよね？」
		ELSE
			PRINTFORMW 「学校ついたらみんないるし、今だけ……いいでしょ？」
		ENDIF
	ELSE

	ENDIF
CASE "一番乗り"
	IF COND("発情期")
		PRINTFORMW 「…其他人还没来吗？」
		PRINTFORMW 「…怎么办…现在%CNAME("一人称")%的里面想做的不得了…可要是途中谁进来的话…」
		PRINTFORMW %CALLNAME:TARGET%躲闪着视线苦恼着…
	ELSEIF RAND:3 == 0 && TALENT:恋慕
		PRINTFORMW 「啊…难得的二人世界、吗…」
		PRINTFORMW 「什么嘛、只是现实真的是这种情况的才这样说的哦…」
	ELSEIF RAND:2
		PRINTFORMW 「嘛~、难得的清净也不错呢」
		SIF CHARA_NUM("集合予定") >= 3
			PRINTFORMW 「嘛、反正很快就会变得热闹起来吧…」
	ELSE
		PRINTFORMW 「谁都不在呢…可能都在忙？」
	ENDIF
CASE "ただいま"
	IF COND("発情期")
		CALL EMOTION_PLUS, "重置/躁动难忍", TARGET
		PRINTFORMW 「…回、回来了…唔……」
		PRINTFORMW %CALLNAME:TARGET%看起来忍得很辛苦的样子跟着回家了…
	;週一で夕飯お魚？
	ELSEIF RAND:7 == 0
		CALL EMOTION_PLUS, "重置/不審", TARGET
		PRINTFORMW 「……君君…」
		PRINTFORMW 「…はぁ……隔壁烧鱼的气味飘过来了…」
	ELSEIF RAND:3 == 0
		CALL EMOTION_PLUS, "重置/不審", TARGET
		PRINTFORMW 「回来了、呜啊！」
		PRINTFORMW 「…谁的鞋子啊、这个…？脱下来后好好地收拾好啊…」
	ELSEIF RAND:2
		CALL EMOTION_PLUS, "重置/吊眼飘忽", TARGET
		PRINTFORMW 「…%CNAME("一人称")%回来了」
		PRINTFORMW %CALLNAME:TARGET%抓着一根狗尾巴草回来了…
		PRINTFORMW 「…因、因为长在道路的两边，看到了之后很在意，就不小心带回来了…」
	ELSE
		CALL EMOTION_PLUS, "重置", TARGET
		PRINTFORMW 「…%CNAME("一人称")%回来了」
		PRINTFORMW 「什么嘛，竟然比%CNAME("一人称")%先到家了」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF COND("発情中")
			CALL EMOTION_PLUS, "重置/小悪魔", TARGET
			PRINTFORMW 「啊，欢迎回来…」
			PRINTFORMW %CALLNAME:TARGET%钻进%CALLNAME:MASTER%的怀里磨蹭着…
		ELSEIF COND("発情期") 
			CALL EMOTION_PLUS, "重置/躁动难忍", TARGET
			PRINTFORMW 「！！……啊、欢、欢迎回来…」
			PRINTFORMW 一瞬间，%CALLNAME:TARGET%露出了好像在说“一直在等着%CALLNAME:MASTER%回来哦”这样的寂寞的表情，但马上回到了正常的状态…
		ELSE
			CALL EMOTION_PLUS, "重置/会話口２", TARGET
			IF RAND:2
				PRINTFORMW 「嗯、欢迎回来」
				PRINTFORMW %CALLNAME:TARGET%は躺在地板上很困的样子打了招呼…
			ELSE
				PRINTFORMW 「欢迎回…原来是%CNAME("二人称")%吗…欢迎回家」
			ENDIF
		ENDIF
	ELSE
		IF COND("発情中")
			CALL EMOTION_PLUS, "重置/小悪魔", TARGET
			PRINTFORMW 「没有很迟哦…因为会一直等着%CNAME("二人称")%回来啦…」
			PRINTFORMW 高高地竖起尾巴的%CALLNAME:TARGET%看起来很高兴的样子迎接%CALLNAME:MASTER%回家…
		ELSEIF COND("発情期") 
			CALL EMOTION_PLUS, "重置/躁动难忍", TARGET
			PRINTFORMW 「呜啊…！去、去干什么了呀…！」
			PRINTFORMW %CALLNAME:TARGET%看到%CALLNAME:MASTER%回来后气息逐渐開始变得焦躁起来…
		ELSEIF RAND:2
			CALL EMOTION_PLUS, "重置/半開２", TARGET
			PRINTFORMW 「好慢啊、到哪里去偷吃野食了吗？」
		ELSE
			CALL EMOTION_PLUS, "重置", TARGET
			PRINTFORMW 「啊~终于回来了…怎么了？被强制补习了吗？」
		ENDIF
	ENDIF

CASE "入室打招呼"
	SELECTCASE CFLAG:MASTER:PREV_DAILYCATE
	;Talk
	CASE 1
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "甜言蜜语"
			CALL EMOTION_PLUS, "重置/不審", TARGET
			IF RAND:2
				PRINTFORMW 「…怎、怎么？这个气氛…？感觉很难融入进来…」
			ELSE
				PRINTFORMW 「妙な雰囲気だけど…もしかして%CNAME("一人称")%、出直した方がいい…？」
			ENDIF
		CASE "打招呼舐陰", "打招呼口交"
			IF RAND:2
				CALL EMOTION_PLUS, "重置/不審", TARGET
				PRINTFORMW 「这样的事情、在谁看不到的场合做比较好…？」
				PRINTFORMW %CALLNAME:TARGET%露出了震撼的表情…
			ELSE
				CALL EMOTION_PLUS, "重置/不審/半開２", TARGET
				PRINTFORMW 「看不到看不到、%CNAME("一人称")%什么都没有看到…请继续…」
				PRINTFORMW %CALLNAME:TARGET%捂住眼睛小声碎语着进了房间…
			ENDIF	
		ENDSELECT
	;互动
	CASE 2
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "托一托胸部", "接吻"
			CALL EMOTION_PLUS, "重置/不審", TARGET
			PRINTFORMW 「…在做什么呢？」
			PRINTFORMW 「……!不行不行…在意的话就输了，很在意的话就真的输了，加油啊，%CNAME("一人称")%…」
		ENDSELECT
	;卖个破绽
	CASE 3
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "Ｈ的邀请", "展示内褲", "推倒", "抱住"
			CALL EMOTION_PLUS, "重置/害羞", TARGET
			PRINTFORMW 「欸多……打扰你们了吗…？」
			PRINTFORMW %CALLNAME:TARGET%驻在房间的入口不知道该不该进去…
		ENDSELECT
	;胖次、性処理
	CASE 4, 6
		CALL EMOTION_PLUS, "重置/不審", TARGET
		IF RAND:2
			PRINTFORMW 「…应该要大喊一声“咔擦”模仿相机拍照吗？收敛一点比较好哦…」
			PRINTFORMW %CALLNAME:TARGET%叹着气…
		ELSE
			PRINTFORMW 「常識って言葉はこの部室には存在しないのよね…今更だけど」
			PRINTFORMW 「あー、気にしないで、独り言だから」
		ENDIF
	ENDSELECT

	;猫の日判定
	CALL CATDAY_K13

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF CFLAG:呼び出された
		IF COND("発情中")
			CALL EMOTION_PLUS, "重置/躁动难忍", TARGET
			PRINTFORMW 「……虽然、跟过来了」
			PRINTFORMW %CALLNAME:TARGET%满脸潮红着用期待的眼神看着这边…
		ELSE
			PRINTFORMW 「……不用那么着急也可以的吧…反正这边也会好好地跟过来的」
			PRINTFORMW %CALLNAME:TARGET%一边抱怨着一边走进了房间…
		ENDIF
	ELSE
		PRINTFORMW 「…嗯、看起来%CNAME("一人称")%稍微迟到了一点」
	ENDIF

;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;既に部員の３分の２が集合済
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*2/3 && RAND:3 == 0
		PRINTFORMW 「哇、差不多全员都到齐了…」
	ELSE
		PRINTFORMW 「…嗯、看起来%CNAME("一人称")%稍微迟到了一点」
	ENDIF
;簡易入室打招呼に対する返し。ここでは括弧に『』を使う事。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		ACTOR = FIND_COND("教員", "入室打招呼中")
		IF RAND:2
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%老师…现在很忙吗？』
		ELSE
			PRINTFORMW 『…要坐吗？』
		ENDIF
	ELSEIF PLACE("自宅")

	;前輩にも敬語使いたいかも？
	ELSE
		IF RAND:2
			PRINTFORMW 『终于来了呢』
		ELSE
			PRINTFORMW 『好好地遵守约定了呢』
		ENDIF
	ENDIF
	
CASE "居間に現れた"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "重置/不審", TARGET
		PRINTFORMW 「……有什么事情吗？虽然现在想呆在房间里…」
		PRINTFORMW %CALLNAME:TARGET%\@ COND("発情中") ? 迈着摇摇晃晃的步伐 # 一边抱怨着 \@进到了房间里面…
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「……嗯…果然、%TEXTR("没有办法睡觉/一直醒着睡不着")%……」
	ELSE
		PRINTFORMW 「……啊、原来在这里」
	ENDIF
CASE "抜け出された"
	IF COND("発情期")
		PRINTFORMW 「……大意了…让%CALLNAME:MASTER%逃走了…」
		PRINTFORMW %CALLNAME:TARGET%耳朵和尾巴很没精神地垂了下来…
	ELSEIF RAND:2
		PRINTFORMW 「有事？ふーん…嘛、加油吧」
	ELSE
		PRINTFORMW 「おぉ…怎么说比较好…唔…」
	ENDIF
CASE "便当"
	IF COND("発情中")
		CALL EMOTION_PLUS, "重置/小悪魔", TARGET
	ELSE
		CALL EMOTION_PLUS, "重置/害羞", TARGET
	ENDIF
	IF RAND:9 == 0 && TALENT:恋慕 && COND("発情期")
		PRINTFORMW 「…给、今天的份哦」
		PRINTFORMW %CALLNAME:MASTER%把便当盒打開之后、发现了米饭上面用海苔大大地写着『ＹＥＳ』几个字…
		IF COND("発情中")
