;加筆の際には RETURN 0 を消去し、MAIL_BODY を埋めて使ってください
;地の文も必要であれば適宜変更してください
;-------------------------------------------------
@SELFY_MAIL_K13, ARGS
#DIMS MAIL_BODY

MAIL_BODY = 
;メッセージ本文
IF COND("発情中")
	IF RAND:6 == 0 && GETBITS(BASE:バイオリズム, 0, 5) == 1
		MAIL_BODY = 进入了发情期
	ELSEIF RAND:5 == 0 && COND("昼") && PLACE("部室")
		MAIL_BODY = 地点不变？
	ELSEIF RAND:4 == 0
		MAIL_BODY = 有点烦躁
	ELSEIF RAND:3 == 0
		MAIL_BODY = 来吗？
	ELSEIF RAND:2
		MAIL_BODY = 现在身体有点不太妙
	ELSE
		MAIL_BODY = 想做
	ENDIF
ELSE
	SELECTCASE ARGS
	CASE "ヌード自撮り"
		;RETURN 0
		IF RAND:3 == 0
			MAIL_BODY = 请随意使用
		ELSEIF RAND:2
			MAIL_BODY = 可能变胖了一点
		ELSE
			MAIL_BODY = 虽然没什么自信
		ENDIF
	CASE "阴部自撮り", "阴茎自撮り"
		;RETURN 0
		IF RAND:3 == 0
			MAIL_BODY = 这次是特例哦
		ELSEIF RAND:2
			MAIL_BODY = 不要给别人看哦？
		ELSE
			MAIL_BODY = 不经意间拍下来的
		ENDIF
	CASE "今年の泳装自撮り"
		;RETURN 0
		IF RAND:3 == 0
			MAIL_BODY = 试着穿了一下
		ELSEIF RAND:2
			MAIL_BODY = 想去海边
		ELSE
			MAIL_BODY = 再稍微紧一点的是不是更好啊
		ENDIF
	CASE "学校指定の泳装自撮り"
		;RETURN 0
		IF RAND:2 && TALENT:貧乳
			MAIL_BODY = 虽然没什么值得看的东西
		ELSE
			MAIL_BODY = 最近肚子周围好像变得有点紧了
		ENDIF
	CASE "体操服自撮り"
		;RETURN 0
		IF RAND:2
			MAIL_BODY = 热过頭了
		ELSE
			MAIL_BODY = 全身是汗
		ENDIF
	CASE "胸罩自撮り"
		;RETURN 0
		IF RAND:2 && TALENT:貧乳
			MAIL_BODY = 反正是平胸…
		ELSE
			MAIL_BODY = 今天的内衣
		ENDIF
	CASE "胖次自撮り"
		;RETURN 0
		IF RAND:3 == 0 && COND("昼")
			MAIL_BODY = 今天不要掀裙子哟？
		ELSEIF RAND:2
			MAIL_BODY = 特意给你拍了
		ELSE
			MAIL_BODY = 今天的那个
		ENDIF
	CASEELSE
		;RETURN 0
		IF RAND:2
			MAIL_BODY = 对于我来说、也许拍得不错？
		ELSE
			MAIL_BODY = 在研究摄影的技巧
		ENDIF
	ENDSELECT
ENDIF
;地の文
PRINTFORMW 照片边上还加上「%MAIL_BODY%」这样的信息…

;-------------------------------------------------
;约会の際のセリフ
;-------------------------------------------------
@KOJO_DATE_K13, ARGS
#DIM MEMO_LINECOUNT

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "约会のお誘い"
	PRINTFORMW 「……%CNAME("二人称")%、这个周日有空吗？」
	PRINTFORMW 「\@ EXP:约会経験 == 0 ? たまには # 良ければまた \@、どっか連れてって欲しいな、って思ったんだけど…」
	PRINTFORMW 脸红到耳根的%CALLNAME:TARGET%羞涩地低頭注视着%CALLNAME:MASTER%…
CASE "约会のお誘い：結果"
	;约会決定！
	IF COND("约会的约定")
		IF TALENT:恋慕 && RAND:2
			PRINTFORMW 「…诶多、那个……」
			PRINTFORMW 「……请、请保持期待…」
		ELSE
			PRINTFORMW 「…不要食言哦…」
		ENDIF
	;你に保留されてしまった
	ELSE
		PRINTFORMW 「唔…嘛、好吧」
		PRINTFORMW 「……改变心意了的话记得告诉%CNAME("一人称")%，好吗？」
	ENDIF
;你から週末约会に誘われた
CASE "约会に誘われた"
	IF TALENT:恋慕 && RAND:2
		PRINTFORMW 「可以啊，正好没有什么事情…」
		PRINTFORMW 「目的地交给%CNAME("二人称")%来决定可以吗？」
	ELSEIF EXP:约会経験 > 0
		PRINTFORMW 「知、知道了…那个……就交给%CNAME("二人称")%了」
		PRINTFORMW 「…作为交换、不要让%CNAME("一人称")%感到无聊啊？」
	ELSE
		PRINTFORMW 「…这不是開玩笑，而是认真的邀请对吧？」
		PRINTFORMW 「知道了…因为也没有什么特别的拒绝的理由…」
	ENDIF
;你から约会当日にいきなり誘われた
CASE "邀请去约会"
	;同居していない相手を誘った時
	IF COND("同居中") == 0
		PRINTFORMW 「真的、很突然的邀请呢…这么空闲吗？」
		PRINTFORMW 「嘛、%CNAME("一人称")%也没有资格说别人呢…难得的机会，就一起走吧」
	ELSE
		PRINTFORMW 「完、完全没有听说过今天有一起出門的计划啊？！」
		PRINTFORMW 「……啊、知道了…因为要做点准备所以等一下哦…」
	ENDIF

CASE "待ち合わせ"
	IF COND("同居中") == 0
		PRINTFORMW 「来、来得好早啊…」
		PRINTFORMW 「所以，要去哪里已经决定了吗？」
	ELSE
		PRINTFORMW 「没有忘记的东西吧？」
		PRINTFORMW 「那么，要去哪里呢？」
	ENDIF

CASE "帰り際"
	IF CHECK_PANTIES("Ｈ的内衣") && COND("発情中")
		PRINTFORMW 「对不起、稍微等一下…%CNAME("一人称")%、这之后有想去的地方…」
		IF EQUIP:裙 && RANDIF(1, CHECK_SHIRT("女式衬衫"))
			PRINTFORMW %CALLNAME:TARGET%确认了周围没有其他人之后，掀起了%SKIRT(TARGET)%让%CALLNAME:MASTER%看了里面的早已准备好的情趣内衣…
			CALL TEXT_PANTU_CHECK, SKIRT(TARGET)
		ELSEIF CHECK_SHIRT("女式衬衫")
			PRINTFORMW %CALLNAME:TARGET%紧紧地贴到%CALLNAME:MASTER%身上，故意拉低了%SHIRT(TARGET)%的领子，展示里面早已准备好的情趣内衣…
			PRINTFORMW 在暴露出来的胸部前、色情的%DETAIL_BRA(TARGET)%被%CALLNAME:MASTER%看到了…
		ELSE
			
		ENDIF
		PRINTFORMW 「%TEXTR(@"…酱と、準備してきたから……いいでしょ？/…%CNAME("一人称")%の行きたい所、わかった？/連れてってくれると、嬉しいんだけどなー…")%」
		CALL SET_CEVENT("约会：内衣見せ")
	ELSE
		PRINTFORMW 「那…这之后的有什么预定吗？还是说有在考虑什么吗？」
	ENDIF
CASE "约会本番"
	SELECTCASE SAVESTR:约会
	CASE "散歩"
		PRINTFORMW 「%TEXTR(@"附近竟然有这种地方…/说是『没有目的地』…那我们到底要去什么地方呀…？/从这片空地过去到对面的那个小道可是一条近路哦……虽然今天不会从那里走啦")%」
	CASE "逛街"
		PRINTFORMW 「%TEXTR(@"…这个、怎么想？很不错的感觉不是吗？/うーん…没有遇到特别想要的东西啊…/%CNAME("二人称")%也想要买什么吗？可以的话给%CNAME("一人称")%看看如何…")%」
	CASE "去电影院"
		PRINTFORMW 「%TEXTR("最近的电影啊都是这个感觉呢…很有吸引力…/啊、那个是……漫画的実写化吗、只有这个吗？/如果途中睡着了的话记得叫醒我哟？")%」
	CASE "去水族馆"
		PRINTFORMW 「%TEXTR("已经要往前走了吗？我还想去那个水箱看看…/那…那条鱼的名字是…？看，那个，变色了呢…/…已经到处转来转去地看了很多东西，但还是不满足啊…这是为什么呀？")%」
	;CASE "滑雪"
	;	PRINTFORMW 「%TEXTR(@"不要在这么冷的时候到这么冷的地方来不就好了嘛…/冷静下来、记得要用尾巴保持平衡哟、%CNAME("一人称")%……喵ﾞ啊啊啊？！/等、等一下不要拍我的屁股…没问题的！不要再碰啦！")%」
	CASEELSE

	ENDSELECT
CASE "到暗处去……"
	IF COND("発情中")
		PRINTFORMW 「可以哦、%CNAME("一人称")%也变得想要那个了…♪」
	ELSE
		IF RAND:2
			PRINTFORMW 「暂、暂且呆着的话…没有问题……」
			PRINTFORMW 「真的只是暂且哦！」
		ELSE
			PRINTFORMW 「…なんでこんな場所で大きくしてるのよ」
			PRINTFORMW 「一緒に歩いてる私が変な目で見られるじゃない…！」
			PRINTFORMW 「何とか……するしかない、わよね」
		ENDIF
	ENDIF
	
CASE "手をつなぐ"
	IF RAND:2
		PRINTFORMW 「%CNAME("二人称")%が勝手にどっかいかないようにしとかないとね…」
	ELSE
		PRINTFORMW 「……どうせなら、もっと酱と手握ってよね」
	ENDIF
	
CASE "泳装に着替えた"
	;どの泳装に着替えたか？
	SELECTCASE SWIM(CSTR:今年の泳装, "種類")
	CASE "比基尼"
		PRINTFORMW 「……那个、嘛、好害羞的感觉…」
		IF RAND:3 == 0
			PRINTFORMW 「……」
			PRINTFORMW 「…看、看什么啊，快点走啦！」
		ENDIF
	;恥ずかしがったら負けな気がしたらしい
	CASE "Micro比基尼", "彈弓泳衣"
		PRINTFORMW 「……怎、怎么了啊？哪里很奇怪吗？」
		PRINTFORMW %CALLNAME:TARGET%羞红了脸，虚张声势地挺起胸部…
	CASE "連体泳装"
		PRINTFORMW 「……那个、嘛、好害羞的感觉…」
		SIF RAND:3 == 0
			PRINTFORMW 「……这个看起来会不会太幼稚了？」
	CASE "競泳泳装"
		PRINTFORMW 「确实露出来的部分很少呢…」
		PRINTFORMW 「但身体曲线被紧紧地勒出来了，稍微有点…」
	CASEELSE
		PRINTFORMW 「……うう、这件、是不是太色情了…？」
	ENDSELECT
CASE "接吻"
	IF COND("発情中")
		IF RAND:2
			PRINTFORMW 「嗯姆……」
			CALL PRINT_STR, "「………ふふ_H_」_W"
			PRINTFORMW %CALLNAME:TARGET%紧紧地把%CALLNAME:MASTER%的手臂抱在怀里…
		ELSE
			PRINTFORMW 「……あっ…」
			PRINTFORMW 「不行的说…这个时间在这种地方做这种不知廉耻的事情……」
			PRINTFORMW 「……回家后再做不好吗？」
		ENDIF
	ELSE
		IF RAND:2
			PRINTFORMW 「………！！」
			CALL PRINT_STR, "「………～～～ッ_／／／_」_W"
			PRINTFORMW 脸看上去像发烧了一样，%CALLNAME:TARGET%哒哒哒地小步在前面走着拉開了一点距离…
		ELSE
			PRINTFORMW 「……突、突然袭击过来做什么啊…！」
			PRINTFORMW 「…啊啊…已经对%CNAME("二人称")%的色情程度有更深刻的体会了…！」
		ENDIF
	ENDIF
CASE "摸屁股"
	IF COND("発情期")
		IF BASE:欲求不満 >= 50
			IF RAND:2
				PRINTFORMW 「……现在…请稍微忍耐一下呀…」
				PRINTFORMW 「即使是%CNAME("一人称")%，也同样在拼命地忍耐着…讷？」
				PRINTFORMW %CALLNAME:TARGET%温柔地安抚着%CALLNAME:MASTER%…
			ELSE
				PRINTFORMW 「啊…♪」
				PRINTFORMW 「……已经变得那么想要了吗？」
				PRINTFORMW 露出坏坏的笑容的%CALLNAME:TARGET%也同样用欲望的眼神看着%CALLNAME:MASTER%…
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORMW 「！！！！」
				PRINTFORMW 「……不、行……不要、快停下来呀…！」
				PRINTFORMW %CALLNAME:TARGET%用弱弱的颤抖的声音抵抗着%CALLNAME:MASTER%…
			ELSE
				PRINTFORMW 「咿！？」
				PRINTFORMW 「八卡……！这种时候在做什么蠢事啊…！」
				PRINTFORMW 不小心发出很大的声音的%CALLNAME:TARGET%红着脸死死地瞪着%CALLNAME:MASTER%…
			ENDIF
		ENDIF
	ELSE
		IF RAND:2
			PRINTFORMW 「……！！！」
			PRINTFORMW 「八、八卡…！不会注意周围的气氛吗…！」
			PRINTFORMW %CALLNAME:TARGET%皱起眉頭拼命地压低声音责怪着%CALLNAME:MASTER%…
		ELSE
			PRINTFORMW %CALLNAME:TARGET%一瞬间就像炸毛了一样剧烈地颤抖起来、但没有抵抗%CALLNAME:MASTER%的骚扰…
			PRINTFORMW 但是在一小段时间的忍耐之后%CALLNAME:TARGET%小声地进行了抗议…
			PRINTFORMW 「太过分了……差不多就可以住手了……！」
		ENDIF
	ENDIF
	TFLAG:地の文スキップ = 1

CASE "约会終了"
	;エッチをする場合
	IF TFLAG:床イン
		IF CEVENT("约会：内衣見せ")
			PRINTFORMW 「呼呼…♪那么、请好好享受哦…」
			CALL DEL_CEVENT("约会：内衣見せ")
		ELSEIF COND("発情中")
			PRINTFORMW 「%TEXTR("说的也是，欢乐的时光现在才要開始呢…♪/ふふ…实际上一直很期待做这种事情吧…♪")%」
		ELSE
			PRINTFORMW 「%TEXTR("被这样邀请的话…那，那个，突然变得紧张起来了…/！！……对，对呢，还有很多的时间…那样的话就没有办法了…")%」
		ENDIF
	ELSE
		IF CEVENT("约会：内衣見せ")
			PRINTFORMW 「……%CNAME("一人称")%知道了…」
			CALL DEL_CEVENT("约会：内衣見せ")
		ELSE
			PRINTFORMW 「%TEXTR(@"…那个，如果下次还有空的话…还会再邀请%CNAME("一人称")%吗？/也是呢，%CNAME("一人称")%也差不多累了，一起回家吧")%」
		ENDIF
	ENDIF
ENDSELECT

