;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K14, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 俺_俺
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF TALENT:PLAYER:男性
			LOCALS = 你_你
		ELSE
			LOCALS = 你_你
		ENDIF
	
	;相手が成美
	ELSEIF PLAYER:COND("成美", PLAYER)
		LOCALS = 班长_班长
		
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "１年生", "初等部", "中等部"
			IF TALENT:ASSI:男性
				LOCALS = %CALLNAME:PLAYER%君_%CSTR:PLAYER:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:PLAYER%酱_%CSTR:PLAYER:ふりがな%酱
			ENDIF
		CASE "２年生"
			LOCALS = %CALLNAME:PLAYER%桑_%CSTR:PLAYER:ふりがな%桑
		CASE "３年生", "研究生"
			LOCALS = 前輩_前輩
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = 老师_老师
			ELSE
				LOCALS = %CALLNAME:PLAYER%桑_%CSTR:PLAYER:ふりがな%桑
			ENDIF
		ENDSELECT
	ENDIF

CASE "主人"
	LOCALS = 主人大人_主人大人

CASE "助手"
	IF ASSI:COND("成美", ASSI)
		LOCALS = 班长_班长
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "１年生", "初等部", "中等部"
			IF TALENT:ASSI:男性
				LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:ASSI%酱_%CSTR:ASSI:ふりがな%酱
			ENDIF
		CASE "２年生"
			LOCALS = %CALLNAME:ASSI%桑_%CSTR:ASSI:ふりがな%桑
		CASE "３年生"
			LOCALS = 前輩_前輩
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = 老师_老师
			ELSE
				LOCALS = %CALLNAME:ASSI%桑_%CSTR:ASSI:ふりがな%桑
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

