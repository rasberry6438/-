;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K70, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
		CALL PRINT_STRW, @"RINTFORMW 「………%CNAME("二人称")%。」"
		CALL PRINT_STRW, @"「%CNAME("二人称")%が悪いのですよ_BBHH_」"
		CALL PRINT_STRW, @"「%CNAME("一人称")%のここが、こんなになるまで放置して…」"
		CALL PRINT_STRW, @"%CALLNAME:TARGET%は内衣を下ろし、溢れる程の愛液を垂らしている。"
		CALL PRINT_STRW, @"「本日は、お仕置きです…_BBHH_反省してくださいね_BBHH_」"
;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"
		CALL PRINT_STRW, @"「あ_WH_%CNAME("二人称")%おはようございます_BH_」"
		PRINTFORMW 「只今、朝餉の方を用意しておりますのでごゆっくりお待ちください」
		PRINTL 
		PRINTFORMW 「……………それと、」
		CALL PRINT_STRW, @"「%CNAME("一人称")%は朝から食べ頃ですがいかがなさいますか_BH_」"
	CASE "朝乳交"

	CASE "朝面部騎乗"

	CASE "朝騎乗位"
		PRINTFORMW 「あぁ、%CNAME("二人称")%おはようございます。」
		PRINTFORMW 「今、朝勃ちの処理をしておりますので…是。寝てる間に済ませておきたかったのですが…」
		PRINTFORMW 「どうでしょう…このまま出せ是这様吗？…もっと激烈地いたしましょうか？」
	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
			PRINTFORMW 「朝一番の精液は特に臭いですねぇ」
			PRINTFORMW 「若い男児特有のいい臭さです。」
	CASE "朝騎乗位"
			PRINTFORMW 「ほっほっほ、朝勃ちは生理現象ですので仕方ないことですよ」
			PRINTFORMW 「これからも%CNAME("一人称")%が処理してあげますからね」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 
	ELSE
		PRINTFORMW 「朝にございますよ、%CNAME("二人称")%」
		PRINTFORMW 「朝食の支度は済ませておりますので、一緒に召し上がりましょう」
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")

	ELSEIF DAYEV("試験返却日")

	ELSEIF COND("朝袭相手")

	ELSEIF COND("体育")

	ELSEIF COND("水泳")

	ELSE

	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
		PRINTFORMW 
CASE "ただいま"
		PRINTFORMW 「%CNAME("一人称")%が只今戻りましたよ～！」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
			PRINTFORMW 「お帰りなさいませ。本日はどのようなことがあったのでしょう」
			PRINTFORMW 「%CNAME("一人称")%に聞かせてはくれませんか。」
	ELSE
			PRINTFORMW 「お早いお目覚めですね。今日は採購に行こうと考えていますが」
			PRINTFORMW 「%CNAME("二人称")%は何か食べたいものはございますか？」
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
			PRINTFORMW 「%CNAME("二人称")%どうかなさいましたか？」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
			PRINTFORMW 「あらま！そういうことは床でやるものですよ」
	ELSE
			PRINTFORMW 「おや%CNAME("二人称")%は闲にございましょうか？」
			PRINTFORMW 「お菓子の用意でもしましょう…」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CFLAG:呼び出された
			PRINTFORMW 「是是%CNAME("一人称")%が来ましたよ～」
	ELSE
			PRINTFORMW 「%CNAME("二人称")%～～！」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			PRINTFORMW 「%CNAME("二人称")%にはご友人が多いようで…」
	ELSE
			PRINTFORMW 「こんにちは%CNAME("二人称")%。」
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
;你と一緒に摆脱人はPARTNER()で参照できる。名前はCALLNAME:PARTNER()です
CASE "抜け出された"
	PRINTFORMW 「おや？どこに行くのですか？早く帰ってきてくださいよ」
CASE "便当"
		PRINTFORMW 
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	PRINTFORMW 「床を使うのですね。では、疲れるでしょうから茶の方をご用意させていただきます。」
ENDSELECT

