		PRINTFORMW 「……很抱歉，只靠着从%CNAME("二人称")%这里得到的魂，把这种大小变成人类的级别还是太勉强了些」
		PRINTFORMW 「这样也没问题吗？」
	ENDSELECT

;其他汎用
CASEELSE
	;汎用拒否
	IF STRCOUNT(ARGS, "拒否")
		PRINTFORMW 「那样的话……很抱歉」
	;汎用取引（特に記述することがないならスキップ）
	ELSEIF STRCOUNT(ARGS, "取引")

	ELSE
		PRINTFORMW 「我明白了」
	ENDIF
ENDSELECT


;-------------------------------------------------
;购物での台詞
;-------------------------------------------------
@KOJO_SHOP_K25, ARGS
#DIMS NEED_TABLE

SELECTCASE ARGS
CASE "一緒に出発"
	;初回
	IF SEVENT(ARGS) == 0
		IF YOKUBO()
			CALL PRINT_STRW, @"「呵呵_H_真不愧是%CNAME("二人称")%呢_H_」"
			PRINTFORMW %CALLNAME:PLAYER%和%CALLNAME:TARGET%一起出去买东西了
			PRINTFORMW %CALLNAME:TARGET%的脸上浮现出了欢乐的笑颜……
		ELSE
			PRINTFORMW 「……真、真的要买吗……？　这里的东西……？」
			PRINTFORMW %CALLNAME:PLAYER%和%CALLNAME:TARGET%一起出去买东西了
			PRINTFORMW %CALLNAME:TARGET%的面上浮现出困惑不解的神情，双颊如火烧一般通红……
		ENDIF
	;着替えてもらう
	ELSEIF CHECK_CLO("裸体") || CHECK_CLO("泳装姿") || CHECK_CLO("体操服姿") || CHECK_CLO("内衣姿")
		PRINTFORMW 「穿着这一身去确实有问题呢」
	ELSE
		IF YOKUBO()
			CALL PRINT_STRW, @"「明白了。我们快点去吧、%CNAME("二人称")%_H_」"
		ELSE
			PRINTFORMW 「我、我知道了，我们快点买完东西吧……」
		ENDIF
	ENDIF
CASE "目送"
	PRINTFORMW 「路上小心」
CASE "没収胖次出発"
	;初回
	IF SEVENT(ARGS) == 0

	;既に没穿内褲
	ELSEIF EQUIP:胖次 == 0
		IF YOKUBO()
			PRINTFORMW 「……就这样，连胖次都没穿就被带来」
			CALL PRINT_STRW, @"「好的、可以哟。……稍微、有点心跳不已、忐忑不安呢_H_」"
		ELSE
			PRINTFORMW 「……诶？　就、就这样去吗……？」
		ENDIF	
	ELSE
		IF YOKUBO()
			CALL PRINT_STRW, @"「呵呵_H_不脱下胖次就不能一起去吗_H_」"
			CALL PRINT_STRW, @"「被发现的话会变成什么样呢……心跳不已了呢_H_」"
		ELSE
			PRINTFORMW 「把、把胖次脱下来……！？」
			CALL PRINT_STRW, @"「虽、虽然我明白了……呜呜、暴露了的话该怎么办啊……_H_」"
		ENDIF
	ENDIF
CASE "保持现在的装扮出発"
	IF CHECK_CLO("泳装姿")

	ELSEIF CHECK_CLO("体操服姿")
		PRINTFORMW 「想一起去店里吗？」
		PRINTFORMW 「我明白了，那么就换衣服……诶？ 穿这身去？」
		PRINTFORMW 「如果%CNAME("二人称")%觉得没问题的话倒是不介意……」
	ELSEIF CHECK_CLO("内衣姿") || CHECK_CLO("裸体")
		IF YOKUBO() || TALENT:恋慕
			PRINTFORMW 「不，果然穿着这身去店里……」
			PRINTFORMW 「一定要穿吗？ 哈、没办法……」
			CALL PRINT_STRW, @"「……作为交换，回来的时候请好好奖励我吧_H_」"
		ELSE
			PRINTFORMW 「不、果、果然穿着这身衣服去店里不行的！」
			CALL PRINT_STRW, "「无、无论如何也要穿吗？ 那、那样的话就没办法了……_H_」"
		ENDIF
	ENDIF
CASE "会計交代"
	;初回
	IF SEVENT(ARGS) == 0 || SEVENT("商品購入") == 0

	ELSE
		IF YOKUBO()
			PRINTFORML 「我明白了」
			CALL PRINT_STRW, "「一起去买些「好东西」吧_H_」"
		ELSE
			PRINTFORML 「诶！？　……那个，让%CNAME("一人称")%来买吗……？」
			CALL PRINT_STRW, "「呜……_H_我、我明白了哦……」"
		ENDIF
	ENDIF
CASE "询问有什么想要的东西"
	IF YOKUBO()
		PRINTFORML 「想要什么东西吗……」
		;ここで足りない/欲しい商品があると、それについて話す
		;話の順番とかを自然にするためにBIT変数を使って必須道具は管理
		;……だったが、ryusimi桑のおかげでグンとやりやすくなった
		;必須道具は潤滑液と避孕套
		;足りない商品がある時は、そちら優先
		NEED_TABLE = 
		SIF ITEM:潤滑液 <= 5
			NEED_TABLE += "潤滑液"
		SIF ITEM:避孕套 <= 10
			NEED_TABLE += "避孕套"

		IF STRCOUNTS(NEED_TABLE, "潤滑液", "避孕套")
			IF STRCOUNTS(NEED_TABLE, "潤滑液")
				PRINTFORML 「虽然不是想要的东西……但是买润滑液怎么样」
				PRINTFORML 「要是H的时候没有的话就麻烦了」
			ENDIF
			IF STRCOUNTS(NEED_TABLE, "避孕套")
				IF STRCOUNTS(NEED_TABLE, "潤滑液")
					PRINTFORML 「还有，避孕套也不够了呢」
				ELSE
					PRINTFORML 「……说起来，避孕套不够了呢」
				ENDIF
				IF VAGINA(MASTER)
					PRINTFORML 「如果%CNAME("二人称")%不想要却怀上了那就麻烦了，我觉得还是有比较好」
				ELSE
					PRINTFORML 「和%CNAME("一人称")%H的时候没有也没関系，但是和其他人做的时候还是有比较好」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF

		NEED_TABLE = 
		SIF ITEM:創口貼 <= 1
			NEED_TABLE += "創口貼"
		SIF ITEM:撮影機材 == 0
			NEED_TABLE += "撮影機材"
		SIF ITEM:飞机杯 == 0
			NEED_TABLE += "飞机杯"
		SIF ITEM:肛用振動棒 == 0
			NEED_TABLE += "肛用振動棒"
		SIF PENIS(MASTER) == 0 && ITEM:假阳具 == 0
			NEED_TABLE += "假阳具"

		SELECTCASE TEXTR(NEED_TABLE)
		CASE "創口貼"
			PRINTFORML 「……说起来、創口貼怎么样？」
			PRINTFORML 「如果不小心受伤了，有东西可以进行应急处置就安心了呢」
			PRINTFORML 「……而且，也许也可以用在H上呢？」
		CASE "撮影機材"
			PRINTFORML 「那么，买那个摄影器材怎么样？」
			PRINTFORML 「那台设备是著名品牌的，画质非常好」
			PRINTFORML 「而且是最新的，所以有各种方便的功能」
			PRINTFORML 「……一開始我还怀疑是不是真品，但无论怎么看都是真品呢」
			PRINTFORML 「但是非常便宜……不知道他们从哪里进货的……」
		CASE "飞机杯"
			PRINTFORML 「这个飞机杯怎么样」
			PRINTFORML 「看起来很舒服，而且可以多次使用」
			CALL PRINT_STRW, @"「……%CNAME("二人称")%、不想用这个飞机杯给%ADANA("阴茎")%上下撸上几下吗_H_」"
		CASE "肛用振動棒"
			PRINTFORML 「肛用振动棒怎么样」
			PRINTFORML 「有了这个就可以随意虐待%ADANA("肛門")%了，想摸其他地方也想抚慰%ADANA("肛門")%的话……这种时候会很好用的」
			CALL PRINT_STRW, @"「呵呵、如果被震动棒挿入的话会怎么样呢……_H_」"
		CASE "假阳具"
			PRINTFORML 「%CNAME("二人称")%、穿戴式假阴茎怎么样？」
			PRINTFORML 「如果有穿戴式假阴茎的话、不用具现或者变成扶她或男人也可以和女人做爱了」
		CASEELSE
			PRINTFORML 「……没有特别想要的」
		ENDSELECT
	ELSE
		PRINTFORML 「那、那个是……那个……」
		PRINTFORML 「那种事情，太害羞了，说不出口呢……」
	ENDIF
CASE "商品購入"
	;今買った物
	SELECTCASE STR:購入道具
	;ギリギリ通常商品の範疇or你用
	CASE "撮影機材"
		;初めて買うとき
		IF SEVENT(STR:購入道具) == 0
			PRINTFORMW 「……没想到这样的店里还有卖这样的摄像机……但是这些实在是很不错的东西哦……」
			PRINTFORMW 「那个，除了用在色色的事上，%CNAME("一人称")%也可以使用吗？」
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「啊、这个部件，可以用在%CNAME("二人称")%的摄影机上哦」
			CASE 1
				PRINTFORMW 「%CNAME("二人称")%！　这个、可是不得了的大发现哦！」
			CASEELSE
				PRINTFORMW 「啊、用了这个的话就能比以前拍得更漂亮了吧」
			ENDSELECT
		ENDIF
	CASE "潤滑液"
		IF YOKUBO()
			IF RAND:2
				CALL PRINT_STRW, @"「呵呵_H_润滑剂是很重要的东西呢_H_」"
			ELSE
				PRINTFORMW 「没有润滑液的话就麻烦了……」
			ENDIF
		ELSE
			PRINTFORMW 「嘛、嘛啊、这种程度的话……」
		ENDIF
	CASE "飞机杯"
		IF YOKUBO()
			SELECTCASE RAND:2
			CASE 0
				CALL PRINT_STRW, @"「为了能让%CNAME("二人称")%満足，会努力挑选的_H_」"
			CASEELSE
				CALL PRINT_STRW, @"「……这个怎么样呢？　感觉会很舒服哦_H_」"
			ENDSELECT
		ELSE
			PRINTFORMW 「……诶、要由%CNAME("一人称")%来选择这些吗……」
			PRINTFORMW 「因为是同性所以可以选择能接受的东西吗？　虽、虽然说不定确实是这样……」
		ENDIF
	CASE "肛用振動棒"
		IF SEVENT(STR:購入道具) == 0
			IF YOKUBO()
				CALL PRINT_STRW, @"「呵呵_H_去买肛門震动棒吧_H_」"
				CALL PRINT_STRW, @"「从现在開始让各种各样的人用屁股变得舒服起来吧，%CNAME("二人称")%_H_」"
			ELSE
				PRINTFORMW 「……诶！？　要、要买这个吗……？」
				PRINTFORMW 「……但是、比想象的要细……」
				PRINTFORMW 「……诶、这个，用在屁股里……？」
			ENDIF
		ENDIF
	CASE "避孕套"
		;この商品を初めて買わされた
		IF SEVENT(STR:購入道具) == 0

		ELSE
			IF YOKUBO()
				PRINTFORMW 「这个超薄型避孕套怎么样？　%CNAME("二人称")%」
			ELSE
				PRINTFORMW 「诶、啊、那个……那么、就用这个……」
			ENDIF
		ENDIF
	;えっちな商品
	CASEELSE
		IF YOKUBO()
			PRINTFORML 「咦……还有卖这种东西吗……」
			CALL PRINT_STRW, @"「呵呵呼、一买东西就会变得開心起来呢，%CNAME("二人称")%_H_」"
		ELSE
			PRINTFORML 「诶！？　…………诶……」
			PRINTFORMW 「……要、要买这个吗……？」
		ENDIF
	ENDSELECT
CASE "店内待機"
	;とりあえず何か一個買った時or33％で喋る
	SIF COND("買上品数") != 1 && RAND:3
		RETURN 0
	;胖次没収中
	IF COND("買い物胖次没収中")
		IF YOKUBO()
			CALL PRINT_STRW, @"「啊哈……_H_凉飕飕的……_H_」"
		ENDIF
	ELSE
		IF YOKUBO()
			CALL PRINT_STRW, @"「诶……居然还有这样的……_H_」"
		ELSE
			CALL PRINT_STRW, @"「诶、做、做到这样的地步……_H_」"
		ENDIF
	ENDIF
CASE "购物終了"
	;胖次没収中
	IF COND("買い物胖次没収中")
		IF YOKUBO()
			CALL PRINT_STRW, "「不穿胖次的事什么时候会被发现呢，心里躁动不安呢……_H_」"
		ELSE
			CALL PRINT_STRW, "「……………………好不容易才暴露出来的……！」"
			PRINTFORMW 「内、内衣之类的没有暴露真是太好了！」
		ENDIF
	;なにも買わなかった
	ELSEIF COND("買上品数") == 0
		IF YOKUBO()
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「没有什么想要的东西吗？」
			CASEELSE
				PRINTFORMW 「今天是只看不买了吗」
			ENDSELECT
		ELSE
			PRINTFORMW 「要回去了吗？」
			PRINTFORMW 「再稍微待一会儿……不！ 我们回去吧！ 是的！ 我们马上回去吧！」
		ENDIF
	ELSEIF CHECK_CLO("泳装姿") || CHECK_CLO("体操服姿")
	
	ELSE
		IF YOKUBO()
			CALL PRINT_STRW, @"「请再邀请我去吧，%CNAME("二人称")%_H_」"
		ELSE
			PRINTFORMW 「好、好羞耻……」
		ENDIF
	ENDIF
ENDSELECT

