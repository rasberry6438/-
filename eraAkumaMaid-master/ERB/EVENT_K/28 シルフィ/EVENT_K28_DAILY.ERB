;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K28, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		PRINTFORMW 
	CASE "朝舐陰"
		PRINTFORMW 
	CASE "朝足交"
		PRINTFORMW 
	CASE "朝の新妻PLAY"
		PRINTFORMW 
	CASE "朝乳交"
		PRINTFORMW 
	CASE "朝騎乗位"
		PRINTFORMW 
	CASE "朝Ａ騎乗位"
		PRINTFORMW 
	CASE "朝接吻"
		PRINTFORMW 
	CASE "朝添い寝"
		PRINTFORMW 
	CASE "朝口交", "朝六九式"
		PRINTFORMW 
	ENDSELECT
	;今回中出しされた回数を加算
	SIF COND("妊娠中") == 0
		CFLAG:中出漏出 += EX:Ｖ中出し
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
	IF EXP_GAIN("射精経験", PLAYER)
		PRINTFORMW 「困倦ていても射精は射精です」
		PRINTFORMW 「権利を享受したら義務を果たすのが筋ですよ」
	ELSE
		PRINTFORMW 「む、気づかれましたか…」
		PRINTFORMW 「もう少し寝たふりしててもかまいませんよ？」
	ENDIF
	CASE "朝騎乗位"
	IF EXP_GAIN("射精経験", PLAYER)
		PRINTFORMW 「言っときますけど、ボランティアでやってるわけじゃないですよ」
		PRINTFORMW 「魂は回収できるときに回収しておかないといけませんからね」
	ELSE
		PRINTFORMW 「まったく、しぶといですね」
		PRINTFORMW 「魂をもらうのも一苦労ですよ」
	ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「早上好」
			PRINTFORMW 「早く起きないと置いていきますよ」
		ELSEIF RAND:2
			PRINTFORMW 「遅刻しても知りませんよ、容赦なく見捨てますからね」
		ELSE
			PRINTFORMW 「……あと10秒以内に応答がなければ、実力行使に出ます」
		ENDIF
	ENDIF
CASE "一番乗り"
	IF RAND:2
		PRINTFORMW 「……%CNAME("二人称")%、先に座ってください」
		PRINTFORMW 「どのあたりが適正な距離かは%CNAME("一人称")%が決めます」
	ELSE
		PRINTFORMW 「うーわ……どうしますかねこれ…」
		PRINTFORMW 「ああ、独り言です、今のは気にしないでください」
	ENDIF
CASE "ただいま"
	IF RAND:2
		PRINTFORMW 「…ただいま戻りました」
		PRINTFORMW 「はぁ、一日通して人类を装うのは疲れますね…」
	ELSE
		PRINTFORMW 「戻りました」
		PRINTFORMW %CALLNAME:TARGET%を見ると、服の至るところに木の葉や草が付着している…
		PRINTFORMW 一体どこを通って来たのだろう…？
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF RAND:2
			PRINTFORMW 「……おかえりなさい」
			PRINTFORMW %CALLNAME:TARGET%正在认真地读着标题为%CALLNAME:MASTER%の方を見ようともしない…
		ELSE
			PRINTFORMW 「おかえりなさい」
			PRINTFORMW 「今後は日が落ちる前に帰宅することをお勧めしますよ」
		ENDIF
	ELSE
		IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*2/3 && RAND:2
			PRINTFORMW 「皆桑が待ってましたよ」
			PRINTFORMW 「…%CNAME("一人称")%はそうでも有ません」
		ELSE
			PRINTFORMW 「随分のんびりしてましたね」
			PRINTFORMW 「見習いたい姿勢です、皮肉じゃなく」
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORMW 「…来てあげましたよ」
		PRINTFORMW 「どうせ下らない用事なんでしょうけど」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		PRINTFORMW 「……ふーん、是这様吗…そういうことですか…」
		PRINTFORMW 何かを納得されてしまった…
	ELSE
		PRINTFORMW 「…どうも」
		PRINTFORMW 「……%CNAME("一人称")%に対して用事がないなら帰りますからね」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF RAND:2
			PRINTFORMW 「窮屈ですね、まったく…」
		ELSE
			PRINTFORMW 「圆圈で待合室ですね…」
		ENDIF
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「なんですか、何の用ですか」
	ELSE
		IF RAND:2
			PRINTFORMW 「お邪魔しますよ」
		ELSE
			PRINTFORMW 「来ましたよ」
		ENDIF
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 「お疲れ様です」
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「ああ、そういえば…いませんでしたね」
	ELSE
		PRINTFORMW 「来ましたか」
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「…%CNAME("一人称")%のことを、呼べば来るやつだと、そう思ってます？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		IF RAND:2
			PRINTFORMW 「……騒がしいわけです、まだやってたんですね」
		ELSE
			PRINTFORMW 「寝てなかったのか、ですか……？こっちのセリフです」
		ENDIF
	ELSE
		PRINTFORMW 「おっと、こちらにいましたか」
	ENDIF
CASE "抜け出された"
	IF RAND:2
		PRINTFORMW 「…やはりオスというのは複数のメスを欲しがるものなんですね」
	ELSE
		PRINTFORMW 「……」
		PRINTFORMW %CALLNAME:TARGET%正在认真地读着标题为不服そうに%CALLNAME:MASTER%の後姿を眺めている…
	ENDIF
CASE "便当"
	IF RAND:7 == 0
		PRINTFORMW 「フィードバックとしての味の感想を要求します」
		PRINTFORMW 「料理をするにも向上心を持って臨みたいんですよ」
		SIF TALENT:恋慕
			PRINTFORMW 「前輩に褒められて安易に自尊心を満たしたいというわけでは有ません。断じて違います。」
	ELSEIF RAND:6 == 0
		PRINTFORMW 「なんですか？『もっと肉要素が欲しかった』？」
		PRINTFORMW 「……完食してから文句を言われましても、ね？」
	ELSEIF RAND:5 == 0
		PRINTFORMW 「どうして食べないんですか、何も盛っちゃいませんよ」
		PRINTFORMW 「普段%CNAME("一人称")%をどんな目で見てるんですか。全く」
	ELSEIF RAND:4 == 0
		PRINTFORMW 「感謝して食べてくださいよ？」
		PRINTFORMW 「自然の恵みとあらゆる命に、あと特に作り手には絶対に」
	ELSEIF RAND:3 == 0
		PRINTFORMW 「命をいただくっていう意識は酱と持つべきです」
		PRINTFORMW 「い移開は%CNAME("二人称")%も土に還ってその連鎖に組み込まれるんですからね」
	ELSEIF RAND:2
		PRINTFORMW 「野菜が多すぎる…？気のせいです」
		PRINTFORMW 「文句言わず食べましょうよ、兒童っぽいにしても兒童じゃないんですから」
	ELSE
		PRINTFORMW 「あーあーあー、そんな一気に掻き込んで…」
		PRINTFORMW 「はぁ……きっちり味付け考えてるのがバカバカしくなります」
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"

ENDSELECT

