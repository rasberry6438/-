@EVENT_NTRDAILY, ARGS
#DIM LCOUNT
#DIMS ACTION
#DIMS EVENT_TABLE

SELECTCASE ARGS
CASE "电车痴汉"
	;PRINTFORMW NTRDAILY TEST

	;PRINTFORMW 当前对象：%CALLNAME:TARGET%

	IF CEVENT("电车乘坐可能",TARGET) == 0
		RETURN 0
	ENDIF
	PRINTFORML
	PRINTFORML
	PRINTFORML
	DRAWLINE
	PRINTFORMW -车站-
	PRINTFORMW 被要求乘坐电车上学的%CALLNAME:TARGET% 来到了车站 站在了站台前
	A = 1
	IF RAND:3 == 0
		PRINTFORMW 意外的是，今天乘车的人似乎不是很多
		A = 0
	ELSE
	 	PRINTFORMW 不出意外，今天等车的人还是一如既往的多  
	ENDIF
	IF CFLAG:公车猥亵次数 == 0
		PRINTFORMW %CALLNAME:TARGET%似乎有些紧张
	ELSEIF CFLAG:公车猥亵次数 < 5 || CFLAG:公车猥亵等级 < 1
		PRINTFORMW 想到之前在电车上被做的事情，%CALLNAME:TARGET%不由得紧张起来
	ELSEIF CFLAG:公车猥亵次数 < 15 || CFLAG:公车猥亵等级 < 3
		PRINTFORMW 想到之前在电车上被做的事情，%CALLNAME:TARGET%有些紧张，但心底却有些期待起来
	ELSEIF CFLAG:公车猥亵次数 < 30 || CFLAG:公车猥亵等级 < 4
		PRINTFORMW 想到之前在电车上被做的事情，%CALLNAME:TARGET%不由得有些期待起来
	ELSE
		PRINTFORMW 想到之前在电车上跟许多男人做的事情，%CALLNAME:TARGET%不由面色潮红，嘴里吐出了香甜的气息
	ENDIF
	PRINTFORMW
	;猥亵事件
	SIF CFLAG:公车猥亵等级 == 0
		EVENT_TABLE += "摸胸/摸屁股/身后抱住/"
	SIF CFLAG:公车猥亵等级 == 1
		EVENT_TABLE += "接吻/伸入胸罩/手交/";揉弄阴蒂/指插入/揉弄乳头/上下其手/
	SIF CFLAG:公车猥亵等级 == 2
		EVENT_TABLE += "座位口交/后背素股/浓密接吻/";
	IF ABL:露出癖 >= 10 && CFLAG:公车猥亵等级 == 2
		EVENT_TABLE += "上身完全暴露揉胸/"
	ENDIF
	SIF CFLAG:公车猥亵等级 == 3
		EVENT_TABLE += "两根口交/座位肛交/全裸浓密接吻素股/插入菊穴/";/乳交
	IF TALENT:処女 == 0 && CFLAG:公车猥亵等级 == 3
		EVENT_TABLE += "插入小穴/座位性交/";后背位性交/
	ENDIF
	SIF CFLAG:公车猥亵等级 == 4 && CEVENT("公车便器",TARGET) == 0
		EVENT_TABLE += "浴精/轮流肛交/当众自慰/多根口交/";
	IF TALENT:処女 == 0 && CFLAG:公车猥亵等级 == 4
		EVENT_TABLE += "双穴插入/4P/";轮奸/
	ENDIF
	SIF CFLAG:公车猥亵等级 == 4 && CEVENT("公车便器",TARGET)
		EVENT_TABLE += "公车便器"

	PRINTFORMW 跟随者人潮进入了车厢中、列车缓缓启动
	; PRINTFORMW %PRINT(EVENT_TABLE)%
	CALL EVENT_NTRBUS,EVENT_TABLE
	FOR LCOUNT, 0, 3
		; PRINTFORMW 111
		IF A == 0 && RAND:2 == 0
			PRINTFORMW 无事发生...
			CONTINUE
		ENDIF
		ACTION = %TEXTR(EVENT_TABLE)%
		PRINTFORMW %ACTION%
		CALL EVENT_NTRBUS,ACTION
		CFLAG:当前猥亵等级猥亵次数 += 1
		CFLAG:公车猥亵次数 += 1
		PRINTFORMW 电车猥亵次数 += 1
		PRINTFORMW
		PRINTFORMW 电车轰鸣着、不停地向前行驶着...
	NEXT


	;公车猥亵等级提升事件
	IF CFLAG:当前猥亵等级猥亵次数>5
		IF CFLAG:公车猥亵等级 == 0
			CALL EVENT_NTRBUS,"伸入胸罩"
		ELSEIF CFLAG:公车猥亵等级 == 1
			CALL EVENT_NTRBUS,"肉棒插入内裤中"
		ELSEIF CFLAG:公车猥亵等级 == 2
			IF TALENT:処女 == 0 && CFLAG:公车猥亵等级 == 2
				CALL EVENT_NTRBUS,"插入小穴"
			ELSE
				CALL EVENT_NTRBUS,"插入菊穴"
			ENDIF
		ELSEIF CFLAG:公车猥亵等级 == 3
			IF TALENT:処女 == 0 && CFLAG:公车猥亵等级 == 3
				CALL EVENT_NTRBUS,"双穴插入"
			ELSE
				CALL EVENT_NTRBUS,"浴精"
			ENDIF
		ELSEIF CFLAG:公车猥亵等级 == 4 && CEVENT("公车便器",TARGET) == 0
			IF TALENT:処女 == 0 && CFLAG:公车猥亵等级 == 4
				CALL EVENT_NTRBUS,"公车便器达成"
			ELSE
				PRINTFORMW 想要达到下一个猥亵等级，需要%CALLNAME:TARGET%为非处女
			ENDIF
		ENDIF
	ENDIF
	PRINTFORMW
	PRINTFORMW 过了许久、列车终于驶入了目的地、车门打开、将一位位乘客送出...
	CFLAG:公车猥亵次数 += 1
	PRINTFORML
	PRINTFORML
	PRINTFORML
	PRINTFORML
	PRINTFORML
	PRINTFORML
	PRINTFORML
	PRINTFORML
	PRINTFORML

ENDSELECT


