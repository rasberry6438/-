;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K23, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:称号
			CASE "実母"
				LOCALS = お母様_おかあ大人
			CASEELSE
				LOCALS = 貴方_你
		ENDSELECT
	ENDIF
CASE "主人"
	LOCALS = 主人様_主人大人
CASE "助手"
	IF NO:ASSI == 9
		LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
	ELSEIF NO:ASSI == 25
		LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
	ELSE
		LOCALS = %CALLNAME:ASSI%桑_%CSTR:ASSI:ふりがな%桑
	ENDIF
CASE "調教時二人称"
	;主人と助手の取り扱いが逆になっていたので修正
	;助手
	IF ASSIPLAY 
		IF NO:ASSI == 9
			LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
		ELSEIF NO:ASSI == 25
			LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
		ELSE
			LOCALS = %CALLNAME:ASSI%桑_%CSTR:ASSI:ふりがな%桑
		ENDIF
	;主人
	ELSE
		LOCALS = 主人様_主人大人
	ENDIF
ENDSELECT
RETURNF LOCALS

