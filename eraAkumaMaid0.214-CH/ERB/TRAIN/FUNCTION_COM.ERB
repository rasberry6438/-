;-------------------------------------------------
;派生指令などを判定する関数
;-------------------------------------------------
@CHECK_COM(ARGS, ARG)
#FUNCTION

VARSET LOCAL

SELECTCASE ARGS
CASE "快楽模索中"
	SIF ARG == 0
		ARG = SELECTCOM
	SELECTCASE ARG
	CASE TNUM("自慰")
		RETURNF 1
	CASEELSE
		SIF TEQUIP:推倒
			RETURNF 1
	ENDSELECT
	RETURNF 0
;穴の拡張 >= 阴茎 かを見る
CASE "完全挿入可能"
	SELECTCASE ARG
	CASE 60 TO 69
		SELECTCASE ABL:Ｖ拡張 - SIZE("阴茎", PLAYER)
		CASE IS > 0
			RETURNF 1
		CASE 0
			SIF EXP:Ｖ性交経験 >= 20 && CALCF("阴茎好き度", TARGET) >= SIZE("阴茎", PLAYER)
				RETURNF 1
			SIF COND("陶酔")
				RETURNF 1
		CASE -1
			SIF COND("陶酔") && EXP:Ｖ性交経験 >= 50 && CALCF("阴茎好き度", TARGET) >= SIZE("阴茎", PLAYER)
				RETURNF 1
		ENDSELECT
	CASE 70 TO 79
		SELECTCASE ABL:Ａ拡張 - SIZE("阴茎", PLAYER)
		CASE IS > 0
			RETURNF 1
		CASE 0
			SIF EXP:Ａ性交経験 >= 20 && CALCF("阴茎好き度", TARGET) >= SIZE("阴茎", PLAYER)
				RETURNF 1
			SIF COND("陶酔")
				RETURNF 1
		CASE -1
			SIF COND("陶酔") && EXP:Ａ性交経験 >= 50 && CALCF("阴茎好き度", TARGET) >= SIZE("阴茎", PLAYER)
				RETURNF 1
		ENDSELECT
	CASE 83 TO 88
		SELECTCASE ABL:尿道拡張 - SIZE("阴茎", PLAYER)
		CASE IS > 0
			RETURNF 1
		CASE 0
			SIF EXP:尿道性交経験 >= 20 && CALCF("阴茎好き度", TARGET) >= SIZE("阴茎", PLAYER)
				RETURNF 1
			SIF COND("陶酔")
				RETURNF 1
		CASE -1
			SIF COND("陶酔") && EXP:尿道性交経験 >= 50 && CALCF("阴茎好き度", TARGET) > SIZE("阴茎", PLAYER)
				RETURNF 1
		ENDSELECT
	CASE 120 TO 124
		SELECTCASE ABL:PLAYER:Ｖ拡張
		CASE IS > SIZE("阴茎", TARGET)
			RETURNF 1
		CASE SIZE("阴茎", TARGET)
			SIF EXP:PLAYER:Ｖ性交経験 >= 20
				RETURNF 1
		ENDSELECT
	CASE 125 TO 129
		SELECTCASE ABL:PLAYER:Ａ拡張
		CASE IS > SIZE("阴茎", TARGET)
			RETURNF 1
		CASE SIZE("阴茎", TARGET)
			SIF EXP:PLAYER:Ａ性交経験 >= 20
				RETURNF 1
		ENDSELECT
	ENDSELECT
	RETURNF 0
;その指令が喜欢的体位か判定
CASE "喜欢的体位"
	SELECTCASE ARG
	CASE 65, 75
		IF TEQUIP:推倒
			RETURNF GETBIT(TALENT:喜欢的体位, ARG - 59)
		ELSE
			RETURNF GETBIT(TALENT:喜欢的体位, ARG - 60)
		ENDIF
	CASE 60 TO 64, 70 TO 74
		RETURNF GETBIT(TALENT:喜欢的体位, ARG - 60)
	CASE 66 TO 69
		SIF COND("喜欢的体位中")
			RETURNF 1
	CASE 80
		RETURNF GETBIT(TALENT:喜欢的体位, 19)
	CASE 88
		IF TEQUIP:推倒
			RETURNF GETBIT(TALENT:喜欢的体位, 26)
		ELSE
			RETURNF GETBIT(TALENT:喜欢的体位, 25)
		ENDIF
	CASE 83 TO 88
		RETURNF GETBIT(TALENT:喜欢的体位, ARG - 63)
	ENDSELECT
CASE "連続実行"
	SIF PREVCOM == SELECTCOM && COND("前Ｔ調教者") == PLAYER
		RETURNF 1
CASE "潤滑液マッサージ"
	SIF COND("前Ｔ調教者") != PLAYER || TEQUIP:推倒
		RETURNF 0
	;ある程度の露出癖を必要にする
	SIF ABL:露出癖 < 3
		RETURNF 0
	SELECTCASE ARG
	CASE TNUM("愛撫")
		SIF PREVCOM == TNUM("潤滑液")
			RETURNF 1
	ENDSELECT
CASE "万謝的亲吻"
	SIF ARG == 0
		ARG = SELECTCOM
	SIF ARG != 1
		RETURNF 0
	;指导者相手には行わない
