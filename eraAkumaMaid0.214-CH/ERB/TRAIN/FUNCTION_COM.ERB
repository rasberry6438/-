	;50以上で完美契合なので、それ未満は意味が無いので0を返す
	SIF LOCAL < 50
		RETURNF 0
;Ｖに最後に入れたもののサイズを返す。入っていた振動棒のサイズ判定などに
CASE "Ｖ最新受け入れサイズ"
	RETURNF TOINT(EXTSTR(CSTR:ARG:Ｖ插入履歴, "：", "＠") )
CASE "Ａ最新受け入れサイズ"
	RETURNF TOINT(EXTSTR(CSTR:ARG:Ａ插入履歴, "：", "＠") )
CASE "尿道最新受け入れサイズ"
