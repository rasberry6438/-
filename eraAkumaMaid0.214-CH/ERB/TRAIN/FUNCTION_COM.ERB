		RETURNF 1
	ELSE
		SELECTCASE ARGS
		CASE "口内放尿"
			SIF COND("合意：飲尿PLAY", ARG)
				RETURNF 1
			SIF EXP:ARG:飲尿経験 < 5 - MAX(ABL:ARG:受虐属性, ABL:ARG:奉仕精神/3, ABL:ARG:欲望/3)
				RETURNF 0
		CASE "膣内放尿"
			SIF EXP:ARG:膣内浴尿経験 < 5 - MAX(ABL:ARG:受虐属性, ABL:ARG:奉仕精神/3, ABL:ARG:欲望/3)
				RETURNF 0
		CASE "子宮内放尿"
			SIF EXP:ARG:子宮浴尿経験 < 5 - MAX(ABL:ARG:受虐属性, ABL:ARG:奉仕精神/3, ABL:ARG:欲望/3)
