			RETURNF 0
		IF TALENT:ARG:淫壷
			LOCAL += 5
		ELSE
			;害羞
			SIF TALENT:ARG:害羞
				LOCAL -= 5
			;清楚
			SIF TALENT:ARG:清楚
				LOCAL -= 3
		ENDIF
	ELSEIF INSERT("Ａ挿入", ARG) && SELECTCOM >= 70 && SELECTCOM <= 79
		SIF EXP:ARG:Ａ経験 + EXP:ARG:Ａ性交経験*2 <= 50 || EXP:ARG:腰靈活経験 <= 20
			RETURNF 0
		SIF ABL:ARG:Ａ感覚 < 3
			LOCAL -= 5
		SELECTCASE PALAM:ARG:Ａ潤 + PALAM:PLAYER:Ｐ潤
		CASE IS < 3000
			LOCAL -= 5
		CASE IS < 10000
			LOCAL -= 2
		ENDSELECT
		IF TALENT:ARG:尻穴狂い
			LOCAL += 5
		ELSE
			;害羞
			SIF TALENT:ARG:害羞
				LOCAL -= 5
			;清楚
			SIF TALENT:ARG:清楚
				LOCAL -= 3
		ENDIF
	ELSEIF INSERT("尿道挿入", ARG) && SELECTCOM >= 83 && SELECTCOM <= 88
		SIF EXP:ARG:尿道経験 + EXP:ARG:尿道性交経験*2 <= 50 || EXP:ARG:腰靈活経験 <= 20 || PENIS(ARG)
			RETURNF 0
		SIF TALENT:ARG:尿道性感 == 0
			LOCAL -= 5

		IF TALENT:ARG:尿道狂
			LOCAL += 5
		ELSE
			;害羞
			SIF TALENT:ARG:害羞
				LOCAL -= 5
			;清楚
			SIF TALENT:ARG:清楚
				LOCAL -= 3
		ENDIF
	ELSE
		RETURNF 0
	ENDIF
	;強気
	IF TALENT:ARG:強気
		LOCAL += 2
		;PLAYERがあまりイってない
		SIF EXP_GAIN("强制绝顶回数", ARG) < 5
			LOCAL += 1
	ENDIF
	;好色
	SIF TALENT:ARG:好色
		LOCAL += 1
	;淫乱
	SIF TALENT:ARG:淫乱
		LOCAL += 2
	;腰靈活
	SIF ABL:ARG:腰靈活 || EXP:ARG:腰靈活経験 >= 100
		LOCAL += 1
	SIF COND("発情期", ARG)
		LOCAL += 2
	SIF LOCAL >= RAND:3 + 3
		RETURNF 1
