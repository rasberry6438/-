	RETURNF 1
CASE "阴茎"
	SIF ARG == 0
		ARG = PLAYER
	;假阳具
	SIF PENIS(ARG) == 0
		RETURNF 3

	LOCAL = 1
CASE "振動棒"
	SIF ARG == 0
		ARG = TARGET
	SIF TEQUIP:ARG:振動棒 == 0
		RETURNF 0
	LOCAL = 3
