		RETURNF TEQUIP:ARG:体位
CASE "尿道挿入"
	SIF ARG:1 > 0 && TEQUIP:(ARG:1):尿道挿入 != ARG
		RETURNF 0
	SIF GETBIT(TEQUIP:ARG:体位, 12)
		RETURNF TEQUIP:ARG:体位
CASE "複数挿入"
	SIF GETBIT(TEQUIP:ARG:体位, 10) && GETBIT(TEQUIP:ARG:体位, 11) && GETBIT(TEQUIP:ARG:体位, 12)
