;-------------------------------------------------
;物の尺寸を判定する関数
;-------------------------------------------------
@SIZE(ARGS, ARG)
#FUNCTION
#DIM VALUE

VARSET LOCAL

SELECTCASE ARGS
;弛緩していると深さはともかく受け入れられるサイズ自体は増える
CASE "Ｖ拡張", "Ａ拡張"
	SIF ARG == 0
		ARG = TARGET
	VALUE = ABL:ARG:ARGS
	SIF TEQUIP:ARG:弛緩
		VALUE += 1

CASE "尿道探针"
	SIF ARG == 0
		ARG = TARGET
	RETURNF TEQUIP:ARG:尿道探针
CASE "指の直径"
	SIF ARG == 0
		ARG = TARGET
	;薬指の一番粗大的部分の値の平均値を使う
	;身高を(50+13x)cmとすると指は(7+x)mmぐらいか
	;小只的女性で13mm、高大な女性でも16～17mmぐらい。男性だとそれぞれ+2mm
	VALUE = MAX(BASE:ARG:身高 - 50, 0)/13 + 7

	SIF TALENT:ARG:男性
		VALUE += 2
CASE "指"
	RETURNF 0

CASE "初期阴茎尺寸"
	SIF ARG == 0
		ARG = PLAYER
	RETURNF COND("初期阴茎", ARG)

CASE "基本阴茎尺寸"
	SIF ARG == 0
		ARG = PLAYER
	RETURNF TALENT:ARG:阴茎

CASE "阴茎の直径"
	SIF ARG == 0
		ARG = PLAYER

	;女性は假阳具
	IF TALENT:ARG:女性 && STATE("具現", ARG) == 0
		VALUE = 7
	ELSE
		VALUE = SIZE("阴茎", ARG)*7
	ENDIF

	RETURNF 21 + VALUE + BASE:ARG:識別番号%7

CASE "阴茎"
	SIF ARG == 0
		ARG = PLAYER
	;女性は假阳具
	SIF TALENT:ARG:女性 && STATE("具現", ARG) == 0
		RETURNF 1
	VALUE = SIZE("基本阴茎尺寸", ARG)

	IF TALENT:ARG:一般人
		SELECTCASE RAND:100
		CASE IS >= 95
			VALUE = 3
		CASE IS >= 85
			VALUE = 2
		CASE IS >= 10
			VALUE = 1
		CASE 0
			VALUE = 0
		ENDSELECT
	ENDIF
CASE "Ｗ阴茎"
	IF PLAYER:1
		RETURNF 1 + SIZE("阴茎", MASTER) + SIZE("阴茎", PLAYER:1)
	ELSE
		RETURNF 1 + SIZE("阴茎", MASTER)
	ENDIF
CASE "振動棒"
	SIF ARG == 0
		ARG = TARGET
	SIF TEQUIP:ARG:振動棒 == 0
		RETURNF 0

	SELECTCASE GETBITS(TEQUIP:ARG:振動棒, 0, 9)
	CASE 1p0
		VALUE = 0
	CASE 1p1
		VALUE = 1
	CASE 1p2
		VALUE = 2
	CASE 1p3
		VALUE = 3
	CASE 1p4
		VALUE = 4
	CASE 1p5
		VALUE = 5
	ENDSELECT

CASE "Ａ振動棒"
	SIF ARG == 0
		ARG = TARGET
	SIF TEQUIP:ARG:肛用振動棒 == 0
		RETURNF 0

	SELECTCASE GETBITS(TEQUIP:ARG:肛用振動棒, 0, 9)
	CASE 1p0
		VALUE = 0
	CASE 1p1
		VALUE = 1
	CASE 1p2
		VALUE = 2
	CASE 1p3
		VALUE = 3
	CASE 1p4
		VALUE = 4
	CASE 1p5
		VALUE = 5
	ENDSELECT

CASE "肛塞"
	SIF ARG == 0
		ARG = TARGET
	RETURNF TEQUIP:ARG:肛塞

CASE "尿道振動棒"
	SIF ARG == 0
		ARG = TARGET
	SIF TEQUIP:ARG:尿道振動棒 == 0
		RETURNF 0

	SELECTCASE GETBITS(TEQUIP:ARG:尿道振動棒, 0, 9)
	CASE 1p0
		VALUE = -1
	CASE 1p1
		VALUE = 0
	CASE 1p2
		VALUE = 1
	CASE 1p3
		VALUE = 2
	CASE 1p4
		VALUE = 3
	CASE 1p5
		VALUE = 4
	CASE 1p6
		VALUE = 5
	ENDSELECT

CASE "尿道振動棒の直径"
	SIF ARG == 0
		ARG = TARGET
	SIF TEQUIP:ARG:尿道振動棒 == 0
		RETURNF 0

	SELECTCASE GETBITS(TEQUIP:ARG:尿道振動棒, 0, 9)
	CASE 1p0
		RETURNF 18
	CASE 1p1
		RETURNF 24
	CASE 1p2
		RETURNF 31
	CASE 1p3
		RETURNF 38
	CASE 1p4
		RETURNF 45
	CASE 1p5
		RETURNF 52
	CASE 1p6
		RETURNF 59
	CASE 1p7
		RETURNF 66
	ENDSELECT

CASE "Ｗフィスト"
	SIF ARG == 0
		ARG = PLAYER
	RETURNF SIZE("片手フィスト", ARG) + 1
CASE "片手フィスト", "フィスト"
	SIF ARG == 0
		ARG = PLAYER
	VALUE = 5
	SIF TALENT:ARG:男性 == 0
		VALUE -= 1
	SIF TALENT:ARG:娇小 || TALENT:ARG:伪娘
		VALUE -= 1
CASE "二本フィスト"
	SIF ARG == 0
		ARG = PLAYER
	VALUE = SIZE("片手フィスト", ARG)
	RETURNF MAX(VALUE*2 - 1, VALUE + 2)
ENDSELECT

RETURNF VALUE

