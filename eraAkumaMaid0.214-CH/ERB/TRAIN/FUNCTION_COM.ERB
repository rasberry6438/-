;-------------------------------------------------
;ひょっとしたら最も重要かもしれない値である情緒の計算
;最低が０で最高は240
;-------------------------------------------------
@MOOD()
#FUNCTION
#DIM VALUE_MOOD

;基本はFLAG:情緒
VALUE_MOOD = FLAG:情緒

;恭順と欲情で25まで
SELECTCASE PALAM:恭順 + PALAM:欲情
CASE IS > 240000
	VALUE_MOOD += 25
;MAX25
CASE IS > 120000
	VALUE_MOOD += (PALAM:恭順 + PALAM:欲情 - 120000)/24000 + 20
;MAX20
CASE IS > 60000
	VALUE_MOOD += (PALAM:恭順 + PALAM:欲情 - 60000)/12000 + 15
;MAX15
CASE IS > 30000
	VALUE_MOOD += (PALAM:恭順 + PALAM:欲情 - 30000)/6000 + 10
;MAX10
CASE IS > 10000
	VALUE_MOOD += (PALAM:恭順 + PALAM:欲情 - 10000)/4000 + 5
;MAX5
CASEELSE
	VALUE_MOOD += (PALAM:恭順 + PALAM:欲情)/2000
ENDSELECT

;恐怖、反感でキツい减少補正
VALUE_MOOD -= (NOW_PALAM("恐怖") + NOW_PALAM("反感") )/100

IF TARGET > 0 && CFLAG:情緒補正
	VALUE_MOOD += CFLAG:情緒補正
	VALUE_MOOD = LIMIT(VALUE_MOOD, CFLAG:情緒補正, 240)
ELSE
	VALUE_MOOD = LIMIT(VALUE_MOOD, 0, 240)
ENDIF

RETURNF VALUE_MOOD

;-------------------------------------------------
;情緒の値を爱心マークの個数に変換(减少の値も取り得る)
;-------------------------------------------------
@MOOD_HEART()
#FUNCTION

SELECTCASE MOOD()
;-1～-30で-1を返すようにする
CASE IS < 0
	LOCAL = (MOOD() - 29) / 30
CASEELSE
	LOCAL = MOOD() / 30
ENDSELECT
RETURNF LOCAL

;-------------------------------------------------
;指令選択による情緒増減の格子
;ARGには指令番号が代入される（通常はSELECTCOM）
;-------------------------------------------------
@MOOD_COM, ARG
#DIM ADD_MOOD

ADD_MOOD = 0
SELECTCASE ARG
CASE TNUM("愛撫"), TNUM("肛門愛撫"), TNUM("胸愛撫"), TNUM("阴部愛撫"), TNUM("指挿入"), TNUM("舐肛")
	ADD_MOOD += 2
CASE TNUM("舐陰")
	SIF CHECK_COM("万謝的亲吻", ARG)
		ADD_MOOD += 10
	ADD_MOOD += 3
CASE TNUM("接吻")
	;開幕接吻はBOUNS
	SIF CHECK_COM("テンダー接吻", ARG)
		ADD_MOOD += 10
	ADD_MOOD += 5
;其他の愛撫
CASE 0 TO 29
	ADD_MOOD += 3
;挿入
CASE 60 TO 89
	SIF TRAIN_GENRE("対面座位", ARG)
		ADD_MOOD += 3
	SIF COND("喜欢的体位中")
		ADD_MOOD += 3
	ADD_MOOD += 1
;奉仕
CASE 90 TO 119
	SIF CHECK_COM("阴茎接吻", ARG)
		ADD_MOOD += 10
	SIF CHECK_COM("展示肉棒", ARG)
		ADD_MOOD += 10
	ADD_MOOD += 3
CASE TNUM("抱上去")
	;開幕だとBOUNS
	SIF CHECK_COM("温柔地抱了", ARG)
		ADD_MOOD += 10
	ADD_MOOD += 8
ENDSELECT

;爱心数が少ない時にはBOUNS
SELECTCASE MOOD_HEART()
CASE 0
	TIMES ADD_MOOD, 2.00
CASE 1
	TIMES ADD_MOOD, 1.50
CASE 4, 5
	TIMES ADD_MOOD, 0.67
CASE IS >= 6
	TIMES ADD_MOOD, 0.33
ENDSELECT

;初回選択で無いなら効果は半減
SIF CHOSECOM(ARG)
	ADD_MOOD /= 2

FLAG:情緒 += ADD_MOOD


