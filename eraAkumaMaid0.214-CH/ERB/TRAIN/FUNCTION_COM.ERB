		ARG = TARGET
	SIF TEQUIP:ARG:肛用振動棒 == 0
		RETURNF 0
	IF COND("肛門拉珠", ARG)
		LOCAL = 1
	ELSE
		SELECTCASE GETBITS(TEQUIP:ARG:肛用振動棒, 0, 9)
		CASE 1p0
			LOCAL = 2
		CASEELSE
			LOCAL = 3
		ENDSELECT
	ENDIF
CASE "尿道振動棒"
	SIF ARG == 0
		ARG = TARGET
	SIF TEQUIP:ARG:尿道振動棒 == 0
		RETURNF 0
	SELECTCASE GETBITS(TEQUIP:ARG:尿道振動棒, 0, 9)
	CASE 1p0, 1p1, 1p2
		LOCAL = 2
	CASEELSE
		LOCAL = 3
	ENDSELECT

CASE "フィスト"
	SIF ARG == 0
		ARG = PLAYER
	LOCAL = 2
	;男性
	SIF TALENT:ARG:男性
		LOCAL += 1
ENDSELECT
