;	PRINTFORM  (%CALLNAME:PLAYER%)
;ELSEIF PLAYER == ASSI
;	PRINTFORM （%TEXTS("助手の名称")%）
;ENDIF
;PRINTL 

FOR LCOUNT, 2, CHARANUM
	SIF NOWEX:LCOUNT:吸能 == 0
		CONTINUE

	EXP:LCOUNT:吸精経験 += 1
	PRINTFORML 吸精経験＋1\@ LCOUNT != TARGET ? （%CALLNAME:LCOUNT%） # \@

	;体力回復はTCVAR:体力回復 で管理する
	TCVAR:LCOUNT:体力回復 += NOWEX:LCOUNT:吸能*100
	PRINTFORML %CALLNAME:LCOUNT%的体力回復了{NOWEX:LCOUNT:吸能*100}点！

	BASE:LCOUNT:精力 += NOWEX:LCOUNT:吸能*3
	TCVAR:LCOUNT:精力減少 -= NOWEX:LCOUNT:吸能*3
	PRINTFORML 而且%CALLNAME:LCOUNT%的精力回復了{NOWEX:LCOUNT:吸能*3}点！
