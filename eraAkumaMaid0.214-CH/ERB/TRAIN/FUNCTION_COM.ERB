		ELSEIF GETBIT(TEQUIP:ARG:体位, 1)
			RETURNF 71
		ELSEIF GETBIT(TEQUIP:ARG:体位, 2)
			RETURNF 72
		ELSEIF GETBIT(TEQUIP:ARG:体位, 3)
			RETURNF 73
		ELSEIF GETBIT(TEQUIP:ARG:体位, 4)
			RETURNF 74
		ELSEIF GETBIT(TEQUIP:ARG:体位, 5)
			RETURNF 75
		ENDIF
	ELSE
		IF GETBIT(TEQUIP:ARG:体位, 0)
			RETURNF 125
		ELSEIF GETBIT(TEQUIP:ARG:体位, 1)
			RETURNF 126
		ELSEIF GETBIT(TEQUIP:ARG:体位, 5)
			RETURNF 127
		ELSEIF TEQUIP:ARG:体位
			RETURNF 125
		ENDIF
	ENDIF
CASE "尿道対応指令"
	IF GETBIT(TEQUIP:ARG:体位, 0)
		RETURNF 83
	ELSEIF GETBIT(TEQUIP:ARG:体位, 1)
		RETURNF 84
	ELSEIF GETBIT(TEQUIP:ARG:体位, 2)
		RETURNF 85
	ELSEIF GETBIT(TEQUIP:ARG:体位, 3)
		RETURNF 86
	ELSEIF GETBIT(TEQUIP:ARG:体位, 4)
		RETURNF 87
	ELSEIF GETBIT(TEQUIP:ARG:体位, 5)
		RETURNF 88
	ENDIF
CASE "狂野摇摆"
	SIF TEQUIP:ARG:体位
		RETURNF GETBIT(TEQUIP:ARG:特殊行動, 0)
ENDSELECT
RETURNF 0

;-------------------------------------------------
;ARGとARG:1の挿入＆挿入状態を解除させる関数
;通常はARGとARG:1にTARGETとPLAYERを代入する
;高速化のために@INSERTでできることを別の関数で行う事も有る
;TEQUIP:避孕套については、この関数で挿入が解除された場合に限り取下
;-------------------------------------------------
@RESET_INSERT, ARG, ARG:1
#DIM LCOUNT
;避孕套取り外しの判定に使用(避孕套使用中の場合にはこれが１以上になる)
#DIM IS_CONDOM, 100

VARSET IS_CONDOM
;避孕套を取下かどうかの格子
FOR LCOUNT, 1, CHARANUM
	SELECTCASE LCOUNT
	CASE MASTER, ASSI, PLAYER, PLAYER:1, TARGET, TARGET:1
		SIF TEQUIP:LCOUNT:避孕套
			IS_CONDOM:LCOUNT = MAX(TEQUIP:LCOUNT:Ｖ挿入, TEQUIP:LCOUNT:Ａ挿入, TEQUIP:LCOUNT:尿道挿入)
	ENDSELECT
NEXT

;ARGに関するTEQUIP:Ｖ挿入とTEQUIP:Ａ挿入を初期化する
FOR LOCAL, 0, (ARG > 0) + (ARG:1 > 0)
	;IF INSERT("挿入", ARG:LOCAL) || INSERT("挿入相手", ARG:LOCAL)
	IF TEQUIP:ARG:体位 || MAX(FINDCHARA(TEQUIP:Ｖ挿入, ARG:LOCAL), FINDCHARA(TEQUIP:Ａ挿入, ARG:LOCAL), FINDCHARA(TEQUIP:尿道挿入, ARG:LOCAL), 0)
		;挿入している相手のフラグを消去
		FOR LCOUNT, 1, CHARANUM
			SELECTCASE LCOUNT
			CASE MASTER, ASSI, PLAYER, PLAYER:1, TARGET, TARGET:1
				SIF TEQUIP:LCOUNT:Ｖ挿入 == ARG:LOCAL
					TEQUIP:LCOUNT:Ｖ挿入 = 0
				SIF TEQUIP:LCOUNT:Ａ挿入 == ARG:LOCAL
					TEQUIP:LCOUNT:Ａ挿入 = 0
				SIF TEQUIP:LCOUNT:尿道挿入 == ARG:LOCAL
					TEQUIP:LCOUNT:尿道挿入 = 0
			ENDSELECT
		NEXT
	ENDIF

	TEQUIP:(ARG:LOCAL):Ｖ挿入 = 0
	TEQUIP:(ARG:LOCAL):Ａ挿入 = 0
	TEQUIP:(ARG:LOCAL):尿道挿入 = 0
NEXT


FOR LCOUNT, 1, CHARANUM
	SELECTCASE LCOUNT
	CASE MASTER, ASSI, PLAYER, PLAYER:1, TARGET, TARGET:1
		;挿入を解除された全ての角色の避孕套を取下
		SIF IS_CONDOM:LCOUNT && MAX(TEQUIP:LCOUNT:Ｖ挿入, TEQUIP:LCOUNT:Ａ挿入, TEQUIP:LCOUNT:尿道挿入) == 0
			CALL SETFLAG, "避孕套取り外し", LCOUNT, TEQUIP:LCOUNT:避孕套

		;挿入されていない全ての角色のTEQUIP:体位を初期化
		SIF MAX(FINDCHARA(TEQUIP:Ｖ挿入, LCOUNT), FINDCHARA(TEQUIP:Ａ挿入, LCOUNT), FINDCHARA(TEQUIP:尿道挿入, LCOUNT), 0)
			CONTINUE
		TEQUIP:LCOUNT:体位 = 0
		;狂野摇摆初期化
		CLEARBIT TEQUIP:LCOUNT:特殊行動, 0
	ENDSELECT
NEXT



;-------------------------------------------------
;挿入の関数を設定する関数
;ARGに挿入される側、ARG:1に挿入する側、ARG:2にSELECTCOM
;ARGSに"解除判定"が代入された場合には解除されるかを格子するのみで挿入フラグの発生は無い
;TEQUIP:体位はビットで状態を管理する
;～9bit	体位、10bit Ｖ性交、11bit Ａ性交、12bit 尿道性交
;TEQUIP:避孕套については、この関数で挿入が解除された場合に限り取下
;-------------------------------------------------
@SET_INSERT, ARG, ARG:1, ARG:2, ARGS
#DIM LCOUNT
#DIM IS_CONDOM, 100
#DIM MEMO_SELECTCOM
;竿役
#DIM ACTOR

SIF ARG <= 0 || ARG:1 <= 0
	RETURN 0

ACTOR = ARG:1

MEMO_SELECTCOM = SELECTCOM
SELECTCOM = ARG:2
VARSET IS_CONDOM
;避孕套を取下かどうかの格子
FOR LCOUNT, 1, CHARANUM
	SELECTCASE LCOUNT
	CASE MASTER, ASSI, PLAYER, PLAYER:1, TARGET, TARGET:1
		SIF TEQUIP:LCOUNT:避孕套
			IS_CONDOM:LCOUNT = MAX(TEQUIP:LCOUNT:Ｖ挿入, TEQUIP:LCOUNT:Ａ挿入, TEQUIP:LCOUNT:尿道挿入)
	ENDSELECT
NEXT

VARSET LOCAL

SELECTCASE SELECTCOM
;挿入指令
CASE 60 TO 79, 83 TO 88, 120 TO 129
	IF ARGS != "解除判定"
		;これから挿入する側が挿入されている場合は挿入を解除する
		SELECTCASE SELECTCOM
		CASE 60 TO 79, 83 TO 88
			SIF INSERT("挿入", ACTOR)
				CALL RESET_INSERT, ACTOR
		CASE 120 TO 129
			SIF INSERT("挿入", ARG)
				CALL RESET_INSERT, ARG
		ENDSELECT

		;挿入される側の体位フラグ
		SELECTCASE SELECTCOM
		CASE 60, 70, 83
			TEQUIP:ARG:体位 = 1p0
		CASE 61, 71, 84
			TEQUIP:ARG:体位 = 1p1
		CASE 62, 72, 85
			TEQUIP:ARG:体位 = 1p2
		CASE 63, 73, 86
			TEQUIP:ARG:体位 = 1p3
		CASE 64, 74, 87
			TEQUIP:ARG:体位 = 1p4
		CASE 65, 75, 88
			TEQUIP:ARG:体位 = 1p5
		;子宮挿入
		CASE 68
			IF TEQUIP:ARG:体位 == 0
				IF TEQUIP:ARG:推倒
					TEQUIP:ARG:体位 = 1p5
				ELSE
					TEQUIP:ARG:体位 = 1p0
				ENDIF
			ENDIF
		;以下は逆挿入
		CASE 120, 125
			TEQUIP:ACTOR:体位 = 1p0
		CASE 121, 126
			TEQUIP:ACTOR:体位 = 1p1
		CASE 122, 127
			TEQUIP:ACTOR:体位 = 1p5
		ENDSELECT

		;挿入する側がどこに入れたかのフラグ
		SELECTCASE SELECTCOM
		CASE 60 TO 69
			SETBIT TEQUIP:ARG:体位, 10
			TEQUIP:ACTOR:Ｖ挿入 = ARG
			TEQUIP:ACTOR:Ａ挿入 = 0
			TEQUIP:ACTOR:尿道挿入 = 0
		CASE 70 TO 79
			SETBIT TEQUIP:ARG:体位, 11
			TEQUIP:ACTOR:Ｖ挿入 = 0
			TEQUIP:ACTOR:Ａ挿入 = ARG
			TEQUIP:ACTOR:尿道挿入 = 0
		CASE 83 TO 88
			SETBIT TEQUIP:ARG:体位, 12
			TEQUIP:ACTOR:Ｖ挿入 = 0
			TEQUIP:ACTOR:Ａ挿入 = 0
			TEQUIP:ACTOR:尿道挿入 = ARG
		;以下は逆挿入
		CASE 120 TO 124
			SETBIT TEQUIP:ACTOR:体位, 10
			TEQUIP:ARG:Ｖ挿入 = ACTOR
			TEQUIP:ARG:Ａ挿入 = 0
			TEQUIP:ARG:尿道挿入 = 0
		CASE 125 TO 129
			SETBIT TEQUIP:ACTOR:体位, 11
			TEQUIP:ARG:Ｖ挿入 = 0
			TEQUIP:ARG:Ａ挿入 = ACTOR
			TEQUIP:ARG:尿道挿入 = 0
		ENDSELECT
		;立位判定
		IF CONFIG("立位")
			SIF INSERT("立位可能体位", ARG)
				SETBIT TEQUIP:ARG:体位, 20
			SIF INSERT("立位可能体位", ACTOR)
				SETBIT TEQUIP:ACTOR:体位, 20
		ENDIF
	ENDIF
;以下は指令実行時の解除判定。それぞれ逆挿入状態かどうかも格子
CASE TNUM("自慰")
	SIF INSERT("挿入", ACTOR) || CHECK_COM("挿入自慰") == 0
		CALL RESET_INSERT, ARG, ACTOR
CASE TNUM("阴部愛撫")
	SIF INSERT("挿入", ACTOR) || CHECK_COM("擦拭", SELECTCOM)
		CALL RESET_INSERT, ARG, ACTOR
;指挿入
CASE TNUM("指挿入")
	TEQUIP:ARG:Ｖ挿入 = 0
	;ACTORがARGに挿入している場合には解除
	SIF INSERT("Ｖ挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:Ｖ挿入 = 0
	TEQUIP:ARG:尿道挿入 = 0
	SIF INSERT("尿道挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:尿道挿入 = 0
;尿道指挿入(TARGETが阴茎を挿入している場合には挿入解除)
CASE TNUM("尿道指挿入")
	IF PENIS(ARG)
		TEQUIP:ARG:Ｖ挿入 = 0
		TEQUIP:ARG:Ａ挿入 = 0
		TEQUIP:ARG:尿道挿入 = 0
	ENDIF
	SIF INSERT("尿道挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:尿道挿入 = 0
;给对方手交(TARGETが誰かを犯している場合には挿入解除)
CASE TNUM("给对方手交")
	TEQUIP:ARG:Ｖ挿入 = 0
	TEQUIP:ARG:Ａ挿入 = 0
	TEQUIP:ARG:尿道挿入 = 0
CASE TNUM("手淫"), TNUM("内褲冲"), TNUM("飞机杯冲")
	TEQUIP:ACTOR:Ｖ挿入 = 0
	TEQUIP:ACTOR:Ａ挿入 = 0
	TEQUIP:ACTOR:尿道挿入 = 0
CASE 0, 2, 5 TO 7
	;背中側の人には色々できない(逆挿入の時)
	SIF INSERT("挿入", ACTOR) && INSERT("後背位", ACTOR) + INSERT("背面座位", ACTOR)
		CALL RESET_INSERT, ARG, ACTOR
CASE TNUM("扒開阴道")
	TEQUIP:ARG:Ｖ挿入 = 0
	SIF INSERT("Ｖ挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:Ｖ挿入 = 0
	TEQUIP:ARG:尿道挿入 = 0
	SIF INSERT("尿道挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:尿道挿入 = 0
	SIF INSERT("挿入", ACTOR) || INSERT("後背位", ARG) || INSERT("対面座位", ARG) || INSERT("背面座位", ARG)
		CALL RESET_INSERT, ARG, ACTOR
CASE TNUM("扒開菊穴")
	TEQUIP:ARG:尿道挿入 = 0
	SIF INSERT("尿道挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:尿道挿入 = 0
	TEQUIP:ARG:Ａ挿入 = 0
	SIF INSERT("Ａ挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:Ａ挿入 = 0
	SIF INSERT("挿入", ACTOR) || (INSERT("後背位", ARG) == 0 && INSERT("騎乗位", ARG) == 0)
		CALL RESET_INSERT, ARG, ACTOR
;洗面奶は高大でない場合は、巨乳or巨根でないと挿入解除
CASE TNUM("洗面奶")
	;从后面将は駄目
	IF INSERT("後背位", ARG) || INSERT("背面座位", ARG) || INSERT("後背位", ACTOR) || INSERT("背面座位", ACTOR)
		CALL RESET_INSERT, ARG, ACTOR
	;騎乗位は相手と同程度の背がないと駄目
	ELSEIF INSERT("騎乗位") || INSERT("騎乗位", ACTOR)
		;挿入側が高大
		IF TALENT:ACTOR:高大
			SIF TALENT:ARG:高大 == 0 && TALENT:ARG:巨乳 == 0 && SIZE("阴茎", ACTOR) < 2
				CALL RESET_INSERT, ARG, ACTOR
		;挿入側が普通身高
		ELSEIF TALENT:ACTOR:娇小 == 0
			SIF TALENT:ARG:娇小 && TALENT:ARG:巨乳 == 0 && SIZE("阴茎", ACTOR) < 2
				CALL RESET_INSERT, ARG, ACTOR
		ENDIF
	ENDIF
;放尿は尿道挿入されながらは無理
CASE TNUM("放尿")
	SIF INSERT("尿道挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:尿道挿入 = 0
;そのグループにおいては稀な挿入継続指令
CASE 30, 130, 138, 139, 170, 174, 176, 177

;問答無用で阴茎を拔出指令(調教者が別の人ならば、その限りでは有ません)
CASE IS < 0, 1, 4, 9, 11, 12, 16, 30 TO 59, 80 TO 119, 130 TO 169, 170 TO 199, 260 TO 279
	TEQUIP:ARG:Ｖ挿入 = 0
	TEQUIP:ARG:Ａ挿入 = 0
	TEQUIP:ARG:尿道挿入 = 0
	SIF INSERT("Ｖ挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:Ｖ挿入 = 0
	SIF INSERT("Ａ挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:Ａ挿入 = 0
	SIF INSERT("尿道挿入相手", ARG) == ACTOR
		TEQUIP:ACTOR:尿道挿入 = 0
ENDSELECT

FOR LCOUNT, 1, CHARANUM
	SELECTCASE LCOUNT
	CASE MASTER, ASSI, PLAYER, PLAYER:1, TARGET, TARGET:1
		IF TEQUIP:LCOUNT:避孕套
			;挿入を解除された全ての角色の避孕套を取下
			IF IS_CONDOM:LCOUNT && MAX(TEQUIP:LCOUNT:Ｖ挿入, TEQUIP:LCOUNT:Ａ挿入, TEQUIP:LCOUNT:尿道挿入) == 0
				CALL SETFLAG, "避孕套取り外し", LCOUNT, TEQUIP:LCOUNT:避孕套
			;挿入中なら相手を記録
			ELSE
				TEQUIP:LCOUNT:避孕套 = MAX(TEQUIP:LCOUNT:Ｖ挿入, TEQUIP:LCOUNT:Ａ挿入, TEQUIP:LCOUNT:尿道挿入, 1)
			ENDIF
		ENDIF

		;挿入している相手がいる
		IF INSERT("挿入相手", LCOUNT)
			;複数挿入の時には体勢が変更される
			IF INSERT("複数挿入", LCOUNT)
				SELECTCASE SELECTCOM
				CASE 60 TO 69
					TEQUIP:LCOUNT:体位 = 1p4
				CASEELSE
					TEQUIP:LCOUNT:体位 = 1p5
				ENDSELECT
			;推倒のときは騎乗位
			ELSEIF TEQUIP:LCOUNT:推倒 && INSERT("挿入", LCOUNT)
				TEQUIP:LCOUNT:体位 = 1p5
			ENDIF
			;体位変更に伴う挿入フラグ補正
			IF INSERT("Ｖ挿入相手", LCOUNT)
				SETBIT TEQUIP:LCOUNT:体位, 10
				SIF CHECK_CLO("Ｖ露出", LCOUNT) == 0
					CALL SETFLAG, "下身衣装を露出：テ接吻ト没有", LCOUNT
			ENDIF
			IF INSERT("Ａ挿入相手", LCOUNT)
				SETBIT TEQUIP:LCOUNT:体位, 11
				SIF CHECK_CLO("Ａ露出", LCOUNT) == 0
					CALL SETFLAG, "下身衣装を露出：テ接吻ト没有", LCOUNT
			ENDIF
			IF INSERT("尿道挿入相手", LCOUNT)
				SETBIT TEQUIP:LCOUNT:体位, 12
				SIF CHECK_CLO("Ｖ露出", LCOUNT) == 0
					CALL SETFLAG, "下身衣装を露出：テ接吻ト没有", LCOUNT
			ENDIF
		;誰かに挿入中
		ELSEIF INSERT("挿入中", LCOUNT)
			SIF CHECK_CLO("Ｖ露出", LCOUNT) == 0
				CALL SETFLAG, "下身衣装を露出：テ接吻ト没有", LCOUNT
		;挿入も挿入もしていない
		ELSE
			CALL RESET_INSERT, LCOUNT
		ENDIF
	ENDSELECT
NEXT

SELECTCOM = MEMO_SELECTCOM

;-------------------------------------------------
;各角色の挿入フラグを一括管理する
;ARGSに"解除判定"が代入された場合には解除されるかを格子するのみで挿入フラグの発生は無い
;-------------------------------------------------
@CHECK_INSERT, ARGS
;時間を経過させない指令なら変化无
SIF NOTTIME_PLAY(SELECTCOM)
	RETURN 0
;現在は必ずここの分岐に入る
IF COND("調教側") == 0
	CALL SET_INSERT, TARGET, PLAYER, SELECTCOM, ARGS
	SIF SELECTCOM:1 >= 0
		CALL SET_INSERT, TARGET:1, PLAYER, SELECTCOM:1, ARGS
	SIF SELECTCOM:2 >= 0
		CALL SET_INSERT, TARGET, PLAYER:1, SELECTCOM:2, ARGS
ELSEIF ASSI
	CALL SET_INSERT, ASSI, TARGET, SELECTCOM:1, ARGS
ENDIF

;-------------------------------------------------
;挿入ではない特殊な体位フラグの管理
;-------------------------------------------------
@CHECK_EX_EQUIP
;推倒舐陰orＡ舐めは面部騎乗フラグON
SELECTCASE SELECTCOM
CASE 1, 9
	SIF TEQUIP:推倒
		TEQUIP:面部騎乗 = SELECTCOM
ENDSELECT


;接吻継続…ここではフラグONのみ
;フラグOFFはEVENTCOMで行う
SELECTCASE SELECTCOM
CASE 6, 311
	;連続するほどに消費や効果にBOUNS
	TEQUIP:接吻 += 1
