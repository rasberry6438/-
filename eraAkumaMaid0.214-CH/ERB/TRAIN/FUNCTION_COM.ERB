;-------------------------------------------------
;主にPLAYが可能かどうかだが、他にも様々な内容のものを判定する
;苦痛なく受け入れられるかの判定にも多用する
;-------------------------------------------------
@CHECK(ARGS, ARG, ARG:1)
#FUNCTION
;挿入されるものの尺寸
#DIM SIZE_INSERT
VARSET LOCAL
SIF ARG == 0 && TARGET
	ARG = TARGET

IF ARGS == "実行中の挿入"
	IF INSERT("Ｖ挿入")
		ARGS = Ｖ挿入
	ELSEIF INSERT("Ａ挿入")
		ARGS = Ａ挿入
	ELSEIF INSERT("尿道挿入")
		ARGS = 尿道挿入
	ENDIF
ENDIF

VARSET SIZE_INSERT
;挿入や挿入系は共通部分が多いので上下に分ける
SELECTCASE ARGS
CASE "指挿入", "振動棒"
	SIZE_INSERT = MAX(SIZE("振動棒", ARG), 1)
CASE "Ｖ挿入"
	SIZE_INSERT = MAX(SIZE("阴茎", PLAYER), 1)
	;Ｖ性交経験がある程度必要
	SIF TALENT:ARG:好色 == 0 && EXP:ARG:Ｖ性交経験 < MAX(EXP_LV(2*(SIZE_INSERT - TALENT:ARG:Ｖ性向) - TALENT:ARG:好色 - ABL:ARG:Ｖ拡張), 1)
		RETURNF 0
CASE "Ａ指挿入", "Ａ振動棒"
	SIZE_INSERT = MAX(SIZE("Ａ振動棒", ARG), 1)
CASE "Ａ挿入"
	SIZE_INSERT = MAX(SIZE("阴茎", PLAYER), 1)
	;Ａ性交経験がある程度必要
	SIF TALENT:ARG:好色 == 0 && EXP:ARG:Ａ性交経験 < MAX(EXP_LV(2*(SIZE_INSERT - TALENT:ARG:Ａ性向) - TALENT:ARG:好色 - ABL:ARG:Ａ拡張), 1)
		RETURNF 0
CASE "尿道振動棒"
	SIZE_INSERT = MAX(SIZE("尿道振動棒", ARG), 1)
CASE "尿道挿入"
	SIZE_INSERT = MAX(SIZE("阴茎", PLAYER), 1)
	;尿道性交経験がある程度必要
	SIF EXP:ARG:尿道性交経験 < MAX(EXP_LV(SIZE_INSERT*2 - ABL:ARG:尿道拡張), 1)
		RETURNF 0
CASE "フィスト"
	SELECTCASE SELECTCOM
	CASE 141
		ARGS = Ｖ拳交
	CASE 142
		ARGS = Ａ拳交
	CASE 143
		ARGS = 両穴拳交
	ENDSELECT
ENDSELECT
;ARGS変化後格子
SELECTCASE ARGS
CASE "Ｖ拳交"
	SIZE_INSERT = SIZE("片手フィスト", PLAYER)
CASE "Ａ拳交"
	SIZE_INSERT = SIZE("片手フィスト", PLAYER)
CASE "両穴拳交"
	SIZE_INSERT = SIZE("Ｗフィスト", PLAYER)
ENDSELECT

SELECTCASE ARGS
CASE "指挿入", "振動棒", "Ｖ挿入", "Ｖ拳交"
	SELECTCASE ARG
	CASE PLAYER
		SIF ABL:ARG:Ｖ拡張 < SIZE_INSERT
			RETURNF 0
	CASE TARGET
		SIF CUP:ARG:快Ｖ >= 3000 && MAX(CUP:ARG:苦痛, CUP:ARG:恐怖/10, CUP:ARG:反感) < 300
			RETURNF 1
		;反感が很强とダメ
		SIF CUP:ARG:反感 >= 500
			RETURNF 0
		;好色でない人はうるさい
		IF TALENT:ARG:好色 == 0
			;侵入してくるモノを気持ちよく受け入れられるかを判定
			SIF MIN(MAX(ABL:ARG:Ｖ感覚, ABL:ARG:受虐属性), ABL:ARG:Ｖ拡張) < MAX(SIZE_INSERT, 1)
				RETURNF 0
			;経験がある程度必要
			SIF EXP:ARG:Ｖ経験 < MAX(EXP_LV(2*(SIZE_INSERT - TALENT:ARG:Ｖ性向) - ABL:ARG:Ｖ拡張), 1)
				RETURNF 0
			;Ｖ潤がある程度必要
			SELECTCASE ARGS
			CASE "Ｖ挿入"
				SIF PALAM:ARG:Ｖ潤 + PALAM:PLAYER:Ｐ潤 < PALAM_LV(SIZE_INSERT - ABL:ARG:Ｖ拡張 + 2)
					RETURNF 0
			CASEELSE
				SIF PALAM:ARG:Ｖ潤 < PALAM_LV(SIZE_INSERT - ABL:ARG:Ｖ拡張 + 2)
					RETURNF 0
			ENDSELECT
		ENDIF
		;疼痛が少ないことが必要
		SIF CUP:ARG:苦痛 >= 500 + ABL:ARG:受虐属性*100
			RETURNF 0
	ENDSELECT
	RETURNF 1
CASE "Ａ指挿入", "Ａ振動棒", "Ａ挿入", "Ａ拳交"
	SELECTCASE ARG
	CASE PLAYER
		SIF ABL:ARG:Ａ拡張 < SIZE_INSERT
			RETURNF 0
	CASE TARGET
		SIF CUP:ARG:快Ａ >= 3000 && MAX(CUP:ARG:苦痛, CUP:ARG:恐怖/10, CUP:ARG:反感) < 300
			RETURNF 1
		;反感が很强とダメ
		SIF CUP:ARG:反感 >= 500
			RETURNF 0
		;好色でない人はうるさい
		IF TALENT:ARG:好色 == 0
			;侵入してくるモノを気持ちよく受け入れられるかを判定
			SIF MIN(MAX(ABL:ARG:Ａ感覚, ABL:ARG:受虐属性), ABL:ARG:Ａ拡張) < MAX(SIZE_INSERT, 1)
				RETURNF 0
			;経験がある程度必要
			SIF EXP:ARG:Ａ経験 < MAX(EXP_LV(2*(SIZE_INSERT - TALENT:ARG:Ａ性向) - ABL:ARG:Ａ拡張), 1)
				RETURNF 0
			;Ａ潤がある程度必要
			SELECTCASE ARGS
			CASE "Ａ挿入"
				SIF PALAM:ARG:Ａ潤 + PALAM:PLAYER:Ｐ潤 < PALAM_LV(SIZE_INSERT - ABL:ARG:Ａ拡張 + 2)
					RETURNF 0
			CASEELSE
				SIF PALAM:ARG:Ａ潤 < PALAM_LV(SIZE_INSERT - ABL:ARG:Ａ拡張 + 2)
					RETURNF 0
			ENDSELECT
		ENDIF
		;疼痛が少ないことが必要
		SIF CUP:ARG:苦痛 >= 500 + ABL:ARG:受虐属性*100
			RETURNF 0
	ENDSELECT
	RETURNF 1
CASE "尿道挿入", "尿道振動棒"
	;快楽が強くて苦痛などが少ないならＯＫ
	SIF CUP:ARG:快Ｃ >= 3000 && MAX(CUP:ARG:苦痛, CUP:ARG:恐怖/10, CUP:ARG:反感) < 300
		RETURNF 1
	;反感が很强とダメ
	SIF CUP:ARG:反感 >= 500
		RETURNF 0
	;倒錯的でない人はうるさい
	IF TALENT:ARG:倒錯的 == 0
		;侵入してくるモノを気持ちよく受け入れられるかを判定
		SIF ABL:ARG:尿道拡張 + TALENT:ARG:尿道狂 <= MAX(SIZE_INSERT, 1)
			RETURNF 0
	ENDIF
	;疼痛が少ないことが必要
	SIF CUP:ARG:苦痛 >= 500 + ABL:ARG:受虐属性*100
		RETURNF 0
	RETURNF 1
CASE "両穴拳交"
	SIF MIN(CUP:ARG:快Ｖ, CUP:ARG:快Ａ) >= 3000 && MAX(CUP:ARG:苦痛, CUP:ARG:恐怖/10, CUP:ARG:反感) < 300
		RETURNF 1
	;反感が很强とダメ
	SIF CUP:ARG:反感 >= 500
		RETURNF 0
	SIF MIN(MAX(ABL:ARG:Ｖ感覚, ABL:ARG:受虐属性), ABL:ARG:Ｖ拡張) < MAX(SIZE_INSERT, 1)
		RETURNF 0
	SIF MIN(MAX(ABL:ARG:Ａ感覚, ABL:ARG:受虐属性), ABL:ARG:Ａ拡張) < MAX(SIZE_INSERT, 1)
		RETURNF 0
	SIF MIN(EXP:ARG:Ｖ経験, EXP:ARG:Ａ経験) < EXP_LV(SIZE_INSERT)
		RETURNF 0

	SELECTCASE ARG
	CASE TARGET
		;苦痛が殆どないならＯＫ
		SIF CUP:ARG:苦痛 < 100
			RETURNF 1
		;Ｖ潤がある程度必要
		SIF PALAM:ARG:Ｖ潤 < PALAM_LV(SIZE_INSERT - ABL:ARG:Ｖ拡張 + 2)
			RETURNF 0
		;Ａ潤がある程度必要
		SIF PALAM:ARG:Ａ潤 < PALAM_LV(SIZE_INSERT - ABL:ARG:Ａ拡張 + 2)
			RETURNF 0
		;疼痛が少ないことが必要
		SIF CUP:ARG:苦痛 >= 500 + ABL:ARG:受虐属性*100
			RETURNF 0
	ENDSELECT
	RETURNF 1
CASE "两根挿入Ｖ中"
	SIF ABL:Ｖ拡張 <= SIZE("Ｗ阴茎") || ABL:Ｖ感覚 < 3
		RETURNF 0
	RETURNF 1
CASE "两根挿入Ａ中"
	SIF ABL:Ａ拡張 <= SIZE("Ｗ阴茎") || ABL:Ａ感覚 < 3
		RETURNF 0
	RETURNF 1
CASE "二穴挿入"
	SIF MIN(ABL:Ｖ拡張, ABL:Ａ拡張) < 2 || MIN(ABL:Ｖ感覚, ABL:Ａ感覚) < 3
		RETURNF 0
	RETURNF 1
CASE "尿道PLAY"
	;反感が很强とダメ
	SIF CUP:ARG:反感 >= 500
		RETURNF 0
	;苦痛が殆どないならＯＫ
	SIF CUP:ARG:苦痛 < 100
		RETURNF 1
	;疼痛が少ないことが必要
	SIF CUP:ARG:苦痛 >= 500 + ABL:ARG:受虐属性*100
		RETURNF 0
	RETURNF 1
CASE "喉姦"
	SIF ARG:1 == 0 && PLAYER > 0
		ARG:1 = PLAYER
	;短小だと届かない
	SIF SIZE("阴茎", ARG:1) == 0
		RETURNF 0
	;喉まで受け入れられるかを判定。
	SIF ABL:ARG:喉拡張 <= SIZE("阴茎", ARG:1)
		RETURNF 0
	SIF TALENT:ARG:嘔吐反射耐性 == 0 && EXP:ARG:口交経験 < MIN(EXP_LV(SIZE("阴茎", ARG:1) + 2), 100)
		RETURNF 0
	LOCAL = ABL:ARG:喉拡張 - SIZE("阴茎", ARG:1) + TALENT:ARG:嘔吐反射耐性 + 1
CASE "口内放尿", "膣内放尿", "子宮内放尿", "温泉浣腸", "乳内放尿", "膀胱内放尿"
	;你の場合
	IF ARG == MASTER
		SELECTCASE ARGS
		CASE "口内放尿"
			SIF EXP:ARG:飲尿経験 == 0
				RETURNF 0
		CASE "膣内放尿"
			SIF EXP:ARG:膣内浴尿経験 == 0
				RETURNF 0
		CASE "子宮内放尿"
			SIF EXP:ARG:子宮浴尿経験 == 0
				RETURNF 0
		CASE "温泉浣腸"
			SIF EXP:ARG:温泉浣腸経験 == 0
				RETURNF 0
		CASE "乳内放尿"
			SIF EXP:ARG:乳内浴尿経験 == 0
				RETURNF 0
		CASE "膀胱内放尿"
			SIF EXP:ARG:輸尿経験 == 0
				RETURNF 0
