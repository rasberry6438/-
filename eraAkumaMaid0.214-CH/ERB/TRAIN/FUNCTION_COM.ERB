@ENERGY_DRAIN
#DIM LCOUNT
#DIM MEMO_TARGET, 2
#DIM VALUE_ED_PRE
#DIM VALUE_ED

;ちんこが無いor既に吸能が発生した場合はここで終わり
SIF PENIS(PLAYER) == 0 || CHARA_NUM("吸能")
	RETURN 0


VALUE_ED_PRE = 0
VARSET MEMO_TARGET
MEMO_TARGET:1 = TARGET:1
;PLAYERがTARGET:1だった場合の処理
SIF PLAYER == TARGET:1
	TARGET:1 = 0

;吸能が発生するか格子
FOR LCOUNT, 2, CHARANUM
	SELECTCASE LCOUNT
	CASE PLAYER
		CONTINUE
	CASE TARGET

	CASE TARGET:1
		SIF SELECTCOM:1 < 0 || STATE("撃沈", TARGET:1)
			CONTINUE
	CASEELSE
		CONTINUE
	ENDSELECT

	SIF TALENT:LCOUNT:吸精体質 == 0
		CONTINUE
	SIF NOWEX:LCOUNT:Ｖ中出し + NOWEX:LCOUNT:Ａ中出し + NOWEX:LCOUNT:口内射精 + NOWEX:LCOUNT:乳内射精 + NOWEX:LCOUNT:尿道内射精 == 0
		CONTINUE
	SIF NOWEX:LCOUNT:暴発 == 0 && COND("絶頂", PLAYER) < RAND:4
		CONTINUE
	SIF NOWEX:LCOUNT:暴発 == 0 && RAND:(3 + TALENT:LCOUNT:吸精体質 + TALENT:LCOUNT:恋慕 + TALENT:LCOUNT:淫乱) < 3
		CONTINUE

	VALUE_ED_PRE = TALENT:LCOUNT:吸精体質

	SELECTCASE SELECTCOM
	CASE 65, 75, 88
		VALUE_ED_PRE += 1
	CASE 60 TO 89, 91, 140, 150
	CASEELSE
		SIF INSERT("挿入", LCOUNT, PLAYER) == 0 && TEQUIP:LCOUNT:六九式 != 91
			VALUE_ED_PRE = 0
	ENDSELECT

	NOWEX:LCOUNT:吸能 = MAX(RAND:(VALUE_ED_PRE + 1), NOWEX:LCOUNT:暴発)

	SIF NOWEX:LCOUNT:吸能 == 0
		CONTINUE

	EX:LCOUNT:吸能 += NOWEX:LCOUNT:吸能
NEXT


VARSET LOCALS
SELECTCASE CHARA_NUM("吸能")
;吸能が起きないならここまで
CASE 0
	;PLAYERがTARGET:1だった場合の後処理
	TARGET:1 = MEMO_TARGET:1
	RETURN 0
CASE 1
	IF NOWEX:吸能
		LOCALS = %CALLNAME:TARGET%
	ELSEIF NOWEX:(TARGET:1):吸能
		LOCALS = %CALLNAME:(TARGET:1)%
	ENDIF
CASE 2
	LOCALS = 二人
ENDSELECT

PRINTL 
CALL PRINT_STRL, "爱心粉紅_＜吸能！＞"

;表記の簡略
VALUE_ED = NOWEX:吸能 + NOWEX:(TARGET:1):吸能

