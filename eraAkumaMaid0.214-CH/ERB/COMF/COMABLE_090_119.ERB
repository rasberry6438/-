@COM_ABLE99
;六九式実行判定
SIF COMABLE_DEF(99)
	RETURN 0
;解除是つでも可能
SIF TEQUIP:六九式
	RETURN 1
CALL COM_ABLE91
IF RESULT == 0
	CALL COM_ABLE98
	SIF RESULT == 0
		RETURN 0
ENDIF
CALL COM_ABLE1
IF RESULT == 0
	CALL COM_ABLE4
	SIF RESULT == 0
		RETURN 0
ENDIF

CALL COM_ORDER_COM99, 1
RETURN LIMIT(RESULT, 0, 1)

@COM_ABLE100
;脚を舐める実行判定
SIF COMABLE_DEF(100)
	RETURN 0
;調教者の施虐属性が必要
;SIF ABL:PLAYER:施虐属性 == 0
;	RETURN 0
;調教者の施虐属性か相手の受虐属性が3LV必要
