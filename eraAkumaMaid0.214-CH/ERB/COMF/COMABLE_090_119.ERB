@COM_ABLE90
;手淫実行判定
SIF COMABLE_DEF(90)
	RETURN 0
;調教者が男性か扶她じゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0

CALL COM_ORDER_COM90, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE91
;口交実行判定
SIF COMABLE_DEF(91)
	RETURN 0
;調教者が男性か扶她じゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0
