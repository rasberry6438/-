				ENDIF
			ELSEIF PREVPLAY("屈曲位")
				STR:写真 += "保持着屈曲体勢の"
			ENDIF
			IF PREVEX("Ｖ中出し") && PREVEX("Ａ中出し")
				STR:写真 += "前後穴都有精液不斷流淌の"
			ELSEIF PREVEX("Ｖ中出し")
				IF PREVEX("絶頂漏尿")
					STR:写真 += "股間有尿和精液不斷流淌の"
				ELSE
					STR:写真 += @"从膣穴不断漏出%SPERMS%精液的"
				ENDIF
			ELSEIF PREVEX("Ａ中出し")
				IF PREVEX("前列腺高潮射精")
					STR:写真 += "肛門和阴茎中不断漏出精液的"
				ELSE
					STR:写真 += @"从肛門不断漏出%SPERMS%精液的"
				ENDIF
			ELSE
				STR:写真 += "精疲力尽の"
			ENDIF
		ELSEIF PREVPLAY("背面座位")
			IF PREVEX("前列腺高潮射精")
				STR:写真 += @"靠在%CALLNAME:PLAYER%的身上、从阴茎中咕噜咕噜地不断流出精液的"
			ELSE
				STR:写真 += @"因疲憊而倚靠在%CALLNAME:PLAYER%的懷中休息著の"
			ENDIF
		ELSE
			IF RAND:2 && NAMES("床") == "床"
				STR:写真 += "在被各種汁液浸透的床單上半倚著直起腰"
			ELSE
				STR:写真 += @"在%NAMES("床")%上仰面躺下"
			ENDIF
			IF PREVEX("Ｖ中出し") && PREVEX("Ａ中出し")
				STR:写真 += "、前後穴都有精液不斷流淌、"
			ELSEIF PREVEX("Ｖ中出し")
				IF PREVEX("絶頂漏尿")
					STR:写真 += "、阴部有尿和精液不斷流淌、"
				ELSE
