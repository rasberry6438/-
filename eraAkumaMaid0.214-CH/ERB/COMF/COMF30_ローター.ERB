@COM30
;跳蛋
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:前回指令 = %STR:今回指令%

RETURN 1

@FLAG_COM30, ARG

@SOURCE_COM30, ARG
CALL VARSET_COMF

CALL SOURCE_DOWNBASE_HP, 30

SOURCE:0:露出 = 120
SOURCE:0:逸脱 = 70

;ABL:Ｃ感覚をみる
SOURCE:0:Ｃ快楽 = MULTIPLY(CALCF("Ｃ刺激", 0), 90 + ITEM:跳蛋*10)

;阴部が露出していないと快Ｃ減
IF !CHECK_CLO("Ｖ露出") && COND("調教中")
	;休闲褲等を是ていると快Ｃ激減
	IF CHECK_BOTTOM("休闲褲") || CHECK_PANTIES("皮革")
		TIMES SOURCE:0:Ｃ快楽, 0.20
		TIMES SOURCE:0:露出, 0.20
	ELSE
		TIMES SOURCE:0:Ｃ快楽, 0.60
		TIMES SOURCE:0:露出, 0.50
	ENDIF
ENDIF

SIF TALENT:Ｃ性向
	CALL CALC, "好みのPLAY", SOURCE:0:Ｃ快楽

CALL CALC, "調教ソース", ARG


@TRAIN_MESSAGE_COM30
PRINTFORM %CALLNAME:PLAYER%将
;挿入中
IF INSERT("合体中")
	PRINTFORM %TEXTS("合体中", PLAYER)%
ELSEIF TEQUIP:電話中
	PRINTFORM %TEXTS("電話中の")%
ELSEIF !CHECK_CLO("Ｖ露出")
	PRINTFORM 將%CLO("股間")%挪開、
;ELSE
;	SIF ABL:Ｃ感覚 >= 3 && (MARK:屈服刻印 == 3 || TALENT:恋慕)
;		PRINT 脚を広げて待っている
ENDIF
PRINTFORM %CALLNAME:TARGET%的
IF PENIS(TARGET)
	IF TALENT:淫核 && PALAM:欲情 > 8000
		PRINT 腫脹肥大的
	ELSEIF PALAM:快Ｃ > 3000
		PRINT 興奮勃起的
	ENDIF
	PRINT 阴茎
ELSE
	IF CALCF("Ｃ発情") >= 3 && RAND:2
		PRINT 腫脹肥大的
	ELSEIF CHECK_CLO("Ｖ露出") && TALENT:陰蒂環 && RAND:2
		PRINT 被鐵環装飾的
	ELSEIF CALCF("Ｃ発情") >= 2 && RAND:2
		PRINT 淫穢的立起的
	ELSEIF CHECK_CLO("Ｖ露出") && COND("陰毛") >= 6 && RAND:2
		PRINT 被陰毛遮掩的
	ELSEIF CALCF("Ｃ発情") >= 2 && RAND:2
		PRINT 漸漸膨脹的
	ELSEIF TALENT:娇小
		PRINT 小小的
	ENDIF
	PRINT 阴蒂
ENDIF
IF PREVCOM == SELECTCOM
	PRINTW 用細微的震動著的跳蛋執拗地持續玩弄著……
ELSE
	PRINTW 用不斷震動的跳蛋緊緊貼住……
ENDIF
