;---------------------------------------------------------
;妊娠判定格子
;シチュエーションARGSでARGが妊娠するかどうかを判定する。ARG:1は精液量。
;ARGSは"一般人"or TOSTR(BASE:(精液を注いだ角色):識別番号)が入っている
;SPERMで子宮に入る精子量、IS_FATHERで誰が父親になるかの番号管理を行う
;ARGS:1でどっちに注がれたかを判断する
;---------------------------------------------------------
@CHECK_PREGNANT, ARG, ARG:1, ARGS, ARGS:1
#DIM SPERM
#DIM SPERM_DENS
#DIM IS_FATHER

;精液量がない場合
SIF ARG:1 <= 0
	RETURN 0

;SPERMで子宮に入る精子量、IS_FATHERで誰が父親になるかの番号管理を行う
SPERM = ARG:1 * (10 + RAND:21)
SPERM_DENS = 90 + RAND:21

SELECTCASE ARGS
CASE "一般人"
	;処女は判定しない
	SIF TALENT:ARG:処女
		RETURN 0
	IS_FATHER = -4
	;精液量は館のメンバーに比べると少ない
	SPERM = MAX(SPERM/2, 1)

CASEELSE
	IS_FATHER = TO_NUMBER(TOINT(ARGS) )

	SELECTCASE ARG
	;調教者の妊娠格子
	CASE PLAYER

	;TARGETやASSI等の妊娠格子
	CASEELSE
		;絶頂を迎える事で確率上昇
		SIF NOWEX:ARG:絶頂数合計
			SPERM += MIN(NOWEX:ARG:絶頂数合計, 2)/2

		IF COND("子宮挿入フラグ", ARG)
			SPERM *= 10
		;挿入子宮口玩弄の時には妊娠率上昇
		ELSEIF SELECTCOM == 67
			TIMES SPERM, 1.20
		ENDIF
	ENDSELECT
	;濃厚精液判定
	IF COND("濃厚射精", IS_FATHER)
		SPERM *= 3
		TCVAR:ARG:濃厚中出しされた = IS_FATHER
	ENDIF
ENDSELECT

;妊娠判定フラグ
SELECTCASE ARGS:1
CASE "肛門", "Ａ"
	BASE:ARG:Ａ胎内精液量 += SPERM * SPERM_DENS
CASEELSE
	BASE:ARG:胎内精液量 += SPERM * SPERM_DENS
ENDSELECT

;避妊中で妊娠不可能なら精子は受け入れない
SIF COND("避妊中", ARG) && COND("避妊中だが妊娠可能", ARG) == 0
	SPERM = 0
;人形だと生殖能力没有
SIF IS_FATHER > 0 && COND("種族：人形", IS_FATHER) + COND("種族：自动人形", IS_FATHER)
	SPERM = 0
;你以外は孕ませ不可にしている場合
SIF CONFIG("允许女性为你以外的其他人受孕") == 0 && IS_FATHER != MASTER
	SPERM = 0

SELECTCASE ARGS:1
CASE "肛門", "Ａ"
	;吸精体質の人は精子を頂いちゃう
	SIF TALENT:ARG:吸精体質
		SPERM = MAX(MIN(1 + EX:ARG:Ａ中出し*2, SPERM), SPERM/POWER(2, TALENT:ARG:吸精体質) )
	;夜袭などでの妊娠率を落とす
	SIF COND("調教中", ARG) == 0
		TIMES SPERM, 0.66

	;父親一時フラグ
	IF SPERM && BASE:ARG:胎児 == 0 && TALENT:ARG:Ａ妊娠体質
		IF BASE:ARG:Ａ胎内精液量 == 0 || COND("精液の優勢", ARG) == 0 || RAND:3 == 0
			SELECTCASE IS_FATHER
			CASE IS > 0
				BASE:ARG:精液の優勢 = BASE:IS_FATHER:識別番号
			CASEELSE
				BASE:ARG:精液の優勢 = IS_FATHER
			ENDSELECT
		ENDIF
	ENDIF

	SIF COND("精液の優勢", ARG)
		BASE:ARG:Ａ胎内精子量 += SPERM

	;排卵誘発剤
	SIF TEQUIP:ARG:排卵誘発剤 && TALENT:ARG:Ａ妊娠体質
		SETBIT BASE:ARG:バイオリズム, 13

	;中出しされた最新の日期を上書き
	CFLAG:ARG:Ａ最新中出し日 = DAY
CASEELSE
	;吸精体質の人は精子を頂いちゃう
	SIF TALENT:ARG:吸精体質
		SPERM = MAX(MIN(1 + EX:ARG:Ｖ中出し*2, SPERM), SPERM/POWER(2, TALENT:ARG:吸精体質) )
	;夜袭などでの妊娠率を落とす
	SIF COND("調教中", ARG) == 0
		TIMES SPERM, 0.66

	;父親一時フラグ
	IF SPERM && BASE:ARG:胎児 == 0 && TALENT:ARG:Ａ妊娠体質 == 0
		IF BASE:ARG:胎内精子量 == 0 || COND("精液の優勢", ARG) == 0 || RAND:3 == 0
			SELECTCASE IS_FATHER
			CASE IS > 0
				BASE:ARG:精液の優勢 = BASE:IS_FATHER:識別番号
			CASEELSE
				BASE:ARG:精液の優勢 = IS_FATHER
			ENDSELECT
		ENDIF
	ENDIF

	SIF COND("精液の優勢", ARG)
		BASE:ARG:胎内精子量 += SPERM

	;排卵誘発剤
	SIF TEQUIP:ARG:排卵誘発剤 && TALENT:ARG:Ａ妊娠体質 == 0
		SETBIT BASE:ARG:バイオリズム, 13

	CFLAG:ARG:最新中出し日 = DAY
ENDSELECT

