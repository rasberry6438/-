;---------------------------------------------------------
;妊娠イベントカウント処理
;---------------------------------------------------------
@EVENT_N1, ARG
#DIM FATHER
#DIM P_BASE
#DIM 腰圍上昇幅
#DIM LCOUNT

LOCAL = TARGET
TARGET = ARG

CALL CHECK_NINSHIN, TARGET, "システム処理"

;妊娠イベントカウント処理
IF COND("夜")
	;***着床判定追加パッチで追加
	IF BASE:ARG:受精 > 0 && COND("精液の優勢")
		;受精からの経過日数カウントアップ
		BASE:ARG:受精からの日数 += 1
		SIF CADV("繁栄の光")
			BASE:ARG:受精からの日数 += RAND(1, 2)

		;十分な日数が経っていたら着床判定(４～６日は長ので１日短縮)
		IF BASE:ARG:受精からの日数 >= RAND(3, 6)
			IF COND("着床確率")
				;着床判定
				DRAWLINE
				DRAWLINE
				PRINTFORM %CALLNAME:ARG%的受精卵准备着床了…　
				CALL PRINT_STRW, @" (着床確率:_黄色_{COND("着床確率")}％)"
				CALL PRINT_STRW, @"%CALLNAME:ARG%的子宮内的%CONDS("精液の優勢", ARG)%和%CALLNAME:ARG%的受精卵…"
				;受精卵が子宮に着床するか判定…
				IF RAND:100 <= COND("着床確率")
					;おめでた
					BASE:ARG:胎児 = 1
					;***種付け強化パッチで追加
					;父親の孕ませ経験UP
					FATHER = COND("精液の優勢")
					EXP:FATHER:孕ませ経験 += 1
					;孕ませ素質の追加は妊娠発覚時に行う
					;20%で息子（産まれる時に選べるようにもしてあるので実はなくても良い）
					SIF RAND:5 == 0
						CALL SETFLAG, "妊娠中：息子", ARG
					CALL PRINT_STRW, "平安地着床了…_H"
					PRINTFORMW %CONDS("精液の優勢")%の孕ませ経験+１ (現在：{EXP:FATHER:孕ませ経験})
				ELSE
					PRINTFORMW 没有着床
				ENDIF
			ENDIF

			;着床確実フラグOFF(事后避孕药などで着床を阻止された場合は、ここでOFFになり効果を失う)
			CFLAG:ARG:着床確実 = 0
			;受精状態は解除
			BASE:ARG:受精 = 0
			BASE:ARG:受精からの日数 = 0
		ENDIF
	;受精状態じゃない時には着床確実フラグをOFFにしておく
	ELSE
		CFLAG:ARG:着床確実 = 0
	ENDIF

	;事后避孕药は徐々に効果を失う
	SIF TALENT:ARG:事后避孕药 > 0
		TALENT:ARG:事后避孕药 -= 1

	;お腹の中の胎児の成長
	IF COND("妊娠中")
		;すくすく育ってます
		BASE:胎児 += 1
		;中出しによって速度アップ
		SIF RAND:(3 + BASE:胎児への栄養/10)
			BASE:胎児 += 1
		SIF CADV("繁栄の光")
			BASE:胎児 += 2

		;分娩室待ち
		IF COND("妊娠中：分娩直前")
			CALL SETFLAG, "妊娠中：分娩", TARGET
		;臨月～出産日(人なら三日ほど)
		ELSEIF COND("妊娠中：臨月")
			;出産間近イベント(子育て中には発生しない)
			IF COND("妊娠中：各イベント進行度") >= 3
				IF BASE:育児中
					BASE:胎児 -= 1 + RAND:2
				ELSE
					CALL SETFLAG, "妊娠中：分娩直前", TARGET
				ENDIF
			ENDIF
		;妊娠発覚終了～臨月まで(人なら10日ほど)
		ELSEIF COND("妊娠中：妊娠発覚済み")
			;臨月イベント
			SIF COND("妊娠中：各イベント進行度") >= 11
				CALL SETFLAG, "妊娠中：臨月", TARGET
		;妊娠発覚イベント待ち
		ELSEIF COND("妊娠中：妊娠発覚直前")

		;妊娠潜伏期間～妊娠発覚イベント(人なら三日ほど)
		ELSEIF COND("妊娠中：各イベント進行度") >= 4
			CALL SETFLAG, "妊娠中：妊娠発覚直前", TARGET
		ENDIF

		;妊娠発覚後は腰圍が変化するように
		IF COND("妊娠中：分娩")
			BASE:腰圍 = CFLAG:妊娠前腰圍
			CFLAG:妊娠前腰圍 = 0
			CFLAG:腰圍増加フラグ = 0
		ELSEIF COND("妊娠中：妊娠発覚済み")
			;妊娠前腰圍が0の時（＝妊娠発覚時）、妊娠前腰圍に現在的腰圍を入力する
			SIF CFLAG:妊娠前腰圍 == 0
				CFLAG:妊娠前腰圍 = BASE:腰圍
			
			;前日腰圍、つまり計算前の腰圍をここで書き込む
			CFLAG:前日腰圍 = BASE:腰圍

			;妊娠の日数に合わせて増加の仕方を変えたいので、妊娠初期、中期、後期で底の値を変化させる
			IF COND("妊娠初期")
				P_BASE = 1
			ELSEIF COND("妊娠中期")
				P_BASE = 2
			ELSEIF COND("妊娠後期")
				P_BASE = 3
			ENDIF
			;さやえんどう桑のくださった計算結果を就这様パクり、直接追加する（少数の使い方とか現実的ではないので）
			;一応変数の名前とか程度のオリジナル要素はある
			FOR LCOUNT, 1, COND("妊娠中：各イベント進行度")+1
				IF GETBIT(CFLAG:腰圍増加フラグ, LCOUNT) == 0 && COND("妊娠中：各イベント進行度") <= 10
					SELECTCASE LCOUNT
					CASE 1
						腰圍上昇幅 = 0
					CASE 2
						腰圍上昇幅 = 0
					CASE 3
						腰圍上昇幅 = 1
					CASE 4
						腰圍上昇幅 = 2
					CASE 5
						腰圍上昇幅 = 3
					CASE 6
						腰圍上昇幅 = 5
					CASE 7
						腰圍上昇幅 = 5
					CASE 8
						腰圍上昇幅 = 3
					CASE 9
						腰圍上昇幅 = 2
					CASE 10
						腰圍上昇幅 = 1
					ENDSELECT
					SETBIT CFLAG:腰圍増加フラグ, LCOUNT
				ENDIF
			NEXT

			;腰圍の計算。お腹が小さくなるのは理論上はあるかもしれない（酱と調べていないので不明）だが、なんかリアリティはないので前日腰圍と比べて小さい時は前日腰圍を維持することにする
			;臨月以降は乗算を無くす
			IF COND("妊娠中：臨月") || COND("妊娠中：分娩直前")
				BASE:腰圍 = LIMIT(BASE:腰圍 + RAND:2, CFLAG:前日腰圍, 120)
			ELSE
				BASE:腰圍 = LIMIT(BASE:腰圍 + 腰圍上昇幅 + RAND(0, 1*P_BASE), CFLAG:前日腰圍, 100)
			ENDIF
		ENDIF
	ENDIF

	;子育て。人で２週間程度。
	IF BASE:育児中
		BASE:育児中 += 1
		SIF CADV("繁栄の光")
			BASE:育児中 += 2
		;子育て終了
		SIF COND("育児中：兒童が大きくなった")
			CALL SETFLAG, "育児中：育児終了", TARGET
	ENDIF
ENDIF

TARGET = LOCAL

;---------------------------------------------------------
;妊娠判定処理
;ARGSには"システム処理"が入る。これ以外の場合は時間経過によるシステム処理以外での妊娠判定となる
;CHECK_NINSHIN_ALLACTORは全部のPLAYER:0～1とTARGET:0～3について行う
;---------------------------------------------------------
@CHECK_NINSHIN_ALLACTOR, ARGS
#DIM LCOUNT

FOR LCOUNT, 1, CHARANUM
	SELECTCASE LCOUNT
	CASE PLAYER, PLAYER:1, TARGET, TARGET:1, TARGET:2, TARGET:3
		CALL CHECK_NINSHIN, LCOUNT, ARGS
	ENDSELECT
NEXT

@CHECK_NINSHIN, ARG, ARGS
#DIM LCOUNT
#DIM MEMO_LINECOUNT
#DIMS DYNAMIC NOTES

SIF ARG < 0
	RETURN 0
SIF ARG == 0 && TARGET
	ARG = TARGET

SELECTCASE ARGS
CASE "システム処理"
	;PRINTFORM [{ARG, 2}]%TEXT_LJ(CALLNAME:ARG, 16)% {DAY - CFLAG:ARG:最新中出し日, 3}日前, 妊娠率：{COND("受精確率"), 3}％, {COND("胎内の精液量", ARG), 6}/{BASE:ARG:胎内精子量, 4} → 

	;中出し時に既に判定行っているので精子量の減少処理を行う
	CALL CHECK_NINSHIN_SPERM, ARG

	IF TALENT:ARG:Ａ妊娠体質 == 0
		;***着床判定追加パッチで変更
		;妊娠判定(出産後すぐに妊娠できるようにしてみる)　(着床判定有効時は、受精判定)
		IF BASE:ARG:胎内精子量 && BASE:ARG:受精 == 0 && BASE:ARG:胎児 == 0 && COND("Ｖ妊娠可能", ARG)
			IF CALCF("妊娠判定", ARG)
				;受精
				BASE:ARG:受精 = CALCF("受精強度算出", ARG)
				BASE:ARG:受精からの日数 = 0
				;超危険日ならば着床確実フラグON
				SIF COND("排卵中", ARG)
					CFLAG:ARG:着床確実 = 1
			ENDIF
		ENDIF
	ELSE
		IF BASE:ARG:Ａ胎内精子量 && BASE:ARG:受精 == 0 && BASE:ARG:胎児 == 0 && COND("Ａ妊娠可能", ARG)
			IF CALCF("妊娠判定", ARG)
				;受精
				BASE:ARG:受精 = CALCF("受精強度算出", ARG)
				BASE:ARG:受精からの日数 = 0
				;超危険日ならば着床確実フラグON
				SIF COND("排卵中", ARG)
					CFLAG:ARG:着床確実 = 1
			ENDIF
		ENDIF
	ENDIF

	;創口貼装着日数カウント
	SIF CFLAG:ARG:創口貼装着
		CFLAG:ARG:創口貼装着 += 1
	;5日で剥がれる
	IF CFLAG:ARG:創口貼装着 > 9
		PRINTFORML 　
		DRAWLINE
		PRINTFORML %CALLNAME:ARG%的創口貼由于时间而剥落了…
		CFLAG:ARG:創口貼装着 = 0
		;とりあえず半分に
		SIF BASE:ARG:胎内精液量
			BASE:ARG:胎内精液量 /= 2
		SIF BASE:ARG:胎内精子量
			BASE:ARG:胎内精子量 /= 2
	ENDIF


	;PRINTFORML 妊娠率：{COND("受精確率"), 3}％, {COND("胎内の精液量", ARG), 6}/{BASE:ARG:胎内精子量, 4}
CASEELSE
	;受精確率が変動しているかを格子
	SIF COND("受精確率", ARG) == TCVAR:ARG:受精確率
		RETURN 0
	
	;【イヴァ阴蒂ナ生理系】妊娠判定弾き処理
	IF CEVENT("イヴァ阴蒂ナ：非求愛状態",ARG)
		CALL PRINT_STRL, @"%CALLNAME:ARG%的受精確率：_青_%CALLNAME:ARG%的卵子还没有准备好。"
		RETURN 0
	ENDIF

	;受精確率の変動を表示
	IF CONFIG("妊娠設定：受精確率表示ＯＦＦ") == 0
		LOCALS = %CALLNAME:ARG%の受精確率：_

		;最終確率
		SELECTCASE COND("受精確率", ARG)
		CASE IS >= 100
			;***着床判定追加パッチで変更
			LOCALS = %LOCALS%爱心粉紅_受精確実！_ 
		CASE IS > 50
			LOCALS = %LOCALS%爱心粉紅_{COND("受精確率", ARG)}％_
		CASE IS > 20
			LOCALS = %LOCALS%黄色_{COND("受精確率", ARG)}％_
		CASEELSE
			LOCALS = %LOCALS%{COND("受精確率", ARG)}％_
		ENDSELECT

		NOTES = 
		SIF TCVAR:ARG:濃厚中出しされた
			NOTES = ：濃厚中出しBOUNS発生中！

		;伸び幅
		SELECTCASE COND("受精確率", ARG) - TCVAR:ARG:受精確率
		CASE IS >= 15
			LOCALS = %LOCALS%粉紅_（↑{COND("受精確率", ARG) - TCVAR:ARG:受精確率}％%NOTES%）_
		CASE IS >= 10
			LOCALS = %LOCALS%爱心粉紅_（↑{COND("受精確率", ARG) - TCVAR:ARG:受精確率}％%NOTES%）_
		CASE IS >= 5
			LOCALS = %LOCALS%黄色_（↑{COND("受精確率", ARG) - TCVAR:ARG:受精確率}％%NOTES%）_
		CASE IS > 0
			LOCALS = %LOCALS%（↑{COND("受精確率", ARG) - TCVAR:ARG:受精確率}％%NOTES%）_
		CASE IS < 0
			LOCALS = %LOCALS%青_（↓{TCVAR:ARG:受精確率 - COND("受精確率", ARG)}％%NOTES%）_
		ENDSELECT

		CALL PRINT_STRL, LOCALS
		PRINTL 
	ENDIF

	;ここで妊娠判定も行ってしまう
	IF CALCF("妊娠判定", ARG)
		;***着床判定追加パッチで変更
		;淫紋ビフォー
		IF MARK:ARG:淫紋
			CALL PRINT_CUTIN, "腹部", ARG
			PRINTFORMW %CALLNAME:ARG%的淫紋的様子……颜色和形状不停变化着！
			CALL ANIME_LEVELUP_INMON,  @"CIP淫紋%CONDS("淫紋の種類", ARG)%%CONDS("淫紋の色", ARG)%おめでた", ARG
		ENDIF

		;受精
		BASE:ARG:受精 = CALCF("受精強度算出", ARG)
		BASE:ARG:受精からの日数 = 0
		;超危険日ならば着床確実フラグON
		SIF COND("排卵中", ARG)
			CFLAG:ARG:着床確実 = 1

		CALL SETFLAG, "受精確認フラグ", ARG
		CALL PRINT_CUTIN, "受精", TARGET
		CALL PRINT_STRFW, @"UH_%CALLNAME:ARG%被%CONDS("精液の優勢", ARG)%的精子受精了_UH"
		PRINTL 

		IF MARK:ARG:淫紋
			;セリフがあるかの判定
			MEMO_LINECOUNT = LINECOUNT

			CALL KOJO_GUEST, "受精確認", ARG

			SIF LINECOUNT > MEMO_LINECOUNT
				PRINTL 
		ENDIF
	ENDIF

	TCVAR:ARG:受精確率 = COND("受精確率", ARG)

ENDSELECT

;---------------------------------------------------------
;胎内の精液量＆精子量を減少させる。だいたい３日程で死滅。
;ちなみにCFLAG:最新中出し日は最新の中出し日
;Ａ妊娠体質は通常のＶ妊娠とは排他の関係にある
;---------------------------------------------------------
@CHECK_NINSHIN_SPERM, ARG
#DIM TIME_PASS
;DEC_VALUE:0は精液の減少レートで、DEC_VALUE:1は精子の減少レート
#DIM DEC_VALUE, 2
VARSET DEC_VALUE

IF TALENT:ARG:Ａ妊娠体質
	TIME_PASS = DAY - CFLAG:ARG:Ａ最新中出し日
	BASE:ARG:胎内精液量 = 0
	BASE:ARG:胎内精子量 = 0
ELSE
	TIME_PASS = DAY - CFLAG:ARG:最新中出し日
	BASE:ARG:Ａ胎内精液量 = 0
	BASE:ARG:Ａ胎内精子量 = 0
ENDIF

SELECTCASE TIME_PASS
;出した直後
CASE 0
	DEC_VALUE:0 = 120 + TALENT:ARG:吸精体質*10 + MARK:ARG:淫紋*5 + 10*RAND:6
	DEC_VALUE:1 = 100 + TALENT:ARG:吸精体質*10 + MARK:ARG:淫紋*5 + 10*RAND:6
CASE 1
	DEC_VALUE:0 = 150 + TALENT:ARG:吸精体質*10 + MARK:ARG:淫紋*5 + 10*RAND:11
	DEC_VALUE:1 = 100 + TALENT:ARG:吸精体質*10 + MARK:ARG:淫紋*5 + 10*RAND:6
CASE 2
	SIF COND("昼")
		DEC_VALUE:0 = 200 + TALENT:ARG:吸精体質*10 + MARK:ARG:淫紋*5 + 10*RAND:11
	DEC_VALUE:1 = 100 + TALENT:ARG:吸精体質*10 + MARK:ARG:淫紋*5 + 10*RAND:6
CASE IS >= 3
	DEC_VALUE:0 = 200 + TALENT:ARG:吸精体質*10 + MARK:ARG:淫紋*5 + 10*RAND:11
	SIF COND("昼")
		DEC_VALUE:1 = 100 + TALENT:ARG:吸精体質*10 + MARK:ARG:淫紋*5 + 10*RAND:6
ENDSELECT

IF TALENT:ARG:Ａ妊娠体質
	;肛塞没有だと精液が減少
	SIF TALENT:ARG:常時Ａ肛塞 == 0 && DEC_VALUE:0
		BASE:ARG:Ａ胎内精液量 = DIVIDE(BASE:ARG:Ａ胎内精液量, DEC_VALUE:0)

	IF CONFIG("胎内精子で妊娠") && DEC_VALUE:1
		BASE:ARG:Ａ胎内精子量 = DIVIDE(BASE:ARG:Ａ胎内精子量, DEC_VALUE:1)
	ELSE
		BASE:ARG:Ａ胎内精子量 = 0
	ENDIF
	;精子が死滅したら父親フラグは消滅
	;***着床判定追加パッチで変更
	SIF BASE:ARG:Ａ胎内精子量 == 0 && BASE:ARG:胎児 == 0 && BASE:ARG:受精 == 0
		BASE:ARG:精液の優勢 = 0
ELSE
	;創口貼没有だと精液が減少
	SIF CFLAG:ARG:創口貼装着 == 0 && DEC_VALUE:0
		BASE:ARG:胎内精液量 = DIVIDE(BASE:ARG:胎内精液量, DEC_VALUE:0)
	;胎内精子で妊娠がONでも精子は減っていく　4日程で死滅
	IF CONFIG("胎内精子で妊娠") && DEC_VALUE:1
		BASE:ARG:胎内精子量 = DIVIDE(BASE:ARG:胎内精子量, DEC_VALUE:1)
	ELSE
		BASE:ARG:胎内精子量 = 0
	ENDIF
	;精子が死滅したら父親フラグは消滅
	;***着床判定追加パッチで変更
	SIF BASE:ARG:胎内精子量 == 0 && BASE:ARG:胎児 == 0 && BASE:ARG:受精 == 0
		BASE:ARG:精液の優勢 = 0
ENDIF


;---------------------------------------------------------
;妊娠発覚＆出産予定報告＆子の親出口処理 ＆卵の孵化処理
;---------------------------------------------------------
@EVENT_N2, ARG
#DIM MEMO_TARGET
#DIM CHOICE
MEMO_TARGET = TARGET

TARGET = ARG

;子育て中
IF BASE:育児中
	;子の親出口
	IF COND("育児中：育児終了")
		DRAWLINE
		;PRINTFORMW %CALLNAME:TARGET%の育てていた兒童は館に充満する力のせいか、かなり早く大きくなりました。
		PRINTFORMW %CALLNAME:TARGET%养育的孩子、很快就长大了。
		PRINTFORMW 已经不需要再照顾了。

		CALL KOJO_EVENT, "子出口"

		BASE:育児中 = 0
	ELSEIF COND("育児中：孵化") && COND("育児中：孵化中") 
		DRAWLINE
		;PRINTFORMW %CALLNAME:TARGET%の育てていた兒童は館に充満する力のせいか、かなり早く大きくなりました。
		PRINTFORMW 从%CALLNAME:TARGET%温暖着的蛋中传出了摇晃的动静、接着外壳逐渐碎裂開从里面钻出了婴儿。
		PRINTFORMW 从现在開始就要认真地培育孩子了。

		;"育児中：孵化中"の取消
		CLEARBIT BASE:育児中, 19

		PRINTL 
		PRINTFORML 出生的孩子……
		PRINTFORML [ 0] 女孩子
		PRINTFORML [ 1] 男孩子
		CALL INPUT_SELECT, 2
		CHOICE = RESULT
			
		CALL BIRTH_RECORD, "出産記録：保存", TARGET, CHOICE
		IF CHOICE == 0
			CLEARBIT BASE:胎児, 20
		ELSE
			CALL SETFLAG, "妊娠中：息子", TARGET
		ENDIF
					
		IF COND("妊娠中：息子")
			;ここで父親が你かどうかの判定どうやろうかね？
			;PRINTFORMW \@ FATHER > 0 ? 长得和父親很像的 # \@元気的男孩子。
			PRINTFORMW 元気な男の子です。
			CFLAG:息子の数 += 1
			;息子フラグ
			CALL SETFLAG, "育児中：息子", TARGET
		ELSE
			PRINTFORMW \@ TARGET == MASTER ? %CALLNAME:MASTER% # 和母親 \@很像的可愛的女孩子。
			CFLAG:娘の数 += 1
		ENDIF
		;恋慕
		IF TALENT:恋慕
