;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K77, ARGS
#FUNCTIONS
;名前を呼ぶ相手の角色番号の代入に使う
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 主人_主人
	;相手が主人ではない
	;フェリ酱桑
	ELSEIF NO:PLAYER == 1
		LOCALS = 弗理小姐_弗理小姐
	ELSEIF COND("教員", PLAYER)
		LOCALS = %CALLNAME:PLAYER%老师_%CSTR:PLAYER:ふりがな%老师
	ELSE
		SELECTCASE COND("学年", PLAYER)
		;学生ではない
		CASE 0
			LOCALS = %CALLNAME:PLAYER%桑_%CSTR:PLAYER:ふりがな%桑
		;相手が同学年
		CASE COND("学年")
			LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
		;相手が下の学年
		CASE IS < COND("学年")
			IF TALENT:PLAYER:男性
				LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
			ELSE
				LOCALS = %CALLNAME:PLAYER%酱_%CSTR:PLAYER:ふりがな%酱
			ENDIF
		;相手が上級生
		CASE IS > COND("学年")
			LOCALS = %CALLNAME:PLAYER%前辈_%CSTR:PLAYER:ふりがな%前辈
		ENDSELECT
	ENDIF
;この下は三人称。
CASE "主人"
	LOCALS = 主人_主人
CASE "助手"
	SELECTCASE CONDS("学年", ASSI)
	CASE "初等部", "中等部", "１年生", "２年生", "３年生", "研究生"
		LOCALS = %CALLNAME:ASSI%
	CASE "教員"
		LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
	CASEELSE
		LOCALS = %CALLNAME:ASSI%
	ENDSELECT
CASEELSE
	;名前を呼ぶ相手の角色番号をACTORに代入する
	SELECTCASE ARGS
	;相手が助手
	CASE "助手"
		ACTOR = ASSI
	;ここにはARGSに角色のCALLNAMEが入ったりしている場合(ARGS == "弗理希艾尔" など)に来る
	CASEELSE
		ACTOR = FIND_NO(ARGS)
	ENDSELECT

	SELECTCASE ARGS
	CASE "弗理希艾尔"
		LOCALS = フェリ桑
	CASE "寧子"
		LOCALS = 宁子前辈
	CASE "米娅"
		LOCALS = 米娅亲
	CASE "乃々"
		LOCALS = 乃乃君
	CASEELSE
		;相手が教員
		IF COND("教員", ACTOR)
			LOCALS = %CALLNAME:ACTOR%老师_%CSTR:ACTOR:ふりがな%老师

		;自分も相手も学生なら学年比較
		ELSEIF COND("学生") && COND("学生", ACTOR)
			;相手が同学年
			IF COND("学年") == COND("学年", ACTOR)
				LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
			;相手が下の学年
			ELSEIF COND("学年") > COND("学年", ACTOR)
				IF TALENT:ACTOR:男性
					LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
				ELSE
					LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
				ENDIF
			;相手が上級生
			ELSE
				LOCALS = %CALLNAME:ACTOR%前輩_%CSTR:ACTOR:ふりがな%前輩
			ENDIF

		;それ以外
		ELSE
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		ENDIF
	ENDSELECT
ENDSELECT
RETURNF LOCALS

