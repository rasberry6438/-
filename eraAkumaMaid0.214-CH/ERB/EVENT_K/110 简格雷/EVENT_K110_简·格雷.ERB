@CNAME_K110, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 主人様_主上大人
CASE "助手"
	IF ASSIPLAY 
		IF NO:ASSI == 23
			LOCALS = %CALLNAME:ASSI%前辈_%CALLNAME:ASSI%姐姐
		ELSEIF NO:ASSI == 100
			LOCALS = %CALLNAME:ASSI%小姐_%CALLNAME:ASSI%姐姐
		ELSEIF NO:ASSI == 31
			LOCALS = %CALLNAME:ASSI%同学_%CALLNAME:ASSI%小姐
		ELSE
			LOCALS = %CALLNAME:ASSI%桑_%CALLNAME:ASSI%桑
		ENDIF
	ENDIF
ENDSELECT
RETURNF LOCALS
