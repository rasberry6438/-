		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 1
		GOTO RYOKO_DAILY_LP1
	ENDIF

	; 汎用
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRL,  "「………………」"
			PRINTFORML %CNAME("玲子")%焦点不定睡眼惺忪地枕着自己的胸部趴在桌子上……
		CASE 1
			CALL PRINT_STRL,  "「呜……嗯……。呼哇啊……」"
