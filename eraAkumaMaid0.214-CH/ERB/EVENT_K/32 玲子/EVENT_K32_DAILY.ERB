;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K32, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

$RYOKO_GREETING_LP1
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	RETURN 0
;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	RETURN 0
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR,  "「从早上就这么激烈……真是的……_BH_」_W"
				CALL PRINT_STR,  "「我要让你清醒清醒……嗯啾_BH_」_W"
		ENDSELECT
	CASE "朝騎乗位"
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR,  "「嗯っ_BH_　啊っ_BH_　嗯っ_BH_　嗯嗯～～_BH_」_W"
				CALL PRINT_STR,  "「啊、嗯、再、再稍微、睡一会儿也可以哦……_BH_　哇_BH_」_W"
				CALL PRINT_STR,  "「我会、让_BH_　你舒服起来的、不用、担心、啊、啊啊っ_BH_　啊呜っ_BH_」_W"
				PRINTFORMW 跨坐再%CALLNAME:PLAYER%下半身上的%CNAME("玲子")%大方地展示着结合的地方，贪婪地贪图快乐……
				CALL PRINT_STR, @"（呜呜_BH_　明明、明明是早上、但却这么厉害，这么硬%CNAME("阴茎")%……_BH_）_W"
				CALL PRINT_STR, @"（再、再做一会儿、就行了_BH_　就算只有一下也可以_BH_　早上第一次如果不好好%TEXTR("榨/射")%出来的话_BH_　一定很难受的、对吧_BH_）_W"
				CALL PRINT_STR, @"「呜呜_BH_　%CNAME("阴茎")%好像要%TEXTR("噗噗/biubiu")%地出来了……_BH_」_W"
				CALL PRINT_STR, @"「射出来_BH_　就这样射出来_BH_　把精液射出来_BH_　晨勃%CNAME("阴茎")%的新鲜%CNAME("精液")%_BH_　多biubiu地射出来_BH_」_W"
				CALL PRINT_STR,  "「呜、呜呜～～～～_BH_　哦～……_BH_　哦哦ー……_BH_」_W"
		ENDSELECT
	CASEELSE
		RETURN 0
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "「啊啦、要迟到了」"
				CALL PRINT_STRW,  "「哪里有近路可以抄吗？」"
		ENDSELECT
	ENDSELECT

CASE "入室打招呼"
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRW,  "「失礼了」"
		CASE 1
			CALL PRINT_STRW,  "「我迟到了……抱歉咯」"
	ENDSELECT
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0 && CHARANUM > 6
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL,  "「……啊啦？　莫非是有很多大佬都来了?」"
				CALL PRINT_STRL,  "（与其说是大佬不如说是大姥、正确的说……哈啊）"
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL,  "「抱歉、稍微迟到了」"
		ENDSELECT
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW, "『啊啦、你们是在一起的吗？』"
		ENDSELECT

	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "『太迟了啦。大家都已经到齐了』"
			CASE 1
				CALL PRINT_STRW,  "『是去哪里绕路了吗？』"
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW, @"『你好、%CALLNAME_ACTOR%』"
		ENDSELECT
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
	IF TALENT:恋慕
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "「…………真欺负人、真是的」"
				CALL PRINT_STRW,  "（至少悄悄地走吧……）"
			CASE 1
				CALL PRINT_STRW, @"「虽然%CNAME("一人称")%也清楚要干什么……」"
				CALL PRINT_STRW, @"「……%CNAME("一人称")%的份也要留下一点哦？_／／／_」"
		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 1
		GOTO RYOKO_GREETING_LP1
	ENDIF

	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRW, @"「啊啦、是要出去吗？　就把%CNAME("一人称")%这么丢下」"
			CALL PRINT_STRW,  "（这是……应该也跟着去吗?不过，要是被发现了就太差劲了……）"
		CASE 1
			CALL PRINT_STRW,  "「别做得太出格了哦」"
	ENDSELECT

CASE "便当"
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STR, @"「今天，是%TEXTR("三明治/咖喱面包/豆沙面包/羊角面包")%。……啊、便当的话比较好？」_W"
			CALL PRINT_STR,  "「是吗？　如果想吃的话就说一声。……呼唔」_W"
			PRINTFORM 从便利店的塑料袋里拿出没開封的%TEXTR("三明治/咖喱面包/豆沙面包/羊角面包")%递给了%CALLNAME:PLAYER%。%CNAME("玲子")%
			PRINTFORMW 坐在%CALLNAME:PLAYER%的旁边，取出自己的%TEXTR("三明治/咖喱面包/豆沙面包/羊角面包")%。
			CALL PRINT_STR,  "「…………哈啊」_W"
			IF TALENT:MASTER:男性
				CALL PRINT_STR,  "（穿着制服、和异性一起吃便当、嘎啊……）_W"
			ELSE
				CALL PRINT_STR,  "（穿着制服、和女孩子一起吃便当、嘎啊……）_W"
			ENDIF
			CALL PRINT_STR,  "（如果我能照爸爸说的那样上高中的话，就有这样的机会了……）_W"
			PRINTFORMW 有一种悲伤的气氛%CNAME("玲子")%，不过，好像并不讨厌在一起。……
		CASE 1
			CALL PRINT_STR,  "「我买了很多东西、想吃什么？」_W"
			PRINTFORMW %CNAME("玲子")%从手里提着的便利店购物袋里取出三明治和饭团摆在%CALLNAME:PLAYER%面前……
			CALL PRINT_STR,  "「啊啦、太多了？　但是下午也很耗体力吧？」_W"
			CALL PRINT_STR,  "「……又、又没说错话」_W"
		CASE 2
			CALL PRINT_STR,  "「今天多吃点甜的东西。真的，糖分缺失……」_W"
			CALL PRINT_STR,  "「……刚才的课有不懂的地方，可以边吃边问吗？」_W"
			CALL PRINT_STR,  "（初中毕业后就一直在修行和进修，所以说实话，上课很痛苦……）_W"
	ENDSELECT
CASEELSE
	DEBUGPRINTFORM KOJO_GREETING_K32, ARGS:%ARGS%
	RETURN 0
ENDSELECT

SIF MEMO_LINECOUNT - LINECOUNT
	RETURN 1
GOTO RYOKO_GREETING_LP1

