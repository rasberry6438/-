;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K32, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你/你_你
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 主人_主人大人
CASE "助手"
	LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
; 以下ライ胸罩リ
CASE "愛液"
	LOCALS = お汁_おつゆ/おツユ_おつゆ
	SIF MARK:屈服刻印 > 1
		LOCALS += "/マン汁_まんじる"
CASE "肛門"
	LOCALS = 屁股穴_おしりのあな/肛門_あなる/菊花_菊花
	SIF MARK:屈服刻印 >= 2
		LOCALS += "/ケツ穴_けつあな/おケツマンコ_おけつまんこ"
CASE "陰毛"
	LOCALS = マン毛_まんげ/おけけ_おけけ/ケ_け
CASE "自慰", "自慰"
	LOCALS = 自慰_おなにー/ひとりエッチ_ひとりえっち/ソロPLAY_そろぷれい
CASE "オマンコ"
	LOCALS = オマンコ_おまんこ/阴部_あそこ
CASE "危険日"
	LOCALS = 危険日_きけんび/排卵日_是らんび/赤酱のデキちゃう日_あか酱のできちゃうひ
CASE "避孕套", "避孕套"
	LOCALS = 避孕套_こんどーむ/避孕套_ごむ
CASE "子宮"
	LOCALS = 子宮_しきゅう/赤酱的房間_あか酱のへや
CASE "射精"
	LOCALS = 射精_しゃせい/お射精_おしゃせい/びゅるびゅる_びゅるびゅる/どっぴゅん_どっぴゅん
CASE "射精擬音"
	LOCALS = びゅるびゅる/びゅくびゅく/どぴゅどぴゅ/どっぴゅん/びゅーびゅー
CASE "精液"
	LOCALS = 精液_せいえき/精液_ざーめん
	SIF MARK:屈服刻印 > 0
		LOCALS += "/ザー汁_ざーじる/赤酱ミルク_あか酱みるく"
	SIF MARK:屈服刻印 > 1
		LOCALS += "/ちんぽ汁_ちんぽじる/おちんぽミルク_おちんぽみるく/孕ませ汁_はらませじる"
CASE "中出し"
	LOCALS = 中出し_なかだし/ナカダシ_なかだし/膣内射精_ちつないしゃせい
CASE "ぱんつ", "胖次"
	LOCALS = ぱんつ_ぱんつ/三角胖次_ぱんてぃ/内衣_したぎ
CASE "阴茎"
	LOCALS = おちんちん_おちんちん/おちんぽ_おちんぽ
CASEELSE
	;DEBUGPRINTFORML エラー！　%ARGS%は未定義の単語です
ENDSELECT

