@KOJO_GREETING_K19, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS

CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「嗯呐……　\@　CALLNAME:MASTER != "主人公" ? 、 %CALLNAME:MASTER% # \@。 %TEXTR("早上好……/早上好，吓了一跳……？/早上好，偶尔被这样叫醒也不错吧？/嗯，哼……嗯，嗯/早上好，嗯……")%」
		ELSE
			PRINTFORMW 「\@　CALLNAME:MASTER != "主人公" ? 、 %CALLNAME:MASTER%、 # \@ あの、%TEXTR("啊，没什么……嗯/嗯，嗯哈、嗯啊/嗯，不这样做的话会很辛苦吧/嗯，嗯，嗯，嗯哈")%」
			PRINTFORMW 听起来像个不诚实的坏话，但还是相当热心地持续着口交。
		ENDIF
	CASE "朝騎乗位"
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「んふ……　\@　CALLNAME:MASTER != "你" ? 、 %CALLNAME:MASTER% # \@。 %TEXTR("早上好う……んっ/早上好、んふ……はぁ/あ、んっっ、早上好ぅ/んっ、ぁぁおは、よぉ/はぁ、はぁ、んふ、早上好ぅ")%」
			PRINTFORMW 目覚めと睡眠の狭間で下半身が我慢の効かない遡りに目を覚ましたら、その絶対的質量をゆさぶる、一糸まとわぬ万音と目があった。
			PRINTFORMW どれくらいまたがられていたのだろう…… \@ EXP_GAIN("射精経験", PLAYER) ? もうすぐにでも射精してしまいそうだった。 # 気を拔出とすぐにでも暴発してしまいそうった。\@
			PRINTFORMW 「ん、就这様……我慢しないで……おまんこの奥で……精液飲ませてぇ、っんぁああ！！」
			PRINTFORMW こちらの限界を悟った万音が、自己的絶頂を重ねるように下半身を沈め、最奥へ深々と迎え入れていた。
			;たぶん覚醒まのんの朝騎乗位とか射精を忍耐ほうが難しい
		ELSE
			PRINTFORMW 「\@　CALLNAME:MASTER != "你" ? 、 %CALLNAME:MASTER%、 # \@ あの、%TEXTR("あ、お、おきちゃっんんっ！！/あ、え、えと、あっ、ぁぁっ！！/ま、まってまってっ/ふぁ、っ！？　ぁうぅ！")%」
			PRINTFORMW 目覚めと睡眠の狭間で下半身が我慢の効かない遡りに目を覚ましたら、その絶対的質量をゆさぶる、一糸まとわぬ万音と目があった。
			PRINTFORMW どれくらいまたがられていたのだろう…… \@ EXP_GAIN("射精経験", PLAYER) ? もうすぐにでも射精してしまいそうだった。 # 気を拔出とすぐにでも暴発してしまいそうった。\@
			PRINTFORMW 「や、やぁ、えと、これはその、あのっ、っっうああああ！！」
			PRINTFORMW \@　CEVENT("相思相愛：万音") ? 想いが通い合っていても初々しい反応を返す万音がかわいくて、思わず腰を両手で引き付けていた。 # あれだけ反発していても体は渇望に耐えられないらしい。最奥にみっちり先端を押し付けてやった。 \@
		ENDIF
	ENDSELECT
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝六九式"
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW \@ EXP_GAIN("射精経験", PLAYER) ? 精を吐き出し切った"それ"から万音は目を離すことができず、再び口に含んで奉仕を始めた…… # じらすように舌先でつついたり、口に含んだままゆるく舌をはわせたどらせるだけで、こちらを弄ぶかのように万音は口淫を続けていた。\@
		ELSE
			PRINTFORMW \@ EXP_GAIN("射精経験", PLAYER) ? 精液を飲み込んで恍惚とした万音を、ゆっくりからかうことにした。 # 口腔でゆったり感じさせてもらえる様を見つめ一边继续、朝のひとときを楽しんだ。\@
		ENDIF
	CASE "朝騎乗位"
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW \@ EXP_GAIN("射精経験", PLAYER) ? ぐっ、ぐっとよりぴったり、より深く密着しようとする万音の腰靈活に、大量的吐精させられていた。 # 万音はもう、こちらが射精するまで深部を押し付け続けずに是られなかった。\@
			PRINTFORMW 「お、おは……っっぅ……！！」
			PRINTFORMW 万音は膣内を占める感触に絶頂を抜けられず、だが脱力をこらえながら幾度も腰を沈め続けて、貪欲に精を搾ろうと試みていた。
			;たぶん覚醒まのんの朝騎乗位とか射精を忍耐ほうが難しい
		ELSE
			IF EXP_GAIN("射精経験", PLAYER)
				PRINTFORMW 「お、おはっ、んんんっっ！　早上好ぉ、やぁ、だめぇ、出しながら動かさないでぇっ」
				PRINTFORMW 大量の吐精任せに万音の最奥をぐいぐいと突き上げて、困惑する様を愉しませてもらった。
			ELSE
				PRINTFORMW 「お、おはっ、んんんっっ！　だ、だめぇ、もうっ、ぁぁっ！」
				PRINTFORMW とろけるような精感が万音と結ばれる部分にこみ给まで、困惑する万音を幾度も幾度も突き上げた。
			ENDIF
		ENDIF
		CASE "朝の新妻PLAY"
			IF CEVENT("淫魔覚醒：万音")
				PRINTFORMW \@ EXP_GAIN("射精経験", PLAYER) ? いつにないほど突き抜けるような射精感に、尻肉を押しつぶすくらい腰を押し付けて、どくどくと濃縮された白濁を吐き出していた。 # 万音の狭さと締め付けと柔らかい尻肉を味わうように腰を押し付けた。\@
				PRINTFORMW 「ちょ、ちょっと……っっぅ……！！　っはぁ……はぁ、もう、興奮しすぎぃ……でも、朝ごはん佳肴大人ね……」
				PRINTFORMW 万音は蜜内を占める感触に絶頂を抜けられず、だが脱力しそうな膝を踏みとどめて、最奥の圧迫感にひたっていた。
				;たぶん覚醒まのんの朝騎乗位とか射精を忍耐ほうが難しい
			ELSE
				IF EXP_GAIN("射精経験", PLAYER)
					PRINTFORMW 「まって、まって、わ、私こんな、でも、っ、っあああ！！」
					PRINTFORMW この上ないほど強くこみ给ままに、濃縮された精液を注ぎ一边继续、困惑する万音の尻肉を押しつぶすように腰を擦り付けた。
					PRINTFORMW 万音は蜜内を占める感触に絶頂を抜けられず、だが脱力しそうな膝を踏みとどめて、最奥にたたきつけられる吐精の感触にひたっていた。
				ELSE
					PRINTFORMW 「ちょ、ちょっとっ！　あっ、すご、すごいって、だ、だめぇ、もうっ、ぁぁっ！」
					PRINTFORMW とろけるような精感が万音と結ばれる部分にこみ给まで、困惑する万音を幾度も幾度も从后面将責め続けた。
					PRINTFORMW 万音は蜜内を占める感触に絶頂を抜けられず、だが脱力しそうな膝を踏みとどめて、最奥の圧迫感にひたっていた。
				ENDIF
			ENDIF
	CASEELSE
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「んふ……　\@　CALLNAME:MASTER != "你" ? 、 %CALLNAME:MASTER% # \@。 %TEXTR("早上好う/早上好、んふ……ん？　別に何もないよ？/おはぁよ/早上好ー")%」
		ELSE
			PRINTFORMW 「\@　CALLNAME:MASTER != "你" ? 、 %CALLNAME:MASTER%、 # \@ あの、%TEXTR("お、お、早上好うございましゅってかんじゃったっ/お、早上好う、ございまｓ……/お、お、早上好ぅぅ/お、早上好うっ、")%」
		ENDIF
	PRINTL
	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1

	IF COND("体育") && CEVENT("学校食堂の大姐姐：万音")
		;ままのんが体育の発言するのは学校を知っているときなので。
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「%TEXTR("あら、今日は体育のある日なの？/今日は何か浮かれてる？　そっか、体育のある日。男の子じゃ仕方ないか/え、今日は保健のほうの体育なんだ。でもいつも授業してるよね……私たち")%」
		ELSE
			PRINTFORMW 「%TEXTR("きょ、今日は体育のある日？　え、べ、別になにも……/体育のある日、って、ど、どうって、私は学生じゃないけど、でもそれだとお腹すかせて、うん、だからがんばるよ/ほ、ほほほほけんたいいくって、そ、それってつまりえっちな……そ、そそういう意味じゃなくてっ！")%」
		ENDIF
	ENDIF
CASE "一番乗り"
	IF CEVENT("学校食堂の大姐姐：万音")
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「%TEXTR("今日は私だけだけど……ふふ、他の人来る前にいいところいかない？/今ここには我と你だけ……それがどういう意味かはわかるよねぇ？/お先してるよーまだ他の人いないし、始めちゃおうっか。何って、ご・っ・君(したのおくち)")%」
		ELSE
			PRINTFORMW 「%TEXTR("おおお、お先して、あ、あの……いえ、なんでも/え、え、私そんな干劲ってわけじゃ、でも、何してるか気になるっていうか/わ、私なんでここにいるのかって、私も、いちおう部員、だから")%」
		ENDIF
	ENDIF
CASE "ただいま"
	IF COND("合意：同居") && CEVENT("学校食堂の大姐姐：万音") == 0
		CALL INAUGURATION_SCHOOL_MEAL, "帰宅"
	ELSEIF CEVENT("淫魔覚醒：万音")
		PRINTFORMW 「ただいまもどったよ～　\@　CALLNAME:MASTER != "你" ? 、 %CALLNAME:MASTER% # \@。 %TEXTR("今日もお疲れ様/いつもお疲れ様/少し、待ち遠しかった/待ってた……よ？/ね、今日も……する？")%」
	ELSE
		PRINTFORMW 「た、た、ただいま\@　CALLNAME:MASTER != "你" ? 、 %CALLNAME:MASTER% # \@。 あの、%TEXTR("おつかれ、じゃないかな？/えと、その、うぅ……(言葉に詰圆圈)！/お、おつかれ大人！/え、えと……その、する？　あ、あの、もちろん入れちゃ嫌だからね！")%」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF COND("合意：同居") && CEVENT("学校食堂の大姐姐：万音") == 0
			CALL INAUGURATION_SCHOOL_MEAL, "出迎え"
		ELSEIF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「欢迎回来、%TEXTR("今天也辛苦了/总是很辛苦呢/有点，久等了……/。哟？那……今天也是要……做吗？")%」
		ELSE
			PRINTFORMW 「那个、嗯、嗯、%TEXTR("欢迎回来！/你回来啦！/欢迎回来！啊，你在说什么啊/等一下……像个色狼一样！/嗯、嗯、嗯……咦？")%」
		ENDIF
	ELSE
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「我先来了、%TEXTR("从现在開始做吧……可以吗？/今天轮到我就好了/嗯……想做想做的事/啊，已经……好郁闷啊/呐，今天也……做吗?")%」
		ELSE
			PRINTFORMW 「那个、那个、%TEXTR("今天快点……诶，你那么想要吗？/今天……好期待啊……啊！/欢迎回来！啊，你在说什么啊……/等一下……因为不是那样的！/哦、哦、哦……咦？？")%」
		ENDIF
	ENDIF
CASE "入室打招呼"
	IF CEVENT("淫魔覚醒：万音")
		PRINTFORMW 「你好～%CALLNAME:TARGET%来了呦。 %TEXTR("辛苦工作后的亲吻~/总觉得好重来着……想揉一揉吗？呵呵~♪/啊，今天轮到我了吧，对吧？/呜呜，呜呜呜？/呐，今天也……做吗？")%」
	ELSE
		PRINTFORMW 「や、やっと終わったあぁ…… あ、あの、%TEXTR("ごめんなさ、おつかれ、なのは一緒だよね……/えと、その、うぅ……(言葉に詰圆圈)！/な、なんで来たって、そ、そんなの、わた、私も部員だから/え、えと……その、します？　あ、あの、もちろん入れちゃ嫌ですよっ！/き、期待してなんてっ、ち、ちがうよ！(どきどき)")%」
	ENDIF
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「您好～%CALLNAME:TARGET%到着ぅ。 あら、みんなお揃いだねぇ」
		ELSE
			PRINTFORMW 「や、やっと終わったあぁ…… あ、あの、皆桑您好」
		ENDIF
	ELSE
		IF CEVENT("淫魔覚醒：万音")
			PRINTFORMW 「您好～%CALLNAME:TARGET%到着ぅ」
		ELSE
			PRINTFORMW 「や、やっと終わったあぁ…… あ、あの、您好」
		ENDIF
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;打招呼してきた人に弗理希艾尔が混じっている
	IF FIND_COND("弗理希艾尔", "入室打招呼中")
		IF CEVENT("淫魔覚醒：万音")
			IF RAND:3
				PRINTFORML 「您好女仆」
				PRINTFORMW 万音はそっけなく打招呼したが、\@ CHARA_NUM("入室打招呼中") > 1 ? 真っ先に弗理希艾尔を選んだ辺り実は一番意識しているようだ。 # 打招呼だけはとりあえず欠かさないようだ。\@
			ELSEIF RAND:2
				PRINTFORML 「駄女仆您好」
				PRINTFORMW 万音はあえて挑発するような打招呼をしている。
			ELSE
				PRINTFORML 「いちおう您好」
				PRINTFORMW とりあえず弗理希艾尔に対しても打招呼はするようだ。
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORML 「こ、您好女仆」
			ELSE
				PRINTFORML 「い、い、いちおう您好」
				PRINTFORMW とりあえずでも打招呼はするようだ
			ENDIF
		ENDIF
	;打招呼してきた人に老师が混じっている
	ELSEIF FIND_COND("教員", "入室打招呼中")
		;打招呼してきた老师の中から一人選ぶ
		ACTOR = FIND_COND("教員", "入室打招呼中")
		;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
		CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

		IF CEVENT("淫魔覚醒：万音")
			IF RAND:2
				PRINTFORML 「您好ぁ%CNAME(CALLNAME_ACTOR)%老师。って、いちお学校関係者だしいてもいいんだよね？」
			ELSE
				PRINTFORML 「%CNAME(CALLNAME_ACTOR)%老师おじゃま大人ー、いやほんとにお邪魔とか言わないでね？」
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORML 「こ、您好%CNAME(CALLNAME_ACTOR)%老师」
			ELSE
				PRINTFORML 「あ、ああああの、%CNAME(CALLNAME_ACTOR)%老师您好、おじゃましてます……」
			ENDIF
		ENDIF
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K19, %ARGS%, 遅かったじゃない
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K19, %ARGS%
		[ENDIF]
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"

CASE "便当"

ENDSELECT

@INAUGURATION_SCHOOL_MEAL, ARGS
IF ARGS == "帰宅"
	PRINTFORMW その日、万音はただいまの打招呼も半端に、いつもと違う事項を切り出した。
ELSE
	PRINTFORMW その日、万音はおかえりの打招呼も半端に、いつもと違う事項を切り出した。
ENDIF
IF CEVENT("淫魔覚醒：万音")
	PRINTFORMW 「啊，对了。\@ TEXTS("曜日") == "土" ? 后天開始 # 明天開始\@我也要去学校了」
	PRINTFORMW 「为什么？那果然是……白天也想要在一起」
	PRINTFORMW 让人心頭一颤的有点挑衅意味的湿润眼神。
	PRINTFORMW 「啊哈哈……不能总是只寄居着吃饭呀？所以，我想能不能赚到什么钱，正好有个食堂工作。那么，明天就是第一天了。」
	PRINTFORMW 「从今往后平日也能见面了，简直就像做梦一样……嗯，就是这么回事，所以请多多関照。」
	PRINTFORML
	PRINTFORMW %CALLNAME:MASTER%没想到的是，%CALLNAME:TARGET%的工作所具有的罕见的质量引发了话题。成为了学校食堂的名产。不过这是后话了……
	PRINTFORML
	PRINTFORMW 「这么说来」
	PRINTFORMW 「参加社团活动吗？我当然会参加」
ELSE
	PRINTFORMW 「啊，呃，啊，有必须告诉你的事，那个……」
	PRINTFORMW 「啊，我不能一直白白吃饭，所以要工作！」
	PRINTFORMW 「那个，工作单位是学校的食堂、\@ TEXTS("曜日") == "土" ? 后天開始 # 明日開始\@、虽然如此……」
	PRINTFORMW 「至少在家里什么也做不了……」
	PRINTFORMW 「突然这样很抱歉，但是，今后也请多多関照」
	PRINTFORML
	PRINTFORMW %CALLNAME:TARGET%深深地低下了頭,不过，那个脸颊带的红，一定是学校也想在一起的欲望的表现吧，%CALLNAME:MASTER%随意地理解到。
	PRINTFORML
	PRINTFORMW 之后、%CALLNAME:TARGET%所拥有的绝对的两种质量引起了话题，这之后知道了它将成为学校食堂的名产……但是，由于混杂着口吃而战战兢兢的样子，也因其可爱而受到好评。
	PRINTFORML
	PRINTFORMW 另外，她是强制参加社团活动的
	PRINTFORMW 在同一个学校的食堂里工作，一直追着他，可以说是理所当然的吧。
ENDIF
PRINTL
PRINTFORMW %CALLNAME:TARGET%が「学校食堂の大姐姐」として就職しました。それに伴い、日中も学校で%CALLNAME:TARGET%に出会うことができるよう成为了。
CFLAG:夜型 = 0
CSTR:学年 = 学校食堂の大姐姐
CALL SET_CEVENT, "学校食堂の大姐姐：万音"

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K19, ARGS
