;CALL HANA_HANNYO
@HANA_HANNYO
CFLAG:随机 = 0
SELECTCASE SELECTCOM
;愛撫系
CASE 0 TO 12
	CALL AIBU
;尿道指
CASE 15
	CALL U_YUBI
;道具系
CASE 30 TO 59
	CALL DOUGU
;子宮挿入
CASE 68
	CALL SIKYU
;挿入系
CASE 60 TO 69, 70 TO 79
	CALL SOUNYU
;乳頭奸
CASE 82
	CALL N_FUCK
;尿道挿入
CASE 83 TO 88
	CALL U_SEX
;口交
CASE 91
	CALL FELLATIO
;素股
CASE 93, 101
	CALL SUMATA
;強制口交
CASE 140
	CALL DEEP
;尿道探针
CASE 151
	CALL BOUGIE
CASEELSE
	RETURN 0
ENDSELECT

@AIBU
	REPEAT 1 + (PALAM:欲情 >= 500) + (PALAM:欲情 >= 3000) + (PALAM:欲情 >= 30000)
		CFLAG:随机 = RAND:6
		IF CFLAG:随机 == 0
			PRINTFORM 對ぁっ…
		ELSEIF CFLAG:随机 == 1
			PRINTFORM んっ
		ELSEIF CFLAG:随机 == 2
			PRINTFORM あっ
		ELSEIF CFLAG:随机 == 3
			PRINTFORM ひんっ…
		ELSEIF CFLAG:随机 == 4 && TALENT:恋慕
			CALL HEARTMARK
		ELSE
			PRINTFORM んんっ、
		ENDIF
	REND
