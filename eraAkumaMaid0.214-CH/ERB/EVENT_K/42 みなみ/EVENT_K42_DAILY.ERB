	SELECTCASE CSTR:朝袭
	CASE  "朝接吻", "朝の新妻PLAY", "朝添い寝"
		SELECTCASE RAND:3
		CASE 0
			CALL PRINT_STR, @" 「嗯、早上好……_HH_」_W"
		CASE 1
			PRINTFORML 「嗯呢……啊、醒了吗？」
			IF TEXTS("曜日") == "日"
				PRINTFORMW 「差不多该吃早饭了吧？」
			ELSE
				PRINTFORMW 「そろそろ学校行く時間だから、起きなきゃダメだよ。」
				CALL PRINT_STRW, @" 「……诶、再睡一会？ 真没办法……_HH_」"
			ENDIF
		CASEELSE
			PRINTFORML 「啊、早上好、%CNAME("二人称")%。」
		ENDSELECT
	CASE "朝騎乗位", "朝Ａ騎乗位","朝乳交", "朝口交", "朝足交", "朝舐陰"
		PRINTFORMW 「%CNAME("二人称")%起床啦～」
