	CASE "朝六九式"
		PRINTFORMW 「%CNAME("二人称")%、起来了？」
		CALL PRINT_STRW, @" 「%TEXTR("もう下の方は起きちゃってるけど……_HH_/妹朝まんこ_HH_味わってね_HH_")%」"
	CASE "朝逆正常位"
		CALL PRINT_STRW,@"「%CNAME("二人称")%、哦哇 哟…_HH_」"
		CALL PRINT_STRW,@"「%CNAME("二人称")%%TEXTR("に朝出し_HH_きもちよかったぁ……_HH_/のナカ_HH_朝から独り占めしちゃった_HH_/の寝顔がえっちすぎたから、朝えっち_HH_しちゃった_HH_")%」"
	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育だよ。」
		PRINTFORMW 「%CNAME("二人称")%、体操服忘れてない？」
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「今日は%TEXTR("座学の/実技の")%日だね」
			CASE 1
				PRINTFORMW 「ちょっと待って、茶入れてくるね」
			CASEELSE
				PRINTFORMW 「あっ、%CNAME("二人称")%！ ……えへへ、なんでも！」
