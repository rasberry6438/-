;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K45, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF TALENT:PLAYER:男性
			LOCALS = 后辈君_后辈君
		ELSE
			LOCALS = 后辈桑_后辈桑
		ENDIF
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	IF TALENT:PLAYER:男性
		LOCALS = 后辈君_后辈君
	ELSE
		LOCALS = 后辈桑_后辈桑
	ENDIF
CASE "助手"
	LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
ENDSELECT
RETURNF LOCALS

