;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		CALL EMOTION_PLUS, "躁动难忍", TARGET
		CALL PRINT_STR, "「んっ_H_ ……ぃ_H_ ぁ_H_ ひぃ_H_」_W"
		PRINTFORMW 「う、お、あ、ご、ごめんなさい、ごめんなさいぃ」
		CALL PRINT_STR, "「が、がまん…で、でで、できなくって……ご、ごめっあひっ_H_」_W"
