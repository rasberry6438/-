			ENDIF
		;それ以外
		ELSE
			;恋慕
			IF TALENT:恋慕
				IF CEVENT("恋慕：ささくれ")
					IF RAND:3 == 0
						CALL PRINT_STRW, @"「――っ_HH_ %TEXTR("咕/哈啊/啊啊")%、%TEXTR("恶心/太糙了/想吐/最差劲/不如去舔地板？/快停下/烂透了/蠢狗")%」"
