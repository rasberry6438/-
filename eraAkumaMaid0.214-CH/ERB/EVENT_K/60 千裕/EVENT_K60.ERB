;-------------------------------------------------
;肛門愛撫
;-------------------------------------------------
IF SELECTCOM == 2
	;初めて
	IF FIRSTCOM(SELECTCOM) == 0
		;気持ちいい
		IF CHECK("Ａ指挿入")
			IF TALENT:恋慕
				IF CEVENT("恋慕：ささくれ")
					CALL PRINT_STRW, "「――嗯唏呀啊_HH_ っ、太糟、太糟了！」"
					PRINTFORMW 「说真的、到底、到底在想什么啊、%CNAME("二人称")%」
				ELSE
					CALL PRINT_STRW, "「哦哦_HH_ 哦、哦？ 这、这个意外的不错嘛HH_」"
					PRINTFORMW 「是%CNAME("一人称")%有那份素质，还是%CNAME("二人称")%的手指太厉害了――到底是哪边呢？」
				ENDIF
			ELSE
				PRINTFORMW 「嗯嗯！ 等一下！ 嗯呼！ 唔嗯、等、等一下啦～！」
