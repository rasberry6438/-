			DATAFORM 「……%CNAME("二人称")%真变态」
			DATA 「呜呜，感觉好羞耻……」
		ENDDATA
	ENDIF
CASE "接吻"
	IF MARK:屈服刻印 <= 2
		IF RAND:2
			PRINTFORMW 「诶、诶！？　在、在外面接吻吗！？」
			PRINTFORMW 「……嗯……倒、倒是也不讨厌……」
			PRINTFORMW 「……我、我知道了。可以哦」
		ELSE
			PRINTFORMW 「诶！？　在、在这里接吻！？」
			PRINTFORMW 「唔……那个……」
			CALL PRINT_STRW, @"「……快、快住手……_H_」"
		ENDIF
	ENDIF
	CALL PRINT_STRW, @"「……%AEGI(TARGET, "要", "接吻", 3 + RAND:2)%」"
	IF TALENT:相思相愛
		PRINTFORMW 「……这样紧紧地贴在一起，就能再次感受到和%CNAME("二人称")%成为情侣的感觉」
		SIF MARK:屈服刻印 <= 2
			PRINTFORMW 「……同样也有一些害羞」
		PRINTFORMW 「……那个、%CNAME("二人称")%、可以再这样待一会儿吗」
		IF YOKUBO()
			PRINTFORMW 「像往常一样让理性消失得让人舒服当然也很喜欢、但我也喜欢这样的接触」
			PRINTFORMW 「……你也知道这些吧、%CNAME("二人称")%」
		ENDIF
	ELSEIF YOKUBO()
		CALL PRINT_STRW, @"「……在这种人来人往的地方接吻、%CNAME("二人称")%真是够大胆的呢_H_」"
		PRINTFORMW 「但是，真的只是接吻就够了吗？」
		CALL PRINT_STRW, @"「……你不想变成一个在外面做爱，在街上做出变态行为的无可救药的野兽吗_H_」"
	ENDIF
CASE "摸屁股"
	IF YOKUBO()
		CALL PRINT_STRW, @"「……%CNAME("二人称", "甘え")%_H_　这样的话会被周围的人发现的_H_」"
		CALL PRINT_STRW, @"「……或者干脆直接暴露吧？　%CNAME("一人称")%和%CNAME("二人称")%是在人前摸屁股的变态\@TALENT:相思相愛 ? 情侣 # 同士\@_H_」"
	ELSEIF TALENT:恋慕
		PRINTFORMW 「%CNAME("二人称")%！　你在公共场合干什么呢！」
		CALL PRINT_STRW, "「呜……不、不讨厌就是了……_H_」"
	ELSE
		PRINTFORMW 「……真是的、拿%CNAME("二人称")%没办法……」
	ENDIF
;你が草丛口交や接吻や性騷擾をしない紳士な場合はここに来ます
CASE "下次去哪里呢？询问"
	PRINTDATAW
		DATA 「是呢……下次想去这附近」
		DATA 「嗯……那么，下次去这边怎么样？」
		DATA 「那么下次去这里怎么样？」
	ENDDATA
;文化祭イベント
;ここでは"约会相手ではなく出し物をしている人"が喋ります。
;本来の约会相手はPARTNER()に角色番号が入ってます。呼びかける場合にはCNAME("partner")を使ってください
CASE "女仆咖啡厅", "執事咖啡厅", "西式料理店"
PRINTFORMW 「欢迎光临……啊、是%CNAME("二人称")%和%CNAME("partner")%吗」
	PRINTFORMW 「二位要点些什么？」
	SIF TALENT:料理擅長
		PRINTFORMW 「既然如此，这次就让%CNAME("一人称")%为你们两位做吧」
CASE "演劇"
	PRINTFORMW （……啊、%CNAME("二人称")%在看……）
	PRINTFORMW （得更加专注一些……）
CASE "烦悩相談室"
	PRINTFORMW 「……啊、这不是%CNAME("二人称")%吗」
	PRINTFORMW 「你有什么想要咨询的吗？」
;帰り際のセリフです。不动声色地エッチに邀请のもいいでしょう
CASE "帰り際"
	IF TALENT:恋慕
		IF EXP:约会経験 >= 20 && RAND:2
			PRINTFORMW 「%CNAME("二人称")%，一直以来谢谢你」
			PRINTFORMW 「能和%CNAME("二人称")%一起创造了许多快乐的回忆，现在的我非常幸福」
		ELSE
			PRINTFORMW 「……谢、谢谢你……」
			PRINTFORMW 「……那、那个，如果可以的话，下次你能再带我去约会吗……？」
		ENDIF
	ELSE
		IF EXP:约会経験 >= 20 && RAND:2
			PRINTDATAW
				DATAFORM 「%CNAME("二人称")%、一直以来谢谢你带我去各种地方」
				DATAFORM 「%CNAME("二人称")%、今天也谢谢你陪伴我」
			ENDDATA
		ELSE
			PRINTFORMW 「那个，今天谢谢你。多亏了你我才能拥有快乐的回忆」
		ENDIF
	ENDIF
CASE "约会終了"
	;エッチをする場合
	IF TFLAG:床イン
		IF TALENT:恋慕 && RAND:3
			CALL PRINT_STRW, "「……是_H_」"
		ELSEIF YOKUBO()
			CALL PRINT_STRW, @"「……那么，下次去那家旅馆吧_H_」"
		ELSE
			PRINTFORMW 「……拜、拜托了……」
		ENDIF
	ELSE
		PRINTFORMW 「嗯，那么再见」
	ENDIF
	CALL DEL_CEVENT, "どたばたと準備"
ENDSELECT

