;-------------------------------------------------
;部員同士での交流。自分はTARGETとして呼ばれ、話相手の部員はPARTNER()として呼ばれる
;話相手への呼びかけはCNAME("partner")で出来る
;TFLAG:台詞スキップ = 1で話している相手のセリフのスキップ、TFLAG:地の文スキップ = 1で続く地の文のスキップをする事が出来る
;-------------------------------------------------
@KOJO_COMM_K73, ARGS
#DIM MEMO_LINECOUNT
;対話相手にしたい角色の名前を/区切りでこれに突っ込んでFIND_TaiTa(EVENT_TABLE)とするとそのうち一人が選ばれる
#DIMS EVENT_TABLE
#DIM MEMO_TARGET
#DIM LCOUNT
#DIM MEMO_PLAYER
EVENT_TABLE = 
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;話しかける側のセリフ。COND("対話可能")を満たす相手を選んでPARTNER()にする事で、特定の相手に話しかけることもできる
CASE "お誘いする"
	;特定の相手に話しかける場合はEVENT_TABLEに対話相手の呼び名(CALLNAME)を / 区切りで並べて ; を消す
	;例１）弗理希艾尔or濡烏の君or杏珠から一人選ぶ場合
	;EVENT_TABLE = 弗理希艾尔/濡烏の君/杏珠
	;例２）部員全てから一人選ぶ場合
	;EVENT_TABLE = 全員
	TFLAG:地の文スキップ = 1
	IF FIND_TaiTa(EVENT_TABLE)
		;話し相手をpartnerにする
		CALL SET_PARTNER, FIND_TaiTa(EVENT_TABLE)
		
	ENDIF
	MEMO_TARGET = TARGET
	MEMO_PLAYER = PLAYER
	PLAYER = FINDCHARA(NO, 73)
	TARGET = PARTNER()
	IF TALENT:PARTNER():男性 || COND("只和你Ｈ", PARTNER() ) || NO:PARTNER() == 54 || NO:PARTNER() == 6 || NO:PARTNER() == 56 || NO:PARTNER() == 44 || NO:PARTNER()== 39
		$TADANOKAIWA
		CALL EMOTION_PLUS, "交流中", PLAYER, PARTNER()
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 日向と%CALLNAME:TARGET%通过談笑しているようだ…
		CASE 1
			PRINTFORMW 日向と%CALLNAME:TARGET%通过、普段你とどんな風にしているのか語り合ったようだ…
		CASE 2
			PRINTFORMW 日向は%CALLNAME:TARGET%を誘ってみたが、断られたようだ…
		ENDSELECT	
	ELSEIF NO:PARTNER() == 15 || NO:PARTNER() == 51
		IF !TALENT:PARTNER():恋慕
			GOTO KOURYU
		ENDIF
		GOTO TADANOKAIWA
	ELSEIF NO:PARTNER() == 55
		IF !CEVENT("I love you, my dear.", PARTNER())
			GOTO KOURYU
		ENDIF
		GOTO TADANOKAIWA
	ELSEIF NO:PARTNER() == 55
		IF !CEVENT("I love you, my dear.", PARTNER())
			GOTO KOURYU
		ENDIF
		GOTO TADANOKAIWA
	ELSEIF NO:PARTNER() == 19
		IF !CEVENT("淫魔覚醒：万音", PARTNER()) == 0
			GOTO KOURYU
		ENDIF
		GOTO TADANOKAIWA
	ELSEIF NO:PARTNER() == 42
		IF CFLAG:PARTNER():好感度 < 1500 || TALENT:PARTNER():処女
		ELSE
			GOTO KOURYU
		ENDIF
		GOTO TADANOKAIWA
	ELSE
		$KOURYU
		CALL EMOTION_PLUS, "对话框爱心/红潮恍惚", PARTNER(), PLAYER
		SELECTCASE RAND:2
			CASE 0
				CALL PRINT_STRW, @"「ねぇ、%CALLNAME:TARGET%酱…_BBHH_」"
				CALL PRINT_STRW, @"「後輩酱…どこかに行っちゃったし、僕と楽しいことしようよ_BBHH_」"
			CASE 1
				CALL PRINT_STRW, @"「%CALLNAME:TARGET%ちゃ～ん_BBHH_」"
				CALL PRINT_STRW, @"「一緒に赤酱作ろ♪いいでしょ_BBHH_」"
		ENDSELECT
		EJAC:TARGET = 1
		CALL MESSAGE_PLAY_K73
		PRINTFORML
		PRINTFORM 你が
		IF FLAG:休憩
			PRINTFORM 在消磨时间时、、
		ELSEIF PLACE("情人旅館")
			PRINTFORM 在与%CALLNAME:TO_NUMBER(FLAG:昼の相手)%一起休憩的时候、
		ELSE
			PRINTFORM 在与%CALLNAME:TO_NUMBER(FLAG:昼の相手)%做Ｈ的事情的时候、
		ENDIF
		SELECTCASE RAND:6
			CASE 0
				PRINTFORMW 日向は%CALLNAME:TARGET%に種付けをしていたようだ…
			CASE 1
				PRINTFORMW 日向と%CALLNAME:TARGET%通过子作りに励んでいるようだ…
			CASE 2
				PRINTFORMW 日向と%CALLNAME:TARGET%通过生での行為を楽しんだようだ…
			CASE 3
				PRINTFORMW 日向と%CALLNAME:TARGET%通过互相用体液を交換したようだ…
			CASE 4
				PRINTFORMW 日向は%CALLNAME:TARGET%を孕ませるために腰をふったようだ…
			CASE 5	
				IF RAND:2
					CALL PRINT_STRW, @"日向と%CALLNAME:TARGET%が一緒に楽しんでいたようです_BBHH_"
				ELSE
					CALL PRINT_STRW, @"日向と%CALLNAME:TARGET%が親交を深めていたようです_BBHH_"
				ENDIF
		ENDSELECT
	ENDIF
	TARGET = MEMO_TARGET
	PLAYER = MEMO_PLAYER
	TFLAG:地の文スキップ = 1
;話しかけられる側のセリフ。括弧は『』を使うのが望ましい
CASE "誘われた"


ENDSELECT
