	ELSEIF DAYEV("試験返却日")

	ELSEIF COND("朝袭相手")

	ELSEIF COND("体育")

	ELSEIF COND("水泳")

	ELSE

	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
	PRINTFORMW 「あいや、誰も居ない。%CNAME("一人称")%たちが一番乗りね」
CASE "ただいま"
	PRINTFORMW 「戻ったよー。%CNAME("主人")%いるかー？」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「おー、お帰り。%TEXTR("待ってたよ/どっか繞道してたか？/お腹空いたか？ ごはん作るよ？")%」
	ELSE
		PRINTFORMW 「おー、来たか。%TEXTR("待ってたよ/どっか繞道してた？/今日はどうする？")%」
	ENDIF

