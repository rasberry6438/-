CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		CALL PRINT_STRW, @"「んん……_UH_ 朝からいっぱい出たな、%CNAME("主人")%_UH_ お腹いっぱいね_UH_」"
	CASE "朝騎乗位"
		CALL PRINT_STRW, "「んふ_UH_ 朝からいい運動变成了ね_UH_ それじゃ、淋浴浴びてくるよ」"
	ENDSELECT

