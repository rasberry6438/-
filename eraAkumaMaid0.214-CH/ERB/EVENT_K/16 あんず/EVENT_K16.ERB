;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K16, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:称号
		CASE "実母"
			IF TALENT:相思相愛
				LOCALS = 義母桑_かあ桑
			ELSE
				LOCALS = おば桑_おば桑
			ENDIF
		CASE "実妹"
				LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
		CASEELSE
			SELECTCASE CSTR:PLAYER:学年
			CASE "１年生", "初等部", "中等部"
				IF TALENT:PLAYER:男性
					LOCALS = %CALLNAME:PLAYER%君_%CALLNAME:PLAYER%君
				ELSE
					LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
				ENDIF
			CASE "２年生"
				IF TALENT:PLAYER:男性
					LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
				ELSE
					LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
				ENDIF
			CASE "３年生", "研究生"
				LOCALS = %CALLNAME:PLAYER%前辈_%CSTR:PLAYER:ふりがな%前辈
			CASEELSE
				IF COND("教員", PLAYER)
					LOCALS = %CALLNAME:PLAYER%老师_%CSTR:PLAYER:ふりがな%老师
				ELSE
					LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
				ENDIF
			ENDSELECT
		ENDSELECT
	ENDIF
CASE "主人"
	LOCALS = アイツ_あいつ
CASE "助手"
		SELECTCASE CSTR:ASSI:称号
		CASE "実母"
			IF TALENT:相思相愛
				LOCALS = 義母桑_かあ桑
			ELSE
				LOCALS = おば桑_おば桑
			ENDIF
		CASE "実妹"
				LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
		CASEELSE
			SELECTCASE CSTR:PLAYER:学年
			CASE "１年生", "初等部", "中等部"
				IF TALENT:PLAYER:男性
					LOCALS = %CALLNAME:PLAYER%君_%CALLNAME:PLAYER%君
				ELSE
					LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
				ENDIF
			CASE "２年生"
				IF TALENT:PLAYER:男性
					LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
				ELSE
					LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
				ENDIF
			CASE "３年生", "研究生"
				LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
			CASEELSE
				IF COND("教員", ASSI)
					LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
				ELSE
					IF TALENT:PLAYER:男性
						LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
					ELSE
						LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
					ENDIF
				ENDIF
			ENDSELECT
		ENDSELECT

ENDSELECT
RETURNF LOCALS

