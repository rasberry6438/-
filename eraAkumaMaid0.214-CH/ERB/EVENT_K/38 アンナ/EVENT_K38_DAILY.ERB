;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K38, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"
		IF CEVENT("名没有の呼び出し") || RAND:2
			IF RAND:3 == 0
				PRINTFORMW 「哟，感觉怎么样？怎么回事？不用介意」
				CALL PRINT_STRW, "「只是一大清早被脚底胡乱地榨取精液的感觉罢了_HH_」"
				CALL PRINT_STRW, @"「啊、这里生气可是不行的。不管怎么说这里可是变态所喜欢的%CNAME("一人称")%在做_HH_」"
			ELSEIF RAND:2
				PRINTFORMW 「哦，起来了也不要动。不然东西可能会变得无法使用的哦？」
				CALL PRINT_STRW, "「对了对了，就这样乖乖听话吧。_HH_ 所谓的猎物应该是这样的_HH_」"
			ELSE
				CALL PRINT_STRW, "「不管做多少次，这样的事还是没办法忍受的啊_HH_？ ……喂、不是装着睡的话就起来张開腿吧」"
				CALL PRINT_STRW, "「这是服从的姿势哦、ふ・く・じゅ・う（服从，日语的服从是4个音）_HH_ 像这样用脚后跟把它磨碎也是能做到的_HH_」"
				CALL PRINT_STRW, "「对了对了、为了让它更容易打開_HH_ 君君（发音大致是哼哼，犬类的撒娇声）地可爱地叫了的话会温柔些的_HH_」"
			ENDIF
		ELSE
			IF TALENT:恋慕
				IF RAND:3 == 0
					PRINTFORMW 「啊啦，早上好。我的脚舒服吗？？」
					CALL PRINT_STRW, "「是、是啊、是脚、あ・し（脚的日语发音，故意拉长了说）_HH_ 在脚底抖动着呢_HH_」"
					IF ABL:MASTER:受虐属性 >= 1
						CALL PRINT_STRW, "「为什么变大了呢、真难以理解……呋呒_HH_」"
					ELSE
						CALL PRINT_STRW, "「呋呒_HH_事到如今再萎下去也是没用的？ 就这样把它榨干吧_HH_」"
						CALL PRINT_STRW, "「当然…是用脚来做_HH_」"
					ENDIF
				ELSEIF RAND:2
					PRINTFORMW 「如果您喜欢这种适当的早晨服务，今后也可以这样吗？」
					IF ABL:MASTER:受虐属性 >= 3
						CALL PRINT_STRW, "「啊啦，小鸡鸡先有反应了_HH_ 想要快一点被踩呢，对不起_HH_」"
					ELSEIF ABL:MASTER:受虐属性 >= 1
						CALL PRINT_STRW, "「……呋呒、既不说YES也不说NO。我喜欢诚实的孩子_HH_」"
					ELSE
						CALL PRINT_STRW, "「開玩笑_HH_ ……即便如此偶尔開一下也不错吧_HH_」"
					ENDIF
				ELSE
					PRINTFORMW 「如果是继续躺着的话，应该可以找个借口吧」
					PRINTFORMW 「对，对，当然是指那个用脚踩着，哭得很開心的小鸡鸡的事情」
					CALL PRINT_STRW, "「像这样用脚…摩擦_HH_…摩擦_HH_♪ ……真的、不像样_HH_」"
				ENDIF
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「早上好，用脚打扰了吧？」
					PRINTFORMW 「就这样轻松点……我会让你就这样射精的」
				ELSEIF RAND:2
					PRINTFORMW 「不要、如果你这么说的话我停下也可以……是吗、你说不要也可以」
					PRINTFORMW 「呋呒……不、失礼了呢。实在是那个，太滑稽了……呋呒呋…♪」
				ELSE
					PRINTFORMW 「…既然过来的话，让停下来不是更合理吗？」
					PRINTFORMW 「这个就这样吧，不、既然你这么说，那我们继续吧」
				ENDIF
			ENDIF
		ENDIF
	CASE "朝の新妻PLAY"

	CASE "朝乳交"
		PRINTFORMW 

	CASE "朝騎乗位"
		IF CEVENT("名没有の呼び出し") || RAND:2
			IF RAND:3 == 0
				CALL PRINT_STRW, "「嗯_HH_……呋呒_HH_ 醒过来了？」"
				CALL PRINT_STRW, "「你问我在做什么？这不是理所当然的嘛_HH_」"
			ELSEIF RAND:2
				CALL PRINT_STRW, "「啊对不起、睡脸无忧无虑地样子所以就忍不住開吃了_HH_」"
				CALL PRINT_STRW, "「不解风情的话就不要说了_HH_ 就这样带你去天堂吧_HH_」"
			ELSE
				CALL PRINT_STRW, "「别一脸呆呆的！既然起来了，就动动腰啊_HH_」"
				CALL PRINT_STRW, "「如果能让我漂亮地叫出来，我给你打100分。_HH_」"
			ENDIF
		ELSE
			IF TALENT:恋慕
				IF RAND:3 == 0
					CALL PRINT_STRW, "「呋呒、感觉怎么样？刚睡醒的时候被袭击了……被好好地招待……_BH_」"
					CALL PRINT_STRW, "「哇_HH_ ……真是得意忘形。好吧、開始早上的个人特别授业吧_HH_」"
				ELSEIF RAND:2
					PRINTFORMW 「明明是被偷袭了，却露出那么高兴的表情……该怎么说才好呢？」
					PRINTFORMW 「嗯……好吧、这可是事関面子。让我试着让你沉浸到溺死的程度吧」
				ELSE
					PRINTFORMW 「……起床了吧？你装睡我也知道，小鸡鸡的反应完全不一样」
					CALL PRINT_STRW, "「继续装下去的话没関系，就那样吧。如果没有抵抗的话，就会全部吃光的_HH_」"
				ENDIF
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「你对%CNAME("一人称")%的早上的侍奉有什么疑问吗？」
					PRINTFORMW 「那是当然的，嗯，嗯。像这样让%CNAME("二人称")%被迷住才是老师的工作啊、呐」
				ELSEIF RAND:2
					PRINTFORMW 「很暖和吧，很舒服吧、%CNAME("一人称")%的里面」
					PRINTFORMW 「沉浸吧，堕落吧，这一定是没办法的事」
				ELSE
					PRINTFORMW 「……看起来就像溺水了一样呢」
					PRINTFORMW 「不，是说我这边的事情，我这边的」
				ENDIF
			ENDIF
		ENDIF
	CASE "朝Ａ騎乗位"
		IF CEVENT("名没有の呼び出し") || RAND:2
			IF RAND:3 == 0
				CALL PRINT_STRW, "「哦、我先吃早饭了_HH_」"
				CALL PRINT_STRW, @"「%CNAME("一人称")%的肚子填饱了、%CNAME("二人称")%的感觉舒服了、不是双赢嘛？_HH_」"
			ELSEIF RAND:2
				CALL PRINT_STRW, @"「啊、感觉很舒服呢_HH_ 那个让%CNAME("一人称")%变成现在这样的家伙_HH_」"
				CALL PRINT_STRW, "「喂，刚睡醒可不能马虎_HH_ 你还年轻，要表现出让对方屈服的气概_HH_」"
			ELSE
				CALL PRINT_STRW, @"「抵抗是徒劳的_HH_%CNAME("二人称")%就老实的，可怜的，被屁股榨出来吧_HH_」"
				CALL PRINT_STRW, "「呜…_HH_ 呋呒_HH_ 嗯、不错呢、虽然说了没用，但是如果不抵抗的话就没有意思了_HH_」"
			ENDIF
		ELSE
			IF TALENT:恋慕
				IF RAND:3 == 0
					CALL PRINT_STRW, "「呋呒……_HH_ 怎么了吗？」"
					CALL PRINT_STRW, "「在早上用屁股做您不喜欢吗？_HH_」"
				ELSEIF RAND:2
					CALL PRINT_STRW, "「害怕、是吗？ 还是很舒服……？_HH_」"
					CALL PRINT_STRW, "「请放心……马上……不，对不起。不会马上结束的_HH_」"
				ELSE
					CALL PRINT_STRW, "「啊、那个、来做吧_HH_」"
					CALL PRINT_STRW, "「像这样一大早就被用屁股吃的脸……真的、真的……好可爱」"
					CALL PRINT_STRW, "「一不小心就想榨干了怎么办啊_HH_」"
				ENDIF
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「请不要太过抵抗啊」
					PRINTFORMW 「要是不小心，今天可能就动不了也说不定」
				ELSEIF RAND:2
					CALL PRINT_STRW, "「……呋呒_HH_ 啊、忍不住了」"
				ELSE
					PRINTFORMW 「啊，早上好。我先要開动了。」
					PRINTFORMW 「对、对。早上的你特别的美味……呋呒」
				ENDIF
			ENDIF
		ENDIF
	CASE "朝接吻"
		IF CEVENT("名没有の呼び出し") || RAND:2
			IF RAND:3 == 0
				PRINTFORMW 「呦……你这张脸是怎么回事？奶奶的吻会让你睡不好吗」
				PRINTFORMW 「是是、我做了一些我年龄不应该做的事。对不起、对不起――呜…等等、啊……嗯噗……」
				PRINTFORMW 「……虽然%CNAME("一人称")%来说不合适，但是%CNAME("二人称")%也真是够坏心眼的、可恶」
			ELSEIF RAND:2
				PRINTFORMW 「啊、你脸红了……大概是刚睡醒的酒起作用了吧」
				PRINTFORMW 「……没有闻到酒气？不要说不必要的话，不解风情」
			ELSE
				PRINTFORMW 「怎么样？醒了吗」
				PRINTFORMW 「被这样的美女吻醒，当然是最棒的吧？」
			ENDIF
		ELSE
			IF TALENT:恋慕
				IF RAND:3 == 0
					PRINTFORMW 「就像打招呼一样。如果你喜欢更激烈一点的话……呜」
					PRINTFORMW 「あ……ん、ふ……ちゅっ……くちゅっ……あ、舌頭……ん、じゅる……（没有翻译的部分是舌吻声）」
					PRINTFORMW 「…………啊……到此为止。我要认真上了」
				ELSEIF RAND:2
					PRINTFORMW 「是什么呢、回礼？ ……说的也是。因为是能得到的东西就得到的主义嘛」
					PRINTFORMW 「……嗯……咕啾…啊……等、等一下……嗯……哇、好激烈…………哦……呜……」
					PRINTFORMW 「…………唔、这就结束了吗？再借我一把，我还你三倍」
				ELSE
					PRINTFORMW 「啾噜……啾……嗯……还……啾…………啊……不行……嗯……」
					PRINTFORMW 「呜……不行啊…………啾……嗯……啊、哈啊……啾噜……嗯……噗啊…」
					PRINTFORMW 「FU……只是温柔地吻一下就起床也很好，不过，用让人缺氧的吻一下就起床也很棒吧？」
				ENDIF
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「做了好梦吗？现在就是梦的后续了」
				ELSEIF RAND:2
					PRINTFORMW 「只是打招呼啊，招呼」
				ELSE
					PRINTFORMW 「被啄一般的吻叫醒也不错吧？」
				ENDIF
			ENDIF
		ENDIF
	CASE "朝添い寝"
		PRINTFORMW 

	CASE "朝口交"
		IF CEVENT("名没有の呼び出し") || RAND:2
			IF RAND:3 == 0
				CALL PRINT_STRW, @"「因为混蛋恶魔的错%CNAME("一人称")%也等着你呢_HH_」"
				CALL PRINT_STRW, "「早上被吮吸的份舒服吧、えぇおい（含住的声音）_HH_」"
				CALL PRINT_STRW, "「不会生气的，我会把你榨到连尿都不像样地漏出来为止_HH_」"
			ELSEIF RAND:2
				CALL PRINT_STRW, "「啾噗…_HH_ 啾噜…_HH_ ……啊？终于起来了」"
				CALL PRINT_STRW, "「哈啊，我本来想在起床前多榨几发，所以就安静地干了，真是不好意思。」"
				CALL PRINT_STRW, "「嘛、既然这样的话就尽情地用婊子的嘴来榨你了做好心理准备吧_HH_」"
			ELSE
				CALL PRINT_STRW, "「哦哦，明明是早上第一发，却拖拖拉拉没有节操_HH_」"
				CALL PRINT_STRW, "「真好啊，年轻的人不这样就不行啊，_HH_ 那就加油，别把今天的份都被榨出来了了_HH_」"
			ENDIF
		ELSE
			IF TALENT:恋慕
				IF RAND:3 == 0
					PRINTFORMW 「啊，你醒了？真遗憾」
					PRINTFORMW 「本想就这样把今天的东西全部用嘴榨干的…」
				ELSEIF RAND:2
					PRINTFORMW 「呋呒、还这么有精神呢……真是、无底洞呢」
					PRINTFORMW 「如果你那么想让我含住那就好吧，再继续一会儿吧」
				ELSE
					PRINTFORMW 「流这么多汁，奶奶早上的嘴就那么舒服吗？」
					PRINTFORMW 「…………只是点了頭有点为难啊。是该高兴呢，还是该为没有否定我是奶奶而生气呢」
				ENDIF
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「ん……じゅる……ちゅ……ぷぁっ（之前为口交拟声词）…………起来了吗」
					PRINTFORMW 「你做了好梦吗？那我们继续这个梦吧」
				ELSEIF RAND:2
					PRINTFORMW 「啊……看到了吗？这张嘴含着%CNAME("二人称")%的小鸡鸡」
					PRINTFORMW 「那么就这样躺着……一边想象这张嘴是怎么动的，一边继续吧」
				ELSE
					PRINTFORMW 「啊……嗯……啊，发现了」
					PRINTFORMW 「大清早勃起的这么大，还散发着好厉害的气味……不含进去才失礼吧？」
				ENDIF
			ENDIF
		ENDIF

	CASE "朝六九式"
		PRINTFORMW 

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		PRINTFORMW 「早上好。…怎么了？」
		PRINTFORMW 「哦呀、好像被吓了一跳的样子呢…喂、不打起精神的话」
		PRINTFORMW 「又会被吃掉的吧？」
		PRINTFORMW %CALLNAME:TARGET%微微张開口腔，露出因唾液而闪闪发光的舌尖。
	CASE "朝騎乗位"
		PRINTFORMW 「明明是在接受别人的侍奉，却一脸茫然，这可是要扣分的」
		PRINTFORMW 「看来惩罚是必要的呢。没错、特别严厉的」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 
	ELSE
		PRINTFORMW 「早上好」
		PRINTFORMW 「去洗个脸怎么样？现在是吃早饭的时间」
	ENDIF
CASE "一番乗り"
	IF CEVENT("名没有の呼び出し")
		PRINTFORMW 「……什么啊、是%CNAME("二人称")%啊」
		PRINTFORMW 「我可是很胆小的，你不要吓我啊」
	ELSE
		PRINTFORMW 「只有两人吗」
		PRINTFORMW 「今天…身体状态不错吧？」
	ENDIF
CASE "ただいま"
	IF CEVENT("名没有の呼び出し")
		PRINTFORMW 「哟，我回来了（前面是口语化的发音）……嗯、咳咳！！ ……我回来了（正式端正的发音）」
		PRINTFORMW 「……对不起，那个……我好像疏忽了」
	ELSE
		PRINTFORMW 「…………啊…不、只是有点发呆」
		PRINTFORMW 跟一直站在大門前的%CALLNAME:TARGET%打了声招呼，她罕见地慌慌张张地走了进去。
		PRINTFORMW 「真讨厌啊年纪大了……我希望你立刻否定这一点啊，真是的。」

	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF CEVENT("名没有の呼び出し")
			IF CHARA_NUM("室内の邪魔者") == 0
				PRINTFORMW 「哦，欢迎回来。这么晚了，有什么有趣的事情吗？」
			ELSE
				PRINTFORMW 「啊……欢迎回来」
				PRINTFORMW 「有一点担心呢。」
			ENDIF
		ELSE
			PRINTFORMW 「欢迎回来」
			PRINTFORMW 「你还是学生，不要太晚，请注意安全」
		ENDIF
	;部室
	ELSE
		IF CEVENT("名没有の呼び出し")
			PRINTFORMW 「啊、谢谢」
			PRINTFORMW 「…………那么今天和谁？不，没有别的意思」
		ELSE
			PRINTFORMW 「您好、%CNAME("一人称")%（怀疑原作者标错了，应该是想要用第"二人称"）」
			PRINTFORMW 「%CNAME("二人称")%已经做好准备了……（怀疑原作者标错了，应该是想要用第"一人称"）」
			PRINTFORMW 「什么样的准备、是吗？ ……不知道、什么样的呢」
		ENDIF
	ENDIF
CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORMW 
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF CEVENT("名没有の呼び出し")
			PRINTFORMW 「啊？…那个……这是这是……失礼了呢、请尽管继续」
			PRINTFORMW 进入室内的%CALLNAME:TARGET%虽然是和颜悦色的声音，却有一种奇怪的压迫感…
		ELSE
			PRINTFORMW 「啊啦……在狩猎之中吗」
			PRINTFORMW %CALLNAME:TARGET%一进屋就露出微笑。
			PRINTFORMW 然后走近%CALLNAME:MASTER%的身边，悄悄在他耳边说。
			PRINTFORMW 「……你不想沉溺于梦境吗？」
		ENDIF
	ELSE
		PRINTFORMW 「你好……。哎呀，今天有点晚了呢」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「让您久等了，职员会议拖了很久」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「找老师有什么事吗？」
	ELSE
		PRINTFORMW 「失礼了…那么，没有做坏事吧？」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『あら、%CNAME(CALLNAME_ACTOR)%、お疲れ様です』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF RAND:2
			PRINTFORMW 『好晚啊』
		ELSE
			PRINTFORMW 『哦呀、迟到什么的……、应该没有这回事吧』
		ENDIF
	ELSE
