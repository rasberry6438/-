;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K30, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝接吻"
		CALL EMOTION_PLUS, "重置/左闭眼/半目/小口/紅潮２"
		PRINTFORML 「嗯、啾……%TEXTR("早上好/唔、醒了啊")%、%CNAME("二人称")%」
		IF TEXTS("曜日") == "日"
			PRINTFORMW 「今日是休沐之日、%TEXTR("可有安排行程？/打算如何消遣？")%%TEXTR(@"%CNAME("一人称")%想要稍微出門逛逛……要是没事的话能一起来吗？/要是没安排的话不如待在家中休息呢/%CNAME("一人称")%要独自享受清闲时光了")%」
		ELSE
			PRINTFORMW 「%TEXTR(@"今日也一起努力度过吧/要不要来个早安吻呢、%CNAME("二人称")%啊/只是亲吻就已经半勃了吗……晨起便如此精神实乃幸事")%」
		ENDIF
	CASE "朝口交", "朝六九式"
		CALL EMOTION_PLUS, "重置/半目/小口/紅潮２"
		PRINTFORMW 「嗯…嗯咕…嗯…啾啵、啾噜………哦、总算醒了吗、%CNAME("二人称")%」
		PRINTFORMW 「…不过早晨只能做这些呢？晚些时候再陪你尽兴」
	CASE "朝騎乗位"
		CALL EMOTION_PLUS, "重置/ワ/紅潮２/对话框爱心"
		CALL PRINT_STRW, "「嗯啊、好棒、很好……咕、啊啊_H_」"
		CALL PRINT_STRW, @"「哈、哈啊……早上好、%CNAME("二人称")%。呵呵、叫起骑乘位效果拔群呢……啊_H_」"
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日还有体育课。给、运动服」
	ELSE
		IF RAND:2
			PRINTFORMW 「快起来、你这小懒虫」
			PRINTFORMW 「还有吃早餐的时间。赶紧吃完做好上学准备」
		ELSE
			PRINTFORMW 「快起来、%CNAME("二人称")%よ」
			IF FINDCHARA(NO, 1) > 0
				PRINTFORMW 「%CALLNAME:FINDCHARA(NO, 1)%已经在做早餐了」
			ENDIF
		ENDIF
	ENDIF
CASE "一番乗り"
	PRINTFORMW 「打扰了…怎么、谁也不在吗」
CASE "ただいま"
	PRINTFORMW 「妾身回来了、%CNAME("二人称")%」
	PRINTFORMW 「%TEXTR(@"一直都在角落注视着你、今天一天都很努力呢。来、让妾身摸摸頭吧/今天的晚饭是什么？已经饿得不行了/现在、浴室没人用吗……那%CNAME("一人称")%可以先去洗澡吗？")%」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「回来了吗、%CNAME("二人称")%啊」
		PRINTFORMW 「%TEXTR("一直都在角落注视着你、今天一天都很努力呢。来、让妾身摸摸頭吧/浴室的热水已经烧好了。先去洗澡吧/疲倦的时候甜食最有效了。来、给你糖")%」
	ELSE
		PRINTFORMW 「来了吗、%CNAME("二人称")%」
		PRINTFORMW 「%TEXTR(@"上课打瞌睡可真不像话。若非%CNAME("一人称")%偷偷叫醒你、真不知道会变成怎样…/今天倒是认真学习了。下午也要保持这个势頭继续努力哦/抱歉妾身先休息了一会儿。……要来杯茶吗？")%」
	ENDIF

CASE "入室打招呼"
	SELECTCASE CFLAG:MASTER:PREV_DAILYCATE
	;Talk
	CASE 1
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "甜言蜜语"
			PRINTFORMW 「…%TEXTR("嚯/嗯")%、正在打情骂俏吗」
		CASE "打招呼舐陰", "打招呼口交"
			PRINTFORMW 「…%TEXTR("嗯/嚯")%、大白天的就这么兴致勃勃呢？」
		ENDSELECT
		PRINTFORMW %CALLNAME:TARGET%正饶有兴致地注视着%CALLNAME:MASTER%他们、露出笑容。
	;互动
	CASE 2
	;卖个破绽
	CASE 3
	;胖次
	CASE 4
		IF RAND:2
			CALL EMOTION_PLUS, "重置/斜眼２/斜眉"
			PRINTFORMW 「……啊不、不用在意%CNAME("一人称")%继续就好」
			PRINTFORMW 「或者说妾身该暂时回避一下？」
		ELSE
			CALL EMOTION_PLUS, "重置/斜眼２/斜眉/無口系/对话框・・・"
			PRINTFORMW 「啊那个、%CNAME("二人称")%呀…」
			PRINTFORMW 「…也罢。在这里做这种事也不会有人苛责呢」
		ENDIF
	CASE 6
		IF RAND:2
			CALL EMOTION_PLUS, "重置/斜眼２/斜眉/無口系/对话框・・・"
			PRINTFORMW 「……啊不、不用在意%CNAME("一人称")%继续就好」
			IF TALENT:恋慕
				PRINTFORMW 「当然、之后也会陪%CNAME("一人称")%的吧？」
			ELSE
				PRINTFORMW 「或者说妾身该暂时回避一下？」
			ENDIF
		ELSE
			CALL EMOTION_PLUS, "重置/斜眼２/△"
			PRINTFORMW 「啊那个、%CNAME("二人称")%呀…」
			IF TALENT:恋慕
				PRINTFORMW 「%CNAME("一人称")%也可以那个……陪你做各种各样的事哦？」
			ELSE
				PRINTFORMW 「…也罢。在这里做这种事也不会有人苛责呢」
			ENDIF
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF CFLAG:呼び出された
		;IF COND("夜")
			
		;ELSE
			PRINTFORML 「%TEXTR("怎么了/什么事/呼唤妾身了吗/有何贵干/即刻赶来了")%、%CNAME("二人称")%」
			IF RAND:2
				PRINTFORML 「…%TEXTR("呵呵/真是/哈啊")%、不必那么惊讶嘛……%CNAME("二人称")%真是可爱」
				PRINTFORMW %TEXTR(@"从视野的死角/从%CALLNAME:MASTER%的身后/不经过門窗")%无声无息悄然现身的%CALLNAME:TARGET%正露出恶作剧得逞的愉快笑容……。
			ELSE
				PRINTFORML 「%TEXTR("想念妾身了吗？呵呵♪/对妾身有事相求吗？/需要妾身的力量吗/妾身即刻赶来了、毕竟说不定是重要的契约者在求救呢")%」
				PRINTFORMW 被召唤的%CALLNAME:TARGET%正露出得意的微笑。
			ENDIF
		;ENDIF
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR("…来晚了吗/嗯、大家似乎都到齐了")%」
	ELSE
		PRINTFORMW 「妾身来打扰了」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『…又要上课又要处理这边、教师很辛苦吧？』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『太慢了。大家基本都到齐了』
	ELSE
		PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%吗。妾身就先休息了』
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "便当"
	IF CEVENT("初めての便当") == 0
		CALL EMOTION_PLUS, "重置/飘忽/困扰眉/無口系"
		PRINTFORMW 「好了、按照约定把便当做好了……。要是合%CNAME("二人称")%的口味就好了」
		PRINTFORMW 颇为不自信的%CALLNAME:TARGET%略显迟疑递过来的便当是……奶油水果三明治。
		PRINTFORMW 尝了一口、醇厚奶油的微甜和水果那清爽的酸味绝妙的搭配、相当美味…。
		CALL EMOTION_PLUS, "重置/笑颜４/ヮ"
		PRINTFORMW 「…从这反应来開……看来做出能吃的东西了」
		PRINTFORMW 「能让别人觉得自己做的食物好吃……还挺让人高兴的呢」
		PRINTFORMW 就着%CALLNAME:TARGET%那带着酸甜味的腼腆笑容、将便当吃得一干二净…。
		CALL SET_CEVENT, "初めての便当", TARGET
	ELSE
		CALL EMOTION_PLUS, "重置/笑颜４"
		IF RAND:3 == 0
			PRINTFORMW 「今天的便当是%TEXTR("苹果派/无花果挞/黄油太妃肉桂派")%」
			PRINTFORMW 「…说是便当但其实是甜点？抱歉、突然就很想吃这个……」
		ELSEIF RAND:2
			PRINTFORMW 「今天的便当是%TEXTR("咖喱饭/芝士鸡肉盖饭/海鲜烩饭")%配可乐饼」
			PRINTFORMW 「家父曾大肆宣扬这是人间界最美味的食物、有阵子天天让厨师做这个…」
		ELSE
			PRINTFORMW 「今天的便当是%TEXTR("炸猪排三明治/水果三明治/鸡蛋三明治/ＢＬＴ三明治")%哦」
			PRINTFORMW 说着%CALLNAME:TARGET%递过来三个便当盒…。
			PRINTFORMW 「……干劲十足导致做过頭了。看来%CNAME("一人称")%今天的下午茶点心也会是三明治了」
		ENDIF
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K30, ARGS
#DIM MEMO_LINECOUNT
#DIM ACTOR
MEMO_LINECOUNT = LINECOUNT


SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「悪魔の%CALLNAME:TARGET%だ。请多関照」
	PRINTFORML %CALLNAME:TARGET%は部員達に握手して回っている。
CASE "浑身是汗"
	CALL EMOTEFACE, "重置/半目/蹙眉/へ字２/对话框汗２/頬汗"
	IF RAND:2
		PRINTFORML 「呼、好热…。衣服都被汗浸透了」
		PRINTFORML %CALLNAME:TARGET%一边不停的擦汗、一边用团扇向胸口扇风。
	ELSE
		PRINTFORML 「%CNAME("一人称")%还不是容易出汗的人…」
		PRINTFORML 筋疲力尽的%CALLNAME:TARGET%无力地擦着汗…。
	ENDIF
CASE "刚睡醒"
	IF CHOSE_DEVENT("睡眠姦接吻")
		CALL EMOTEFACE, "重置/左闭眼２/無口系/紅潮２/对话框爱心/托下巴"
		PRINTFORML 「%TEXTR(@"呜呣…王子的吻…吗/嗯…觉醒地比我想象地要快/%CNAME("二人称")%啊。%CNAME("一人称")%地嘴上怎么有我的味道？库库ッ♪")%」
	ELSEIF CHOSE_DEVENT("睡眠姦口交")
		CALL EMOTEFACE, "重置/斜眼２/困扰眉/ڡ/紅潮２"
		IF RAND:2
			PRINTFORML 「嗯、ぅ…ッ、你的精液真浓…咳咳…」
			IF ABL:精液中毒
				PRINTFORML %CALLNAME:TARGET%在那里抱怨着、脸却放松下来、舌頭好像在品味精液般慢慢搅动…。
			ELSE
				PRINTFORML %CALLNAME:TARGET%熏红了脸轻轻地咳嗽…。
			ENDIF
		ELSE
			PRINTFORML 「…%CNAME("一人称")%睡觉时的嘴巴舒服吗？」
			IF ABL:精液中毒
				PRINTFORML %CALLNAME:TARGET%舔着嘴唇，用火热的目光注视着%CALLNAME:MASTER%…。
			ELSE
				PRINTFORML %CALLNAME:TARGET%用呆滞的目光看着%CALLNAME:MASTER%…。
			ENDIF
		ENDIF
	ELSEIF CHOSE_DEVENT("睡眠姦Ｖ中出し")
		CALL EMOTEFACE, "重置/左闭眼２/微笑６/紅潮２/对话框爱心"
		IF RAND:3 == 0 && ABL:信頼 >= 5 && CEVENT("両親の夢")
			PRINTFORML 「…我梦到自己在%TEXTR(@"父亲死的那天晚上/母亲的葬礼之后")%，被%CNAME("二人称")%抱在怀中」
			PRINTFORML 「梦中的%CNAME("二人称")%呢…一边温柔地安慰哭泣的%CNAME("一人称")%、一边尽力我身体里注入精液…？」
			PRINTFORML 用手指擦掉眼角的泪痕、%CALLNAME:TARGET%一脸得意地靠近%CALLNAME:MASTER%…。
		ELSEIF RAND:2
			PRINTFORML 「嗯…谁的精子啊，感觉子宫好重…。%CNAME("二人称")%、你知道是谁吗？」
			PRINTFORML %CALLNAME:TARGET%故意揉了揉下腹问道…。
		ELSE
			PRINTFORML 「呣、这个是…内褲上地是精液吗」
			PRINTFORML 「库库、%CNAME("一人称")%睡觉时地小穴很舒服吧…%CNAME("二人称")%觉得呢」
			PRINTFORML %CALLNAME:TARGET%优雅地用手指搅动上面精液、向%CALLNAME:MASTER%露出威胁的笑容…。
		ENDIF
	ELSE
		;奥丝卡が夢でうなされているのを一度でも見ると追加
		IF RAND:3 == 0 && CEVENT("両親の夢") + CEVENT("悪夢")
			CALL EMOTEFACE, "重置/左涙目＞＜/蹙眉/哈欠大"
			PRINTFORML 「%TEXTR(@"夢啊…夢、原来是夢…/…啧、又是那个梦吗…/…怎么了%CNAME("二人称")%。%CNAME("一人称")%的脸上有什么东西吗？")%」
			PRINTFORML %CALLNAME:TARGET%的脸颊与眼角，出现了淡淡的泪痕…。
		ELSEIF RAND:2
			CALL EMOTEFACE, "重置/左闭眼２/哈欠小"
			PRINTFORML 「嗯嗯嗯恩…！呃啊啊啊…」
			PRINTFORML 醒来的%CALLNAME:TARGET%挺起了脊背、舒展的身体强调着丰满的胸部。
		ELSE
			CALL EMOTEFACE, "重置/左闭眼２/半目/～/对话框困倦/唇边手"
			PRINTFORML 「呼…呼哇～…咕噜咕噜…」
			PRINTFORML %CALLNAME:TARGET%优雅地用手遮住嘴巴打了个哈欠。
		ENDIF
	ENDIF
CASE "Cosplay中"
	PRINTFORML 
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K30
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML （父亲…母亲…我……会幸福的…!）
	PRINTFORML 眼睛里含着泪的%CALLNAME:TARGET%握着%CALLNAME:MASTER%的手祈祷道…。
CASE "约会的约定"
	PRINTFORML （好了，现在開始得准备衣服了…）
;これは悪魔のみ
CASE "契約成立"
	IF RAND:3 == 0
		CALL EMOTEFACE, "重置/小口/托下巴"
		PRINTFORML 「不过、一天就能再生一人还多的魂…」
		PRINTFORML 「…你真幸运没被那些人渣注意到」
	ELSEIF RAND:2
		CALL EMOTEFACE, "重置/左笑颜４/对话框開心"
		PRINTFORML 「库库、这场交易真不错♪　下次继续」
		PRINTFORML %CALLNAME:TARGET%看起来非常高兴，对%CALLNAME:MASTER%露出了笑容。
	ELSE
		CALL EMOTEFACE, "重置/左闭眼２/半目/ڡ２"
		PRINTFORML 「嗯哼、真是优质的魂。那么马上…啊、嗯…」
		IF RAND:2
			PRINTFORML %CALLNAME:MASTER%只见白色的魂白色摇曳的灵魂缠绕在%CALLNAME:TARGET%艳丽的舌頭上被慢慢吞下…。
			PRINTFORML 「…着是什么表情。只是美美地享用了%CNAME("二人称")%交易的魂而已…库库库♪」
		ELSE
			PRINTFORML 「…呼呼、感觉得到吗…%CNAME("二人称")%的魂喝%CNAME("一人称")%融为一体了…」
			PRINTFORML %CALLNAME:TARGET%把双手放在乳沟上，心满意足地呼出了口气…。
		ENDIF
	ENDIF
CASE "摆脱"
	PRINTFORML 
CASE "主人公的房間に連れ込まれた"
	IF RAND:2
		PRINTFORML 「怎么了、特意把%CNAME("一人称")%带进来」
		PRINTFORML 「…难道想对妾身做什么吗？呵呵♪」
		SIF BASE:欲求不満 >= 75
			PRINTFORML %CALLNAME:TARGET%虽然像在開玩笑一样咯咯地笑着、但注视着%CALLNAME:MASTER%的眼神中却盈满了期待…。
	ELSE
		PRINTFORML 「嗅嗅……每次经过房門前都会察觉到、果然充斥着%CNAME("二人称")%的气息呢」
		PRINTFORML 「青涩的、充满精力的年轻雄性的味道…。呵呵、至少%CNAME("一人称")%并不讨厌这股味道哦？」
		SIF BASE:欲求不満 >= 75
			PRINTFORML %CALLNAME:TARGET%将身子软绵绵地贴向%CALLNAME:MASTER%、像是要留下标记一样腰肢紧贴过来磨蹭着…。
	ENDIF
CASE "自室に招待"
	IF RAND:2
		PRINTFORML 「%CNAME("二人称")%呀。有空的话不如就在%CNAME("一人称")%的房间消磨时间如何？」
		PRINTFORML 「%TEXTR("说起来有点不好意思、妾身有点寂寞啊/该怎么说呢、妾身有点无聊")%…。明明本该习惯独自一人生活了…」
	ELSE
		PRINTFORML 「特意让你过来真不好意呀、%CNAME("二人称")%」
		PRINTFORML 「没什么、只是单纯想和%CNAME("二人称")%二人独处罢了」
		SIF BASE:欲求不満 >= 75
			PRINTFORML 这样说着的%CALLNAME:TARGET%将身子贴近%CALLNAME:MASTER%、挑逗地伸出手在%CALLNAME:MASTER%的胸口和下腹部持续抚摸着…。
	ENDIF
CASE "自室に訪問された"
