;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K35, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 俺_俺
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "３年生", "研究生"
			LOCALS = 你_你
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = 你_你
			ELSE
				LOCALS = 你_你
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	IF NAME:PLAYER == "你"
		LOCALS = 部长_部长
	ELSE
		LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
	ENDIF
CASE "助手"
	IF NO:ASSI == 34
		LOCALS = 真_まこと
	ELSEIF NO:ASSI == 48
		LOCALS = 莉_れい
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
			ELSE
				IF NO:PLAYER == 1
					LOCALS = 弗理小姐_弗理小姐
				ELSE
					LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

