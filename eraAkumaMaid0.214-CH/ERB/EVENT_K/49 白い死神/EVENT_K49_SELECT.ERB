						IF ABL:受虐属性 >= 5 && RAND:2
							CALL PRINT_STRW, @"「はぁ……_HH_はぁ……_HH_此れ……ダメ、だね……」"
							PRINTFORMW 荒れた息でぽつりと、%CALLNAME:TARGET%が呟く。
							CALL PRINT_STRW, @"「斯うして%CNAME("二人称")%に乱暴されるのを……楽しみにしてる……%CNAME("一人称")%がいる……から……_／／／_」"
