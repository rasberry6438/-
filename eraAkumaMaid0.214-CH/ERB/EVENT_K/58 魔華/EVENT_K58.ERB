;-------------------------------------------------
@CNAME_K58, ARGS
#FUNCTIONS
;名前を呼ぶ相手の角色番号の代入に使う
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 前輩_前輩
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
;ここ莉雅拉老师のマネ,CASEELSEの下に持ってくるのはまずいらしい
CASE "精子"
	LOCALS  = 精子_精子/子种_子种/
	LOCALS += "精子_精子/子种_子种/基因_基因/精虫_精虫/精细胞_精细胞/"
CASE "精液"
	LOCALS  = 精液_精液/白浊_白浊/精液_精液/肉棒汁_肉棒汁/浓厚牛奶_浓厚牛奶/
	LOCALS += "性欲结晶_性欲结晶/雄汁_雄汁/雄兽汁液_雄兽汁液/精液_精液/蛋白液_蛋白液/"
	LOCALS += "精液_精液/精液_精液/"
CASE "射精音"
	LOCALS  = 噗噜噗噜_噗噜噗噜/咻呜咻呜_咻呜咻呜/咕嘟咕嘟_咕嘟咕嘟/噗咻噗咻_噗咻噗咻/噗咕噗咕_噗咕噗咕/咻噜咻噜_咻噜咻噜/
	LOCALS += "噗噜噗噜_噗噜噗噜/咻呜咻呜_咻呜咻呜/咕嘟咕嘟_咕嘟咕嘟/噗咻噗咻_噗咻噗咻/噗咕噗咕_噗咕噗咕/咻噜咻噜_咻噜咻噜/"
CASE "子宮"
	LOCALS  = 子宮_子宫/蜜穴深处_蜜穴深处/雌性器官_雌性器官/精液容器_精液容器/
	LOCALS += "子宫_子宫/蜜穴深处_蜜穴深处/"
	LOCALS += "白浊回收站_白浊回收站/精液废弃场_精液废弃场/雄汁处理站_雄汁处理站/欲望排泄口_欲望排泄口/"
CASE "膣"
	LOCALS  = 小穴_小穴/肉便器_肉便器/侍奉穴_侍奉穴/榨精器_榨精器/自慰穴_自慰穴/
	LOCALS += "撒娇穴_撒娇穴/飞机杯_飞机杯/手淫孔_手淫孔/手淫排泄口_手淫排泄口"
CASE "陰茎形容"
	SELECTCASE SIZE("阴茎")
	;馬根、規格外
	CASE IS >= 3
			LOCALS = 怪物_怪物/猛兽_猛兽/杀人_杀人/非人_非人/
	;巨根
	CASE 2
			LOCALS = 凶恶_凶恶/极恶_极恶/超粗_超粗/凶暴_凶暴/
	;普通
	CASE 1
			LOCALS = 绝伦_绝伦/变态_变态/炮王_炮王/超强_超强/
	;短小
	CASEELSE
			LOCALS = 可爱_可爱/萌萌的_萌萌的/
	ENDSELECT
;この下は三人称。
CASE "主人"
	LOCALS = 前輩_前輩
CASEELSE
	;名前を呼ぶ相手の角色番号をACTORに代入する
	SELECTCASE ARGS
	;相手が助手
	CASE "助手"
		ACTOR = ASSI
	;ここにはARGSに角色のCALLNAMEが入ったりしている場合(ARGS == "弗理希艾尔" など)に来る
	CASEELSE
		ACTOR = FIND_NO(ARGS)
	ENDSELECT

	;相手が教員
	IF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%老师_%CSTR:ACTOR:ふりがな%老师

	;自分も相手も学生なら学年比較
	ELSEIF COND("学生") && COND("学生", ACTOR)
		;相手が同学年
		IF COND("学年") == COND("学年", ACTOR)
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		;相手が下の学年
		ELSEIF COND("学年") > COND("学年", ACTOR)
			IF TALENT:ACTOR:男性
				LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
			ELSE
				LOCALS = %CALLNAME:ACTOR%酱_%CALLNAME:ACTOR%酱
			ENDIF
		;相手が上級生
		ELSE
			LOCALS = %CALLNAME:ACTOR%前輩_%CSTR:ACTOR:ふりがな%前輩
		ENDIF

	;それ以外
	ELSE
		LOCALS = %CALLNAME:ACTOR%桑_%CALLNAME:ACTOR%桑
	ENDIF

ENDSELECT

RETURNF LOCALS

