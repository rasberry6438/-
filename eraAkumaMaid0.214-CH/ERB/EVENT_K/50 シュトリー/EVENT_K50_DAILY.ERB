;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K50, ARGS
#DIM MEMO_LINECOUNT
#DIM LCOUNT
#DIM NUM_FIND
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR


MEMO_LINECOUNT = LINECOUNT
ACTOR = 0
NUM_FIND = 0

;文字色変更
CALL CHANGE_OFC, "セット", TARGET

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K50, %ARGS%, %CSTR:朝袭%
		[ENDIF]
	CASE "朝騎乗位"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K50, %ARGS%, %CSTR:朝袭%
		[ENDIF]
	ENDSELECT

	;何か発言してたらここまで
	IF LINECOUNT > MEMO_LINECOUNT
		CALL CHANGE_OFC, "重置", TARGET
		RETURN 1
	ENDIF

	IF COND("体育")
		PRINTFORMW 「早上好ー！ %CNAME("二人称")%」
		PRINTFORMW 「是、授業で使う体操服、忘れないでね？」
	ELSE
		PRINTFORMW 「早上好ー！ %CNAME("二人称")%」
		IF RAND:3 == 0
			PRINTFORMW 「目は覚めた？ まだぼーっとしてるかな？」
		ELSEIF RAND:2
			PRINTFORM 「のんびりしてると
			IF TEXTS("曜日") != "日" && RAND:2
				PRINTFORMW 遅刻しちゃうよ？」
			ELSE
				PRINTFORMW ごはん冷めちゃうよ？」
			ENDIF
		ELSE
			PRINTFORMW 「んふふー、今の気分はどう？」
		ENDIF
		
	ENDIF
CASE "一番乗り"
	IF COND("同伴中")
		CALL EMOTION_PLUS, "重置/笑颜/微笑/红晕", TARGET
		IF CHARA_NUM("集合予定") == 3
			PRINTFORMW 「%CNAME("弗理希艾尔")%はまだ来てないみたいだね！ ……なにしよっか？」
		ELSE
			PRINTFORMW 「皆はまだ来てないみたいだね！ ……なにしよっか？」
		ENDIF
	ELSE
		IF RAND:2 && CHARA_NUM("集合予定") > 3
			CALL EMOTION_PLUS, "重置/眯眯眼/对话框兴奋", TARGET
			PRINTFORMW 「%CNAME("二人称")%、您好」
			PRINTFORMW 「二番目は%CNAME("二人称")%かー。……次は誰だろね？」
		ELSE
			CALL EMOTION_PLUS, "重置/眯眯眼/困扰眉/红晕/对话框汗", TARGET
			PRINTFORMW 「%CNAME("二人称")%、您好」
			PRINTFORMW 「ちょっと早く来すぎちゃったみたい」
		ENDIF
	ENDIF
CASE "ただいま"
	PRINTFORMW 「ただいまー、%CNAME("二人称")%」
	;打招呼に弗理希艾尔がいる
	IF FINDT_COND_K50(TARGET, "入室打招呼中", "弗理希艾尔") && RAND:4 == 0
		PRINTFORMW 「%CNAME("フェリシェル")%と買い物してたら遅くなっちゃった」
	;会話に参加している角色がいる
	ELSEIF FINDT_COND_K50(TARGET, "入室打招呼中") && RAND:3 == 0
		PRINTFORMW 「归途で%CNAME(@"{FINDT_COND_K50(TARGET, "入室打招呼中")}")%とたまたま一緒变成了んだよ」
	ENDIF
	CALL SET_CEVENT, "Ｄイベ：帰宅の打招呼", TARGET
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		;翌日でも归途の行動が変わらないパターンもあるので連続で発生不会うにする
		SIF SAVESTR:归途の行動 != "相合傘" && CEVENT("雨の会話")
			CALL DEL_CEVENT, "雨の会話", TARGET
		IF DAYEV("試験間近")
			PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
			PRINTFORMW 「ねぇ、%CNAME("二人称")%、後で勉強教えてね！」
		ELSEIF SAVESTR:归途の行動 == "相合傘" && CEVENT("雨の会話") == 0
			PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
			PRINTFORMW 「雨が降ったけど%CNAME("二人称")%は大丈夫だった？」
			CALL SET_CEVENT, "雨の会話", TARGET
		ELSEIF COND("同伴中", MASTER) && RAND:2
			PRINTFORMW 「%CNAME("二人称")%、%CNAME("partner")%、おかえりなさい」
		ELSE
			;超レアだとは思うけど同居人が休托莉だけのパターン
			IF RAND:4 == 0 && TALENT:恋慕 && CHARA_NUM("集合予定") == 2
				CALL EMOTION_PLUS, "重置/紅潮/对话框爱心", TARGET
				PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
				PRINTFORMW 「ねぇねぇ、%CNAME("二人称")%♪」
				CALL OFC_PRINT_STRW, "「ちゅっ_CBBH_ ……えへへっ_／／／_ 」", TARGET
				PRINTFORMDW %CALLNAME:TARGET%とお帰りの接吻をした
			ELSEIF RAND:3 && CHARA_NUM("集合予定") == 2
				PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
				PRINTFORMW 「ご飯の準備できてるよ？それとも先にお浴室にする？」
			ELSEIF RAND:2
				PRINTFORML 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
				IF TEXTS("季節詳細") == "夏"
					PRINTFORMW 「今日は暑かったね！ 熱中症にならないでね？」
				ELSEIF TEXTS("季節詳細") == "冬"
					PRINTFORMW 「今日は寒かったね！ 風邪をひいちゃうダメだよ？」
				ELSE
					PRINTFORMW 「今日は過ごしやすかったね。いい天気が続くといいな」
				ENDIF
			ELSE
				PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
				PRINTFORMW 「ねっ！ %CNAME("二人称")%、%TEXTR("この游戏でやろうよ！/新刊の漫画買ってきたけど読む？/この配信見た？ 面白色よ！")%」
			ENDIF
		ENDIF
		CALL SET_CEVENT, "Ｄイベ：帰宅の打招呼", TARGET
	;こっちは部室
	ELSE
		IF COND("同伴中", MASTER) && RAND:2
			PRINTFORMW 「%CNAME("二人称")%、%CNAME("partner")%、您好」
		ELSE
			IF RAND:4 == 0 && FINDT_COND_K50(TARGET, "点心当番")
				CALL EMOTION_PLUS, "重置/笑颜", TARGET
				PRINTFORMW 「%CNAME("二人称")%、您好」
				PRINTFORMW 「%CNAME(@"{FINDT_COND_K50(TARGET, "点心当番")}")%が点心を用意してくれてるよ！」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「%CNAME("二人称")%、您好」
				PRINTFORMW 「%CNAME("一人称")%もさっき着いたところ！」
			ELSEIF RAND:2
				PRINTFORMW 「%CNAME("二人称")%、您好」
				PRINTFORMW 「どこかでつまみ食いでもしてた？」
			ELSE
				PRINTFORMW 「%CNAME("二人称")%、您好」
			ENDIF
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "重置/＞＜/憤怒眉/对话框兴奋", TARGET
		PRINTFORMW 「是はーい！ お待たせ！」
		PRINTFORMW 「%CNAME("一人称")%に%TEXTR("何か御用？/して欲しいことでもあるの？")%」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF EXP_GAIN("性処理経験")
			CALL EMOTION_PLUS, "重置/斜眉/嘲笑/紅潮/对话框爱心", TARGET
			PRINTFORMW 「お昼前に%CNAME("一人称")%が%TEXTR("いっぱい出/お世話")%してあげたのに♪」
			CALL OFC_PRINT_STRW, @"「%CNAME("二人称")%ってほんっと元気いいよね_CBBH_ 」", TARGET
		ELSE
			CALL EMOTION_PLUS, "重置/＞＜/紅潮", TARGET
			PRINTFORMW 「您好ー！」
			PRINTFORMW 「%CNAME("二人称")%、今日もえろいねー♪」
			PRINTFORMDW %CALLNAME:PARTNER()%とのやりとりを見られたようだ
		ENDIF
	;全裸の人がいる
	ELSEIF FINDT_COND_K50(TARGET, "全裸状態終了可能")
		CALL EMOTION_PLUS, "＞＜/斜眉/躁动难忍/对话框惊讶", TARGET
		PRINTFORMW 「您好ー！」
		PRINTFORMW 「わっ！ %CNAME(@"{FINDT_COND_K50(TARGET, "全裸状態終了可能")}")%、赤裸的だー！ すごーい！」
	;内衣の人がいる
	ELSEIF FINDT_COND_K50(TARGET, "薄着状態終了可能")
		CALL EMOTION_PLUS, "＞＜/斜眉/躁动难忍/对话框惊讶", TARGET
		PRINTFORMW 「您好ー！」
		PRINTFORMW 「わっ！ %CNAME(@"{FINDT_COND_K50(TARGET, "薄着状態終了可能")}")%、すごい格好！」
	ELSEIF CSTR:PREV_DAILYCOM == "Talk"
		PRINTFORMW 「您好ー！」
		PRINTFORMW 「なんだか盛り上がってるね♪」
	ELSE
		IF RAND:2
			SIF CFLAG:心情好 == 0
				CFLAG:心情好 = 2
			PRINTFORMW 「您好ー♪」
			PRINTFORMDW どうやら今日の%CALLNAME:TARGET%通过心情が良いようだ
		ELSE
			PRINTFORMW 「您好ー！」
			PRINTFORMDW %CALLNAME:TARGET%通过今日も元気みたいだ
		ENDIF
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL EMOTION_PLUS, "重置/〇〇/憤怒眉/红晕/对话框惊讶", TARGET
		PRINTFORML 「わっ！？ もう皆集まってる！」
		PRINTFORMW 「您好ー！」

	ELSEIF CFLAG:呼び出された
		CALL EMOTION_PLUS, "重置/＞＜/憤怒眉/红晕/对话框兴奋", TARGET
		PRINTFORML 「您好ー！」
		PRINTFORML 「%CNAME("二人称")%、お待たせ！」
		PRINTFORMDW %CALLNAME:TARGET%通过小走りで%CALLNAME:MASTER%のもとへやってきた
	ELSE
		CALL EMOTION_PLUS, "重置/笑颜", TARGET
		PRINTFORMW 「您好ー！」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		CALL EMOTION_PLUS, "重置/微笑/对话框兴奋", TARGET
		PRINTFORMW 『%CNAME(@"{FIND_COND("教員", "入室打招呼中")}")%、您好』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL EMOTION_PLUS, "重置/对话框兴奋", TARGET
		PRINTFORML 『%CNAME(@"{FIND_COND("入室打招呼中")}")%、您好』
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部"
			PRINTFORMW 『遅かったね？ 何かあったの？』
		CASE "１年生", "２年生", "３年生"
			PRINTFORMW 『もう皆あつまってるよ！』
		CASEELSE
			IF TALENT:ACTOR:外見年齢 <= 2 && TALENT:PLAYER:萝莉老太婆 == 0
				PRINTFORMW 『遅かったね？ 何かあったの？』
			ELSEIF TALENT:ACTOR:外見年齢 == 4
				WAIT
			ELSE
				PRINTFORMW 『もう皆あつまってるよ！』
			ENDIF
		ENDSELECT
	ELSE
		PRINTFORMW 『%CNAME(@"{FIND_COND("入室打招呼中")}")%、您好』
	ENDIF

CASE "居間に現れた"
	;酔っぱらいがいないか尋找
	FOR LCOUNT, 2, CHARANUM
		SIF LCOUNT == TARGET
			CONTINUE
		SIF STATE("酔酒", LCOUNT) == 0
			CONTINUE
		NUM_FIND += 1

		SIF RAND:NUM_FIND == 0
			ACTOR = LCOUNT
	NEXT
	;いた場合
	IF ACTOR
			CALL EMOTION_PLUS, "重置/〇〇/斜眉/半開/对话框惊讶", TARGET
			PRINTFORMW 「……呐、%CNAME("二人称")%」
			PRINTFORMW 「%CNAME(@"{ACTOR}")%、なんか%TEXTR("変じゃない/顔が赤くない/酔ってない")%？」
	ELSEIF CFLAG:呼び出された
			PRINTFORMW 「%TEXTR("なーにー？ /呼んだ？ ")%%CNAME("二人称")%」
	;一度も夜に出会ってないパターン
	ELSEIF CMOVE("居間") + CEVENT("Ｄイベ：帰宅の打招呼") + COND("同伴中") == 0
		CALL EMOTION_PLUS, "重置/笑颜/对话框兴奋", TARGET
		PRINTFORMW 「%CNAME("二人称")%、帰ってたんだね」
		PRINTFORMW 「おかえりー♪」
	ELSE
		CALL EMOTION_PLUS, "重置/眯眯眼/斜眉/ω/红晕", TARGET
		PRINTFORMW 「%CNAME("二人称")%、何か面白色ことしない？」
		PRINTFORMW %CALLNAME:TARGET%通过闲を持て余しているようだ
	ENDIF
CASE "抜け出された"
	IF COND("欲求不満") >= 2 || TALENT:恋慕
		CALL EMOTION_PLUS, "重置/困扰眉/無口系/紅潮", TARGET
		PRINTFORMW 「あ……%CNAME("二人称")%……」
		PRINTFORMW 「……いってらっしゃい」
		PRINTFORMDW %CALLNAME:TARGET%通过寂しそうに%CALLNAME:MASTER%を見送った
	ELSE
		CALL EMOTION_PLUS, "重置/斜眼/ω/红晕", TARGET
		PRINTFORMW 「%TEXTR("ははーん/ふーん/へー/ほー")% ……いってらっしゃい、%CNAME("二人称")%」
		PRINTFORMDW %CALLNAME:TARGET%通过ニヤニヤしながら%CALLNAME:MASTER%を見送った
	ENDIF
CASE "便当"
	IF CEVENT("初めての便当") == 0
		CALL EMOTION_PLUS, "重置/困扰眉/無口系", TARGET
		IF FINDT_COND_K50(TARGET, "同居中", "弗理希艾尔") && COND("同居中")
			PRINTFORMW 「%CNAME("弗理希艾尔")%にも手伝ってもらったんだけど……どうかな？」
		ELSE
			PRINTFORMW 「料理の本を見ながら作ったんだけど……どうかな？」
		ENDIF
		PRINTFORMW %CALLNAME:TARGET%の作ってきた便当は、卵焼きやウィンナーといった定番のおかずが入った简约なものだ。
		PRINTFORMW 見た目はおかしなところもなく、よくできているようにも見える。
		PRINTFORMW とりあえず、一口、卵焼きを食べてみることにした。
		PRINTFORMW ……少し癖のある独特の味がするが、結構おいしい。
		PRINTL
		CALL EMOTION_PLUS, "重置/笑颜/斜眉/紅潮", TARGET
		PRINTFORMW 「おいしい？ えへへっ♪ ……よかった」
		PRINTFORMW 「%CNAME("一人称")%が作るとよく魔界っぽい味付けにしちゃうんだけど、大丈夫みたいだね！」
		PRINTFORMW 「それなら、いろいろ作ってきて给♪」
		CALL SET_CEVENT, "初めての便当"
	ELSEIF RAND:3 == 0
		PRINTFORMW 今日の便当は……三明治だ！
		PRINTL
		CALL EMOTION_PLUS, "重置/＞＜/憤怒眉/对话框開心", TARGET
		PRINTFORMW 「どうどう？ おしゃれでしょ！」
		PRINTFORMW 「%CNAME("二人称")%は具は何が好き？」
		PRINTFORMW 「これ？ んふふー♪ 是、あ～～～ん♪」
	ELSEIF RAND:2
		PRINTFORMW 今日の便当は……おかずにコロッケがある！
		PRINTL
		CALL EMOTION_PLUS, "重置/憤怒眉", TARGET
		PRINTFORMW 「全部中身が違うんだよ！」
		PRINTFORMW 「それがポテトで、こっちがカレー味、これには乳酪が入ってるの♪」
		PRINTFORMW 「……あれ？ そっちがポテトだっけ？」
	ELSE
		PRINTFORMW 今日の便当は……大号的飯団が入っている！
		PRINTL
		CALL EMOTION_PLUS, "重置/红晕/对话框開心", TARGET
		PRINTFORMW 「あははっ、%CNAME("二人称")%、ほっぺたにご飯粒付けてる！」
		PRINTFORMW 「じっとしててね！ とって给♪」
		PRINTFORMW %CALLNAME:TARGET%通过%CALLNAME:MASTER%の頬に付いたご飯粒を手で摘み、ぱくっと食べてしまった
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"

ENDSELECT

;文字色を戻す
CALL CHANGE_OFC, "重置", TARGET
;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K50, ARGS
#DIM MEMO_LINECOUNT
#DIM LCOUNT
#DIM ACTOR
MEMO_LINECOUNT = LINECOUNT

;文字色変更
CALL CHANGE_OFC, "セット"

;レイアウトもあるので1～3行くらいで最大5あたり？
;書き込むならTalk台詞の方を充実の方がいい気がする
ACTOR = CHARANUM-1

;特殊事件
;初回台詞
IF ARGS == "初回台詞"
	PRINTFORML 「%NAME:TARGET%です！ 今日からよろしくお願いします♪」
	PRINTFORMDL %CALLNAME:TARGET%通过自己紹介をしている
;新入部員が入った時
;夜は対応しない……母親とか妹とか的家族が急に生えてくるのでどうしたものか
ELSEIF STRCOUNT(CSTR:ACTOR:NOW_DEVENT, "初回台詞") && ACTOR != TARGET
	IF COND("昼")
		IF COND("教員", ACTOR)
			PRINTFORML 「%CNAME(@"{ACTOR}")%、よろしくお願いします」
		ELSE
			SELECTCASE CSTR:ACTOR:学年
			CASE "初等部", "中等部", "１年生", "２年生", "３年生"
				PRINTFORML 「%CNAME(@"{ACTOR}")%、よろしくね♪」
			CASEELSE
				IF TALENT:ACTOR:外見年齢 == 3
					PRINTFORML 「%CNAME(@"{ACTOR}")%、よろしくお願いします」
				ELSE
					PRINTFORML 「%CNAME(@"{ACTOR}")%、よろしくね♪」
				ENDIF
			ENDSELECT
		ENDIF
		PRINTFORMDL %CALLNAME:TARGET%通过%CALLNAME:ACTOR%に自己紹介をしている
	;夜の場合
	ELSE
	ENDIF
ELSEIF CEVENT("Ｄイベ：恶作剧成功") && DEVENT("Ｄイベ：恶作剧成功_DAILY") == 0
