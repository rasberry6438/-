				ENDIF
			IF CEVENT("槞彁：機械翻訳OFF", MASTER) == 0
				CALL LIBRALY_F(100, LCOUNT, ARGS)
					IF RESULT == 1
						AFTER_RESULT = 1
					ELSEIF STR:6 != ""
						STR:5 = - _黄色_翻訳機_ -
						STR:6 = #TEXTC=ccffe5:HTML:『%STR:6%』
