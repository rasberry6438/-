						SIF STR:8 != ""
							STR:8 = #TEXTC=ccffe5:HTML:『%STR:8%』
					ENDIF
			ENDIF
			CALL LIBRALY_F(1000, LCOUNT, ARGS)
			SIF (BEFORE_RESULT == 1)&&(AFTER_RESULT == 1)&&(BASE_RESULT == 1)
				BREAK
			IF STR:2 != ""
