@CNAME_K53, ARGS
#FUNCTIONS
;名前を呼ぶ相手の角色番号の代入に使う
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	LOCALS = 你_你
;この下は三人称。
CASE "主人"
	IF NAME:PLAYER == "你"
		LOCALS = 部长_部长
	ELSE
		LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
	ENDIF
CASEELSE
	;名前を呼ぶ相手の角色番号をACTORに代入する
	SELECTCASE ARGS
	;相手が助手
	CASE "助手"
		ACTOR = ASSI
	;ここにはARGSに角色のCALLNAMEが入ったりしている場合(ARGS == "弗理希艾尔" など)に来る
	CASEELSE
		ACTOR = FIND_NO(ARGS)
	ENDSELECT

	;相手が教員
	IF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%老师_%CALLNAME:ACTOR%老师

	;それ以外
	ELSE
		LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
	ENDIF
ENDSELECT
RETURNF LOCALS

