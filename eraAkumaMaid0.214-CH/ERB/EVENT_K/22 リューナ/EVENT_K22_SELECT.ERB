;お願い・交易での選択肢を追加。番号は80～89を使ってよい
@WISH_K22_ADDSELECT
IF CEVENT("新しい服")
	IF CEVENT("美式校服化：琉娜")
		STR:選択肢 = %STR:選択肢% [80] %TEXT_LJ("美式校服着用を止めて", 16)% … %CALLNAME:TARGET%に美式校服着用を止めてもらいます/
	ELSE
		STR:選択肢 = %STR:選択肢% [80] %TEXT_LJ("美式校服を着て", 16)% … %CALLNAME:TARGET%に美式校服を着て部活に着てもらいます/
	ENDIF
ENDIF
SIF CEVENT("襲来")
	STR:選択肢 = %STR:選択肢% [81] %TEXT_LJ("ラディーネの来訪頻度を変えて", 16)% … 通过%CALLNAME:TARGET%向拉蒂涅提出请求/

;お願い・交易で増やした選択肢の地の文と台詞
@KOJO_WISH_K22_ADDSELECT, ARGS
SELECTCASE ARGS
CASE "美式校服着用を止めて"
	PRINTFORMW %CALLNAME:TARGET%决定听从%CALLNAME:MASTER%的%ARGS%建议。
	PRINTL 
	PRINTFORML 「诶～？不换了吗？」
	PRINTFORMW 「明白了、下次就穿着平时的衣服吧」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%不再穿着情趣水手服了。
	CALL DEL_CEVENT, "美式校服化：琉娜"
CASE "美式校服を着て"
	PRINTFORMW %CALLNAME:TARGET%决定听从%CALLNAME:MASTER%的%ARGS%建议。
	PRINTL 
	PRINTFORML 「嗯、希望我下次穿着情趣水手服来？」
	PRINTFORMW 「好的、下次我会穿来的」
	PRINTL 
	PRINTFORMW %CALLNAME:TARGET%開始穿着情趣水手服了。
	CALL SET_CEVENT, "美式校服化：琉娜"
	;フラグを弄る場合はそれもここで行う
CASE "ラディーネの来訪頻度を変えて"
	PRINTFORMW %CALLNAME:TARGET%决定听从%CALLNAME:MASTER%的%ARGS%建议。
	PRINTL 
	PRINTFORML 「奶奶什么时候来？」
	PRINTFORM 「现在是
	IF CFLAG:3200 == 0
		PRINTFORML 只在周末过来……要让她每天都来吗？」
	ELSE
		PRINTFORML 每天都过来…要改成只在周末过来吗？」
	ENDIF
	CALL PRINT_SELECT, "就这样做/还是算了吧"
	IF RESULT == 0
		PRINTFORMW 「知道了、我会告诉奶奶的」
		IF CFLAG:3200 == 0
			CFLAG:3200 = 1
		ELSE
			CFLAG:3200 = 0
		ENDIF
	ELSE
		PRINTFORMW 「不改了吗？嗯、明白了」
	ENDIF
	;フラグを弄る場合はそれもここで行う
ENDSELECT

