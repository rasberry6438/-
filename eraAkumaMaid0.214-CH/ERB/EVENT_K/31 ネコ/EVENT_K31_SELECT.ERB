@DAILY1_K31_ADDSELECT

SIF MARK:屈服刻印 >= 2 && COND("発情期") == 0
	STR:選択肢 += "話題：マタタビ/"
;STR:選択肢 += "追加選択肢之２/"

;Talk指令で増やした選択肢の地の文と台詞
@KOJO_DAILY1_K31_ADDSELECT, ARGS
SELECTCASE ARGS

;-----------------------------------
;マタタビのヤツ　相変わらずアイリ酱からパクパク有がとうございます
	;CFLAG:マタタビ
	;1 イベントトリガー
	;2 初回お試し済み
	;3 偽酔酒経験済み
	;4 本日マタタビ選択済み(偽酔酒経験没有)
	;5 本日マタタビ選択済み
	;4以上の場合終日にフラグを-2する。
;------------------------------------
CASE "話題：マタタビ"
	SELECTCASE CFLAG:マタタビ
	;マタタビ仕入れ前後
	CASE IS <= 1
		IF CEVENT("ネコイベント：マタタビ話題") == 0
			CALL EMOTION_PLUS, "重置/小口", TARGET
			PRINTFORMW 「……マタタビ？ 嗅いだらどうなる…ったってね」
			CALL EMOTEFACE, "重置/闭眼２/半開", TARGET
			PRINTFORMW 「別に%CNAME("一人称")%マジで猫ってワケじゃないし、面白色事にはなんないと思うけど」
			CALL EMOTEFACE, "重置/左闭眼２/半開", TARGET
			PRINTFORMW 「それに…猫みたいに扱われるのは……いや、別に良いんだけどさ」
			CALL SET_CEVENT, "ネコイベント：マタタビ話題"
		ELSE
			IF RAND:3 == 0
				CALL EMOTION_PLUS, "重置/半開", TARGET
				PRINTFORMW 「まぁ、嗅いでみないと分かんないけど……気ぃ進まんなぁ…」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "重置/小口", TARGET
				PRINTFORMW 「頭のちゅーすーしんけーっての？ が、やられて酔っ払ったみたいになるんだってね」
				CALL EMOTEFACE, "重置/闭眼２/困扰眉/半開", TARGET
				PRINTFORMW 「……猫の話ね、猫の。頷いてこっちみんな%CNAME("一人称")%は猫じゃないっつの」
			ELSE
				CALL EMOTION_PLUS, "重置/闭眼２/困扰眉/半開", TARGET
				PRINTFORMW 「是是、試してみたいのは解ったから…」
				CALL EMOTEFACE, "重置/左闭眼２/困扰眉/栗口２", TARGET
				PRINTFORMW 「持ってきたら考えとくから、持ってきたらね……そうそう手に入らないとは思うけどさ」
			ENDIF
		ENDIF
	;本日マタタビ未選択(マタタビ初回体験後)
	CASE 2
		IF RAND:3 == 0
			CALL EMOTION_PLUS, "重置/闭眼２/困扰眉/半開", TARGET
			PRINTFORMW 「また仕入れたとか要らん情報は良いから…猫扱いやめろっつの……」
		ELSEIF RAND:2
			CALL EMOTION_PLUS, "重置/小口", TARGET
			PRINTFORMW 「まぁ气味はそんなni↘て感じ？ もっと酷い气味なのかと思ったけどさ」
			CALL EMOTEFACE, "重置/闭眼２/困扰眉/半開", TARGET
			PRINTFORMW 「……言っとくけど今度もっかい嗅がせろってコトじゃないからね？」
		ELSE
			CALL EMOTION_PLUS, "重置/闭眼２/半開", TARGET
			PRINTFORMW 「是是、酔っ払わなかったのは見たでしょー？」
			CALL EMOTEFACE, "重置/憤怒眉/小口２", TARGET
			PRINTFORMW 「%CNAME("一人称")%は猫じゃない、以上、終わり。馬鹿なコトしな桑なっての」
		ENDIF
	;本日マタタビ未選択(偽酔酒経験済み)
	CASE 3
		IF RAND:3 == 0
			CALL EMOTION_PLUS, "重置/飘忽/困扰眉/半開/赤面", TARGET
			PRINTFORMW 「うぇっ…い、いやぁ…酔っ払ってる時の記憶は、無い、なぁ…」
			CALL EMOTEFACE, "重置/◯◯/憤怒眉/哈欠大/赤面", TARGET
			PRINTFORMW 「……な、何さその目。うっさい！ ホントに覚えてないんだって！ 覚えてないんだよ！！」
		ELSEIF RAND:2
			CALL EMOTION_PLUS, "重置/飘忽/笑顔２/赤面", TARGET
			PRINTFORMW 「いやー、意外とさ……思ったより、猫だったみたいで」
			PRINTFORMW 「は、ははは……いや、でも猫扱いされんのはヤだけど、うん」
		ELSE
			CALL EMOTION_PLUS, "重置/小口", TARGET
			PRINTFORMW 「よ、酔っ払うって解ったんだしさ、もう嗅がせる必要無いよね？」
			CALL EMOTEFACE, "重置/飘忽/困扰眉/半開/赤面", TARGET
			PRINTFORMW 「……たまになら、まぁその……吝かでも無いけど」
		ENDIF
	;4 本日マタタビ選択済み(偽酔酒経験没有)
	CASE 4
		IF RAND:3 == 0
			CALL EMOTION_PLUS, "重置/闭眼２/半開", TARGET
			PRINTFORMW 「で、酔ってるように見える？」
			PRINTFORMW 「見えない？ そりゃあね、酔っ払うわけないし」
		ELSEIF RAND:2
			CALL EMOTION_PLUS, "重置/闭眼２/半開", TARGET
			PRINTFORMW 「猫扱いした件について何か言うことは？ ……宜しい」
		ELSE
			CALL EMOTION_PLUS, "重置/左闭眼２/半開", TARGET
			PRINTFORMW 「ったく、マジモンの猫じゃないんだからさ…」
			PRINTFORMW 「言っとくけど、あんまし良い気分じゃないんだからね」
		ENDIF
	;本日マタタビ選択済み
	CASE 5
		IF CEVENT("ネコイベント：偽酔酒")
			IF RAND:3 == 0
				CALL EMOTION_PLUS, "重置/◯◯/笑顔２/困扰眉/赤面", TARGET
				PRINTFORMW 「い、いや、ホントに酔ってるから！ あ、いや、酔ってるにゃん！」
				PRINTFORMW 「この顔見なよめっちゃ熱いし！ 红色し！？ これはもう酔っ払ってる以外無くない！？」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "重置/半開３/困扰眉/赤面", TARGET
				PRINTFORMW 「も、もう酔ってるんだからさ、そんな何度も嗅がせようとしなくても……」
				CALL EMOTEFACE, "重置/◯◯/哈欠小/憤怒眉/赤面", TARGET
				PRINTFORMW 「…………ｽﾝｽﾝ……う、うん……えと…………にゃ、にゃーん！！」
				PRINTFORMW %CALLNAME:TARGET%通过何度か鼻を鳴らすと、どうすれば良いか逡巡したようだが結局抱き着いて来た…
			ELSE
				CALL EMOTION_PLUS, "重置/困扰眉/半開２/赤面", TARGET
				PRINTFORMW 「ああ、待って、ちょっと疲惫……いや、酔っ払ってても普通疲れるよね…？」
				CALL EMOTEFACE, "重置/闭眼２/斜眉/半開/赤面", TARGET
				PRINTFORMW 「にゃんはどうしたって…………う、ぐ……にゃ、にゃん…」
			ENDIF
		ELSE
			IF RAND:3 == 0
				CALL EMOTION_PLUS, "重置/飘忽/困扰眉/半開/赤面", TARGET
				PRINTFORMW 「うぇっ…い、いやぁ…酔っ払ってる時の記憶は、無い、なぁ…」
				CALL EMOTEFACE, "重置/◯◯/憤怒眉/哈欠大/赤面", TARGET
				PRINTFORMW 「……な、何さその目。うっさい！ ホントに覚えてないんだって！ 覚えてないんだよ！！」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "重置/飘忽/笑顔２/赤面", TARGET
				PRINTFORMW 「いやー、意外とさ……思ったより、猫だったみたいで」
				PRINTFORMW 「は、ははは……いや、でも猫扱いされんのはヤだけど、うん」
			ELSE
				CALL EMOTION_PLUS, "重置/小口", TARGET
				PRINTFORMW 「よ、酔っ払うって解ったんだしさ、もう嗅がせる必要無いよね？」
				CALL EMOTEFACE, "重置/飘忽/困扰眉/半開/赤面", TARGET
				PRINTFORMW 「……たまになら、まぁその……吝かでも無いけど」
			ENDIF
		ENDIF
	ENDSELECT
	CALL NEKO_POINT
CASE "追加選択肢之２"
	PRINTFORMW %CALLNAME:MASTER%进行了%ARGS%。
	PRINTL 
	PRINTFORMW 「台詞」
	PRINTL 
	PRINTFORMW 受到%ARGS%的%CALLNAME:TARGET%的反应
ENDSELECT




