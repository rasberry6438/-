;本体側でのVerUP後此方に差し替え
SIF CEVENT("ネコイベント：偽酔酒") == 0 && COND("夜") && MARK:屈服刻印 >= 2 && CFLAG:マタタビ > 0 && CFLAG:マタタビ < 4
	STR:選択肢 += "让她闻木天蓼/"


;互动指令で増やした選択肢の地の文と台詞
@KOJO_DAILY2_K31_ADDSELECT, ARGS
SELECTCASE ARGS
;-------------------------------------------------
;ねこじゃらしで逗猫棒
;構文や最初の地の文に関してはアイリ酱とこから圆圈パクパクごっ君ご馳走様です。感謝。
;許可らしきものは頂けたのだ。多分。
;
;CFLAG:じゃれつき具合_寧子
;0=やだ 1=うわ、出してきた 2,3=むきになってきた 4>=うぉおおおおお！！ 
;-------------------------------------------------
CASE "逗猫棒"
	PRINTFORM %CALLNAME:MASTER%%CALLNAME:TARGET%の目の前で猫じゃらしを%TEXTR("ゆらゆらと/小刻みに/＿")%動かし
	IF CSTR:PREV_DAILYCOM != STR:日常イベント名
		PRINTFORMW てみた…
	ELSE
		PRINTFORMW 続けた…
	ENDIF
	PRINTL 
	;hokaguru add
	;そも恋慕じゃないとまともに一直陪着我くれない
	SIF TALENT:恋慕 == 0
		CFLAG:じゃれつき具合_寧子 = 0
	;発情中はえろいことしたいからそれどころではない
	SIF COND("発情中")
		CFLAG:じゃれつき具合_寧子 = 0
	;途中でやめる（この時間に初回でない、かつ連続実行でない）と飽きちゃう
	SIF CHOSE_DEVENT(STR:日常イベント名) && CSTR:PREV_DAILYCOM != STR:日常イベント名
		CFLAG:じゃれつき具合_寧子 = 0
;原本だとこの後気紛れ分岐があるけど恋慕の時点で気紛れとか発生し無さそうなので置いておく
	SELECTCASE CFLAG:じゃれつき具合_寧子
	;0=やだ
	CASE 0
		IF COND("発情中")
			PRINTFORMW 似乎%CALLNAME:MASTER%比%CALLNAME:TARGET%眼前的逗猫棒更加有吸引力、%CALLNAME:TARGET%一直盯着%CALLNAME:MASTER%。
			CALL EMOTION_PLUS, "重置/半開/赤面", TARGET
			PRINTFORMW 「……那个、现在是発情期哟。知道的吗」
			IF TALENT:恋慕
				CALL EMOTEFACE, "重置/左闭眼２/赤面", TARGET
				PRINTFORMW 「……想要做……所以……不想玩这个啦」
			ELSE
				CALL EMOTEFACE, "重置/闭眼２/小口", TARGET
				PRINTFORMW 「…………现在没有做这种事情的心情」
			ENDIF
		;どうやってか出したいけど擅長い事行く分岐を思いつかない
		;ELSEIF TALENT:恋慕
		;	PRINTFORMW 「…………并不是很喜欢被当作猫那样对待、这种事情知道的吧？」
		;	PRINTFORMW %CALLNAME:TARGET%好像有点难以启齿的样子地挤出了一句、啪的一下轻轻地拍在了%CALLNAME:MASTER%手里握着的逗猫棒上
		;	PRINTFORMW 「那ー那个……今天到此为止可以吗？」
		;	PRINTFORMW 吐出舌頭做着鬼脸苦笑着。
		ELSE
			IF TALENT:恋慕
				CALL EMOTION_PLUS, "重置/半開", TARGET
				PRINTFORMW 「……诶多、还要玩吗？」
				CALL EMOTEFACE, "重置/飘忽", TARGET
				PRINTFORMW 「%CNAME("一人称")%ー……%TEXTR("倒是觉得已经可以了/感觉已经冷静下来了")%」
				CALL EMOTEFACE, "重置", TARGET
				PRINTFORMW 「下次还有机会的」
			ELSE
				IF RAND:3 == 0
					CALL EMOTION_PLUS, "重置", TARGET
					PRINTFORMW %CALLNAME:TARGET%用厌烦了一样的眼神盯着%CALLNAME:MASTER%手里拿着的逗猫棒。
					PRINTFORMW 「想要玩吗？」
					CALL EMOTEFACE, "重置/闭眼２/半開/叹气", TARGET
					PRINTFORMW 「……哈啊」
					PRINTFORMW 最终长长地吐出一气转向了另一边。
				ELSEIF RAND:2
					CALL EMOTION_PLUS, "重置/半開", TARGET
					PRINTFORMW 「…………呜啊」
					PRINTFORMW 歪着頭看着%CALLNAME:MASTER%拿出的逗猫棒的%CALLNAME:TARGET%…
					CALL EMOTEFACE, "重置/闭眼２", TARGET
					PRINTFORMW 「无视」
					PRINTFORMW 就说了两个字便看向了别的地方。
				ELSE
					CALL EMOTION_PLUS, "重置", TARGET
					PRINTFORMW 「觉得很好玩吗？」
					CALL EMOTEFACE, "重置/闭眼２/半開", TARGET
					PRINTFORMW 「唔、嘛……虽然%CNAME("二人称")%可能玩得很開心……」
					CALL EMOTEFACE, "重置/左闭眼２", TARGET
					PRINTFORMW 「……但%CNAME("一人称")%讨厌被像猫那样对待。おーらい、知道了吗？」
				ENDIF
			ENDIF
		ENDIF
	;1=うわ、出してきた
	CASE 1
		IF RAND:2
			CALL EMOTION_PLUS, "重置/半開", TARGET
			PRINTFORMW 「不不不……那个啊」
			PRINTFORMW 「都说了多少次不要把逗猫棒拿出来了。还没有记住吗？」
			PRINTFORMW 一边这样说着%CALLNAME:TARGET%像是被吓到了一样弓起肩膀、视线一直左右摇晃着追逐眼前的逗猫棒。
		ELSE
			CALL EMOTION_PLUS, "重置/困扰眉/小口", TARGET
			PRINTFORMW 「把那个拿出来是打算干什么啊」
			CALL EMOTEFACE, "重置/闭眼２/困扰眉/半開", TARGET
			PRINTFORMW 「……啊啊、唔、嘛、唔嗯……好呀……不对、一点也不好」
		ENDIF
	;2,3=むきになってきた
	CASE 2, 3
		IF RAND:2
			CALL EMOTION_PLUS, "重置/憤怒眉", TARGET
			PRINTFORMW 「――――呼嗯っ」
			CALL EMOTEFACE, "重置/闭眼２/憤怒眉/半開", TARGET
			PRINTFORMW 「ﾁｯ……」
			PRINTFORMW 抓住时机伸出手去挠的抓了一个空、%CALLNAME:TARGET%轻轻地咂了一下嘴…
			PRINTFORMW 之后便装作出一副毫无兴趣的样子转过身去、但耳朵还是高高地竖了起来警戒着周围的情况…
		ELSE
			CALL EMOTION_PLUS, "重置/闭眼２/半開３/憤怒眉", TARGET
			PRINTFORML 「所ー以ー说……逗猫棒这种东西――」
			PRINTFORMW 突然一瞬间从侧面伸出一只手去抓。
			CALL EMOTEFACE, "重置/◯◯/憤怒眉/小口", TARGET
			PRINTFORMW 「大概、这样……」
			PRINTFORMW 结果抓了一个空…
			CALL EMOTEFACE, "重置/对话框憤怒", TARGET
			PRINTFORMW 「……%TEXTR("不是、我还没有拿出真本事而已/没得手吗")%」
		ENDIF
	;4 >=うぉおおおおお！！ 
	CASEELSE
		;獲ったどー！！
		IF RAND:4 == 0
			IF RAND:2
				CALL EMOTION_PLUS, "重置/〇〇/憤怒眉/哈欠小/对话框憤怒", TARGET
				PRINTFORMW 「――――nyaっ!」
				PRINTFORMW 等%CALLNAME:MASTER%回过神来时%CALLNAME:MASTER%手里的逗猫棒已经不见了。
				CALL EMOTEFACE, "重置/蹙眉/小口２", TARGET
				PRINTFORMW 「…………哦、哦嗯？」
				PRINTFORMW 一瞬间本人也呆住了、不断来回看着自己手里的逗猫棒和%CALLNAME:MASTER%。
				PRINTFORMW 最终注意到自己已经成功地夺走了逗猫棒的%CALLNAME:TARGET%、
				CALL EMOTEFACE, "重置/闭眼２/小口/紅潮", TARGET
				PRINTFORMW 「呼、呼哼……嘛、嘛啊ーー、%CNAME("一人称")%拿着的话、也就这种程度而已」
				PRINTFORMW 一边慌张地甩动逗猫棒一边羞耻地说…
			ELSE
				CALL EMOTION_PLUS, "重置/〇〇/憤怒眉/哈欠小/对话框憤怒", TARGET
				PRINTFORMW 「……得手了！」
				PRINTFORMW 一瞬间没有注意%CALLNAME:TARGET%便乘机夺走了%CALLNAME:MASTER%的逗猫棒。
				CALL EMOTEFACE, "重置/笑颜４/笑顔２", TARGET
				PRINTFORMW 「嘿嘿、胜利」
				PRINTFORMW 「呀啊、发生了意外的事情呢……」
				CALL EMOTEFACE, "重置/憤怒眉/紅潮", TARGET
				PRINTFORMW 「……因、因为好好地陪%CNAME("二人称")%玩了、要记得感谢哦？」
			ENDIF
			CFLAG:じゃれつき具合_寧子 = 0
		;獲れなかったどー！？
		ELSE
			IF RAND:4 == 0
				CALL EMOTION_PLUS, "重置/〇〇/憤怒眉/無口系", TARGET
				PRINTFORMW 「这样……就っ！」
				PRINTFORMW 锐利的指尖迅速向%CALLNAME:MASTER%摇晃着的逗猫棒靠近。
				CALL EMOTEFACE, "重置/〇〇/憤怒眉/哈欠大", TARGET
				PRINTFORMW 「――――抓到啦、啊啊！？」
				PRINTFORMW 可是因为用力过猛、%CALLNAME:TARGET%的身体就这样冲进了%CALLNAME:MASTER%的怀里…
				CALL EMOTEFACE, "重置/蹙眉/半開", TARGET
				PRINTFORMW 「停……唔ー……可恶」
				IF RAND:5 == 0
					CALL EMOTION_PLUS, "重置/困扰眉/半開/赤面", TARGET
					PRINTFORMW 「……唔、啊？」
					PRINTFORMW 回过神来时才发现压在了%CALLNAME:MASTER%的身上、
