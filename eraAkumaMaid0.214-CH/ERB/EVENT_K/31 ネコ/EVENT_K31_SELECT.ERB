@DAILY2_K31_ADDSELECT
SIF CFLAG:じゃれつき具合_寧子 != 0 && STATE("酔酒") == 0
	STR:選択肢 += "逗猫棒/"


;IF CEVENT("ネコイベント：偽酔酒") == 0 && COND("夜") && MARK:屈服刻印 >= 2 && CFLAG:マタタビ > 0 && CFLAG:マタタビ < 4
;	SIF COND("発情期") == 0
;		STR:選択肢 += "让她闻木天蓼/"
