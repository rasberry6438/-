@EVENT_NTR_DENSHA
#DIM DENSHA_P1
#DIM DENSHA_P2
#DIM DENSHA_LEVEL
;===INIT===
DENSHA_P1 = TARGET
DENSHA_P2 = FIND_COND("电车乘坐可能")
DENSHA_LEVEL = CFLAG:DENSHA_P1:公车猥亵等级

;同步公车猥亵等级
IF DENSHA_P2 != 0 && CFLAG:DENSHA_P2:公车猥亵等级 != DENSHA_LEVEL
	REPEAT 4
		DENSHA_P2 = FIND_COND("电车乘坐可能")
		;PRINTFORMW REPEAT LOOP
		;PRINTFORMW %CALLNAME:DENSHA_P2% 公车猥亵等级 {CFLAG:DENSHA_P2:公车猥亵等级}
		SIF DENSHA_P2 == 0 || CFLAG:DENSHA_P2:公车猥亵等级 == DENSHA_LEVEL
		BREAK
	REND
	
	;若乱数以后还是无法凑合，就单人模式
	SIF CFLAG:DENSHA_P1:公车猥亵等级 != CFLAG:DENSHA_P2:公车猥亵等级
	DENSHA_P2 = 0
ENDIF

;公车便器分支
SIF CEVENT("公车便器",DENSHA_P1) && ( DENSHA_P2 == 0 || CEVENT("公车便器",DENSHA_P2) )
	DENSHA_LEVEL = 10

;PRINTFORMW %CALLNAME:DENSHA_P1% 公车猥亵等级 {CFLAG:DENSHA_P1:公车猥亵等级} 公车便器 {CEVENT("公车便器",DENSHA_P1)}
;PRINTFORMW %CALLNAME:DENSHA_P2% 公车猥亵等级 {CFLAG:DENSHA_P2:公车猥亵等级} 公车便器 {CEVENT("公车便器",DENSHA_P2)}
;PRINTFORMW DENSHA_LEVEL 公车猥亵等级 {DENSHA_LEVEL}

DRAWLINE

CALL EVENT_NTR_DENSHA_0_START, DENSHA_P2, DENSHA_LEVEL

;乱数保底，确保EVENT_NTR_DENSHA_1_EVENTLOOP至少进行两次
A = 0

REPEAT 4
	;无事发生乱数
	SIF A > 2 && RAND:4 == 0
	CONTINUE
	
	CALL EVENT_NTR_DENSHA_1_EVENTLOOP, DENSHA_P2, DENSHA_LEVEL
	A += 1
	
	;公车级别的事件都是长篇，故不重复
	SIF DENSHA_LEVEL == 10
		BREAK
REND

CALL EVENT_NTR_DENSHA_LEVEL_PROMOTION, DENSHA_P1
SIF DENSHA_P2 != 0
CALL EVENT_NTR_DENSHA_LEVEL_PROMOTION, DENSHA_P2
CALL EVENT_NTR_DENSHA_2_END, DENSHA_P2, DENSHA_LEVEL

;
;END OF @EVENT_NTR_DENSHA
;

