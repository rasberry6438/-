	CALL EVENT_NTR_DENSHA_HARASS_3, ARG
CASE 2
	CALL EVENT_NTR_DENSHA_HARASS_2, ARG
CASE 1
	CALL EVENT_NTR_DENSHA_HARASS_1, ARG
CASE 0
	CALL EVENT_NTR_DENSHA_HARASS_0, ARG
CASEELSE
	PRINTFORMW 没符合等级{ARG:1}的事件。
ENDSELECT
	
CFLAG:DENSHA_P1:当前猥亵等级猥亵次数 += 1
CFLAG:DENSHA_P1:公车猥亵次数 += 1

