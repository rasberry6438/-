	CFLAG:DENSHA_P2:当前猥亵等级猥亵次数 += 1
	CFLAG:DENSHA_P2:公车猥亵次数 += 1
	CALL CHANGE_RELATION, 5, DENSHA_P1, DENSHA_P2
ENDIF

;因为公车级只会触发一次，特例计算，虽然到这里数值已经没意义了
IF ARG:1 == 10
	CFLAG:DENSHA_P1:当前猥亵等级猥亵次数 += 9
	CFLAG:DENSHA_P1:公车猥亵次数 += 9
	
	IF DENSHA_P2 != 0
		CFLAG:DENSHA_P2:当前猥亵等级猥亵次数 += 9
		CFLAG:DENSHA_P2:公车猥亵次数 += 9
		CALL CHANGE_RELATION, 15, DENSHA_P1, DENSHA_P2
	ENDIF
ENDIF

