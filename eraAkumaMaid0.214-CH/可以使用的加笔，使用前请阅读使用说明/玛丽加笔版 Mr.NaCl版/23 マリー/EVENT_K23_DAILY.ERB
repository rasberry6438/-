;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K23, ARGS
#DIM LCOUNT
#DIM MEMO_DESIRE
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
#DIM ACTOR2
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		;恋慕
		IF TALENT:恋慕
			;因为不协调感而醒来的你
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("Bonjour（法：早上好）/早上好")%_HH_　%CNAME("主人")%」_L"
			IF RAND:2 == 0
				CALL PRINT_STRW, @"「%TEXTR(@"因为从早上開始就躁动难忍了……%CNAME("主人")%准备了很多种子呢。/看到可爱的脸……忍耐汁出来了呢、从早上開始就准备好很多种子汁了")%_HH_」"
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("哈啊/嗯~")%_HH_　%TEXTR(@"被%CNAME("二人称")%中出了好多……只能接受了、对吧/快要出来了……嗯、请%CNAME("二人称")%的东西在最深处撒种")%_HH_」"
			ENDIF
		ENDIF
	CASE "朝舐陰"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("ちゅっ…じゅるるっ、ちゅっちゅっ/じゅるるっ…んっ_HH_　ちゅっ、ちゅう")%_HH_」_L"
			CALL PRINT_STR, @"「%TEXTR("おはようございます/お目覚めですか、おはようございます")%_HH_　%TEXTR("綺麗にしますから待っててくださいね/綺麗になるまでペロペロしてさしあげますからね")%_HH_」_W"
		ENDIF

	CASE "朝足交"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("Bonjour（法：早上好）/早上好")%_HH_　%CNAME("主人")%」_L"
			PRINTFORMW 「%TEXTR("お願いされた/頼まれていた")%街道に起こしに来ました。あの……%TEXTR("痛かったらいってくださいね/痛くないでしょうか？")%」
		ENDIF
	CASE "朝の新妻PLAY"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 「%TEXTR("早上好/你好（Bonjour）")%%TEXTR(@"吃早饭了/今天是%CNAME("一人称")%亲手做的菜")%」
			PRINTFORML 「%TEXTR("呀啊/欸、怎么了")%！？　%TEXTR(@"吃饭之前想要先吃%CNAME("一人称")%是吗……真是的/喂……做倒是可以，但是要等到可怕的女仆醒过来之前哦？/真是的……既然这样那就约好了要在里面满满的中出哦/……其实是不行的，所以这次是特别的")%」
		ENDIF

	CASE "朝乳交"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("はぁっ…んっ……/んぅ…んっ……")%　おはようございます、%CNAME("主人")%_HH_」_L"
			CALL PRINT_STR, @"「朝なのに起きてくださらないから……%TEXTR("ちょっと悪戯しちゃいました/ご奉仕して起こしてみました")%_HH_」_W"
		ENDIF
	CASE "朝騎乗位"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			IF RAND:2 == 0
				CALL PRINT_STRL, @"「%TEXTR("あっ、あんっ…ああっ、あっ/んっ、んんっ、んぅっ、んうっ")%_HH_」"
				CALL PRINT_STRW, @"「%TEXTR("早上好（敬语）、/早上好、啾_HH_　")%%CNAME("主人")%。%TEXTR("从早上開始就元气十足呢，那个……变得想做了……对不起/虽然是来叫醒你的，明明人没有起来下面却立了起来……这样很害臊吧……")%」"
			ELSE
				CALL PRINT_STRL, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_　%TEXTR("ボンジュール（Bonjour）/おはようございます")%。%CNAME("主人")%」"
				CALL PRINT_STRW, @"「%TEXTR("嗯、啊/嗯、嗯、嗯")%、%TEXTR(@"我想我好久没练习骑马了、就跨上了%CNAME("主人")%/骑马可是圣骑士的嗜好、就算是%CNAME("主人")%也会骑给你看的")%_HH_」"
			ENDIF
		ENDIF
	CASE "朝Ａ騎乗位"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STRL, @"「%TEXTR("Bonjour（法：早上好）/早上好")%_HH_　%CNAME("主人")%」"
			IF RAND:2 == 0
				CALL PRINT_STRW, @"「%CNAME("一人称")%的……%TEXTR("屁股/肛門/不浄の穴")%从%TEXTR("昨日/昨晩/夜")%就%TEXTR("平静不下来/蠢蠢欲动的")%、%TEXTR(@"虽然也试过自慰……但是果然还是不行/无论如何还是想要%CNAME("主人")%的东西")%……%TEXTR("啊/呜")%_HH_」"
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("fufu/唔fufu")%_HH_　%TEXTR("今朝/今日")%，%CNAME("一人称")%的%TEXTR("屁股/肛門/不浄の穴")%的%TEXTR("状况/紧致/舒适")%怎么样？　昨晚開始%TEXTR(@"一直用%CNAME("二人称")%大小的假体进行扩张。这样的坏孩子是需要惩罚的吧？/因为我刚刚拔掉了插頭、%CNAME("一人称")%也有些忍不住了……对不起。很不像样的吧")%」"
			ENDIF
		ENDIF
	CASE "朝接吻"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("ちゅ/ちゅっ/ちゅぅ")%_HH_」_L"
			IF RAND:2 == 0
				CALL PRINT_STR, @"「%TEXTR(@"元気が出る接吻ですよ。今日一日頑張ってくださいね/おはようございます、%CNAME("主人")%/いい朝ですね、今日も一日頑張りましょう")%」_W"
			ENDIF
		ENDIF
	CASE "朝添い寝"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			PRINTFORML 「%TEXTR("您好（Bonjour）/早上好（敬语）")%……%TEXTR(@"我是来叫醒你的，但是当我看到你的睡脸时%CNAME("一人称")%也想睡了/本来是来叫醒你的，结果却睡在了一起。")%
			PRINTFORMW 「%TEXTR(@"那个……再这样做一会儿可以吗？　/再这样做一会儿……请让我这样做。")%%CNAME("主人")%」
		ENDIF
	CASE "朝六九式"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			PRINTFORML 「%TEXTR("嗯、pero、pero……/嗯……嗯、啾、啾……")%　早上好（敬语）」
			PRINTFORMW 「%TEXTR(@"早上做服务的时候%CNAME("一人称")%也湿了……是难得的机会，能不能互相做一下服务？/因为一直不起来就開始做侍奉了……啊、%CNAME("一人称")%的阴蒂酱、不行。感觉太强烈了")%」
		ENDIF
	CASE "朝口交"
		;複数PLAY　これで分岐取れるはず？？？
		;複数角色いると朝口交を狙って起こせない。取り合えず書いてコメントアウト処理しておく
		;一緒にPLAYしている相手の名前は%CNAME(CALLNAME:PARTNER() )%で呼べるのか？　%CNAME("主人")%形式じゃないとダメなのか、解らん……
		IF PARTNER()
			IF RAND:3 == 0
				PRINTFORML 
				PRINTFORML 「%TEXTR("您好（Bonjour）/早上好")%……%TEXTR("嗯、啾、/啾、啾、/pero、pero、")%%CNAME("主人")%……」
				CALL PRINT_STRW, @"「今天是与%CNAME("partner")%%TEXTR("两人/一起")%、进行%TEXTR("早上的口交侍奉/充分的侍奉/直到打起精神来的侍奉")%……%TEXTR("啾、啾/嗯、唔/pero、pero")%_HH_　%TEXTR("要開动了/得手了")%_HH_」"
			ELSEIF RAND:2 == 0
				PRINTFORML 
				PRINTFORML 「%TEXTR("您好（Bonjour）/早上好")%。%CNAME("主人")%」
				CALL PRINT_STRL, @"「%TEXTR("起こし来了ら/おはようの打招呼の領帯ミングで")%丁度%CNAME("partner")%%TEXTR("も来てたので/と鉢合わせたので")%……%TEXTR("ちゅっ、ちゅう/じゅるっ、じゅるる")%_HH_」"
				CALL PRINT_STRW, @"「%TEXTR("今天/今早")%是%TEXTR("两人/一起")%，%TEXTR("请让我为您服务。/请让我为您服务早上。")%_HH_」"
			ELSE
				PRINTFORML 
				PRINTFORML 「%TEXTR("您好（Bonjour）/早上好")%。%CNAME("主人")%」
				CALL PRINT_STRW, @"「%CNAME("一人称")%和%CNAME("partner")%、%TEXTR(@"谁更擅长侍奉/谁能先让%CNAME("主人")%射出来")%……%TEXTR("啾、啾/嗯、唔/pero、pero")%_HH_　%TEXTR("变成了一决胜负的情况。/这是比赛哦")%_HH_」"
			ENDIF
		ELSE
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 
				PRINTFORML 「%TEXTR("哈…嗯……/嗯…嗯……")%　早上好（敬语）。%CNAME("主人")%」
				IF RAND:2 == 0
					CALL PRINT_STRW, @"「本来是来叫醒%CNAME("主人")%的、但是因为睡得很香……%TEXTR("于是就恶作剧了/于是就開始侍奉了")%_HH_」"
				ELSE
					CALL PRINT_STRW, @"「不好好起床的坏孩子，嗯_HH_……%TEXTR("jururu、jubojubo/jurujuru、jururu、gubogubo")%、%TEXTR("我要惩罚你/我要处以吸吮之刑")%？_HH_」"
				ENDIF
			ENDIF
		ENDIF
	CASE "Ｗ朝口交"
		;PARTNERはCSTR:朝袭 = 朝口交、TARGETはCSTR:朝袭 = Ｗ朝口交 となっているため　Ｗ朝口交の時のみ喋る予定
		;淫乱
		IF TALENT:淫乱 && RAND:2 == 0
			PRINTFORML 
			PRINTFORML 「早安、%CNAME("主人")%」
			CALL PRINT_STRL, @"「%TEXTR("んっ、ちゅ、/ちゅっ、ちゅっ、/れろっ、れろっ、")%%TEXTR(@"朝から欲しくなってしまった%CNAME("一人称")%達のお口にたっぷり朝の濃厚ミルクを種付けしてくださいね/ごめんなさい。%CNAME("一人称")%達はコレが欲しくってたまらなくなってしまいました")%_HH_……」"
			CALL PRINT_STRW, @"「%TEXTR("哈/呜")%_HH_……%TEXTR("早上的口交服务。请每天都命令我/仅仅是闻到味道……就兴奋地要去了/只是放进嘴里，就湿了")%……%TEXTR("啾、啾/嗯、呜/peropero、pero")%_HH_　%TEXTR(@"非常棒的说/%CNAME("一人称")%们的侍奉舒服吗")%、%CNAME("主人")%_HH_」"
		;恋慕を想定
		ELSE
			PRINTFORML 
			PRINTFORML 「早上好（敬语）……%TEXTR("嗯，啾，/啾，啾，/pero，pero、")%%CNAME("主人")%……」
			CALL PRINT_STRW, @"「今天是与%CNAME("partner")%%TEXTR("两人/一起")%、进行%TEXTR("早上的口交侍奉/充分的侍奉/直到打起精神来的侍奉")%……%TEXTR("啾、啾/嗯、唔/pero、pero")%_HH_　%TEXTR("要開动了/得手了")%_HH_」"
		ENDIF
	ENDSELECT
CASE "打招呼"
	;2018/01/15　フラグを整理
	;朝は　調教指令側の台詞表示→経験処理表記→事後のココの台詞の順番で表記される
	SELECTCASE CSTR:朝袭
		CASE "朝接吻"
			PRINTFORML 「早安、%CNAME("主人")%」
			IF TEXTS("曜日") == "日"
				PRINTFORML 「今天%TEXTR("有什么安排吗？/有空吗？")%　%TEXTR("如果没有的话、就这样一起无所事事吧？/如果没有的话、不如一起出門吧？")%」
			ELSE
				CALL PRINT_STR, @"「%TEXTR(@"今天也要打起精神来努力哦　啾/今天也能看到%CNAME("二人称")%充满活力的样子、%CNAME("一人称")%很幸福")%_HH_」_W"
			ENDIF
		CASE "朝の母乳搾り"
			CALL PRINT_STR, @"「%TEXTR("手法变得很熟练了呢/其实……本来每天都得自己来处理、现在真是帮大忙了")%_HH_」_W"
		CASE "朝足交"
			PRINTFORMW 「%TEXTR(@"说实在、我不太懂其妙处、但要是%CNAME("主人")%乐意的话、我会再给你做的哦/不脚下留情那样会更爽、就算你这么说、要是痛起来的话就不是我的本意了呢")%」
		CASE "朝舐陰"
			CALL PRINT_STR, @"「%TEXTR("嗯/嗯呜")%……%TEXTR("咀噜噜/咀噜、吧唧吧唧吧唧")%_HH_」_W"
			CALL PRINT_STR, @"「%TEXTR("早安/Bonjour（法：早安）")%%CNAME("主人")%……%TEXTR("现在、这就给您弄干净的啦、请稍等一下哦/为了弄干净而舔着的说、却不断地溢出来呢")%_HH_」_W"
		CASE "朝逆正常位"
			PRINTFORMW 「%CNAME("主人")%……%TEXTR("对不起/我在反省了/您生气了吗？")%」
		CASE "朝騎乗位"
			CALL PRINT_STR, @"「%TEXTR("啾/啾唔")%_HH_　%TEXTR("早安/Bonjour（法：早安）")%」_L"
			IF RAND:2 == 0
				CALL PRINT_STR, @"「%TEXTR("听说这样唤醒恋人很受欢迎/听说会给喜欢的人带来惊喜、就寝袭上了")%_HH_」_W"
			ELSE
				CALL PRINT_STR, @"「%TEXTR("因为在学校不能太卿卿我我的、那在家就好好享受吧/在家里享受的话、在学校就请好好忍耐哦")%_HH_」_W"
			ENDIF
		CASE "朝Ａ騎乗位"
			PRINTFORM 「%TEXTR("对不起/抱歉")%……
			IF FINDCHARA(NO, 1) > 0 && RAND:2 == 0
				PRINTFORM 被弗理希艾尔拜托了
			ENDIF
			PRINTFORML 我来叫%TEXTR(@"%CNAME("主人")%/%CNAME("二人称")%")%起床……」
			CALL PRINT_STR, @"「……%TEXTR("因为看到很难受那样地膨胀起来了/因为用手平息不下来、")%%TEXTR("就想给它放松一下、/想着得侍奉它才行……")%%TEXTR("嗯/啊啊嗯")%_HH_」_L"
			PRINTFORML 「%TEXTR("不由得……就跨坐下来了/虽然知道这样不好……但请允许我用污秽的菊穴为您侍奉")%……」
			IF TALENT:恋慕 && RAND:2 == 0
				PRINTFORML %TEXTR(@"对不起、擅自做了任性的事情。请不要讨厌%CNAME("一人称")%哦/自身做出这种事情的女孩子什么的……挺讨厌的是吧/%CNAME("一人称")%是圣职者却做出这么下流的事情……不反省的话")%……」
			ENDIF
		CASE "朝の新妻PLAY"
			PRINTFORML 「%TEXTR(@"一大早就被充分款爱了/一大早就这么激烈……真是的/因为%CNAME("一人称")%太可爱了、就算这么说也不行哟")%……」
			PRINTFORMW 「%TEXTR("那么、早饭就一起洗个澡再吃吧/饭前先淋个浴吧。都出汗了嘛")%」
		CASE "朝添い寝"
			IF TALENT:恋慕 && RAND:2 == 0
				PRINTFORML 「%TEXTR("早安/Bonjour（法：早安）")%……其实%TEXTR("我做了个噩梦之后、一个人睡很害怕就过来打扰了/我搞错房间了")%。%TEXTR("对不起/添麻烦了么")%」
			ELSE
				PRINTFORML 「%TEXTR("不好、来叫你起床的却一起睡下了/不好意思、抱着的感觉很好就……来叫你起床的却一起睡下了")%」
			ENDIF
		CASE "朝口交", "朝乳交", "朝六九式", "Ｗ朝口交"
			IF TALENT:恋慕
				;包茎
				;IF CHECK_STAIN(STAIN:MASTER:Ｐ, "Ｐ恥垢") && TALENT:MASTER:包茎
				IF TALENT:MASTER:包茎
					IF COND("夏") && RAND:2 == 0
						PRINTFORML 「%TEXTR("嗅嗅/嗜嗜")%……」
						PRINTFORML 「%TEXTR(@"还有，有点味儿呢。汗和耻垢的味道混合在一起……嗯、%CNAME("一人称")%还挺喜欢哦、这个味道/为您服务……因为太闷了，积了很多脏东西呢。后面再说……我会用嘴为您服务，直到您全部变干净。")%」
					ELSE
						;真性包茎
						IF COND("真性包茎", MASTER)
							CALL PRINT_STR, @"「%TEXTR("皮の下に隠れちゃってる亀頭ってちょっと可愛いですよね/おはようございます。すっかり元気になっちゃてるおちんちんの皮を剥いて恥垢をお掃除したらお仕舞いにしましょう")%_HH_」_L"
							IF RAND:2 == 0
								PRINTFORML 「%TEXTR("今天/今早")%也是、把%TEXTR("积存着的耻垢/藏起来的场所下的污垢")%好好清理干净为止、请让我%TEXTR("给您舔哦/侍奉您哦")%」
							ELSE
								PRINTFORML 「%TEXTR(@"如果能让你变干净的话%CNAME("一人称")%也会很高兴/如果积累了羞耻污垢的话请尽早告诉%CNAME("一人称")%哦？")%」
							ENDIF
						;假性包茎
						ELSE
							IF RAND:2 == 0
								PRINTFORML 「fu……肉棒的包皮下面还积累了许多呢……」
								CALL PRINT_STRW, @"「嗯，再仔细一点……我会把所有的东西都弄干净的，放轻松点好吗_HH_」"
							ELSE
								PRINTFORML 「因为隐蔽的地方容易堆积污垢呢」
								PRINTFORML 「随时告诉我的话……%CNAME("一人称")%会让每天保持干净的……」
								CALL PRINT_STRW, @"「今后想要使用%CNAME("一人称")%的嘴的时候、请命令我为您服务吧_HH_」"
							ENDIF
						ENDIF
					ENDIF
				ELSEIF TEXTS("曜日") == "日"
					PRINTFORML 「嘘——、请安静一点。继续这样的话…会被%TEXTR("家人/可怕的女仆小姐")%发现的啦、那就完蛋了」
				ELSE
					PRINTFORML 「%TEXTR("没时间啦/很遗憾")%……已经没时间%TEXTR("继续/实战")%了」
				ENDIF
				IF TEXTS("曜日") == "日"
					PRINTFORML 「%TEXTR("早饭之后/晚上")%再继续吧？」
				ELSE
					PRINTFORML 「后续……%TEXTR("＿/对啊、")%如果是%TEXTR("学校/部室")%で%TEXTR("的话、呢？/的话、就可以哟……")%」
				ENDIF
			ELSE
				PRINTFORML 「因为时间関系、今早就到此为止了」
			ENDIF
			PRINTFORML 
	ENDSELECT
	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1
	IF TALENT:恋慕 && (DAYEV("万圣节", "游戏内") || DAYEV("万圣节")) && CEVENT("個別万圣节イベント：玛丽") == 0
		CALL PRINT_STRL, @"「Trick or Treat_HH_」"
		CALL PRINT_STRW, @"「好了，要么你就乖乖地被捣乱，要么就给我点心。_HH_」"
