			ENDIF
		CASE "１年生", "２年生", "３年生", "研究生"
			LOCALS = %CALLNAME:PLAYER%前輩_%CSTR:PLAYER:ふりがな%前輩
		CASEELSE
			IF COND("教員", PLAYER)
				LOCALS = %CALLNAME:PLAYER%老师_%CSTR:PLAYER:ふりがな%老师
			ELSE
				LOCALS = %CALLNAME:PLAYER%同学_%CALLNAME:PLAYER%同学
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
    IF TRUE_SEX(MASTER) != "男性"
        LOCALS = 欧内酱_欧内酱
    ELSE
        LOCALS = お哥哥_欧尼酱
    ENDIF
CASE "助手"
	IF COND("麦莉爱娜", ASSI)
	    LOCALS = 母亲_母亲
	ELSEIF COND("樱", ASSI)
	    LOCALS = %CALLNAME:ASSI%欧内酱_%CSTR:ASSI:ふりがな%欧内酱
