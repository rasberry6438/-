;	    LOCALS = %CALLNAME:ASSI%欧内酱_%CSTR:ASSI:ふりがな%欧内酱
	ELSEIF NO:PLAYER == 51
		LOCALS = 棗桑_なつめ桑
	ELSEIF NO:PLAYER == 55
		LOCALS = 紬希酱_つむぎ酱
	ELSEIF NO:PLAYER == 62
		LOCALS = 霧江欧内酱_きりえ欧内酱
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:PLAYER:男性
				LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:ASSI%酱_%CSTR:ASSI:ふりがな%酱
			ENDIF
		CASE "１年生", "２年生", "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
		CASE "教師"
			LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
		CASEELSE
	    	LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
		ENDSELECT
   ENDIF
CASEELSE
	;ARGSに角色名が入れられてる場合には角色への呼び方なので格子
	IF FIND_NO(ARGS) > 0
		ACTOR = FIND_NO(ARGS)
		IF NO:ACTOR == 10
			LOCALS = 母亲_母亲
		ELSEIF NO:ACTOR == 6
			LOCALS = 樱欧内酱_樱欧内酱
		ELSEIF NO:ACTOR == 51
			LOCALS = 棗桑_なつめ桑
		ELSEIF NO:ACTOR == 55
			LOCALS = 紬希酱_つむぎ酱
		ELSEIF NO:ACTOR == 62
			LOCALS = 霧江欧内酱_きりえ欧内酱
		ELSE
			SELECTCASE CSTR:ACTOR:学年
			CASE "初等部", "中等部"
				IF TALENT:ACTOR:男性
					LOCALS = %CALLNAME:ACTOR%君_%CSTR:ACTOR:ふりがな%君
				ELSE
					LOCALS = %CALLNAME:ACTOR%酱_%CSTR:ACTOR:ふりがな%酱
				ENDIF
			CASE "１年生", "２年生", "３年生", "研究生"
				LOCALS = %CALLNAME:ACTOR%前輩_%CSTR:ACTOR:ふりがな%前輩
			CASEELSE
				IF COND("教員", ACTOR)
					LOCALS = %CALLNAME:ACTOR%老师_%CSTR:ACTOR:ふりがな%老师
				ELSE
					LOCALS = %CALLNAME:ACTOR%桑_%CSTR:ACTOR:ふりがな%桑
				ENDIF
			ENDSELECT
		ENDIF
	ENDIF
ENDSELECT
RETURNF LOCALS

