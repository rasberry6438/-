;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K52, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSE
		LOCALS = %CALLNAME:ASSI%桑_%CSTR:PLAYER:ふりがな%桑
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 您_您
CASE "助手"
	LOCALS = %CALLNAME:ASSI%桑_%CALLNAME:ASSI%桑
CASE "Master"
	IF NAME:MASTER == "你"
		LOCALS = 你_你
	ELSEIF CONDS("本来の性別", MASTER) == "男性"
		LOCALS = %NAME:MASTER%君_%CSTR:MASTER:ふりがな%君
	ELSE
		LOCALS = %NAME:MASTER%酱_%CSTR:MASTER:ふりがな%酱
	ENDIF
CASE "調教時二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF NAME:MASTER == "你"
			IF TALENT:MASTER:男性
				LOCALS = 你
			ELSE
				LOCALS = 您
			ENDIF
		ELSE
			PRINTFORM %NAME:MASTER%
			IF TALENT:MASTER:男性
				LOCALS = 你
			ELSE
				LOCALS = 您
			ENDIF
		ENDIF
	;相手が主人ではない
	ELSE
		LOCALS = %CALLNAME:ASSI%桑_%CALLNAME:ASSI%桑
	ENDIF
ENDSELECT
RETURNF LOCALS


