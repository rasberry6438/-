@GOD_FUTANARI
CALL COLORMESSAGE("嚯、沒想到人類察覺到了吾的正體",C_D_RED,2,1)
PRINTFORML 
PRINTFORMW 聲音主人的气息変強大了……
CALL COLORMESSAGE("你対扶她肉棒的愛真是了不起",C_D_RED,1,1)
CALL COLORMESSAGE("給你獎勵、說出你的願望吧",C_D_RED,1,1)
$LOOP
CALL ASK_M("沒什麼",1,"長出扶她肉棒",1,"強化扶她肉棒",1,"想吃扶她肉棒",1,"請給我內褲",1)
SELECTCASE RESULT
	CASE 0
		CALL COLORMESSAGE("不懂風趣……你應該展示出光榮接受恩惠的樣子",C_D_RED,1,1)
		CALL COLORMESSAGE("就這樣吧",C_D_RED,1,1)
	CASE 1
		PRINTFORML 選誰？
		DRAWLINE
		FOR LOCAL,0,CHARANUM
			SIF TALENT:LOCAL:性別 == 1
				PRINTFORMLC [{LOCAL}] %NAME_OUTPUT_TRANSLATION("CALLNAME", LOCAL),15,LEFT%
		NEXT
		DRAWLINE
		PRINTFORML [999] 算了
		INPUT
		IF INRANGE(RESULT,0,CHARANUM) && TALENT:RESULT:性別 == 1
			TALENT:RESULT:性別 = 3
			MAXBASE:RESULT:精力 = 1000
			PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", RESULT)%变成了扶她
			CALL COLORMESSAGE("这很容易",C_D_RED,1,1)
		ELSE
			GOTO LOOP
		ENDIF
	CASE 2
		PRINTFORML 誰的？
		DRAWLINE
		FOR LOCAL,1,CHARANUM
			SIF TALENT:LOCAL:性別 == 3 && MAXBASE:LOCAL:精力 < 1500
				PRINTFORMLC [{LOCAL}] %NAME_OUTPUT_TRANSLATION("CALLNAME", LOCAL),15,LEFT%
		NEXT
		PRINTL 
		DRAWLINE
		PRINTFORML [999] 算了
		INPUT
		IF INRANGE(RESULT,1,CHARANUM) && TALENT:RESULT:性別 == 3 && MAXBASE:RESULT:精力 < 1500
			PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", RESULT)%的精力最大値增加
			MAXBASE:RESULT:精力 += 100
			CALL COLORMESSAGE("这很容易",C_D_RED,1,1)
		ELSE
			GOTO LOOP
		ENDIF
	CASE 3
		CALL COLORMESSAGE("呵呵、対扶她肉棒的愛讓吾非常愉快",C_D_RED,1,1)
		CALL COLORMESSAGE("就在這收下它、盡情的品嘗吧",C_D_RED,1,1)
