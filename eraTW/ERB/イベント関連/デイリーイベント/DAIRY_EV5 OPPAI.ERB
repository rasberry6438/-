	SETCOLOR C_AQUA
	FONTBOLD
	PRINTFORML 這個想法只要你沒有忘記、\@ ARG < 0 ? 貧乳# 巨乳\@就永遠是你的夥伴…
	PRINTFORML 給你\@ ARG < 0 ? 貧乳# 巨乳\@的加護吧……
	RESETCOLOR
	FONTREGULAR
	CALL ASK_YN("想要揉搓胸部","希望祝福胸部")
	IF RESULT
		PRINTFORML 誰的？
		DRAWLINE
		SIF FLAG:並び替え
			CALL CHARASORT_EX
		FOR LOCAL:1, 1, EX_CHARANUM
			IF FLAG:並び替え
				LOCAL = RESULT:(LOCAL:1)
				SIF LOCAL < 0
					CONTINUE
			ELSE
				LOCAL = LOCAL:1
				SIF LOCAL >= CHARANUM
					BREAK
			ENDIF
			SIF CFLAG:LOCAL:出禁
				CONTINUE
			SIF TALENT:LOCAL:路人
				CONTINUE
			IF ARG > 0
				SIF TALENT:LOCAL:胸圍 >= 2
					CONTINUE
			ELSE
				SIF TALENT:LOCAL:胸圍 <= -2
					CONTINUE
			ENDIF
			SELECTCASE TALENT:LOCAL:胸圍
			CASE IS <= -2
				LOCALS = (壁)
			CASE -1
				LOCALS = (貧)
			CASE 0
				LOCALS = (普)
			CASE 1
				LOCALS = (巨)
			CASE IS >= 2 
				LOCALS = (爆)
			ENDSELECT
			PRINTFORMLC [{LOCAL, 3}] %NAME_OUTPUT_TRANSLATION("CALLNAME", LOCAL) + LOCALS,15,LEFT%
		NEXT
