@DAILY_CONDITION_EV26
#DIM 発生率
発生率 = 25
SIF FLAG:獲得胖次種数 >= 4
	発生率 += SQRT(FLAG:獲得胖次種数) / 2
発生率 += FLAG:篤信 * 5
IF 発生率 < RAND:1000 || FLAG:異常気象
	RETURN -1
ENDIF
CALL DAILY_EV26
RETURN 1

