VARSET RESULTS
VARSET RESULT
;分布
IF INRANGE(NOW_AREA, 0, 10) && GETBIT(SELL_AREA, NOW_AREA)
	RESULT:0 = 1
;分布無視
ELSEIF NOW_AREA == 99
	RESULT:0 = 1
ENDIF

;布製品は鉱物・技術がかからない
SIF STRFIND(SENSE_MAIN, "布製") >= 0 && STRFIND(SENSE, "鉱物") + STRFIND(SENSE, "技術") >= -1
	RESULT:0 = 0
;鉱製品は繊維がかからない
SIF STRFIND(SENSE_MAIN, "鉱製") >= 0 && STRFIND(SENSE, "繊維") >= 0
	RESULT:0 = 0
;肖像物は鉱物、繊維、技術がかからない
SIF STRFIND(SENSE_MAIN, "肖像") >= 0 && STRFIND(SENSE, "鉱物") + STRFIND(SENSE, "繊維") + STRFIND(SENSE, "技術") >= -2
	RESULT:0 = 0
;香水専用
SIF STRFIND(SENSE_MAIN, "香水") >= 0 && STRFIND(SENSE, "鉱物") + STRFIND(SENSE, "繊維") + STRFIND(SENSE, "技術") + STRFIND(SENSE, "型") >= -3
	RESULT:0 = 0
;生体品は植物がかからない
SIF STRFIND(SENSE_MAIN, "生体品") >= 0 && STRFIND(SENSE, "植物") >= 0
	RESULT:0 = 0

SENSE += NAME_ADJECT + "/"

RESULT:1 = MAGNIF_PRICE
RESULT:2 = SELL_AREA
RESULTS:0 = %NAME_ADJECT%
RESULTS:1 = %SENSE%
RETURN RESULT:0
