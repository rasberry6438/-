@CHARAMOVE_DATA_153(ARG, ARG:2)
LOCAL = 153
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;私室には入らない
			CASE P704河童の宣傳站, P706厄神的家, P709莉莉們的家, P716秋姉妹的家・土間 TO P730仙人的宅邸・庭
				RETURN -100
			CASE P733正邪的隠匿処, P737招财猫的家, P740山童の宣傳站, P736山姥之家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜花道的鳥居, 神社境内, 神社走廊
				RETURN 1
			CASE 中有之道, 三途之川, 彼岸, 地獄, 畜生界
				RETURN 1
			CASE 山麓樹海, 玄武之澤, 大蛤蟆之池, 森の深奥
				RETURN 1
			CASE 九天瀑布, 絶景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ153
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ153(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "美天")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "連衣裙"))
CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "金輪"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "西遊記風の")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "おサルパンツ")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "おサルブラ")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "麺類/丰盛/濃厚/豪華/豪快/新鲜/松脆/神力/畜生流/" + "中華/辣/咸/" * 2 + "炸薯条/肉料理/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "野菜/酸/酸/简易/淡白/質素/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "畜生流/得意料理＊中華＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/帥氣的/大陸風/充滿力量/壮大/" + "神霊/水獭の/" * 3 + "猴子の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/洋風/繊細/知的/" + "狼の/鹫の/犬の/" * 10)
ENDSELECT

