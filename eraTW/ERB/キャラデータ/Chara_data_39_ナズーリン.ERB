@CHARAMOVE_DATA_39(ARG, ARG:2)
LOCAL = 39
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
		;たまに連動
		SIF CFLAG:LOCAL:今日の運勢 % 10 == 0
			RETURN [[星]]
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P114村紗私室, P115星私室, P116一輪私室, P117鵺私室, P118白蓮私室, P109女苑私室, P136響子私室
				RETURN -5
			;別屋、猯藏の棲家には入らない,神霊廟内部は行かない
			CASE P119別屋, P137猯藏の棲家, P125屠自古私室 TO P134蔵, P138鐘楼, P139邪仙楼
				RETURN -100
			;居間に行きやすい
			CASE P108居間
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 地蔵小路, 倒垂的柳樹下, 命蓮寺境内, 命蓮寺方丈, 墓地
				RETURN 1
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大樹, 再思之道, 無縁塚
				RETURN 1
			CASE 中有之道, 三途之川
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
		;白蓮が本堂にいる時はやってくる
		SIF CFLAG:[[白蓮]]:現在位置 == P103本堂 && !CFLAG:[[白蓮]]:诶嘿嘿 && SHIRAHU([[白蓮]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ39
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ39(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "納茲琳")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子+兜帽）套")

CASE "便装：外搭"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外搭", "披肩"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "无袖上衣"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "尾部有洞的灰色")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "编织物短裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "编织物胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "灵摆查找")
CASE "固有指令実行可能"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "香料/中華/油炸食品/辣/丰盛/垃圾/濃厚/野趣/新鲜/怪诞/粘稠/甜/甜/甜/乳酪/乳酪/乳酪/乳酪/乳酪/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "简易/健康/新鲜/咸/咸/淡白/淡白/淡白/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "得意料理＊乳酪＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "灰/可愛的/懷舊/繊細/知的/" + "宝石/遺物/" * 3 + "鼠の/小小の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "充滿力量/誇張/" + "护臂/" * 3 + "猫の/" * 10)

;農民関連
;泣きついたら小言を言いながら何とかしてくれそう
;特に寺の連中が見てたら断りきれない
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "茄子農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "茄子")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "茄子畑")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "命蓮寺")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 1)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 106)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 8)
ENDSELECT

