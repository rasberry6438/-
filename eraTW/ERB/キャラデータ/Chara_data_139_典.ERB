@CHARAMOVE_DATA_139(ARG, ARG:2)
LOCAL = 139
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
		;たまに連動
		SIF CFLAG:LOCAL:今日の運勢 % 8 == 0
			RETURN [[龍]]
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;天界には行かない
			CASE P830玄雲海 TO P842比那名居邸庫房
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜花道的鳥居, 神社境内, 神社走廊
				RETURN 1
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大樹, 霧雨魔法店
				RETURN 1
			CASE 山麓樹海, 玄武之澤, 大蛤蟆之池, 間欠泉地下中樞
				RETURN 1
			CASE 九天瀑布, 絶景之丘, 山之湖, 守矢神社境内, 虹龍洞
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ139
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ139(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "典")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子+兜帽）套")

CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "旗袍（裤子）"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "短布襪"))
	
CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "蠱惑的な")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "純白胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "番の管狐ブラ")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 勝負内衣判定([[典]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/肉料理/肉料理/卵/卵/卵/油炸食品/甜/甜/辣/辣/简易/淡白/豪華/上品/新鲜/粘稠/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "酸/酸/濃厚/質素/質素/野趣/野趣/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "隠し味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "高級/白/混沌/可愛的/樸素/別緻/知的/" + "純白の/狐の/药瓶/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "黒/秩序/誇張/威厳/" + "安物/护符/" * 3 + "狸の/护臂/" * 10)
ENDSELECT

