;例の街道、STRFINDを使用してください
;-------------------------------------------------------------------------------------------
@IMAGE_I143_特殊エフェクトチェック(CHARA, エフェクト)
#DIM CHARA
#DIMS エフェクト

IF CFLAG:CHARA:差し替え適用
	;替换用画像
ELSE
	;通常画像
	;SIF STRFIND(エフェクト, "母乳") != -1
	;	RETURN 1
	SIF STRFIND(エフェクト, "顔精液") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "口精液") != -1
		RETURN 1
ENDIF

RETURN 0

;IMAGE_I143_特殊エフェクト(CHARA)--------------------------------------------------------------------
;角色特有のエフェクト表示条件
;使用時注意点:
;	RESULTS配列に特殊エフェクトを格納
;	RETURNで追加するエフェクト数を返却すること
;-------------------------------------------------------------------------------------------
@IMAGE_I143_特殊エフェクト(CHARA)
#DIM CHARA
#DIM DYNAMIC エフェクト数 = 0
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;替换用画像
	IF FLAG:立絵種類
		;顔絵
	ELSE
		;立絵
	ENDIF
ELSE
	;通常画像
	IF FLAG:立絵種類
		;顔絵
		IF CFLAG:CHARA:脸上精液
			RESULTS:エフェクト数 = 顔精液
			エフェクト数 += 1
		ENDIF
		IF CFLAG:CHARA:口内精液
			RESULTS:エフェクト数 = 口精液
			エフェクト数 += 1
		ENDIF
	ELSE
		;立絵
	ENDIF
ENDIF

RETURN エフェクト数

