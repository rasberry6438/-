@CHARAMOVE_DATA_143(ARG, ARG:2)
LOCAL = 143
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE P612法廷 TO P639八雲邸居間
				RETURN -100
			;他人的家には入らない
			CASE P608潤美的家, P609瓔花的家, P606日狭美の家, P607残無の家
				RETURN -100
			;霊長園には行かない
			CASE P642霊長園 TO P645磨弓的房間
				RETURN -100
			;他所の風呂には行かない
			CASE P652早鬼宅の風呂, P646八千慧宅の風呂
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 中有之道, 三途之川, 彼岸, 地獄, 畜生界
				RETURN 1
			CASE 旧地獄街道, 灼熱地獄跡
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ143
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ143(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "尤魔")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裙子+兜帽")
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "連衣裙"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "勺子"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "モコモコの")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "ひつじさん胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "ひつじさんブラ")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "中華/垃圾/濃厚/畜生流/" + "辣/丰盛/豪快/" * 3)
CASE "料理：讨厌的味道"
	;例外的に讨厌的味道が極端に少ない
	CALLF MAKE_STR(V_NAME, "質素/简易/淡白/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "畜生流/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/混沌/怖い/知的/" + "強欲な/血/鹫の/" * 3  + "羊の/剛欲な/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/誇張/芸術的/" + "神霊/" * 3 + "埴轮の/水獭の/狼の/龟の/龍の/埴轮/" * 10)
ENDSELECT

