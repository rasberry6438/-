@CHARAMOVE_DATA_124(ARG, ARG:2)
LOCAL = 124
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;未設定
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大樹, 霧雨魔法店 , 再思之道, 無縁塚
				RETURN 1
			CASE 中有之道, 三途之川, 迷途之家
				RETURN 1
			CASE 地下室
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ124
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ124(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "隠岐奈")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "烏帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "長靴"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "道士服"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "投影了夜空")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "星辰の三角胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "星辰の胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/煮物/健康/豪華/芸術的/上品/扑鼻/辣/辣/酸/酸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "質素/咸/咸/" + "青蛙/虫肉/謎料理/" * 3 + "天狗流/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "魔性/加護/絢爛/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黄/高級/混沌/美的/芸術的/懷舊/誇張/知的/威厳/" + "巫術/美術品/四季の/" * 3 + "七星の/後戸の/隐匿の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "洋風/別緻/洛麗塔/" + "安物/" * 3 + "天狗の/沉眠の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_124
DISH_NAME = 秘神火腿
RESULTS = 主食
CALL 材料設定("獣肉")
DISH_TASTE = 洋食/肉料理/野菜/酸/丰盛/豪華/上品/神力/
DISH_COMMENT = 用生火腿制成的时髦料理。
RETURN 1

;IMAGE_I124_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I124_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
			;SIF DAY:3 == 28 || FLAG:ファッション == 55 || CFLAG:124:衣装一時変更 == 55
			SIF CURRENT_CLOTHES_SET(CHARA) == 55
				RESULTS = _バニー
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
			IF CURRENT_CLOTHES_SET(CHARA) == 55
				RESULTS = _バニー
			ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
				RESULTS = _変T
			ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
				RESULTS = _セーラー
			ELSEIF CHECK_CLO("bra_visible")
				IF DAY:0 % 2
					RESULTS = _内衣
				ELSE
					RESULTS = _内衣2
				ENDIF
			ENDIF
		ELSE
			;立絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ENDIF
ENDIF

