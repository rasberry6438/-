@CHARAMOVE_DATA_68(ARG, ARG:2)
LOCAL = 68
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;永遠亭の奥には行かない
			CASE P405配药室, P407廊下 TO P430地下室
				RETURN -100
			;他人的家には入らない
			CASE P432影狼的家, P437梅蒂欣的家, P439拉瓦爾的家, P443米斯蒂亜的家, P445妹紅的家
				RETURN -100
			;太陽花田,用餐房間,生活間,庭によく居る
			CASE P438太陽花田, P453用餐房間, P461生活間, P466庭
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 竹林入口, 迷失的小道, 斜角的竹林, 無名之丘, 太陽花田, 夢幻館
				RETURN 1
			CASE 森林的入口, 古木的大樹, 霧雨魔法店, 人偶的洋館, 再思之道, 無縁塚
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ68
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ68(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "幽香")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")


CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：飾品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("飾品", "傘"))
CASE "便装：襪子"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "吊帯襪"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
	ENDIF
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "長裙"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：外搭"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外搭", "背心"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "領巾"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "色气与可爱并存")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "花纹褶子内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "花纹胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 勝負内衣判定([[幽香]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)
	
;CASE "固有指令"
;	CALLF MAKE_STR(V_NAME, "弾幕勝負")
;CASE "固有指令実行可能"
;	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "面包/牛奶/洋食/汁物/蛋糕/丰盛/健康/夏物/豪華/上品/新鲜/软绵绵/夢幻風/咸/咸/酸/酸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "辣/辣/" + "青蛙/虫肉/質素/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "夢幻風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "緑/可愛的/時尚/懷舊/充滿力量/" + "土/植物/" * 3 + "向日葵の/花の/華の/夢幻の/種/傘/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "繊細/狂熱/" + "火/刃物/" * 3)

;農民関連
;「かき」と読みます
;園芸用の草花のこと
;仕事の手伝いと被ってるがどうしよう
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "花卉農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "花苗")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "花圃")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "迷途竹林")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 4)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 438)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 15)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_68
DISH_NAME = 向日葵华夫饼
RESULTS = 甜点
CALL 材料設定("甜味料")
DISH_TASTE = 洋食/卵/牛奶/甜/丰盛/濃厚/温暖/夏物/芸術的/松脆/夢幻風/
DISH_COMMENT = 外表像向日葵一样的可爱华夫饼。如果可以的话真想在夏天吃。
RETURN 1

;IMAGE_I68_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I68_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		SIF EQUIP:CHARA:上身衣服２ == 3
			RESULTS = _睡衣
	ENDIF
ENDIF

