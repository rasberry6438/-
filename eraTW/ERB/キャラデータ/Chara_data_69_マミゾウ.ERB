@CHARAMOVE_DATA_69(ARG, ARG:2)
LOCAL = 69
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113納茲琳私室, P114村紗私室, P115星私室, P116一輪私室, P117鵺私室, P118白蓮私室, P109女苑私室, P136響子私室
				RETURN -5
			;神霊廟メンバーの私室はあんまり入らない
			CASE P125屠自古私室, P127布都私室, P128神子私室, P139邪仙楼
				RETURN -5
			;本堂、枯山水、調薬室には行かない
			CASE P103本堂, P131枯山水, P132調薬室
				RETURN -100
			;よく居る
			CASE P108居間, P129客間
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 地蔵小路, 倒垂的柳樹下, 命蓮寺境内, 墓地, 神霊廟広場
				RETURN 1
			CASE 広場, 商家町, 呑屋小道, 長屋街
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ69
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ69(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "猯藏")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "发饰"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "木屐"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：飾品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("飾品", "眼鏡"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "叶子描绘的风流的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "复式内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "复式内胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;特殊衣装
CASE "おうち约会衣装"
	SIF MAIN_MAP == 2
		CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "猯藏和装"))
CASE "人里约会衣装"
	CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "猯藏和装"))

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "野菜/香料/和食/麺類/濃厚/芸術的/野趣/豪快/软糯的/外界風/赏月/辣/辣/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "酸/酸/面包/面包/面包/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "心跳味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "茶/混沌/美的/奇妙/和風/知的/" + "付喪神/妖術/眼鏡/酒/" * 3 + "狸の/叶子の/烟袋/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/樸素/洋風/洛麗塔/" + "神霊/护臂/" * 3 + "狐の/" * 10)

;農民関連
;地主に違和感がない
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "米農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "大米")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "水田")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "命蓮寺")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 1)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 101)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 500)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 20)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_69
DISH_NAME = 狸猫荞麦面
RESULTS = 主食
CALL 材料設定("汎用")
DISH_TASTE = 和食/麺類/咸/简易/淡白/中立温度/中立温度/上品/芸術的/
DISH_COMMENT = 加入了炸天妇罗渣（面酥）来对抗狐狸的荞麦面。
DISH_TIME = 30
RETURN 1


