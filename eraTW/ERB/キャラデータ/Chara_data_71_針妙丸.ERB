@CHARAMOVE_DATA_71(ARG, ARG:2)
LOCAL = 71
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;主に居間に居る
			CASE P9居間
				RETURN 5
			;私室にはあまり入らない
			CASE P15霊夢私室, P52別屋
				RETURN -5
			;他所
			CASE P27三妖精之家・大廳 TO P35三妖精之家・地下室, P37夢幻遺跡・一階通路 TO P51夢幻遺跡・倉庫, P56阿吽の棲家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜花道的鳥居, 神社境内, 神社走廊
				RETURN 1
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE 地蔵小路, 倒垂的柳樹下, 命蓮寺境内, 神霊廟広場
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ71
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ71(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "針妙丸")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "小碗"))
CASE "便装：和服"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("和服", "和服"))
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "肌襦袢"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "帯"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "大和小都合适的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "小人の胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "小人の吊帯衫")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 勝負内衣判定([[針妙丸]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/卵/和食/葉物/垃圾/豪華/上品/新鲜/软绵绵/甜/甜/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "質素/辣/辣/謎料理/謎料理/謎料理/" + "青蛙/虫肉/鬼の魔力/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "得意料理＊上品＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/秩序/帥氣的/時尚/活潑/繊細/巫術/" + "装飾/" * 3 + "小人の/小小の/針/茶碗/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "充滿力量/知的/" + "烟袋/" * 3 + "鬼の/鬼力の/" * 10)

;農民関連
;ころぽっくる
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "蕗農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "蜂鬥葉")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "蕗畑")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "博麗神社")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 350)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 3)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_71
DISH_NAME = 小人面包
RESULTS = 軽食
CALL 材料設定("汎用")
DISH_TASTE = 洋食/面包/甜/简易/機能的/松脆/魔力/
DISH_COMMENT = 为吃了就能掌握周围地形的小人所制作的魔法面包
RETURN 1

;IMAGE_I71_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I71_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ENDIF
ENDIF

