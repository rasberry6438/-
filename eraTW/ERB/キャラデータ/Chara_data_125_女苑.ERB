@CHARAMOVE_DATA_125(ARG, ARG:2)
LOCAL = 125
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113納茲琳私室, P115星私室, P114村紗私室, P116一輪私室, P117鵺私室, P118白蓮私室, P136響子私室
				RETURN -5
			;別屋、猯藏の棲家には入らない,神霊廟内部は行かない
			CASE P119別屋, P137猯藏の棲家, P125屠自古私室 TO P134蔵, P138鐘楼, P139邪仙楼
				RETURN -100
			;居間に行きやすい
			CASE P108居間
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 地蔵小路, 倒垂的柳樹下, 命蓮寺境内, 命蓮寺方丈
				RETURN 1
			CASE 広場, 商家町, 呑屋小道, 長屋街
				RETURN 1
			CASE 旧地獄街道
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
		;一応女苑は無しで
;		;白蓮が本堂にいる時はやってくる
;		SIF CFLAG:[[白蓮]]:現在位置 == P103本堂 && !CFLAG:[[白蓮]]:诶嘿嘿 && SHIRAHU([[白蓮]])
;			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ125
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ125(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "女苑")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（睡袍）套")

CASE "便装：飾品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("飾品", "太阳镜"))
CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "長靴"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "連衣裙"))
CASE "便装：外搭"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外搭", "夹克衫"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "手链"))
CASE "便装：其他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他２", "耳环"))
CASE "便装：其他３"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他３", "单肩包"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "总之很华丽")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "闪亮的内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "闪亮的胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "高级食材/肉料理/面包/洋食/汁物/派/丰盛/垃圾/濃厚/豪華/上品/新鲜/松脆/甜/甜/辣/辣/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "淡白/" + "酸/質素/野菜/" * 2 + "平价食材/怪诞/謎料理/" * 3 + "青蛙/虫肉/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "絢爛/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "高級/混沌/美的/時尚/洋風/誇張/威厳/壮大/装飾/" + "金/宝石/扇/" * 3 + "高級的/華麗的/钱包/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/奇妙/懷舊/和風/別緻/" + "書物/生体品/遺物/护臂/機械/" * 3 + "安物/主役の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_125
DISH_NAME = 意飯
RESULTS = 主食
CALL 材料設定("番茄")
DISH_TASTE = 洋食/披萨/野菜/獣肉/面包/香料/乳酪/咸/丰盛/垃圾/濃厚/温暖/新鲜/上品/絢爛/
DISH_COMMENT = 在曾经的意大利很流行的料理。披萨就是基于当时的味道而再現的。
RETURN 1

