;IMAGE_I30_特殊エフェクト(CHARA)--------------------------------------------------------------------
;角色特有のエフェクト表示条件
;使用時注意点:
;	RESULTS配列に特殊エフェクトを格納
;	RETURNで追加するエフェクト数を返却すること
;-------------------------------------------------------------------------------------------
@IMAGE_I30_特殊エフェクト(CHARA)
#DIM CHARA
#DIM DYNAMIC エフェクト数 = 0
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;替换用画像
	;午睡口交顔
	IF COM_STR == "K30_午睡口交"
		RESULTS:0 = 精液上
		RESULTS:1 = 吐息
		エフェクト数 = 2
		RETURN エフェクト数
	ENDIF
	;辣もの食べるor作るでお目々キラキラ＆星が湧く
	IF GROUPMATCH(SELECTCOM, 413, 414, 415) && STRFIND(COM_STR, "K30_辛味") > -1
		RESULTS:エフェクト数 = キラ
		エフェクト数 += 1
		RETURN エフェクト数
	ENDIF
	;日記口上から指定する顔絵エフェクト盛り合わせ
	;もう少しスマートなやり方無いもんかね
	IF TFLAG:192 <= -6
		RESULTS:エフェクト数 = 照れ
		エフェクト数 += 1
		IF TFLAG:192 <= -7
			RESULTS:エフェクト数 = ハート
			エフェクト数 += 1
			IF TFLAG:192 <= -8
				RESULTS:エフェクト数 = 汗
				エフェクト数 += 1
				IF TFLAG:192 <= -9
					RESULTS:エフェクト数 = 吐息
					エフェクト数 += 1
				ENDIF
			ENDIF
		ENDIF
		RETURN エフェクト数
	ENDIF
	;バイブデート中
	IF TCVAR:30:364 > 0
		RESULTS:エフェクト数 = 吐息
		エフェクト数 += 1
		RESULTS:エフェクト数 = 断面
		エフェクト数 += 1
	ENDIF
	;母乳
	IF TALENT:CHARA:母乳体質 && NOWEX:CHARA:噴乳 && NUDITYCHECK(CHARA) < 2
		RESULTS:エフェクト数 = 母乳
		エフェクト数 += 1
	ENDIF
	IF FLAG:立絵種類
		;顔絵
		IF CFLAG:CHARA:口内精液 || (TCVAR:CHARA:363 == TIME && INRANGE(TIME, 360, 540))
			;顔ぶっかけ
			RESULTS:エフェクト数 = 精液上
			エフェクト数 += 1
			;朝口交後紅潮
			IF TCVAR:CHARA:363 == TIME
				RESULTS:エフェクト数 = 照れ
				エフェクト数 += 1
			ENDIF
		ENDIF
		IF CFLAG:CHARA:手上精液 && CHECK_NUDE(CHARA) && CFLAG:CHARA:妊娠経過日数 < 40
			;手コキ
			RESULTS:エフェクト数 = 精液手
			エフェクト数 += 1
		ENDIF
		IF CFLAG:CHARA:脸上精液
			;胸ぶっかけ
			RESULTS:エフェクト数 = 精液下
			エフェクト数 += 1
		ENDIF
	ELSE
		;立絵
		IF CFLAG:CHARA:脸上精液
			;胸ぶっかけ
			RESULTS:エフェクト数 = 精液上
			エフェクト数 += 1
		ENDIF
		IF CFLAG:CHARA:手上精液 && CHECK_NUDE(CHARA) && CFLAG:CHARA:妊娠経過日数 < 40
			;手コキ
			RESULTS:エフェクト数 = 精液手
			エフェクト数 += 1
		ENDIF
		IF (CFLAG:CHARA:膣内射精 || CFLAG:CHARA:肛門射精) && GROUPMATCH(NUDITYCHECK(CHARA), 0, 2)
			;中出し
			RESULTS:エフェクト数 = 精液下
			エフェクト数 += 1
		ENDIF
	ENDIF
	SIF FLAG:時間停止
		RETURN エフェクト数
	IF BASE:CHARA:情緒 >= MAXBASE:CHARA:情緒 * 8 / 10 && CFLAG:CHARA:1805 < 2
		IF !(TCVAR:CHARA:强行 || TCVAR:CHARA:386 || 仕事中チェック(CHARA)) 
			RESULTS:エフェクト数 = ハート
			エフェクト数 += 1
		ENDIF
	ELSEIF MARK:CHARA:反発刻印 > 0 || CFLAG:CHARA:ブチギレ
		RESULTS:エフェクト数 = 憤怒
		エフェクト数 += 1
	ELSEIF CFLAG:CHARA:睡眠 && TFLAG:100
		RESULTS:エフェクト数 = 睡眠
		エフェクト数 += 1
	ENDIF
ELSE
	;通常画像
	IF FLAG:立絵種類
		;顔絵
	ELSE
		;立絵
	ENDIF
ENDIF

RETURN エフェクト数

