@CHARAMOVE_DATA_7(ARG, ARG:2)
LOCAL = 7
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
		RESULT:1 = [[桑尼]]
		RESULT:2 = [[露娜]]
		RESULT:3 = [[斯塔]]
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P27三妖精之家・大廳
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜花道的鳥居, 神社境内, 神社走廊, 妖精的大樹
				RETURN 1
			CASE 霧之湖, 冰造的小雪屋, 正門
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大樹, 霧雨魔法店, 人偶的洋館, 再思之道, 無縁塚
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;桑尼に依存
		TCVAR:LOCAL:引力点 = TCVAR:[[桑尼]]:引力点
ENDSELECT

@EXIST_キャラデータ7
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ7(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "斯塔")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（睡袍）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "蝴蝶結"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "連衣裙"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "有星星的様子的刺绣")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "斑点三角胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "半首位胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 勝負内衣判定([[斯塔]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "探访迹象")
CASE "固有指令実行可能"
	SIF CFLAG:7:好感度 >= 200 && GET_MAPID(CFLAG:7:現在位置) == MAIN_MAP
		CALLF MAKE_INT(V_NAME, 1)

;農民関連
;むりやりな斯塔フルーツから秋葵（星形断面）に変更
;秋葵って英語だったのか・・・
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "秋葵農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "秋葵")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "秋葵畑")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "博麗神社")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 25)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 200)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "魚料理/和食/鍋物/健康/芸術的/上品/家庭的/扑鼻/闪耀/甜/甜/酸/酸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "辣/野趣/怪诞/怪诞/怪诞/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "隠し味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/果実/可愛的/洋風/別緻/洛麗塔/" + "星/書物/" * 3 + "星の/星光の/三位一体の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "闇/日/月/生体品/护臂/烟袋/" * 3)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_7
DISH_NAME = 京丸牡丹鍋
RESULTS = 主食
CALL 材料設定("高级食材")
DISH_TASTE = 和食/鍋物/野菜/咸/简易/濃厚/温暖/温暖/豪華/芸術的/謎料理/霊障/
DISH_COMMENT = 将传说中的京丸牡丹按字面意思做成牡丹锅的神秘料理。
RETURN 1

;IMAGE_I7_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I7_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立ち絵種類
			;顔絵
			IF GROUPMATCH(CFLAG:CHARA:差し替え適用, 1)
				IF CURRENT_CLOTHES_SET(CHARA) == 55
					RESULTS = _バニー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
					RESULTS = _変T
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
					RESULTS = _セーラー
				ELSEIF CHECK_CLO("bra_visible")
					RESULTS = _内衣
				ELSEIF DAY:2 == 2
					RESULTS = _夏服
				ENDIF
			ENDIF
		ELSE
			;立ち絵
			IF GROUPMATCH(CFLAG:CHARA:差し替え適用, 1)
				IF CURRENT_CLOTHES_SET(CHARA) == 55
					RESULTS = _バニー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
					RESULTS = _変T
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
					RESULTS = _セーラー
				ELSEIF DAY:2 == 2
					RESULTS = _夏服
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立ち絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

