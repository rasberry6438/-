@IMAGE_I62_別立ち判定_表情

IF NOWEX:62:Ｃ絶頂 || NOWEX:62:Ｂ絶頂 || NOWEX:62:Ｖ絶頂 || NOWEX:62:Ａ絶頂 || NOWEX:62:Ｍ絶頂
	IF EX:62:TotalEX >= 5
		RESULTS:0 = 快感急
		RESULTS:1 = 快感急
	ELSE
		RESULTS:0 = 快感缓
		RESULTS:1 = 快感急
	ENDIF
	RETURN
ENDIF
	
IF CFLAG:62:睡眠
	LOCAL = EX:62:無自覚Ｃ絶頂 + EX:62:無自覚Ｖ絶頂 + EX:62:無自覚Ａ絶頂 + EX:62:無自覚Ｂ絶頂 + EX:62:無自覚Ｍ絶頂
	IF LOCAL >= 5
		RESULTS:0 = 眯笑
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 睡眠
		RESULTS:1 = 睡眠
	ENDIF
	RETURN
ENDIF

IF CFLAG:62:ブチギレ
	RESULTS:0 = 怒{RAND:4+1}
	RESULTS:1 = 怒{RAND:4+1}
	RETURN
ELSEIF BASE:62:憤怒 > 600 || TALENT:62:心情 < 0 || CFLAG:62:心情不快 == 1
	RESULTS:0 = 不悦
	RETURN
ENDIF

IF TALENT:62:恋人
	IF BASE:62:情緒 >= 100
		RESULTS:0 = 笑顔
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
ELSEIF TALENT:62:恋慕 || TALENT:62:愛欲
	IF BASE:62:情緒 >= 300
		RESULTS:0 = 笑顔
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
ELSEIF TALENT:62:思慕 || TALENT:62:炮友
	IF BASE:62:情緒 >= 600
		RESULTS:0 = 笑顔
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
ELSE
	IF BASE:62:情緒 >= 1200
		RESULTS:0 = 笑顔
	ELSE
		RESULTS:0 = 通常
	ENDIF
ENDIF

IF RESULTS:0 == "通常"
	IF MARK:62:反発刻印 <= 0
		RESULTS:1 = 微開口
	ENDIF
ELSEIF RESULTS:0 == "笑顔"
	IF PALAM:62:欲情 >= PALAMLV:10 || TCVAR:62:発情 || CFLAG:62:积攒度 >= 900
		RESULTS:1 = 開口
	ELSEIF MARK:62:反発刻印 <= 0 && (TFLAG:103==109 || TARGET==109)
		RESULTS:1 = 貘笑
	ENDIF
ENDIF

RETURN


