@CHARAMOVE_DATA_78(ARG, ARG:2)
LOCAL = 78
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
		RESULT:1 = [[静葉]]
		RESULT:2 = [[穣子]]
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;各私室
			CASE P704河童の宣傳站, P706厄神的家, P729仙人的宅邸・玄関 TO P730仙人的宅邸・庭
				RETURN -100
			CASE P733正邪的隠匿処, P736山姥之家, P737招财猫的家, P740山童の宣傳站, P741美天の棲み処
				RETURN -100
				;畑によく居る
			CASE P715秋姉妹的家・畑
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 広場, 商家町, 呑屋小道
				;秋の間だけ
				SIF DAY:2 == 3
					RETURN 1
			CASE 山麓樹海, 玄武之澤, 大蛤蟆之池, 森の深奥
				RETURN 1
			CASE 九天瀑布, 絶景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ78
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ78(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "穣子")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "围裙洋装"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "有点甜的香味")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "烤白薯的胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "烤番薯色的胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 勝負内衣判定([[穣子]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "野菜/马铃薯/米/蘑菇/煮物/丰盛/濃厚/豪華/野趣/家庭的/软糯的/甜/甜/咸/咸/" + "秋物/" * 5)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "淡白/質素/酸/酸/" + "青蛙/虫肉/冬物/" * 3 + "謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "豊穣/得意料理＊秋物＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "朱/可愛的/時尚/誇張/" + "土/植物/果実/食器/" * 3 + "葡萄の/紅葉の/秋の/豊穣の/種/秋風の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "氷/刃物/弾/护臂/機械/" * 3 + "夏の/樱花の/向日葵の/西瓜の/" * 5  + "春の/冬の/春風の/" * 10)

;農民関連
;米でもいいが天狗と被るのだよ
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "芋農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "芋头")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "芋头田")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "妖怪之山・山麓")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 7)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 715)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 4)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_78
DISH_NAME = 日式烤红薯
RESULTS = 甜点
CALL 材料設定("马铃薯")
DISH_TASTE = 洋食/野菜/甜/濃厚/温暖/秋物/野趣/家庭的/
DISH_COMMENT = 用红薯制成的日式西点。在海外应该行不通吧。
RETURN 1

;IMAGE_I78_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I78_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:衣装一時変更 == 55 || (CFLAG:CHARA:基本服装セット == 55 && CFLAG:CHARA:衣装一時変更==0)
			IF FLAG:立絵種類
				;顔絵
				RESULTS = _バニー
			ELSE
				;立絵
				RESULTS = _バニー
			ENDIF
		ENDIF
	ENDIF
ENDIF

