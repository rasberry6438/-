CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/活潑/洛麗塔/" + "機械/" * 3 + "河童の/天狗の/" * 10)

;特殊衣装設定、おうち约会で稀にみこかせん
CASE "おうち约会衣装"
	SIF MAIN_MAP == 0 && !RAND:15
		CALLF MAKE_INT(V_NAME, 101)
