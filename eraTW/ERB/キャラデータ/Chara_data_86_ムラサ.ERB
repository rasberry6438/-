@CHARAMOVE_DATA_86(ARG, ARG:2)
LOCAL = 86
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113納茲琳私室, P115星私室, P116一輪私室, P117鵺私室, P118白蓮私室, P109女苑私室, P136響子私室
				RETURN -5
			;別屋、猯藏の棲家には入らない,神霊廟内部は行かない
			CASE P119別屋, P137猯藏の棲家, P125屠自古私室 TO P134蔵, P138鐘楼, P139邪仙楼
				RETURN -100
			;居間に行きやすい
			CASE P108居間
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 地蔵小路, 倒垂的柳樹下, 命蓮寺境内, 命蓮寺方丈, 墓地
				RETURN 1
			CASE 中有之道, 三途之川
				RETURN 1
			CASE 玄武之澤, 間欠泉地下中樞
				RETURN 1
			CASE 九天瀑布, 山之湖
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;白蓮が本堂にいる時はやってくる
		SIF CFLAG:[[白蓮]]:現在位置 == P103本堂 && !CFLAG:[[白蓮]]:诶嘿嘿 && SHIRAHU([[白蓮]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ86
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ86(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "水蜜")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
CASE "便装：裤子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裤子", "短裤"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "水手服"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "代替名字写了锚的标志")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "男孩子气内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "吊帯背心胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 勝負内衣判定([[村紗]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "魚料理/香料/汁物/咖喱/垃圾/濃厚/冷/機能的/豪快/怪诞/扑鼻/辣/辣/辣/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "油炸食品/甜/甜/青蛙/青蛙/青蛙/虫肉/虫肉/虫肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "刺激的/美味的水/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/混沌/可怕的/" + "水/幽霊/妖術/" * 3 + "海の/深海の/錨の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "知的/威厳/" + "光/火/护符/护臂/" * 3)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_86
DISH_NAME = 深海料理
RESULTS = 主食
CALL 材料設定("好吃的魚")
DISH_TASTE = 魚料理/咸/丰盛/淡白/野趣/豪快/怪诞/霊障/
DISH_COMMENT = 使用深海魚製作的略顯怪異的料理。
RETURN 1

;IMAGE_I86_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I86_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ENDIF
ENDIF

