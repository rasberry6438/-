	;替换用画像
	IF FLAG:立絵種類
		;顔絵
	ELSE
		;立絵
	ENDIF
ELSE
	;通常画像
	IF INRANGE(CFLAG:CHARA:現在位置, 200, 297) && !CHECK_NUDE(CHARA)
		SIF CHECK_BLUSH(CHARA, @"%GET_BASESTYLE(CHARA)%", "_人里")
			RESULTS = 赤面
		SIF BASE:CHARA:情緒 >= MAXBASE:CHARA:情緒*8/10
			RESULTS = LOVE
	ENDIF
ENDIF

