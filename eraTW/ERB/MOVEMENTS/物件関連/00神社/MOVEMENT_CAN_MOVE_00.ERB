;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=不能移動,不能看
;        1=  能移動,不能看
;        2=不能移動,  能看
;        3=  能移動,  能看（直通）
;-----------------------------------------------------------
@CAN_MOVE_0(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P1鳥居
		SIF GROUPMATCH(ARG:1, P2境内, P22参道)
			RETURNF 3
		SIF ARG:1 == MAXROOM
			RETURNF 2
	CASE P2境内
		SIF GROUPMATCH(ARG:1, P6庫房, P16破舊小屋, P20本殿内部, P56阿吽の棲家)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P1鳥居, P3賽銭箱, P5手水舎, P21守矢神社分社)
			RETURNF 3
	CASE P3賽銭箱
		SIF GROUPMATCH(ARG:1, P4本堂)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 3
	CASE P4本堂
		SIF GROUPMATCH(ARG:1, P3賽銭箱)
			RETURNF 1
	CASE P5手水舎
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 3
	CASE P6庫房
		SIF GROUPMATCH(ARG:1, P2境内, P20本殿内部)
			RETURNF 1
	CASE P7土間
		SIF GROUPMATCH(ARG:1, P9居間, P20本殿内部)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P8廚房)
			RETURNF 3
	CASE P8廚房
		SIF GROUPMATCH(ARG:1, P9居間, P10走廊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P7土間)
			RETURNF 3
	CASE P9居間
		SIF GROUPMATCH(ARG:1, P7土間, P8廚房, P15霊夢私室, P17小的倉庫, P53倉庫, P54小人的虫籠, P55和室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P10走廊)
			RETURNF 3
	CASE P10走廊
		SIF GROUPMATCH(ARG:1, P8廚房, P12脱衣所, P14廁所, P19大的倉庫, P54小人的虫籠)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P9居間, P11庭)
			RETURNF 3
	CASE P11庭
		SIF GROUPMATCH(ARG:1, P20本殿内部, P52別屋, P69先代巫女的小屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P10走廊)
			RETURNF 3
	CASE P12脱衣所
		SIF GROUPMATCH(ARG:1, P10走廊, P13風呂)
			RETURNF 1
	CASE P13風呂
		SIF GROUPMATCH(ARG:1, P12脱衣所)
			RETURNF 1
	CASE P14廁所
		SIF GROUPMATCH(ARG:1, P10走廊)
			RETURNF 1
	CASE P15霊夢私室
		SIF GROUPMATCH(ARG:1, P9居間, P17小的倉庫, P18壁櫥間)
			RETURNF 1
	CASE P16破舊小屋
		SIF GROUPMATCH(ARG:1, P2境内, P20本殿内部)
			RETURNF 1
	CASE P17小的倉庫
		SIF GROUPMATCH(ARG:1, P9居間)
			RETURNF 1
	CASE P18壁櫥間
		SIF GROUPMATCH(ARG:1, P15霊夢私室)
			RETURNF 1
	CASE P19大的倉庫
		SIF GROUPMATCH(ARG:1, P10走廊)
			RETURNF 1
	CASE P20本殿内部
		SIF GROUPMATCH(ARG:1, P6庫房, P7土間, P11庭, P16破舊小屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 3
	CASE P21守矢神社分社
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 3
	CASE P22参道
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P1鳥居, P36夢幻遺跡前,P68大广场)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P23温泉・脱衣所, P25鎮守之森)
			RETURNF 1
	CASE P23温泉・脱衣所
		SIF GROUPMATCH(ARG:1, P22参道, P24温泉, P25鎮守之森)
			RETURNF 1
	CASE P24温泉
		SIF GROUPMATCH(ARG:1, P23温泉・脱衣所)
			RETURNF 1
	CASE P25鎮守之森
		SIF GROUPMATCH(ARG:1, P22参道)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P23温泉・脱衣所, P27三妖精之家・大廳)
			RETURNF 1

	CASE P27三妖精之家・大廳
		SIF GROUPMATCH(ARG:1, P25鎮守之森, P30三妖精之家・風呂, P31三妖精之家・二階廊下, P35三妖精之家・地下室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P29三妖精之家・廚房)
			RETURNF 3
	CASE P29三妖精之家・廚房
		SIF GROUPMATCH(ARG:1, P27三妖精之家・大廳)
			RETURNF 3
	CASE P30三妖精之家・風呂
		SIF GROUPMATCH(ARG:1, P27三妖精之家・大廳)
			RETURNF 1
	CASE P31三妖精之家・二階廊下
		SIF GROUPMATCH(ARG:1, P27三妖精之家・大廳, P32桑尼私室, P33露娜私室, P34斯塔私室)
			RETURNF 1
	CASE P32桑尼私室, P33露娜私室, P34斯塔私室
		SIF GROUPMATCH(ARG:1, P31三妖精之家・二階廊下)
			RETURNF 1
	CASE P35三妖精之家・地下室
		SIF GROUPMATCH(ARG:1, P27三妖精之家・大廳)
			RETURNF 1

	CASE P36夢幻遺跡前
		SIF GROUPMATCH(ARG:1, P22参道)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路)
			RETURNF 1
	CASE P37夢幻遺跡・一階通路
		SIF GROUPMATCH(ARG:1, P36夢幻遺跡前, P38夢幻遺跡・洗面所, P41夢幻遺跡・食堂兼談話室, P43夢美私室, P44千百合私室, P46夢幻遺跡・二階通路)
			RETURNF 1
	CASE P38夢幻遺跡・洗面所
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路, P39夢幻遺跡・浴室, P40夢幻遺跡・廁所)
			RETURNF 1
	CASE P39夢幻遺跡・浴室, P40夢幻遺跡・廁所
		SIF GROUPMATCH(ARG:1, P38夢幻遺跡・洗面所)
			RETURNF 1
	CASE P41夢幻遺跡・食堂兼談話室
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P42夢幻遺跡・調理室)
			RETURNF 3
	CASE P42夢幻遺跡・調理室
		SIF GROUPMATCH(ARG:1, P41夢幻遺跡・食堂兼談話室)
			RETURNF 3
	CASE P43夢美私室, P44千百合私室
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路)
			RETURNF 1
	CASE P46夢幻遺跡・二階通路
		SIF GROUPMATCH(ARG:1, P37夢幻遺跡・一階通路, P47夢幻遺跡・艦橋, P48夢幻遺跡・第一資料室, P49夢幻遺跡・研究室, P50夢幻遺跡・第二資料室, P51夢幻遺跡・倉庫)
			RETURNF 1
	CASE P47夢幻遺跡・艦橋
		SIF GROUPMATCH(ARG:1, P46夢幻遺跡・二階通路)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P36夢幻遺跡前)
			RETURNF 2
	CASE P48夢幻遺跡・第一資料室, P49夢幻遺跡・研究室, P50夢幻遺跡・第二資料室, P51夢幻遺跡・倉庫
		SIF GROUPMATCH(ARG:1, P46夢幻遺跡・二階通路)
			RETURNF 1

	CASE P52別屋
		SIF GROUPMATCH(ARG:1, P11庭)
			RETURNF 1
	CASE P53倉庫, P55和室
		SIF GROUPMATCH(ARG:1, P9居間)
			RETURNF 1
	CASE P54小人的虫籠
		SIF GROUPMATCH(ARG:1, P9居間, P10走廊)
			RETURNF 1
	CASE P56阿吽の棲家
		SIF GROUPMATCH(ARG:1, P2境内)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P22参道
			RETURNF 1
;顺序:从Case 去 ARG 	
	;广场
	CASE P68大广场
		SIF GROUPMATCH(ARG:1,P57大温泉,P60大客厅,P67宴会厅)
			RETURNF 1
		SIF GROUPMATCH(ARG:1,P22参道)
			RETURNF 3
	;大温泉
	CASE P57大温泉
		SIF GROUPMATCH(ARG:1,P68大广场)
			RETURNF 1
	;主卧
	CASE P58大主卧
		SIF GROUPMATCH(ARG:1,P57大温泉,P68大广场)
			RETURNF 2
		SIF GROUPMATCH(ARG:1,P59大厨房,P60大客厅)
			RETURNF 3
	;大厨房
	CASE P59大厨房
		SIF GROUPMATCH(ARG:1,P58大主卧)
			RETURNF 1
		SIF GROUPMATCH(ARG:1,P60大客厅)
			RETURNF 3
	;大客厅
	CASE P60大客厅
		SIF GROUPMATCH(ARG:1,P58大主卧,P59大厨房,P61地の間,P62水の間,P63火の間,P64风の間,P65光の間,P66暗の間)
			RETURNF 1
		SIF GROUPMATCH(ARG:1,P68大广场)
			RETURNF 3
	;六系炮房
	CASE P61地の間,P62水の間,P63火の間,P64风の間,P65光の間,P66暗の間
		SIF GROUPMATCH(ARG:1,P60大客厅)
			RETURNF 1
	;宴会厅
	CASE P67宴会厅
		SIF GROUPMATCH(ARG:1,P68大广场)
			RETURNF 3
	CASE P69先代巫女的小屋
		SIF	GROUPMATCH(ARG:1,P52別屋,P10走廊)
			RETURNF 2
		SIF	GROUPMATCH(ARG:1,P11庭)
			RETURNF 3
ENDSELECT
RETURNF 0