;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_3(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P301正門
		SIF GROUPMATCH(ARG:1, P303守衛小屋, P304空的小屋, P343湖畔)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P302庭
		SIF GROUPMATCH(ARG:1, P303守衛小屋, P306後院, P309大廳)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P301正門, P307東陽台, P308西陽台)
			RETURNF 3
	CASE P303守衛小屋, P304空的小屋
		SIF GROUPMATCH(ARG:1, P301正門, P302庭, P306後院)
			RETURNF 1
	CASE P305仓库
		SIF GROUPMATCH(ARG:1, P306後院)
			RETURNF 1
	CASE P306後院
		SIF GROUPMATCH(ARG:1, P302庭, P303守衛小屋, P304空的小屋, P305仓库, P313厨房)
			RETURNF 1
	CASE P307東陽台
		SIF GROUPMATCH(ARG:1, P310図書館)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P308西陽台
		SIF GROUPMATCH(ARG:1, P309大廳)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P309大廳
		SIF GROUPMATCH(ARG:1, P302庭, P310図書館, P314食堂, P315會客廳, P316一層廁所, P319二階廊下, P328地下走廊)
			RETURNF 1
	CASE P310図書館
		SIF GROUPMATCH(ARG:1, P307東陽台, P309大廳, P311帕秋莉私室, P312小悪魔私室)
			RETURNF 1
	CASE P311帕秋莉私室, P312小悪魔私室
		SIF GROUPMATCH(ARG:1, P310図書館)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P306後院)
			RETURNF 2
	CASE P313厨房
		SIF GROUPMATCH(ARG:1, P306後院, P314食堂)
			RETURNF 1
	CASE P314食堂
		SIF GROUPMATCH(ARG:1, P309大廳, P313厨房)
			RETURNF 1
	CASE P315會客廳
		SIF GROUPMATCH(ARG:1, P308西陽台, P309大廳)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 2
	CASE P316一層廁所
		SIF GROUPMATCH(ARG:1, P309大廳)
			RETURNF 1
	CASE P319二階廊下
		SIF GROUPMATCH(ARG:1, P309大廳, P320咲夜私室, P321空的部屋, P322二層廁所, P323妖精女僕的值班室, P326蕾米莉亜私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P324大露台)
			RETURNF 3
	CASE P320咲夜私室, P321空的部屋
		SIF GROUPMATCH(ARG:1, P319二階廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P306後院)
			RETURNF 2
	CASE P322二層廁所, P323妖精女僕的值班室
		SIF GROUPMATCH(ARG:1, P319二階廊下)
			RETURNF 1
	CASE P324大露台
		SIF GROUPMATCH(ARG:1, P319二階廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭, P301正門)
			RETURNF 2
	CASE P326蕾米莉亜私室
		SIF GROUPMATCH(ARG:1, P319二階廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P327小露台)
			RETURNF 3
	CASE P327小露台
		SIF GROUPMATCH(ARG:1, P326蕾米莉亜私室)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P302庭, P301正門, P324大露台)
			RETURNF 2
	CASE P328地下走廊
		SIF GROUPMATCH(ARG:1, P309大廳, P329倉庫, P330使用人部屋, P331地下室, P332脱衣所)
			RETURNF 1
	CASE P329倉庫, P330使用人部屋, P331地下室
		SIF GROUPMATCH(ARG:1, P328地下走廊)
			RETURNF 1
	CASE P332脱衣所
		SIF GROUPMATCH(ARG:1, P328地下走廊, P333大浴場)
			RETURNF 1
	CASE P333大浴場
		SIF GROUPMATCH(ARG:1, P332脱衣所)
			RETURNF 1

	CASE P334廃洋館・入口
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P335廃洋館・大廳)
			RETURNF 3
	CASE P335廃洋館・大廳
		SIF GROUPMATCH(ARG:1, P338廃洋館・風呂, P339廃洋館・二階廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P334廃洋館・入口, P337廃洋館・廚房)
			RETURNF 3
	CASE P337廃洋館・廚房
		SIF GROUPMATCH(ARG:1, P335廃洋館・大廳)
			RETURNF 3
	CASE P338廃洋館・風呂
		SIF GROUPMATCH(ARG:1, P335廃洋館・大廳)
			RETURNF 1
	CASE P339廃洋館・二階廊下
		SIF GROUPMATCH(ARG:1, P335廃洋館・大廳, P340露娜薩私室, P341梅露蘭私室, P342莉莉喀私室)
			RETURNF 1
	CASE P340露娜薩私室, P341梅露蘭私室, P342莉莉喀私室
		SIF GROUPMATCH(ARG:1, P339廃洋館・二階廊下)
			RETURNF 1

	CASE P343湖畔
		SIF GROUPMATCH(ARG:1, P347霧之湖)
			RETURNF 3
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P301正門, P334廃洋館・入口, P344蕾蒂的冰屋, P345琪露諾的冰屋, P346大妖精の棲家)
			RETURNF 1
	CASE P344蕾蒂的冰屋, P345琪露諾的冰屋, P346大妖精の棲家
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 1
	CASE P347霧之湖
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P348霧之湖・湖底)
			RETURNF 1
	CASE P348霧之湖・湖底
		SIF GROUPMATCH(ARG:1, P347霧之湖)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P343湖畔
			RETURNF 1
ENDSELECT
RETURNF 0