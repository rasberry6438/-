;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_7(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P701樹海道入口
	SIF GROUPMATCH(ARG:1, P734小屋)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
	SIF GROUPMATCH(ARG:1, P702山道・通往沼澤之道)
		RETURNF 3
CASE P702山道・通往沼澤之道
	SIF GROUPMATCH(ARG:1, P715秋姉妹的家・畑, P716秋姉妹的家・土間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P701樹海道入口, P703玄武之澤)
		RETURNF 3
CASE P703玄武之澤
	SIF GROUPMATCH(ARG:1, P704河童の宣傳站, P709莉莉們的家, P735游泳場, P738森の深奥)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P702山道・通往沼澤之道, P705山道・中腹)
		RETURNF 3
CASE P704河童の宣傳站, P709莉莉們的家, P735游泳場
	SIF GROUPMATCH(ARG:1, P703玄武之澤)
		RETURNF 1
CASE P705山道・中腹
	SIF GROUPMATCH(ARG:1, P703玄武之澤, P707山道・分岐点)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P733正邪的隠匿処)
		RETURNF 1
CASE P707山道・分岐点
	SIF GROUPMATCH(ARG:1, P706厄神的家, P708大蛤蟆之池, P710猫之里, P711休憩所, P731帚木, P736山姥之家, P741美天の棲み処)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P705山道・中腹)
		RETURNF 3
CASE P711休憩所
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P712間欠泉地下中樞, P732通向山頂的路, P736山姥之家, P741美天の棲み処)
		RETURNF 1
CASE P732通向山頂的路
	SIF GROUPMATCH(ARG:1, P711休憩所, P712間欠泉地下中樞)
		RETURNF 1
CASE P734小屋
	SIF GROUPMATCH(ARG:1, P701樹海道入口)
		RETURNF 1
CASE P733正邪的隠匿処
	SIF GROUPMATCH(ARG:1, P705山道・中腹)
		RETURNF 1
CASE P708大蛤蟆之池
	SIF GROUPMATCH(ARG:1, P707山道・分岐点)
		RETURNF 1
CASE P706厄神的家
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P710猫之里)
		RETURNF 1
CASE P710猫之里
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P706厄神的家, P737招财猫的家)
		RETURNF 1
CASE P737招财猫的家
	SIF GROUPMATCH(ARG:1, P710猫之里)
		RETURNF 1
CASE P712間欠泉地下中樞
	SIF GROUPMATCH(ARG:1, P711休憩所, P732通向山頂的路)
		RETURNF 1
CASE P736山姥之家, P741美天の棲み処
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P711休憩所)
		RETURNF 1
CASE P716秋姉妹的家・土間
	SIF GROUPMATCH(ARG:1, P702山道・通往沼澤之道, P715秋姉妹的家・畑)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P718秋姉妹的家・居間)
		RETURNF 3
CASE P718秋姉妹的家・居間
	SIF GROUPMATCH(ARG:1, P717秋姉妹的家・寝室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P716秋姉妹的家・土間, P719秋姉妹的家・座敷)
		RETURNF 3
CASE P717秋姉妹的家・寝室
	SIF GROUPMATCH(ARG:1, P718秋姉妹的家・居間, P719秋姉妹的家・座敷)
		RETURNF 1
CASE P719秋姉妹的家・座敷
	SIF GROUPMATCH(ARG:1, P717秋姉妹的家・寝室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P715秋姉妹的家・畑, P718秋姉妹的家・居間)
		RETURNF 3
CASE P715秋姉妹的家・畑
	SIF GROUPMATCH(ARG:1, P702山道・通往沼澤之道, P716秋姉妹的家・土間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P719秋姉妹的家・座敷)
		RETURNF 3
;仙人的宅邸華扇ちゃん！
CASE P731帚木
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P730仙人的宅邸・庭)
		RETURNF 1
CASE P730仙人的宅邸・庭
	SIF GROUPMATCH(ARG:1, P731帚木)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P729仙人的宅邸・玄関)
		RETURNF 3
CASE P729仙人的宅邸・玄関
	SIF GROUPMATCH(ARG:1, P720仙人的宅邸・廚房, P721仙人的宅邸・風呂, P722仙人的宅邸・道場, P723仙人的宅邸・二階客間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P730仙人的宅邸・庭)
		RETURNF 3
CASE P721仙人的宅邸・風呂, P722仙人的宅邸・道場, P720仙人的宅邸・廚房
	SIF GROUPMATCH(ARG:1, P729仙人的宅邸・玄関)
		RETURNF 1
CASE P723仙人的宅邸・二階客間
	SIF GROUPMATCH(ARG:1, P729仙人的宅邸・玄関, P726仙人的宅邸・居間, P724仙人的宅邸・書斎, P725仙人的宅邸・大鷲的房間)
		RETURNF 1
CASE P724仙人的宅邸・書斎
	SIF GROUPMATCH(ARG:1, P723仙人的宅邸・二階客間)
		RETURNF 1
CASE P725仙人的宅邸・大鷲的房間
	SIF GROUPMATCH(ARG:1, P723仙人的宅邸・二階客間)
		RETURNF 1
	;例外的に大鷲的房間から庭にジャンプできる
	SIF GROUPMATCH(ARG:1, P730仙人的宅邸・庭)
		RETURNF 3
CASE P726仙人的宅邸・居間
	SIF GROUPMATCH(ARG:1, P723仙人的宅邸・二階客間, P727仙人的宅邸・華扇私室, P728仙人的宅邸・空的部屋)
		RETURNF 1
CASE P727仙人的宅邸・華扇私室, P728仙人的宅邸・空的部屋
	SIF GROUPMATCH(ARG:1, P726仙人的宅邸・居間)
		RETURNF 1
;深奥エリア
CASE P738森の深奥
	SIF GROUPMATCH(ARG:1, P703玄武之澤, P740山童の宣傳站)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P739秘天崖)
		RETURNF 3
CASE P739秘天崖
	SIF GROUPMATCH(ARG:1, P738森の深奥)
		RETURNF 3
CASE P740山童の宣傳站
	SIF GROUPMATCH(ARG:1, P738森の深奥)
		RETURNF 1

CASE MAXROOM
	SIF ARG:1 == P701樹海道入口
		RETURNF 1
ENDSELECT

RETURNF 0
