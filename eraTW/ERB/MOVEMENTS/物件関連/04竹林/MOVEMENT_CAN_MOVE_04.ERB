;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_4(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P401永遠亭入口
		SIF GROUPMATCH(ARG:1, P403待合室, P405配药室, P433斜角的竹林, P435井戸)
			RETURNF 1
	CASE P403待合室
		SIF GROUPMATCH(ARG:1, P401永遠亭入口, P404診察室, P406来客用廁所, P407廊下)
			RETURNF 1
	CASE P404診察室
		SIF GROUPMATCH(ARG:1, P403待合室, P405配药室)
			RETURNF 1
	CASE P405配药室
		SIF GROUPMATCH(ARG:1, P401永遠亭入口, P404診察室, P417廚房, P422後院)
			RETURNF 1
	CASE P406来客用廁所
		SIF GROUPMATCH(ARG:1, P403待合室)
			RETURNF 1
	CASE P407廊下
		SIF GROUPMATCH(ARG:1, P403待合室, P408中廊下, P409走廊, P418厠, P419脱衣所, P426二階廊下, P430地下室)
			RETURNF 1
	CASE P408中廊下
		SIF GROUPMATCH(ARG:1, P407廊下, P410帝私室, P411鈴仙私室, P412空的部屋, P413永琳私室, P414輝夜私室, P415大廳, P416居間)
			RETURNF 1
	CASE P409走廊
		SIF GROUPMATCH(ARG:1, P407廊下, P412空的部屋, P413永琳私室, P414輝夜私室, P416居間, P424庫房)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P421庭)
			RETURNF 3
	CASE P410帝私室
		SIF GROUPMATCH(ARG:1, P408中廊下, P411鈴仙私室)
			RETURNF 1
	CASE P411鈴仙私室
		SIF GROUPMATCH(ARG:1, P408中廊下, P410帝私室)
			RETURNF 1
	CASE P412空的部屋
		SIF GROUPMATCH(ARG:1, P408中廊下, P409走廊, P413永琳私室)
			RETURNF 1
	CASE P413永琳私室
		SIF GROUPMATCH(ARG:1, P408中廊下, P409走廊, P412空的部屋, P414輝夜私室)
			RETURNF 1
	CASE P414輝夜私室
		SIF GROUPMATCH(ARG:1, P408中廊下, P409走廊, P413永琳私室)
			RETURNF 1
	CASE P415大廳
		SIF GROUPMATCH(ARG:1, P408中廊下, P416居間, P417廚房)
			RETURNF 1
	CASE P416居間
		SIF GROUPMATCH(ARG:1, P408中廊下, P409走廊, P415大廳, P417廚房)
			RETURNF 1
	CASE P417廚房
		SIF GROUPMATCH(ARG:1, P405配药室, P415大廳, P416居間, P422後院)
			RETURNF 1
	CASE P418厠
		SIF GROUPMATCH(ARG:1, P407廊下)
			RETURNF 1
	CASE P419脱衣所
		SIF GROUPMATCH(ARG:1, P407廊下, P420風呂)
			RETURNF 1
	CASE P420風呂
		SIF GROUPMATCH(ARG:1, P419脱衣所)
			RETURNF 1
	CASE P421庭
		SIF GROUPMATCH(ARG:1, P409走廊, P422後院)
			RETURNF 1
	CASE P422後院
		SIF GROUPMATCH(ARG:1, P405配药室, P417廚房, P421庭)
			RETURNF 1
	CASE P424庫房
		SIF GROUPMATCH(ARG:1, P409走廊)
			RETURNF 1
	CASE P426二階廊下
		SIF GROUPMATCH(ARG:1, P407廊下, P427倉庫, P428二階空的部屋)
			RETURNF 1
	CASE P427倉庫
		SIF GROUPMATCH(ARG:1, P426二階廊下)
			RETURNF 1
	CASE P428二階空的部屋
		SIF GROUPMATCH(ARG:1, P426二階廊下)
			RETURNF 1
	CASE P430地下室
		SIF GROUPMATCH(ARG:1, P407廊下)
			RETURNF 1

	CASE P431兎的洞穴
		SIF GROUPMATCH(ARG:1, P433斜角的竹林)
			RETURNF 1
	CASE P432影狼的家
		SIF GROUPMATCH(ARG:1, P433斜角的竹林)
			RETURNF 1
	CASE P433斜角的竹林
		SIF GROUPMATCH(ARG:1, P401永遠亭入口, P431兎的洞穴, P432影狼的家, P434迷失的小道, P442空的小屋, P443米斯蒂亜的家, P444夜雀的小攤)
			RETURNF 1
	CASE P434迷失的小道
		SIF GROUPMATCH(ARG:1, P433斜角的竹林, P435井戸, P440廃屋, P441温泉, P445妹紅的家, P446祠, P447竹林入口)
			RETURNF 1
	CASE P435井戸
		SIF GROUPMATCH(ARG:1, P401永遠亭入口, P434迷失的小道, P436無名之丘)
			RETURNF 1
	CASE P436無名之丘
		SIF GROUPMATCH(ARG:1, P438太陽花田)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P435井戸, P437梅蒂欣的家)
			RETURNF 1
	CASE P437梅蒂欣的家
		SIF GROUPMATCH(ARG:1, P436無名之丘)
			RETURNF 1
	CASE P438太陽花田
		SIF GROUPMATCH(ARG:1, P439拉瓦爾的家, P450玄関)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P436無名之丘, P466庭)
			RETURNF 3
	CASE P439拉瓦爾的家
		SIF GROUPMATCH(ARG:1, P438太陽花田)
			RETURNF 1
	CASE P440廃屋
		SIF GROUPMATCH(ARG:1, P434迷失的小道, P447竹林入口)
			RETURNF 1
	CASE P441温泉
		SIF GROUPMATCH(ARG:1, P434迷失的小道)
			RETURNF 1
	CASE P442空的小屋
		SIF GROUPMATCH(ARG:1, P433斜角的竹林, P444夜雀的小攤)
			RETURNF 1
	CASE P443米斯蒂亜的家
		SIF GROUPMATCH(ARG:1, P433斜角的竹林, P444夜雀的小攤)
			RETURNF 1
	CASE P444夜雀的小攤
		SIF GROUPMATCH(ARG:1, P433斜角的竹林, P442空的小屋, P443米斯蒂亜的家, P446祠, P447竹林入口)
			RETURNF 1
	CASE P445妹紅的家
		SIF GROUPMATCH(ARG:1, P434迷失的小道, P447竹林入口)
			RETURNF 1
	CASE P446祠
		SIF GROUPMATCH(ARG:1, P434迷失的小道, P444夜雀的小攤)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P447竹林入口)
			RETURNF 3
	CASE P447竹林入口
		SIF GROUPMATCH(ARG:1, P434迷失的小道, P440廃屋, P444夜雀的小攤, P445妹紅的家)
			RETURNF 1
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P446祠)
			RETURNF 3

	CASE P450玄関
		SIF GROUPMATCH(ARG:1, P438太陽花田, P451胡桃私室, P452艾麗私室, P465大堂, P466庭)
			RETURNF 1
	CASE P451胡桃私室
		SIF GROUPMATCH(ARG:1, P450玄関)
			RETURNF 1
	CASE P452艾麗私室
		SIF GROUPMATCH(ARG:1, P450玄関)
			RETURNF 1
	CASE P453用餐房間
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P454浴室
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P455廁所
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P456倉庫
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P457夢月私室
		SIF GROUPMATCH(ARG:1, P458廊下)
			RETURNF 1
	CASE P458廊下
		SIF GROUPMATCH(ARG:1, P457夢月私室, P459幻月私室, P461生活間, P463幽香私室, P465大堂)
			RETURNF 1
	CASE P459幻月私室
		SIF GROUPMATCH(ARG:1, P458廊下)
			RETURNF 1
	CASE P460空的部屋
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P461生活間
		SIF GROUPMATCH(ARG:1, P458廊下, P462陽台, P465大堂)
			RETURNF 1
	CASE P462陽台
		SIF GROUPMATCH(ARG:1, P461生活間, P466庭)
			RETURNF 3
	CASE P463幽香私室
		SIF GROUPMATCH(ARG:1, P458廊下)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P464露台)
			RETURNF 3
	CASE P464露台
		SIF GROUPMATCH(ARG:1, P463幽香私室)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P466庭)
			RETURNF 2
	CASE P465大堂
		SIF GROUPMATCH(ARG:1, P450玄関, P453用餐房間, P454浴室, P455廁所, P456倉庫, P458廊下, P460空的部屋, P461生活間)
			RETURNF 1
	CASE P466庭
		SIF GROUPMATCH(ARG:1, P450玄関)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P438太陽花田, P462陽台)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P464露台)
			RETURNF 2

	CASE MAXROOM
		SIF ARG:1 == P447竹林入口
			RETURNF 1
ENDSELECT
RETURNF 0
