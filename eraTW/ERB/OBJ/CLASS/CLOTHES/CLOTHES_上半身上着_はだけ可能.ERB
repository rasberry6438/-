#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "名前"
	CALLF MAKE_STR(V_NAME, "女式衬衫")
CASE "装備部位"
	CALLF MAKE_STR(V_NAME, "「上身衣服２」")
