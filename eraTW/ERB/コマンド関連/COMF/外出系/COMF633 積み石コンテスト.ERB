				PRINTFORML 「要继续传承積石排名吗？（※如果选择不传承，積石排名将从0重新开始）」
					CALL ASK_YN("传承称号","不传承称号")
					IF !RESULT
						
					ELSE
						PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%失去了積石排名的称号...
						TALENT:MASTER:積み石ランカー = 0
					ENDIF
				PRINTFORML
				PRINTFORML 「那么，请重新介绍一下…」
			ENDIF
		ENDIF
	ELSE
		IF CHK_DATENOW(CFLAG:MASTER:约会中) && TARGET == FLAG:约会的对象 && !TARGET == MASTER
			PRINTFORML 「你好！初次见面！这里是積石竞赛的场地！」
			PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%和%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%在约会时来到了赛之河原举办的神秘比赛场地
			PRINTFORML
			PRINTFORML 「顾名思义，这是一个评判積石技巧的比赛！」
			PRINTFORMW 「无需预约，随时都可以参加，随意来吧！」
			PRINTFORMW 「作为一对情侣，我推荐你们参加情侣组别的比赛！」
			PRINTFORMW 「请查看帮助以获取更多信息！」
			PRINTFORML
			PRINTFORML 「那么，请重新介绍一下......」
		ELSE
			PRINTFORML 「你好！初次见面！这里是積石竞赛的场地！」
			PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%无意中来到了赛之河原举办的神秘比赛场地
			PRINTFORML 「顾名思义，这是一个评判積石技巧的比赛！」
			PRINTFORMW 「无需预约，随时都可以参加，随意来吧！」
			PRINTFORMW 「如果你是新手，我建议你先挑战積石级别，然后逐步提升！」
			PRINTFORMW 「请查看帮助以获取更多信息！」
			PRINTFORML
			PRINTFORML 「那么，请重新介绍一下......」
		ENDIF
	ENDIF
	SELECTCASE TALENT:MASTER:積み石ランカー
		CASE 0
		CASE 1
			STONE_PLAYCOUNT:1 += 7
		CASE 2
			STONE_PLAYCOUNT:1 += 15
		CASE 3
			STONE_PLAYCOUNT:1 += 24
		CASE 4
			STONE_PLAYCOUNT:1 += 34
		CASE 5
			STONE_PLAYCOUNT:1 += 50
		CASEELSE
	ENDSELECT
ENDIF

;受付水子妊娠後
IF STONE_PLAYCOUNT:2 == 16 && (DAY:0 >= STONE_UKETSUKEDAY + 14)
	PRINTFORML 「啊...好久不见了」
	PRINTFORMW 「顺利地生下了」
	PRINTFORML
	PRINTFORMW 「呵呵呵...嘿嘿嘿...」
	PRINTFORMW 「哎呀！还要继续工作呢！...忘记的东西...请来取吧」
	STONE_PLAYCOUNT:2 = 17
ELSEIF STONE_PLAYCOUNT:2 == 16 && !(DAY:0 >= STONE_UKETSUKEDAY + 14)
	PRINTFORMW 『目前積石竞赛休息中，{STONE_UKETSUKEDAY + 14 - DAY:0}天后重新开放』
	RETURN 0
ENDIF

;積み石王
IF STONE_COMBI_TROPHY:31 == 1 && TALENT:MASTER:積み石ランカー == 6
	PRINTFORMW 「哇！太厉害了！！！」
	PRINTFORMW 「在積石竞赛中击败了认真的瓔花小姐，情侣部门全面制霸！？」
	PRINTFORMW 「这已经超越了積石大师的境地，应该称之为積石王了！」
	PRINTFORMW 「%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%先生以后可以免费参加任意比赛！」
	PRINTFORMW 「虽然可能没有什么可做的了...但是如果你能再来玩就太高兴了！」
	TALENT:MASTER:積み石ランカー = 7
ENDIF

;今日はじめて来たときの判定
IF STONE_ENTER < DAY
	STONE_ENTER = DAY
	SIF STONE_PLAYCOUNT:2 > 0
		PRINTFORML 「啊，%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%先生！」
	STONE_PLAYCOUNT:1 += 7
	;受付水子と友達になるとデイリー好感度が増加
	SIF STONE_PLAYCOUNT:2 > 5
		STONE_PLAYCOUNT:1 += 3
	STONE_PLAYCOUNT:0 += 1
ENDIF


IF CHK_DATENOW(CFLAG:MASTER:约会中) && TARGET == FLAG:约会的对象 && !TARGET == MASTER
	CALL STONECouple
;拠点が三途之川～冥界の場合連れ出しているだけでもOK(ただし好感度1000が最低条件)
ELSEIF MAIN_MAP == 6 && CFLAG:TARGET:同行 && CFLAG:MASTER:同行 
	IF CFLAG:TARGET:好感度 < 1000
		PRINTFORMW 「情侣部门有点...被%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%拒绝了...」
		PRINTFORMW 「没办法...我们去参加个人部门吧...」
	ELSE
		CALL STONECouple
	ENDIF
ELSE
	CALL MIZUKO_FACE, "笑顔", "服", "通常"
	IF TALENT:MASTER:積み石ランカー == 6
		PRINTFORML 「欢迎来到積石竞赛！你要挑战哪个级别？作为積石王，你可以免费参加哦」
		ELSE
		PRINTFORML 「欢迎来到積石竞赛！你要挑战哪个级别？参赛费是统一的%金額表示(3000)%，你可以参加比赛哦」
		ENDIF
	$LOOP1
	DRAWLINE
	{
	CALL ASK_M("EASY-拼插級-(易于堆叠的難易度)",1,
	"NORMAL-積石級-(一不小心就会倒塌的難易度)", TALENT:MASTER:積み石ランカー >= 1,
	"HARD-纸牌塔級-(積起第一块也会緊張的難易度)",TALENT:MASTER:積み石ランカー >= 2,
	"LUNATIC-金字塔級-(不扔起石头就根本够不到)", TALENT:MASTER:積み石ランカー >= 3,
	"PHANTASM-巴别塔級-(扔起石头也够不到)", TALENT:MASTER:積み石ランカー >= 4,
	"今日のテーマは？",1, "戦績",1, "HELP",1, "放棄",1)
	}
	IF RESULT == 0
		PRINTFORMW 「挑战拼插級啊！只要连续胜出两次就能夺冠了！」
		CALL Stone_Entry(0, 2, 5000)
	ELSEIF RESULT == 1
		PRINTFORMW 「挑战積石級啊！已经熟悉了堆砌積石了吗？只要连续胜出三次就能夺冠了！」
		CALL Stone_Entry(1, 3, 7000)
	ELSEIF RESULT == 2
		PRINTFORMW 「挑战纸牌塔級啊！大家都在关注着你呢！只要连续胜出四次就能夺冠了！」
		CALL Stone_Entry(2, 4, 10000)
	ELSEIF RESULT == 3
		PRINTFORMW 「挑战金字塔级别啊！你终于来到这一步了！只要连续胜出五次就能夺冠了！」
		CALL Stone_Entry_EX(3, 5, 15000)
	ELSEIF RESULT == 4
		PRINTFORMW 「竟然要挑战巴别塔级别！？挑战被称为幻影的这个级别...请加油！只要连续胜出五次就能夺冠了！」
		CALL Stone_Entry_EX(4, 5, 30000)
	ELSEIF RESULT == 5
		CALL StoneTheme_Intro
		GOTO LOOP1
	ELSEIF RESULT == 6
		CALL StoneBattle_record
		GOTO LOOP1
	ELSEIF RESULT == 7
		CALL Stone_Help
		GOTO LOOP1
	ELSEIF RESULT == 8
		RETURN -1
	ENDIF
	RETURN 1
ENDIF



