;積石竞赛
@COM_ABLE633
;一括管理
SIF GLOBAL_COMABLE(633)
	RETURN RESULT
SIF !INRANGE(TIME, 480, 1080)
	RETURN 0

IF !AT_HOME(MASTER)
	SIF CFLAG:MASTER:現在位置 != 三途之川
		RETURN 0
ELSE
	;冥界が拠点の場合は、賽の河原
	SIF CFLAG:MASTER:現在位置 != 602
		RETURN 0
ENDIF

;约会中は约会的对象と一緒じゃないとダメ
SIF CHK_DATENOW(CFLAG:12) && TARGET != FLAG:约会的对象
	RETURN 0

;睡眠中
SIF CFLAG:睡眠
	RETURN 0
;時間停止中
SIF FLAG:70
	RETURN 0
RETURN 1

;-------------------------------------------------
;積石竞赛実行
;-------------------------------------------------

@Stone_Entry(Rank, MaxBattle, Prize)
#DIM Rank
#DIM MaxBattle
#DIM Prize
#DIM WIN_P
#DIM difficulty
WIN_P = 0
difficulty = Rank
IF !TALENT:MASTER:積み石ランカー == 6 && MONEY < 3000
	PRINTFORMW 「那个……好像钱不够了呢……」
	STONE_PLAYCOUNT:1 -= 50
	IF STONE_PLAYCOUNT:2 >= 14
		PRINTFORMW 「唉，真是没办法的人呢 %UNICODE(0x2764)% 我来替你付款吧 %UNICODE(0x2764)%」
	ELSEIF STONE_PLAYCOUNT:2 >= 5
		PRINTFORMW 「......真是没办法的人呢。作为朋友，我来替你付款吧」
	ELSE
		PRINTFORMW 「唉......这次就特别让你一次吧，但只限这次哦？」
	ENDIF
ENDIF

SIF !TALENT:MASTER:積み石ランカー == 6 && MONEY < 3000
	MONEY -= 3000

	
;持ち込み道具
CALL STONE_MOCHIKOMI

FOR WIN_P, 0, MaxBattle
	IF WIN_P > 0
		STONE_PLAYCOUNT:1 += 1
		STONE_BATTLECOUNT:0 += 1
		STONE_BATTLECOUNT:1 += 1
		PRINTFORMW
		IF TALENT:MASTER:積み石ランカー == 6
			PRINTFORMW 「恭喜你在第{WIN_P}场战斗中取得胜利！你可以选择结束战斗...你打算怎么做？」
		ELSE
			PRINTFORMW 「恭喜你在第{WIN_P}场战斗中取得胜利！你可以选择结束战斗...你打算怎么做？参赛费将会返还...」
		ENDIF
		CALL ASK_YN("继续","结束")
		IF RESULT
			PRINTFORMW 「好的...我们期待你的再次参与！」
			SIF !TALENT:MASTER:積み石ランカー == 6 && MONEY < 3000
			MONEY += 3000
			CALL STONE_UKETSUKE
			TIME += 50
			RETURN -1
		ELSE
			PRINTFORMW 「那就这样吧！」
		ENDIF
	ENDIF
	PRINTFORMW 「那么...第{WIN_P + 1}回合的对手是...」
	CALL STONELEVEL(difficulty)
	IF RESULT == 1
		PRINTFORMW 「你输了，比赛到此结束......辛苦了...我们期待你的再次参与！」
		STONE_BATTLECOUNT:0 += 1
		CALL STONE_UKETSUKE
		TIME += 50
		RETURN 0
	ELSE
		difficulty ++
	ENDIF
NEXT

PRINTFORMW 「恭喜夺冠！请领取奖金%金額表示(Prize)%！」
STONE_PLAYCOUNT:1 += 3
STONE_BATTLECOUNT:0 += 1
STONE_BATTLECOUNT:1 += 1
IF TALENT:MASTER:積み石ランカー > Rank
	PRINTFORML 「你的水平应该能够胜任更高级别的比赛吧？」
ELSE
	IF TALENT:MASTER:積み石ランカー == 0
		PRINTFORML 「作为積み木级别的首次夺冠者...你真称得上是積み石新手！」
		PRINTFORML 「你获得了挑战積み石比赛積石級别的资格！」
		PRINTFORML 「期待你在这里开始你的積み石传奇！」
	ELSEIF TALENT:MASTER:積み石ランカー == 1
		PRINTFORML 「作为積み木级别的首次夺冠者...你真称得上是積み石新秀！」
		PRINTFORML 「你获得了挑战積み石比赛纸牌塔别的资格！」
		PRINTFORML 「你已经成为積み石界的新星，备受关注！」
	ELSEIF TALENT:MASTER:積み石ランカー == 2
		PRINTFORML 「作为積み木级别的首次夺冠者...你真称得上是積み石精英！」
		PRINTFORML 「你获得了挑战積み石比赛金字塔级别的资格！」
		PRINTFORML 「你的职业生涯正在稳步发展！就像積み石一样！」
	ENDIF
	TALENT:MASTER:積み石ランカー = Rank + 1
	STONE_PLAYCOUNT:1 += 7 + RANK
ENDIF
PRINTFORMW 「期待您再次的参与！」
IF !TALENT:MASTER:積み石ランカー == 6 && MONEY < 3000
	MONEY += (Prize - 3000)
ELSE
	MONEY += Prize
ENDIF

SIF STONE_PLAYCOUNT:2 > 2
	ITEM:滑石 += 1
CALL STONE_UKETSUKE
TIME += 60
RETURN 0

@Stone_Entry_EX(Rank, MaxBattle, Prize)
#DIM Rank
#DIM MaxBattle
#DIM Prize
#DIM WIN_P
#DIM difficulty
WIN_P = 0
IF !TALENT:MASTER:積み石ランカー == 6 && MONEY < 3000
	PRINTFORMW 「那个...好像钱不够了...」
	STONE_PLAYCOUNT:1 -= 50
	IF STONE_PLAYCOUNT:2 >= 14
		PRINTFORMW 「唉...真是没办法的人%UNICODE(0x2764)%我来替你付钱吧%UNICODE(0x2764)%」
	ELSEIF STONE_PLAYCOUNT:2 >= 5
		PRINTFORMW 「......真是没办法的人呢。作为朋友，我来替你付钱吧」
	ELSE
		PRINTFORMW 「唉...这次就特别让你过了，但只限这次哦？」
	ENDIF
ENDIF
SIF !TALENT:MASTER:積み石ランカー == 6 && MONEY < 3000
	MONEY -= 3000

;持ち込み道具
CALL STONE_MOCHIKOMI

IF Rank == 3
	difficulty = 5
ELSEIF Rank == 4
	difficulty = 6
ENDIF

FOR WIN_P, 0, MaxBattle
	IF WIN_P > 0
		STONE_PLAYCOUNT:1 += 1
		STONE_BATTLECOUNT:0 += 1
		STONE_BATTLECOUNT:1 += 1
		PRINTFORMW
		;決勝前
		IF WIN_P == MaxBattle - 1
			IF TALENT:MASTER:積み石ランカー == 6
				PRINTFORMW 「恭喜你在第四场获胜！下一个对手是爱子，但是%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%先生你一定没问题的！」
			ELSE
				PRINTFORMW 「恭喜你在第四场获胜！下一个对手是最强的，所以请紧绷神经！你可以选择在这里停下来，你要怎么做？」
			ENDIF
		ELSE
			IF TALENT:MASTER:積み石ランカー == 6
				PRINTFORMW 「恭喜你在第{WIN_P}场获胜！你可以选择在这里停下来，你要怎么做？」
			ELSE
				PRINTFORMW 「恭喜你在第{WIN_P}场获胜！你可以选择在这里停下来，你要怎么做？参赛费将会退还...」
			ENDIF
		ENDIF
		CALL ASK_YN("继续","停止")
		IF RESULT
			PRINTFORMW 「好的...我们期待你的再次参与！」
			SIF !TALENT:MASTER:積み石ランカー == 6 && MONEY < 3000
				MONEY += 3000
			CALL STONE_UKETSUKE
			TIME += 50
			RETURN -1
		ELSE
			PRINTFORMW 「那就这样吧！」
		ENDIF
	ENDIF
	IF WIN_P == MaxBattle - 1
		PRINTFORMW 「好了，最终战终于开始了...你准备好了吗？最终战的对手是...没错！」
		CALL STONELEVEL(Rank + 4)
	ELSE
