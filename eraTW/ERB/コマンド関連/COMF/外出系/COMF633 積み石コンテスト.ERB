		TALENT:MASTER:積み石ランカー = 5
		STONE_PLAYCOUNT:1 += 15
	ENDIF
	SIF STONE_PLAYCOUNT:2 == 13
		CALL STONE_UKETSUKE_H
ENDIF
STONE_PLAYCOUNT:1 += 1
STONE_BATTLECOUNT:0 += 1
STONE_BATTLECOUNT:1 += 1
PRINTFORMW 「请再次参加吧！期待与您再次相见！」
IF !TALENT:MASTER:積み石ランカー == 6 && MONEY < 3000
	MONEY += (Prize - 3000)
ELSE
	MONEY += Prize
ENDIF
SIF STONE_PLAYCOUNT:2 > 2
	ITEM:滑石 += 1
IF STONE_PLAYCOUNT:2 == 9 || STONE_PLAYCOUNT:2 == 11
	CALL STONE_UKETSUKE_H
ELSE
	CALL STONE_UKETSUKE
ENDIF
TIME += 60
RETURN 0


