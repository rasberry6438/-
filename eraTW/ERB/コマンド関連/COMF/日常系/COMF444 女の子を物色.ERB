		PRINTBUTTON "[町娘] ", "町娘"
		PRINTBUTTON "[人妻] ", "人妻"
		PRINTBUTTON "[妖精] ", "妖精"
		PRINTBUTTON "[女仆妖精] ", "女仆妖精"
		PRINTBUTTON "[僵尸妖精] ", "僵尸妖精"
		PRINTBUTTON "[河童] ", "河童"
		PRINTBUTTON "[白狼天狗] ", "白狼天狗"
		PRINTBUTTON "[因幡] ", "因幡"
		PRINTBUTTON "[玉兎] ", "玉兎"
		PRINTBUTTON "[鬼] ", "鬼"
		PRINTBUTTON "[妖怪狸] ", "妖怪狸"
		PRINTBUTTON "[妖怪狐] ", "妖怪狐"
		PRINTBUTTON "[妖怪猫] ", "妖怪猫"
		PRINTBUTTON "[随机] ", "随机"
		INPUTS
		CALL SET_MOB_IMAGE_STR(RANDOM_CHARANUM, RESULTS)
	ENDIF
	;過去に合成されて裸画像の笑顔に[装飾_照れ]が乗ってない場合に追記
	SIF STRFIND(CSTR:RANDOM_CHARANUM:路人子画像文字列_裸_笑顔, "装飾_照れ") == -1
		CSTR:RANDOM_CHARANUM:路人子画像文字列_裸_笑顔 += "[装飾9:装飾_照れ][乗色:朱色]"

	;過去（ver4.760以前）に保存されて種族のTALENT設定が適合しない場合に修正を行う
	SELECTCASE CAN_NANPA(CFLAG:MASTER:現在位置)
		;里
		CASE 1
			;以前の194 人类が現varでは194 幽霊になっている
			IF TALENT:RANDOM_CHARANUM:幽霊
				TALENT:RANDOM_CHARANUM:幽霊 = 0
				TALENT:RANDOM_CHARANUM:人类 = 1
			ENDIF
		;霧之湖、紅魔館、および地底の妖精たち
		CASE 2, 3, 5
			;以前の190 妖精が現verでは190 人类になっている
			IF TALENT:RANDOM_CHARANUM:人类
				TALENT:RANDOM_CHARANUM:人类 = 0
				TALENT:RANDOM_CHARANUM:妖精 = 1
			ENDIF
		;竹林
		CASE 4
			;因幡…妖獣が新設された
			SIF TALENT:RANDOM_CHARANUM:妖怪 != 6
				TALENT:RANDOM_CHARANUM:妖怪 = 6
		;冥界
		CASE 6
			;妖怪狸、狐、猫…妖獣が新設された
			SIF TALENT:RANDOM_CHARANUM:妖怪 != 6
				TALENT:RANDOM_CHARANUM:妖怪 = 6
		;妖怪之山山麓
		CASE 7
			;河童…河童が新設された
			SIF TALENT:RANDOM_CHARANUM:妖怪 != 4
				TALENT:RANDOM_CHARANUM:妖怪 = 4
		;妖怪之山山頂
		CASE 8
			;白狼天狗…以前の197 天狗が現verでは197 追加種族（巫女）になっている
			IF TALENT:RANDOM_CHARANUM:追加種族
				TALENT:RANDOM_CHARANUM:追加種族 = 0
				TALENT:RANDOM_CHARANUM:妖怪 = 5
			ENDIF
		;地底　鬼か妖精
		CASE 9
			;鬼…以前の195 鬼が現verでは195　付喪神になっている
			IF TALENT:RANDOM_CHARANUM:付喪神
				TALENT:RANDOM_CHARANUM:付喪神 = 0
				TALENT:RANDOM_CHARANUM:妖怪 = 2
			;妖精は同上
			ELSEIF TALENT:RANDOM_CHARANUM:人类
				TALENT:RANDOM_CHARANUM:人类 = 0
				TALENT:RANDOM_CHARANUM:妖精 = 1
			ENDIF
		;月　一応分けておく
		CASE 10
			;玉兎…妖獣が新設された
			SIF TALENT:RANDOM_CHARANUM:妖怪 != 6
				TALENT:RANDOM_CHARANUM:妖怪 = 6
	ENDSELECT

	IF FLAG:70	;時間停止中と風俗嬢は反応無
	ELSEIF TALENT:RANDOM_CHARANUM:風俗嬢
	ELSEIF CFLAG:RANDOM_CHARANUM:败犬 && (TALENT:RANDOM_CHARANUM:炮友 || TALENT:RANDOM_CHARANUM:思慕)
		PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%の姿を認めた%NAME_OUTPUT_TRANSLATION("CALLNAME", RANDOM_CHARANUM)%は悔しげな、それでいて期待するような表情でもじもじしている…
	ELSEIF CFLAG:RANDOM_CHARANUM:败犬 && !TALENT:RANDOM_CHARANUM:恋慕 && !TALENT:RANDOM_CHARANUM:思慕 && !TALENT:RANDOM_CHARANUM:愛欲
		PRINTFORMW 发现了%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%的%NAME_OUTPUT_TRANSLATION("CALLNAME", RANDOM_CHARANUM)%带着逞强的表情
	ELSEIF TALENT:RANDOM_CHARANUM:恋慕
		PRINTFORMW 认出了%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%的%NAME_OUTPUT_TRANSLATION("CALLNAME", RANDOM_CHARANUM)%很開心地走了过来
	ELSEIF TALENT:RANDOM_CHARANUM:愛欲
		PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%の姿を認めた%NAME_OUTPUT_TRANSLATION("CALLNAME", RANDOM_CHARANUM)%はこちらにやって来るなり、腕にしなだれかかってきた…
	ELSEIF ABL:RANDOM_CHARANUM:親密 >= 5
		PRINTFORMW 发现了%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%的%NAME_OUTPUT_TRANSLATION("CALLNAME", RANDOM_CHARANUM)%挥了挥手
	ENDIF
ELSE
	RETURN -1
ENDIF

