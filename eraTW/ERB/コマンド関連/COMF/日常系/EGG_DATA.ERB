@EGG_POSSIBLE_DATA(ARG)
;飼育可能生物,1～10まで鳥類,11～20までその他(奇妙な卵勢)を登録可能．枠を拡張するならGET_EGGDATA関数をいじる
;CritterDataより
;ID:20 ヘビ
;ID:23 デスワーム
;ID:24 ツチノコ
;ID:50 野鶏
;ID:51 ライチョウ
;ID:52 ウズラ
;ID:53 ヤマドリ
;ID:54 キジ

SELECTCASE ARG
;鳥類
	CASE 50
		Eggname = 野鶏
		Eggfavorite = 汎用/汎用/野菜/虫/
		RETURN 1
	CASE 51
		Eggname = 雷鸟
		Eggfavorite = 薬草/山の幸/果物/果物/
		RETURN 2
	CASE 52
		Eggname = 鹌鹑
		Eggfavorite = 野菜/野菜/果物/蘑菇/
		RETURN 3
	CASE 53
		Eggname = 山鸟
		Eggfavorite = 虫/虫/山の幸/魚/
		RETURN 4
	CASE 54
		Eggname = 山鸡
		Eggfavorite = 薬草/薬草/汎用/野菜/
		RETURN 5
;その他
	CASE 20
		Eggname = 蛇
		Eggfavorite = 肉/肉/魚/怪诞/
		RETURN 11
	CASE 23
		Eggname = 死亡蠕蟲
		Eggfavorite = 怪诞/怪诞/肉/虫/
		RETURN 12
	CASE 24
		Eggname = 槌之子
		Eggfavorite = 山の幸/山の幸/薬草/虫/
		RETURN 13
ENDSELECT
RETURN 0

