@EGG_FAVORITE_SEARCH(ARG,FAVORITE)
#DIMS FAVORITE
SELECTCASE ARG/100
	CASE 0
		LOCALS = 汎用/
	CASE 1
		LOCALS = 野菜/
	CASE 2
		LOCALS = 蘑菇/
	CASE 3
		LOCALS = 薬草/
	CASE 4
		LOCALS = 山の幸/
	CASE 5
		LOCALS = 果物/
	CASE 6
		LOCALS = 魚/
	CASE 7
		LOCALS = 肉/
	CASE 8
		LOCALS = 怪诞/
	CASE 9
		LOCALS = 虫/
ENDSELECT
LOCAL = STR_MULTI_COUNT(FAVORITE, LOCALS)
RETURN LOCAL