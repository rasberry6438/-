ENDIF
;覚
IF ARG == 38 || ARG == 49
	合計女子力 = 合計女子力 / 10 * 13
	;PRINTFORML 【覚】　　　　　　×１.３
ENDIF
;阿求
IF ARG == 80
	合計女子力 = 合計女子力 / 10 * 15
	;PRINTFORML 【阿礼乙女】　　　×１.５
ENDIF
IF TALENT:ARG:路人
	合計女子力 = 合計女子力 / 10 * 3
	;PRINTFORML 【汎用キャラ】　　　　×０.３
ENDIF
