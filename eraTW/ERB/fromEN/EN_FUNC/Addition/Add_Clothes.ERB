@EXIST_衣装セット93
#LOCALSIZE 1
#LOCALSSIZE 1

@衣装セット93(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "名前"
	CALLF MAKE_STR(V_NAME, "魔理沙寝間着セット")
CASE "キャラ特殊衣装セット"
	CALLF MAKE_INT(V_NAME, 1)
;こっから衣装設定系
CASE "上半身下着１"
	CALLF MAKE_INT(V_NAME, 13)
CASE "OVERRIDE_UNDERWEAR_TOP"
	CALLF MAKE_INT(V_NAME, 1)
CASE "靴下"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "ガーターストッキング"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴下", "靴下"))
	ENDIF
ENDSELECT

