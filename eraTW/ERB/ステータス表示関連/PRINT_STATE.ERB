	FOR LOCAL, 0, 6
		SIF !STRLENS(MARKNAME:LOCAL)
			CONTINUE
		PRINTFORM 　%MARKNAME:LOCAL,12%:
		IF LOCAL == 3
			SETCOLOR C_RED
			PRINTFORML %"×" * (MARK:選択中角色ID:LOCAL)%
		ELSE
