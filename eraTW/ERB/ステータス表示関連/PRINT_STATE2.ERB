@SHOW_CHILDDATA(C_ID, ARGS)
#DIM C_ID
#DIM DADDY

VARSET LOCAL
FOR LOCAL,1,MAXCHILD + 1
	SIF CHILDNAME:C_ID:LOCAL == ""
		CONTINUE
	IF ARGS == "ICON"
		IF CHILD_SEX:C_ID:LOCAL == 1
			SETCOLOR C_PINK
			PRINT ♀ 
		ELSE
			SETCOLOR C_BLUE
			PRINT ♂ 
		ENDIF
		RESETCOLOR
	ELSE
		DADDY = CHILD_DADDY:C_ID:LOCAL
		;C_ID == 0 && DADDY == 0
		;IF DADDY == C_ID
		;	CFLAG:C_ID:(880 + LOCAL) = CFLAG:C_ID:誰の子
		;	DADDY = CFLAG:C_ID:(880 + LOCAL)
		;ENDIF
		PRINTFORM 母：%NAME_OUTPUT_TRANSLATION("CALLNAME", C_ID)%　　父：%NAME_OUTPUT_TRANSLATION("CALLNAME", DADDY)%
		PRINTFORML
		PRINTFORM 第{LOCAL}子：%CHILDNAME:C_ID:LOCAL, 15, LEFT%-%NAME_FROM_PLACE(CHILD_AREA:C_ID:LOCAL)%-爱情度：{CHILD_LOVE:C_ID:LOCAL}　\@ CHILD_SEX:C_ID:LOCAL == 1 ? ♀ # ♂ \@ 
		CALL PRINT_DATE(CHILD_BIRTHDAY:C_ID:LOCAL)
		PRINTFORM ({CHILD_BIRTHDAY:C_ID:LOCAL}日目) 
		SELECTCASE CHILD_WEATHER:C_ID:LOCAL
		CASE "Ｃ敏感"
			PRINTFORM 大家的陰蒂都很敏感的
		CASE "Ｖ敏感"
			PRINTFORM 大家的小穴都很敏感的
		CASE "Ａ敏感"
			PRINTFORM 大家的都很肛門敏感的
		CASE "Ｂ敏感"
			PRINTFORM 大家的都很胸部敏感的
		CASE "Ｍ敏感"
			PRINTFORM 大家的都很嘴唇敏感的
		CASE "裙子"
			PRINTFORM 変得喜歡真空出門的
		CASE "胖次"
			PRINTFORM 胖次滿天飛的
		CASEELSE
			PRINTFORM %CHILD_WEATHER:C_ID:LOCAL%の
		ENDSELECT
		PRINTFORM 日子出生了　
		IF DAY - CHILD_BIRTHDAY:C_ID:LOCAL < CHILD_自立 && CFLAG:C_ID:出産経過日 > 0
			IF CFLAG:C_ID:出産経過日 < CHILD_語彙
				PRINTFORML 幼児期
			ELSEIF CFLAG:C_ID:出産経過日 < CHILD_自立前
				PRINTFORML 幼少期
			ELSE
				PRINTFORML 思春期
			ENDIF
		ELSE
			PRINTFORML 現在是出生的第{(DAY - CHILD_BIRTHDAY:C_ID:LOCAL) / 124 + 1}年
		ENDIF
	ENDIF
	LOCAL:1 ++
NEXT
SIF ARGS == "ICON"
PRINTL 
RETURN LOCAL:1
