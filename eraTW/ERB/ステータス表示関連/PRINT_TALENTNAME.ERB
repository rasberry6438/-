		IF ARG:1 == -1
			RETURNF "回復慢"
		ELSEIF ARG:1 == 1
			RETURNF "回復快"
		ENDIF
	CASE 143
		SELECTCASE ARG:1
			CASE 1
				RETURNF "動物耳"
			CASE 2
				RETURNF "兔耳"
			CASE 3
				RETURNF "犬耳"
			CASE 4
				RETURNF "猫耳"
		ENDSELECT
	CASE 130
		IF ARG:1 == 1
			RETURNF "授乳可能"
		ELSEIF ARG:1 == 2
			RETURNF "母乳体質"
		ENDIF
	;音楽知識
	CASE 161
		SELECTCASE ARG:1
			CASE 1
				RETURNF "楽器知識"
			CASE 2
				RETURNF "歌唱知識"
			CASE 3
				RETURNF "舞踏知識"
		ENDSELECT
	;音感
	CASE 162
		IF ARG:1 == -1
			RETURNF "音痴"
		ELSEIF ARG:1 == 1
			RETURNF "音感"
		ELSEIF ARG:1 == 2
