;-------------------------------------------------
;CHARAの種族素質ARGの名称を返す
;-------------------------------------------------
@GET_TRIBENAME(CHARA, ARG)
#FUNCTIONS
#DIM CHARA
SELECTCASE ARG
CASE GETNUM(TALENT, "人类")
	SELECTCASE TALENT:CHARA:ARG
	CASE 1
		RETURNF "人类"
	CASE 2
		RETURNF "仙人"
	CASE 3
		RETURNF "天人"
	CASE 4
		RETURNF "月人"
	CASE 5
