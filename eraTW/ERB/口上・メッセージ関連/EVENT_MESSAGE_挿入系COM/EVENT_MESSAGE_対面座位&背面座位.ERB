;69,対面座位肛交
;-------------------------------------------------
@MESSAGE_COM69
IF FLAG:70 == 1
	;連続
	IF SELECTCOM == PREVCOM && ASSIPLAY == TFLAG:150
		PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%吻向了時停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%、在舌头交织在一起的同时再次開始摇晃起了腰身…
		RETURN 0
	;抜かず
	ELSEIF TCVAR:101 == PLAYER
		PRINTFORM 沒有從腸内拔出
		;調教者についてる
		IF TALENT:PLAYER:2 & 2
			PRINTFORM %TINKO(PLAYER)%を
		ELSE
			PRINTFORM 假陰茎
		ENDIF
			PRINTFORM 并從背後抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
			PRINTFORML 搖動起身體使用%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%再次開始了自慰…
	ELSE
		PRINTFORM 抱起時停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体
		SIF TEQUIP:0 & 2
			PRINTFORM 强行拨開%SHOW_BOTTOM(TARGET,2)%
		PRINTFORM 用%TINKO(PLAYER)%対準
		;潤滑LV4以上
		SIF PALAM:潤滑 >= PALAMLV:4
			PRINT 充满粘液的
		PRINTFORM 菊穴挿入進去、
	ENDIF
	;口球の有無
	SIF TEQUIP:口球 == 0
		PRINT 一邊接吻一邊
	PRINTFORML 為了使用%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的肛門自慰而摇动起了腰身…
ELSE
	;連続
	IF SELECTCOM == PREVCOM && ASSIPLAY == TFLAG:150
		PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%亲吻着%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%、在舌头交织在一起的同时再次開始摇晃起了腰身…
		RETURN 0
	;抜かず
	ELSEIF TCVAR:101 == PLAYER
		PRINTFORM 沒有從腸内拔出
		;調教者についてる
		IF TALENT:PLAYER:2 & 2
			PRINTFORM %TINKO(PLAYER)%を
		ELSE
			PRINTFORM 假陰茎
		ENDIF
			PRINTFORM 并緊緊抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
	ELSE
		PRINTFORM 抱起%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体
		SIF TEQUIP:0 & 2
			PRINTFORM 强行拨開%SHOW_BOTTOM(TARGET,2)%
		PRINTFORM 慢慢放下的同時挿入了
		;潤滑LV4以上
		SIF PALAM:潤滑 >= PALAMLV:4
			PRINT 充满粘液的
		PRINTFORM 肛門、
	ENDIF
	;口球の有無
	SIF TEQUIP:口球 == 0
		PRINT 一邊接吻一邊
	PRINTL 摇动起了腰身…
	;陥落済み
	SIF TALENT:恋慕
		PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%用手环抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%的脖子…
ENDIF

