;-------------------------------------------------
;67,対面座位
;-------------------------------------------------
@MESSAGE_COM67
IF FLAG:70 == 1
	IF TCVAR:100 == PLAYER && TARGET == TFLAG:103
		IF NOWEX:MASTER:射精
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%抱住時停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%、
			PRINTFORML 在感受著%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的氣味的同时、顺从着上涌的衝動向上耸动起了腰部…
		ELSEIF TEQUIP:体位２ == 4
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%吻向了時停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%、在舌头交织在一起的同时再次開始摇晃起了腰身…
		ELSE
			PRINTFORM 沒有從膣内拔出%TINKO(PLAYER)%并摟住了時停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
			PRINTFORML 一邊品嘗著口內的滋味一邊再次開始搖晃起腰身…
		ENDIF
	ELSE
		PRINTFORM 抱起時停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
		PRINTFORM 用%TINKO(PLAYER)%対準
		SIF TEQUIP:0 & 2
			PRINTFORM 從拨開的%SHOW_BOTTOM(TARGET,2)%的空隙中露出的
		;潤滑LV4以上
		SIF PALAM:潤滑 >= PALAMLV:4
			PRINT 充分湿润的
		;矮小体型
		IF TALENT:体型 < 0
			PRINT 幼小肉穴
		ELSE
			PRINT 膣穴
		ENDIF
		PRINTFORM 挿入進去、
		;口球の有無
		SIF TEQUIP:口球 == 0
			PRINT 一邊接吻一邊
		PRINTFORML 把%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%當做飛機杯如同在自慰一般搖動起腰身…
	ENDIF
ELSE
	IF TCVAR:100 == PLAYER && TARGET == TFLAG:103
		IF NOWEX:MASTER:射精
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%抱住%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%、
			PRINTFORML 顺从着上涌的衝動向上耸动起了腰部…
		ELSEIF TEQUIP:体位２ == 4
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%亲吻着%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%、在舌头交织在一起的同时再次開始摇晃起了腰身…
		ELSE
			PRINTFORM 沒有從膣内拔出%TINKO(PLAYER)%并緊緊地抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
			PRINTFORML 在舌头交织在一起的同时再次開始摇晃起了腰身…
		ENDIF
	ELSE
		PRINTFORM 抱起%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
		PRINTFORM 慢慢放下的同時挿入了
		SIF TEQUIP:0 & 2
			PRINTFORM 從拨開的%SHOW_BOTTOM(TARGET,2)%的空隙中露出的
		PRINTFORM %SLIT(, 1)%挿入进去、
		;口球の有無
		SIF TEQUIP:口球 == 0
			PRINT 一邊接吻一邊
		PRINTL 摇动起了腰身…
	ENDIF
	;SIF TALENT:恋慕
	;	PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%用手环抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%的脖子…
ENDIF
