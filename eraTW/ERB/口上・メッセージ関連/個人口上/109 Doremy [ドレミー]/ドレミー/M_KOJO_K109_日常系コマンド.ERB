			CASE 12 TO 15
				LOCALS:0 = 上
				LOCALS:1 = MIGI
				LOCALS:2 = ひだり
				LOCALS:3 = DOWN
				倍率 = 120
			;スペイン語
			CASE 16,17,18
				LOCALS:0 = encima de
				LOCALS:1 = a la derecha
				LOCALS:2 = a la izquierda
				LOCALS:3 = abajo de
				倍率 = 320
			;スワヒリ語
			CASE 19
				LOCALS:0 = juu
				LOCALS:1 = kulia
				LOCALS:2 = kushoto
				LOCALS:3 = chini
