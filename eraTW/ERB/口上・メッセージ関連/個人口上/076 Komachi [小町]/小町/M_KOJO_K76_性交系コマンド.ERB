			CALL HPH_PRINT,@"「啊啊HPH，好开心，我真是幸福的人HPH」","L"
			PRINTW 
		ENDSELECT
		RETURN 1
	ELSEIF TALENT:愛欲 || TALENT:炮友
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「看着，跟我配合动作吧」
			PRINTFORML 「好的，好的，很深很深地进入，啊，啊啊啊」
			PRINTW 
		CASE 1
			PRINTFORML 「来，再用力一点」
			PRINTFORML 「用你那自豪的男根压迫我的子宫，啊啊，啊啊啊」
			PRINTW 
		ENDSELECT
		RETURN 1
	ELSEIF TALENT:思慕
	SELECTCASE RAND:2
	CASE 0
		PRINTFORML 「啊啊，很棒，就这样进入吧」
		PRINTFORML （不知怎的，一股慈爱之情涌上心头…有点，开始认真起来了呢）
		PRINTW 
	CASE 1
		CALL HPH_PRINT,@"「啊，你要来拥抱我吗？太开心了HPHHPH」","L"
		PRINTFORML 「来，也吸一下奶，嗯嗯HPH，很好HPH」
		PRINTW 
	ENDSELECT
	;それ以外
	ELSE
