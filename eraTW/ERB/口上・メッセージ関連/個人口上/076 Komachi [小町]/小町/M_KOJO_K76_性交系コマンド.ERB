		CASE 0
			PRINTFORML 「啊啊，这个姿势，好深，我喜欢」
			CALL HPH_PRINT,@"「啊，啊HPH，啊啦HPH，很好，很好啊」","L"
			PRINTW 
		CASE 1
			PRINTFORML 「能看到你享受的表情，真不错呢。呵呵，我是不是也一样？」
			PRINTW 
