;==================================================
;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K76_100
CALL M_KOJO_MESSAGE_COM_K76_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K76_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF TEQUIP:Ｖ性交
			CALL HPH_PRINT,@"「啊、啊啊啊HPH，原谅我、原谅我吧啊HPH，嘿、嘿嘿嘿HPH」","L"
			PRINTL 
			PRINTFORML 探索着小町热烈湿润的肉褶，用手掌拍打着想要狠狠啃咬的臀部肉
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 每一次膣肉急剧收缩，仿佛迎合般反复抽插的肉棒紧紧缠绕，不愿放开…
			CASE 1
				PRINTFORML 每一次膣孔和肉棒间真诚汁液飞溅，小町对虐待行为感到愉悦的迹象显而易见…
			ENDSELECT
			PRINTW 
		ELSE
			SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「啊啊HPH，啊啊啊HPHHPH」","L"
				CALL HPH_PRINT,@"「原谅我吧HPH，原谅我啊HPH」","L"
				PRINTL 
				PRINTFORML 男人会想要尽情插入那圆润臀部，每次用手掌拍打
				PRINTFORML 每一次冲击，臀部会弹跳着，仿佛在眼前摇摆，似乎迎合着…
				PRINTW 
			CASE 1
				CALL HPH_PRINT,@"「咿呀HPH，咿呀HPHHPH」","L"
				CALL HPH_PRINT,@"「啊、我，感到快乐…咿呀HPH」","L"
				PRINTL 
				PRINTFORML 是无意识还是挑逗…小町受到大臀的折磨，声音中明显带有诱惑…
				PRINTFORML 似乎为了证明这一点，成熟的雌穴中溢出了黏糊糊的蜜液…
				PRINTW 
			ENDSELECT
		ENDIF
		RETURN 1
	ELSEIF ABL:受虐属性 >= 3
		IF TEQUIP:Ｖ性交
			PRINTFORML 「咿呀！？」
			PRINTFORML 呜呜♥
			PRINTFORML 在被狠狠拍打丰满臀部的瞬间，小町的膣肉急剧收缩，%CALLNAME:MASTER%差点就射精了…
			PRINTW 
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「咿呀，咿呀」
				PRINTFORML 「被拍打着臀部感到快乐什么的…」
				PRINTFORML 尽管这样说着，小町的大臀却期待着下一次的挑逗，在眼前摇摆着…
				PRINTW 
			CASE 1
				PRINTFORML 「啊啊，嘿嘿嘿」
				PRINTFORML 每个男人都想要狠狠啃咬的臀部上，留下了一道道平手的痕迹
				PRINTFORML 然而，仿佛在证明那不仅仅是痛楚，一缕淫汁从膣孔中流淌而下…
				PRINTW 
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		PRINTFORML 「咿呀！？」
		PRINTFORML 「你、你在干什么啊。痛的话就算了吧」
		PRINTW
		RETURN 1
	ENDIF
ENDIF
RETURN 1

