						CALL HPH_PRINT,@"「呜啊...%MASTERNAME:K69_CNUM%...刚刚射精过不是吗...」","W"
						LOCALS = 呜啊,等等,、%MASTERNAME:K69_CNUM%,、刚刚,射精过,才,不是吗
						CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS(LOCALS,3)%","W"
					CASE 1
						CALL HPH_PRINT,@"「呜啊...%MASTERNAME:K69_CNUM%...这般想要咱的孩子吗...」","W"
						LOCALS = 呜啊,、%MASTERNAME:K69_CNUM%,、这般想要,、咱的,孩子,吗,、%MASTERNAME:K69_CNUM%
						CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS(LOCALS,3)%","W"
					ENDSELECT
				ENDIF
			ELSE
				GOTO INSERT_V
			ENDIF
		ELSE
			IF TCVAR:MASTER:V被中出FLAG && RAND:3 == 0
				CALL HPH_PRINT,@"「呜啊...%MASTERNAME:K69_CNUM%...刚刚射精过不是吗...」","W"
			ELSE
				GOTO INSERT_V
			ENDIF
		ENDIF
	ENDIF
ELSE
;Ａ　現状スキップ
ENDIF
RETURN 1
