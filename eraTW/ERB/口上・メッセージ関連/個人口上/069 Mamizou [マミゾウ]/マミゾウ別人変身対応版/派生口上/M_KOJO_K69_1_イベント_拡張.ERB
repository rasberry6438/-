	K69_1_TNAME = \@CFLAG:69:変化先 != 0 ? 化身为%CALLNAME:(CFLAG:69:変化先)%的 # \@猯藏
	
	RESULTS = 
	
	IF TALENT:TARGET:体型 == -2
		IF TALENT:TARGET:胸圍 == 1 || TALENT:TARGET:胸圍 == 2
			Body = 童颜巨乳
		ELSE
			Body = 幼童体型
		ENDIF
		ELSEIF TALENT:TARGET:体型 == -1
			IF TALENT:TARGET:胸圍 == 1 || TALENT:TARGET:胸圍 == 2
				Body = 童颜巨乳
			ELSE
				Body = 萝莉体型
			ENDIF
		ELSEIF TALENT:TARGET:胸圍 == -1 || TALENT:TARGET:胸圍 == -2
			Body = 平板身材
		ELSEIF TALENT:TARGET:胸圍 == 1
			Body = 色气身材
		ELSEIF TALENT:TARGET:胸圍 == 2
			Body = 色气身材
		ELSE
			Body = 裸体
	ENDIF

	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;全裸にする
		;記入チェック（=0, 非表示、1, 表示）
