@別人変身版_MESSAGE_COM_K69_1
#DIM TRANSAFTER
TRANSAFTER = CFLAG:TARGET:変化先 == 0 ? K69_CNUM # CFLAG:TARGET:変化先
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;膣内射精直後
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			LOCAL:99 = RAND:1
			SELECTCASE LOCAL:99
			CASE 0
				;CALL HPH_PRINT,@"「……お前さん、とんだ変態じゃな……、っ、あっ……HPH」","W"
				CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS("…你这家伙,真是个,变态,呢",3)%","W"
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:Ｃ絶頂
		IF FLAG:70
			RETURN 1
		ELSE
			IF TRANSAFTER != K69_CNUM && RAND:5 < 3
				LOCAL:99 = RAND:1
				SELECTCASE LOCAL:99
				CASE 0
					;CALL HPH_PRINT,@"「く……あ……っ、%CALLNAME:TRANSAFTER%のまんこ……っ、イ、イク……ぅ、うああっっHPH」","W"
					LOCALS = 呜,嗯…、,%CALLNAME:TRANSAFTER%,的,小穴,、,要,要去了…啊
					CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS(LOCALS,3)%","W"
				ENDSELECT
			ELSE
				LOCAL:99 = RAND:2
				SELECTCASE LOCAL:99
				CASE 0
					;CALL HPH_PRINT,@"「く……う……っ、イ、イク……ぅ、うああっっHPH」","W"
					CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS("呜,嗯…、,要,要去了…啊",3)%","W"
				CASE 1
					;CALL HPH_PRINT,@"「く……ふ……っま、まて……んあっ……HPH　っああっっHPH」","W"
					CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS("嗯,哈…,等,等等,要去了啊",3)%","W"
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ
		IF TRANSAFTER != K69_CNUM && RAND:5 < 3
			LOCAL:99 = RAND:1
			SELECTCASE LOCAL:99
			CASE 0
				;CALL HPH_PRINT,@"「ん……っ、どうじゃ、%CALLNAME:TRANSAFTER%のまんこの味は……HPHんん……っHPH」","W"
				LOCALS = 如何啊、,%CALLNAME:TRANSAFTER%,的,小穴,的,味道
				CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS(LOCALS,3)%","W"
			ENDSELECT
		ELSE
			LOCAL:99 = RAND:2
			SELECTCASE LOCAL:99
			CASE 0
				;CALL HPH_PRINT,@"「ん……っ、……そんなところ、嬉しそうに舐めおって……、んん……っHPH」","W"
				CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS("这种,地方、,开心地,舔着…",3)%","W"
			CASE 1
				;CALL HPH_PRINT,@"「んあ…っ,くう…っ,んん…っ」","W"
				CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS("嗯啊…,呼…,嗯嗯…",3)%","W"
			ENDSELECT
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		;CALL HPH_PRINT,@"「ん……っ、……そんなところ、嬉しそうに舐めおって……、んん……っHPH」","W"
		CALL HPH_PRINT,@"%K69_AUTO_AEGI_INS("这种,地方、,开心地,舔着…",3)%","W"
		RETURN 1
	ENDIF
ENDIF
RETURN 1
