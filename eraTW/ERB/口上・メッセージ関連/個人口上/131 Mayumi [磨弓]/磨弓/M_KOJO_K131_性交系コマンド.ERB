	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;Ａ感覚４以上
	ELSEIF ABL:Ａ感覚 >= 4
		SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「嗯...HPH稍等...♪」","L"
				CALL HPH_PRINT,@"「这家伙...需要仔细准备呢...HPH」","L"
				PRINTFORMDL 磨弓将从小穴垂落的爱液，涂抹到自己的肛门上
				PRINTFORMDL 目睹如此煽情的举动，%CALLNAME:MASTER%的肉棒也迅速勃起了
				CALL HPH_PRINT,@"「呵…♪你也准备周全了呢…HPH那么…！」","L"
				CALL HPH_PRINT,@"「——咕哦…HPH啊、呼…HPH呼唔——…HPH HPH」","W"
			CASE 1
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%…HPH好好看着插入的地方哦…HPH」","L"
				CALL HPH_PRINT,@"「咕…！HPH…嘶…！嗯！！」","L"
				CALL HPH_PRINT,@"「啊啊啊啊啊————HPH HPH HPH」","L"
				PRINTFORMDL 磨弓将全身重量压下来，一口气把肉棒插到了最深处
				PRINTFORMDW 伴随着与平时截然不同的娇喘，强烈的紧致感猛然袭来…
		ENDSELECT
	RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「嗯…嗯呜…！咿…啊―」
				CALL HPH_PRINT,@"「啊啊HPH HPH…哈啊…！哈啊…！进、进来了吗…？」","L"
				PRINTFORMDL 磨弓沉下腰肢，成功将肉棒插入肛门
				PRINTFORMDW 看来果然还不习惯抽插动作，决定稍作等待直到适应…
			CASE 1
				PRINTFORML 「嗯呜...！居然要我自己...把肉棒...塞进..肛门...！？」
				PRINTFORML 「...要不是你的请求，才不会做这种...嗯嗯...事...！」
				PRINTFORMDL 磨弓正在努力尝试插入，但似乎并不顺利
				PRINTFORMDL %CALLNAME:MASTER%抓住磨弓的腰肢往下按，将肉棒深深贯入
				PRINTFORML 「啊！？等等！还没准...!!??————」
				CALL HPH_PRINT,@"「————咿咕呜呜HPH 哈啊啊啊HPH......HPH HPH」","W"
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
