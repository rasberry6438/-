				RETURN 1
			ENDIF
		ELSE
			;今射精した
			IF NOWEX:PLAYER:11
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「哈啊...！哈啊...！咕...！不行...动一下就会..呀哈啊啊...！」
						PRINTFORMDL 在%CALLNAME:MASTER%临近高潮时，磨弓的动作迟缓起来
						PRINTFORMDL 这样半途而废太难受了吧...%CALLNAME:MASTER%抓住磨弓的腰
						CALL HPH_PRINT,@"「咿咕呜！？HPH HPH等...呀哈啊啊啊啊啊HPH...呜啊啊HPH...HPH这么激烈的话...HPH要去..!.呀啊HPH HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「咕...！啊...HPH、里面...！好、好大...呜...HPH」","L"
						PRINTFORMDL 磨弓开始最后冲刺加快抽插
						PRINTFORMDW %CALLNAME:MASTER%将身体交给快感支配，在磨弓直肠深处射精了
					CASE 2
						CALL MAYUMI_SEX
				ENDSELECT
				RETURN 1
		;前回射精した
			ELSEIF TCVAR:104
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「哈啊…HPH哈啊…HPH啊啊…HPH啊、啊啊…HPH」","L"
						PRINTFORMDL 每当精液注入肠内，磨弓的肢体便不受控制地痉挛着
						PRINTFORMDW 肠壁的颤动刺激肉棒再度勃起
					CASE 1
						CALL HPH_PRINT,@"「咕…HPH精液、射进来了…！好多…HPH」","L"
						PRINTFORMDL 感受到直肠深处被射精的磨弓皱起眉头
						PRINTFORMDW 与%CALLNAME:MASTER%目光交汇后，磨弓会意地缓缓继续晃动
					CASE 2
						CALL MAYUMI_SEX
				ENDSELECT
				RETURN 1
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「呼——！呼——！什么?...还要再快点！？」
						CALL HPH_PRINT,@"「什么啊...就算这样..也已经是...HPH全力了...！」","L"
						PRINTFORMDW 磨弓缓慢的抽插带来焦躁的刺激
					CASE 1
						CALL HPH_PRINT,@"「哈啊…！啊…！呜、唔…！」","L"
						PRINTFORML （声音漏出来了...！好羞耻...！）
						PRINTFORMDL 磨弓紧闭双眼摆动着腰肢
						PRINTFORMDW 每次抽插时菊穴漏出的噗叽声都在刺激她的羞耻心
					CASE 2
						CALL MAYUMI_SEX
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
