			ENDSELECT
		ELSE
			IF TALENT:恋人
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「%CALLNAME:MASTER%、不需要我陪睡吗？」
						PRINTFORMW 「…开玩笑的啦，晚安♪」
					CASE 1
						PRINTFORML 「等等%CALLNAME:MASTER%。你好像忘了重要的事情呢...」
						CALL HPH_PRINT,@"「啾HPH…晚安♪」","W"
						PRINTFORMDW ......！！！
						PRINTFORMDW （这样更睡不着了吧！）
					CASE 2
						PRINTFORML 「…%CALLNAME:MASTER%、睡不着吗？」
						PRINTFORML 「别担心！只要我在，你睡觉时被袭击这种事万分之一的可能性都没有！」
						PRINTFORMDW 是谁要来袭击啊！？你这么说的话反而更害怕了啊喂！
						PRINTFORMDW 因为在意坐在旁边的磨弓，花了很长时间才入睡……
				ENDSELECT
			ELSEIF TALENT:恋慕
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「怎么了%CALLNAME:MASTER%？…陪、陪睡！？」
						PRINTFORML 「别说蠢话了，快点给我睡觉！」
						PRINTFORMDW 被磨弓训斥了……
					CASE 1
						PRINTFORML 「…%CALLNAME:MASTER%？…已经睡着了吗…？」
						CALL HPH_PRINT,@"「……啾HPH…晚安…♪」","W"
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「啊啊…晚安」
						PRINTFORML 「我也再过会就休息吧」
						PRINTFORMDW 向磨弓道过晚安后，闭上了眼睛……
					CASE 1
						PRINTFORMW 「晚安。好好休息吧」
				ENDSELECT
			ENDIF
		ENDIF
	ELSE
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「抱歉啊%CALLNAME:MASTER%…今天没法陪你一起睡了」
					PRINTFORML 「就让埴轮士兵代替我陪着你，就当是我在身边吧」
					PRINTFORMDW 这果然还是太勉强了……
				CASE 1
					PRINTFORML 「晚安，%CALLNAME:MASTER%」
					CALL HPH_PRINT,@"「啾HPH…不许熬夜哦？」","L"
					PRINTFORMDW 脸颊上传来磨弓的吻，随后便沉入了梦乡…
				CASE 2
					PRINTFORMW 「晚安%CALLNAME:MASTER%！明天我还会来见你的哦…♪」
				CASE 3
					PRINTFORML 「%CALLNAME:MASTER%。虽然只是安慰程度，不过我会留下埴輪兵士的」
					PRINTFORML 「这样就算睡梦中被袭击也没关系了！安心休息吧」
					PRINTFORMDW （那要是被偷偷潜入的上司袭击了该怎么办呢）
					PRINTFORMDW 反复告诉自己这是磨弓特有的关心方式，在埴輪兵的守护下闭上了眼睛……
				CASE 4
					PRINTFORML 「%CALLNAME:MASTER%、要睡了吗？…晚安」
					PRINTFORML 「虽然依依不舍，不过我也差不多该回去了…」
					PRINTFORMDW 磨弓带着些许寂寞的神情离开了…
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「%CALLNAME:MASTER%这家伙…被子不是掀开着吗」
					PRINTFORMW 「呼，这样就行了！要是感冒了可就麻烦了…晚安…♪」
				CASE 1
					PRINTFORML 「晚安%CALLNAME:MASTER%」
					PRINTFORML 「为防万一，要派个埴轮士兵护卫你吗？」
					PRINTFORMDW 担心过头了吧...
				CASE 2
					PRINTFORMW 「睡不着的时候，据说数埴轮会很有效哦？...晚安」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「啊，晚安。好好休息吧」
				CASE 1
					PRINTFORML 「这样啊...你需要睡觉呢。晚安，%CALLNAME:MASTER%」
				CASE 2
					PRINTFORML 「那就这样吧%CALLNAME:MASTER%。晚安」
			ENDSELECT
			PRINTFORMDW 向磨弓道了晚安后、沉入了梦乡……
		ENDIF
	ENDIF
ENDIF
RETURN 1
