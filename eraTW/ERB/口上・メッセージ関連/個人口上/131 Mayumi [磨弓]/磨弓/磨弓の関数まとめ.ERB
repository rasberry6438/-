;季節ネタ
;==================================================
@MAYUMI_SEASON
SELECTCASE DAY:2
	;春
	CASE 1
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「...哈啊...」
				PRINTFORML 「——啊！我居然...！太松懈了，杖刀偶磨弓！...打起精神！」
				PRINTFORMDW 或许是受到春日暖意的熏染，磨弓显得有些困倦......
			CASE 1
				PRINTFORML 「每到这个时节，似乎有很多人类会为花粉症所苦...」
				PRINTFORML 「%CALLNAME:MASTER%不要紧吗？」
				PRINTFORMDW ...啊...嚏！！————————
			CASE 2
				PRINTFORML 「嗯...？%CALLNAME:MASTER%在笑什么？」
				PRINTFORML 「啊...！哼哼...是蝴蝶呀...♪」
				PRINTFORMDW 停在磨弓身上的蝴蝶，翩翩飞向不知何处。
			CASE 3
				PRINTFORML 「春季是动物灵们结束冬眠开始活跃的季节」
				PRINTFORML 「你若不提高警惕，可是会被轻易附身的」
				PRINTFORMDW 看来动物即使化作灵体也会冬眠......
			CASE 4
				PRINTFORML 「嗯...？%CALLNAME:MASTER%怎么了？...我脸上沾了什么东西吗？」
				PRINTFORMDW 有只蜜蜂停在了磨弓头上。该怎么赶走它呢...？
		ENDSELECT
	;夏
	CASE 2
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「...%CALLNAME:MASTER%...我知道很热，但太不成体统了...」
				PRINTFORMDW 埴轮士兵冰凉凉的很舒服...%CALLNAME:MASTER%尽情拥抱着磨弓磨蹭着脸颊。
			CASE 1
				PRINTFORML 「这般炎热的天气，用三途川的水来泼洒地面倒是不错」
				PRINTFORMDW 总觉得会遭报应......
			CASE 2
				;お盆のナスときゅうり
				PRINTFORML 「每到这个时节，经常能看到人类灵附在蔬菜上移动...」
				PRINTFORML 「那到底是什么...？%CALLNAME:MASTER%知道些什么吗？」
				PRINTFORMDW 磨弓似乎不了解盂兰盆节的习俗。
			CASE 3
				PRINTFORML 「我可是埴轮兵长！这种程度的炎热根本不值一提！」
				PRINTFORMDW 即使在这种酷暑中，磨弓也完全没有出汗...
			CASE 4
				PRINTFORML 「袿姫大人即便在如此炎热的天气里，也从容不迫地烧制着埴轮」
				PRINTFORMW 「真是位了不起的大人。我们也必须效仿才是」
		ENDSELECT
	;秋
	CASE 3
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「%CALLNAME:MASTER%、这可是艺术之秋啊！」
				PRINTFORML 「你也效仿袿姫大人试着创作些什么如何？」
				PRINTFORMDW 自己要不要也试着制作埴轮呢...？
			CASE 1
				PRINTFORML 「袿姫大人为我重新涂装了铠甲的颜色！」
				PRINTFORML 「说是秋季特别款，怎么样？」
				PRINTFORMDW 看起来和平时毫无区别......
			CASE 2
				PRINTFORML 「―嘿！...哼哼♪如何？」
				PRINTFORMDL 磨弓瞬间抓住了不知从何处飘来的红叶展示给你看。
				PRINTFORMDW 看她这么开心的样子，总之先夸奖几句吧...
			CASE 3
				PRINTFORML 「唔...！」
				PRINTFORML 「每到这个季节，就忍不住想大快朵颐美味的食物......」
				PRINTFORMDW 磨弓的肚子在叫唤。这到底是什么构造啊......？
			CASE 4
				PRINTFORML 「古时候，人们都是靠吃橡子过活的――」
				PRINTFORML 「啊！？啊啊......糟糕了......」
				PRINTFORMDW 从磨弓手中掉落的橡子骨碌碌滚向不知何处......
		ENDSELECT
	;冬
	CASE 4
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「据说天气变冷后鱼就难钓了」
				PRINTFORML 「会在冬天钓鱼的家伙，定是相当有闲情逸致之人吧」
				PRINTFORMDW 回答说这叫独钓寒江的气节
			CASE 1
				PRINTFORML 「稍微有点冷了呢...」
				PRINTFORML 「活动身体就会暖和起来！你要不要也试试？」
				PRINTFORMDW 还是婉拒吧......
			CASE 2
				PRINTFORML 「畜生灵差不多该到冬眠的时节了」
				PRINTFORML 「衷心希望它们能就这样安分地长眠下去......」
				PRINTFORMDW 看来动物即使化作灵体也会冬眠......
			CASE 3
				PRINTFORML 「袿姫大人说这个时期寒冷到连粘土都会冻结」
				PRINTFORML 「%CALLNAME:MASTER%、可有什么好办法？」
				PRINTFORMDW 与磨弓互相提出了各种想法。
			CASE 4
				PRINTFORML 「...三途川也会结冰吗？」
				PRINTFORMDW 看着结冰的水洼，磨弓低声呢喃。
		ENDSELECT
ENDSELECT
;==================================================
;
;天气话题
;==================================================
@MAYUMI_OTENKI
SELECTCASE TIME:5
	;晴天
	CASE 0,1
		IF TCVAR:131:350 == 0
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「天气真不错啊%CALLNAME:MASTER%！」
					PRINTFORML 「这种日子不出去走走就太浪费啦！」
					PRINTFORMDW 磨弓似乎想去什么地方。
				CASE 1
					PRINTFORML 「真是清爽的好天气啊...」
					PRINTFORML 「要是这样他们也能老实下来的话，那就没话可说了......」
					PRINTFORMDW 磨弓露出了有些苦涩的表情......
				CASE 2
					PRINTFORML 「今天可是绝佳的训练好天气啊」
					PRINTFORML 「怎么样%CALLNAME:MASTER%？要一起特训一下吗？」
					PRINTFORMDW 磨弓似乎想要进行训练。
			ENDSELECT
			TCVAR:131:350 = 1
		ELSE
			IF TALENT:恋人
				CALL MAYUMI_KOIBITO
			ELSEIF TALENT:恋慕
				CALL MAYUMI_RENBO
			ELSEIF TALENT:思慕
				CALL MAYUMI_SHIBO
			ELSE
				CALL MAYUMI_NORMAL
			ENDIF
		ENDIF
	;曇り
	CASE 2,3
		IF TCVAR:131:351 == 0
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「虽然不是晴天...但也没有要下雨的迹象」
					PRINTFORML 「这种不上不下的天气最讨厌了」
					PRINTFORMDW 磨弓仰望着铅灰色的天空皱起眉头......
				CASE 1
					PRINTFORMW 「要出门的话，这天气有点微妙啊...」
				CASE 2
					PRINTFORMW 「是因为没有太阳吗，今天稍微有点凉快呢」
			ENDSELECT
			TCVAR:131:351 = 1
		ELSE
			IF TALENT:恋人
				CALL MAYUMI_KOIBITO
			ELSEIF TALENT:恋慕
				CALL MAYUMI_RENBO
			ELSEIF TALENT:思慕
				CALL MAYUMI_SHIBO
			ELSE
				CALL MAYUMI_NORMAL
			ENDIF
		ENDIF
	;雨
	CASE 4,5,6
		IF TCVAR:131:352 == 0
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「嗯~看来暂时还不会停呢......」
					PRINTFORMDW 和磨弓一起避雨时抬头望向天空。
				CASE 1
