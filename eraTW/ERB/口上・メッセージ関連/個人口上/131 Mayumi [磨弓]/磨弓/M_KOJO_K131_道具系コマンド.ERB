;40,ローター
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K131_40
RETURN 0

@M_KOJO_MESSAGE_COM_K131_40
CALL M_KOJO_MESSAGE_COM_K131_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K131_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		SELECTCASE RAND:5
			CASE 0
				CALL HPH_PRINT,@"「嗯嗯HPH HPH啊哈HPH啊、咕呜HPH HPH」","L"
				PRINTFORML 「求…求你了……稍微…轻一点…」
				PRINTFORMDW 磨弓将跳蛋留在膣内，虚弱地恳求着…！
			CASE 1
				CALL HPH_PRINT,@"「嗯嗯…HPH呜…HPH啊HPH嗯…！」","L"
				CALL HPH_PRINT,@"「这…这种机器什么的HPH…我、才不会…输呢…HPH」","L"
				PRINTFORMDW 磨弓在抵抗着跳蛋带来的快感。
			CASE 2
				PRINTFORML 「…喂…！」
				PRINTFORML 「之前就算了，屁股绝对不行…！」
				PRINTFORMDW 想要把跳蛋塞进肛门时被她瞪了……
			CASE 3
				PRINTFORML 「等…！等等！那里是―」
				CALL HPH_PRINT,@"「咿呀啊啊啊HPH HPH HPH」","L"
				PRINTFORMDW 把跳蛋按在阴蒂上时，磨弓的身体猛地弹了起来。
			CASE 4
				CALL HPH_PRINT,@"「噫HPH HPH乳头…啊…HPH在颤抖呜呜…HPH HPH」","L"
				PRINTFORMDW 将跳蛋按在磨弓的乳头上。
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

