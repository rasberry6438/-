;==================================================
;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K96_100
CALL M_KOJO_MESSAGE_COM_K96_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K96_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 在停止的时间里敲打着雷鼓的屁股。
				RETURN 1
			ELSE
				PRINTFORMW 「…呜啊！？」
				PRINTFORMW 「等一下…等一下啊！？嗯啊~%UNICODE(0x2665) *1%」
				RETURN 1
			ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 在停止的时间里敲打着雷鼓的屁股。
		RETURN 1
	;V挿入打屁股
	ELSEIF LOCAL:1 && TEQUIP:50 == PLAYER
		IF RAND:3 == 0
			PRINTFORMW 「嗯…啊%UNICODE(0x2665) *1%」
			PRINTFORMW 「……啊…这个…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊~%UNICODE(0x2665) *1%…呼~%UNICODE(0x2665) *1%」
			PRINTFORMW 「……插着……呢……嗯%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 「哈%UNICODE(0x2665) *1%……啊啊！%UNICODE(0x2665) *1%」
			PRINTFORMW 「嗯…已经…%UNICODE(0x2665) *1%」
			RETURN 1
		ENDIF
	;V挿入打屁股
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		IF RAND:3 == 0
			PRINTFORMW 「啊%UNICODE(0x2665) *1%……」
			PRINTFORMW 「……屁股…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊！%UNICODE(0x2665) *1%…唔…%UNICODE(0x2665) *1%」
			PRINTFORMW 「……这个…好舒服…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 「嗯~%UNICODE(0x2665) *1%…哈%UNICODE(0x2665) *1%」
			PRINTFORMW 「已经…%UNICODE(0x2665) *1%…笨蛋……%UNICODE(0x2665) *1%」
			RETURN 1
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「啊…%UNICODE(0x2665) *1%あっ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「……屁股…变得不行了…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯啊…%UNICODE(0x2665) *1%啊…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…这…%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 「嗯啊…%UNICODE(0x2665) *1%这样的…%UNICODE(0x2665) *1%」
			PRINTFORMW 「因为屁股而变成笨蛋了~%UNICODE(0x2665) *1%」
			RETURN 1
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「啊%UNICODE(0x2665) *1%」
			PRINTFORMW 「……啊%UNICODE(0x2665) *1%……屁股%UNICODE(0x2665) *1%」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊！%UNICODE(0x2665) *1%…唔…%UNICODE(0x2665) *1%」
			PRINTFORMW 「……这个…太……嗯%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 「…嗯啊%UNICODE(0x2665) *1%……哎呀%UNICODE(0x2665) *1%」
			RETURN 1
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「呼…够了…」
			PRINTFORMW 「……我的屁股不是用来打的……」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「太……不要打啊…」
			PRINTFORMW 「……好痛啊…即使这样」
			RETURN 1
		ELSE
			PRINTFORMW 「…啊…」
			PRINTFORMW 「疼…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

