;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K96_80
CALL M_KOJO_MESSAGE_COM_K96_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K96_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 在时间冻结时让雷鼓的手握住了阴茎。
			ELSE
				PRINTFORMW 「呼？」
				PRINTFORMW 「我知道了，我会让你做的」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 在时间冻结时让雷鼓的手握住了阴茎。
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「嗯？已经去了吗？」
			PRINTFORMW 这么说来着，雷鼓更加激烈地開始了撸动。
			PRINTFORMW 「好可爱啊~一跳一跳的%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯…越变越大了呢%UNICODE(0x2665) *1%」
			PRINTFORMW 这么说来着，雷鼓更加激烈地開始了撸动。
			PRINTFORMW 「请把精液都射出来吧%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「差不多该出来了吧」
			PRINTFORMW 这么一说，雷鼓就更加喜欢了。
			PRINTFORMW 「射出很多呢……嗯%UNICODE(0x2665) *1%」
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「嗯…瞧！」
			PRINTFORMW 「让会你舒服的…嗯%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯…又粗又硬…」
			PRINTFORMW 「……但是马上就要出来了呢」
		ELSE
			PRINTFORMW 「嗯…好淘气的孩子啊…」
			PRINTFORMW 「让会你舒服的…嗯%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「嗯…舒服吗？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「这样的事，虽然没怎么做过」
			PRINTFORMW 「但我的手很灵巧」
		ELSE
			PRINTFORMW 「满满的…射出来吧！」
			PRINTFORMW 「……用我的手让你開心。」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯…真恶心啊」
			PRINTFORMW 「……快出去吧。」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「我不太喜欢这样的」
			PRINTFORMW 「如果你喜欢的话，就让你高兴吧。」
		ELSE
			PRINTFORMW 「……嗯…这里是弱点？」
			PRINTFORMW 「……又变大了呢…」
		ENDIF
	ENDIF
ENDIF
RETURN 1

