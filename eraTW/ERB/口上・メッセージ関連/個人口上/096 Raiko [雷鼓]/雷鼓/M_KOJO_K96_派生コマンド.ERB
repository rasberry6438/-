RETURN 1
@M_KOJO_MESSAGE_SCOM_K96_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		;時姦中
			IF FLAG:70
				PRINTFORMW 在冰冻的时间里開始侵犯雷鼓的口腔。
			ELSE
				PRINTFORMW 「……嗯啊…嗯…」
				PRINTFORMW 「啊…射出了那么很多…%UNICODE(0x2665) *1%」
			ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 在时间冻结时让雷鼓手握阴茎并開始侵犯口腔。
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「嗯…嗯~」
			PRINTFORMW 「……来吧…全部射出来吧…？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯…呼…」
			PRINTFORMW 「…再…嗯啊！」
		ELSE
			PRINTFORMW 「嗯…嗯啊…」
			PRINTFORMW 「…我……会把你榨干的%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「啊…来吧…」
			PRINTFORMW 「……嗯…啾…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…啊…」
			PRINTFORMW 「嗯…啊！」
		ELSE
			PRINTFORMW 「……有好好地清洗吗？」
			PRINTFORMW 「那让我来帮你清扫干净吧~」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯…哦…」
			PRINTFORMW 「嗯…这个…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…啊…」
			PRINTFORMW 「……我……啊…」
		ELSE
			PRINTFORMW 「…嗯，嗯…」
			PRINTFORMW 「来吧…嗯…」
		ENDIF
	ENDIF
ENDIF
