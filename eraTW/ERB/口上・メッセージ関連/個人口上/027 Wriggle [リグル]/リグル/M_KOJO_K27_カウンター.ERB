@M_KOJO_MESSAGE_COUNTER_K27_50
CALL M_KOJO_MESSAGE_COUNTER_K27_50_1
RETURN RESULT
@M_KOJO_MESSAGE_COUNTER_K27_50_1
	IF TALENT:MASTER:形状 == 4
		SELECTCASE ABL:指
		CASE IS >= 5
			SELECTCASE RAND:10
			CASE 0
				CALL HPH_PRINT,@"「啊啊…HPH　，无论看多少次，你的%TEXTR("生殖器/阴茎")%都那么美妙…HPH」","L"
				CALL HPH_PRINT,@"「我想要它进入我里面… 不行吗…？ 不行吗…？HPH」","L"
				PRINTW 
			CASE 1
				CALL HPH_PRINT,@"「它闪闪发光… 在我的手中颤动…HPH」","L"
				CALL HPH_PRINT,@"「你是不是已经想要把它放进我里面了？ 我… 随时都可以HPH」","L"
				PRINTW 
			CASE 2
				PRINTFORML 「这样只用手解决可以吗？」
				CALL HPH_PRINT,@"「这根坚硬而壮丽的%TEXTR("生殖器/阴茎")%… 你想和我交配吗？HPH」","L"
				PRINTW 
			CASE 3
				CALL HPH_PRINT,@"「……HPH」","L"
				PRINTFORML 莉格露带着陶醉的表情，有节奏地爱抚着%CALLNAME:MASTER%的阴茎…
				PRINTW 
			CASE 4
				CALL HPH_PRINT,@"「嘿，我好想要这个…HPH」","L"
				CALL HPH_PRINT,@"「其实我希望你全部射进来，但是如果我用手给你快感，你会进来吗…？HPH」","L"
				PRINTW 
			CASE 5
				CALL HPH_PRINT,@"「嘿嘿。在我的手中挣扎吧HPH」","L"
				CALL HPH_PRINT,@"「你想射出来吧？ HPH 如果你和我交配很多次，我会让你射出来HPH」","L"
				PRINTW 
			CASE 6
				PRINTFORML 「我的手真的那么舒服吗？」
				CALL HPH_PRINT,@"「但是，我觉得交配对你和我都会更舒服…HPH」","L"
				PRINTW 
			CASE 7
				CALL HPH_PRINT,@"「因为你的请求，我会用手帮你… 但是精子一定要射到这边才行…HPH」","L"
				PRINTFORML 被闪耀的阴茎填满的莉格露，露出妖娆的表情，用手展示着她的秘处…
				PRINTW 
			CASE 8
				CALL HPH_PRINT,@"「嘿，快点和我交配吧HPH」","L"
			CASE 9
				CALL HPH_PRINT,@"「如果你不立刻说要交配，我就不让你射出来…HPH」","L"
			ENDSELECT
		CASE IS >= 2
			SELECTCASE RAND:5
			CASE 0
				CALL HPH_PRINT,@"「嘿嘿，我变得越来越好了吧？ HPH」","L"
				CALL HPH_PRINT,@"「我为了你努力了… HPH 所以，之后我想要很多你的… HPH」","L"
				PRINTW 
			CASE 1
				PRINTFORML 「嘿，你这里舒服吗？」
				CALL HPH_PRINT,@"「我也想要你多多让我感到舒服… HPH」","L"
				PRINTW 
			CASE 2
				CALL HPH_PRINT,@"「啊哈，好硬啊… HPH」","L"
				CALL HPH_PRINT,@"「而且，很亮。这样的话，我也可以生出强壮的孩子呢… HPH」","L"
				PRINTW 
			CASE 3
				CALL HPH_PRINT,@"「想要用手射吗？ HPH 没问题 HPH」","L"
				CALL HPH_PRINT,@"「只要你能射出和我一样多的精液就行了 HPH」","L"
				PRINTW 
			CASE 4
				CALL HPH_PRINT,@"「嘿，想要射出来吗？ HPH」","L"
				CALL HPH_PRINT,@"「是要这样射出来吗？ HPH 还是和我一起造人？ HPH」","L"
			ENDSELECT
		CASEELSE
			SELECTCASE RAND:5
			CASE 0
				CALL HPH_PRINT,@"「哇啊… HPH，好棒的%TEXTR("生殖器/阴茎")%呢… HPH」","L"
				CALL HPH_PRINT,@"「我会努力为你服务的 HPH」","L"
				PRINTW 
			CASE 1
				CALL HPH_PRINT,@"「你这里…真的闪闪发光呢 HPH」","L"
				PRINTW 
			CASE 2
				PRINTFORML 「能够遇到这么棒的%TEXTR("生殖器/阴茎")%，我真的好幸运♪」
				PRINTFORML 「我会尽力为你服务，如果不舒服的话，请原谅我？」
				PRINTW 
			CASE 3
				CALL HPH_PRINT,@"「这样做的话，%TEXTR("生殖器/阴茎")%会越来越硬呢… HPH」","L"
				CALL HPH_PRINT,@"「啊哈，果然… HPH 它在我的手中变得很厉害呢… HPH」","L"
			CASE 4
				CALL HPH_PRINT,@"「我会努力让你感到舒服的… HPH」","L"
				CALL HPH_PRINT,@"「如果感到舒服了，能和我交配吗… HPH」","L"
			ENDSELECT
		ENDSELECT
	ELSEIF TALENT:恋慕 || TALENT:恋人
		SELECTCASE ABL:指
		CASE IS >= 5
			SELECTCASE RAND:6
			CASE 0
				PRINTFORML 「%TEXTR(@"%CALLNAME:MASTER%/你")%，你完全沉迷于我的手中了呢…♪」
				PRINTFORML 莉格露的手指像蜘蛛缠绕猎物一样，在%CALLNAME:MASTER%的阴茎上爬行…
			CASE 1
				PRINTFORML 「嘿，告诉我吧。你想用我的手射吗？」
				PRINTFORML 「坦白回答的话，我会一直帮你到最后。所以，请告诉我…❤」
				PRINTFORML 莉格露的手指灵巧地蠕动着，让%CALLNAME:MASTER%的阴茎兴奋又沮丧…
			CASE 2
				PRINTFORML 「随时都可以射出来哦。我会一次又一次地帮你…❤」
			CASE 3
				PRINTFORML 「这么硬又热的东西，会进入我的体内…太厉害了…」
			CASE 4
				PRINTFORML 「我知道的哦。用蜜润湿从龟头开始抚摸，%TEXTR(@"%CALLNAME:MASTER%/你")%会感到舒服的呢❤」
				PRINTFORML 莉格露的手指带来的快感，像消化液一样溶解着%CALLNAME:MASTER%的思维…
			CASE 5
				PRINTFORML 「呼，呵呵。呵呵呵。就算这么猛烈，也不会痛的呢」
				PRINTFORML 「对于%TEXTR(@"%CALLNAME:MASTER%/你")%的阴茎，我什么都知道的哦❤ 射精也是，随你想怎么做❤」
			ENDSELECT
			PRINTW 
		CASE IS >= 2
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「我明白了。擦动这里就像触摸幼虫一样，会流出蜜汁呢♪」
			CASE 1
				PRINTFORML 「这样用手指画圈擦和握着擦，%CALLNAME:MASTER%你更喜欢哪个？」
				PRINTFORML 「我会选择让你感到舒服的方式哦？告诉我吧」
			CASE 2
				PRINTFORML 「嘿嘿，或许我稍微变得更熟练了？」
				PRINTFORML 「%TEXTR(@"%CALLNAME:MASTER%/你")%如果露出享受的表情，我会很开心的♪」
			CASE 3
				PRINTFORML 「因为变成妖怪的身体，才能做这样的事，真是感激不尽♪」
				PRINTFORML 「如果还是虫子的话？嗯…在%TEXTR("生殖器/阴茎")%上爬来爬去之类的？」
			CASE 4
				PRINTFORML 「从尖端流出蜜汁…那就是感到舒服的意思呢♪」
			ENDSELECT
			PRINTW 
		CASEELSE
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「对你太温柔了会不舒服吗？嗯，这很难…」
			CASE 1
				PRINTFORML 「%TEXTR(@"%CALLNAME:MASTER%/你")%的%TEXTR("生殖器/阴茎")%，真的很硬呢…❤」
			CASE 2
				PRINTFORML 「这样做可以吗…？如果感到舒服了，请告诉我」
			CASE 3
				PRINTFORML 「只用手擦动就可以吗？嗯…会不会想要交配呢…」
			CASE 4
				PRINTFORML 「手会被烫伤呢…%TEXTR(@"%CALLNAME:MASTER%/你")%的…真的很硬又热呢…❤」
			ENDSELECT
			PRINTW 
		ENDSELECT
	ELSEIF TALENT:愛欲 || TALENT:炮友
		SELECTCASE ABL:指
		CASE IS >= 5
			SELECTCASE RAND:7
			CASE 0
				PRINTFORML 「虽然我也喜欢交配，但我同样喜欢欺负%TEXTR(@"%CALLNAME:MASTER%/你")%的身体❤」
			CASE 1
				PRINTFORML 「虽然顶端像幼虫，但是竿部分像甲壳一样…很有趣呢♪」
			CASE 2
				PRINTFORML 「嘿嘿，是要射精吗？还是想在我体内射出来？❤」
			CASE 3
				PRINTFORML 「我已经看穿了你的弱点❤」
				PRINTFORML 「看，只是轻轻地刺激这里就变得湿湿的了❤」
			CASE 4
				PRINTFORML 「嘿，告诉我吧。是想要我用手吗？」
				PRINTFORML 「老实回答，我会一直帮你到最后的。所以，请告诉我…❤」
				PRINTFORML 莉格露的手指灵巧地动了起来，让%CALLNAME:MASTER%的阴茎处于兴奋与折磨之中…
			CASE 5
				PRINTFORML 「啊哈哈♪你好害羞呀。好可爱❤」
				PRINTFORML 「该怎么办呢，要让你射出来吗♪」
			CASE 6
				PRINTFORML 「嘿嘿，看着你的表情，摩擦这里真有趣♪」
				PRINTFORML 「你这样的表情让我更想欺负你了♪」
			ENDSELECT
			PRINTW 
		CASE IS >= 2
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「我渐渐明白了如何处理%TEXTR(@"%CALLNAME:MASTER%/你")%这里了♪」
			CASE 1
				PRINTFORML 「嘿，这样做感觉很舒服吧？看，它已经变硬了♪」
			CASE 2
				PRINTFORML 「蜜液从顶端开始流出来了。看起来很美味呢…♪」
			ENDSELECT
			PRINTW 
		CASEELSE
			SELECTCASE RAND:3
			CASE 0
