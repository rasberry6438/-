				CALL HPH_PRINT,@"「交配…HPH 交配…HPH 交配…HPH 交配…HPH」","L"
				PRINTFORML 莉格露的眼神失去了理智，似乎只能想到进行交配…
			CASE 1
				CALL HPH_PRINT,@"「喜欢交配…HPH 很舒服的…HPH」","L"
				PRINTFORML 莉格露像贪婪地捕食一样，用炽热湿润的阴道肉体对待着%TINKO(PLAYER)%…
			CASE 2
				CALL HPH_PRINT,@"「要生孩子…HPH 好吗…HPH 很好吧…HPH」","L"
				PRINTFORML 莉格露一边用力拍打着屁股，发出清脆的声音，一边陶醉在交配的快感中…
			ENDSELECT
			RESETCOLOR
			RETURN 1
		ELSE
			SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「逃不掉的…HPH %TEXTR(@"%CALLNAME:MASTER%/你")%的精子都是我的…HPH」","L"
				PRINTFORML 莉格露用双手按住%CALLNAME:MASTER%的胸部，本能地用淫乱的腰部摆动来抽插…
			CASE 1
			CASE 2
			ENDSELECT
			RETURN 1
		ENDIF
	ELSE
		IF TALENT:MASTER:形状 == 4
			RETURN 1
		ELSE
			RETURN 1
		ENDIF
	ENDIF
RETURN 1

