;==================================================
;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K104_100
CALL M_KOJO_MESSAGE_COM_K104_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K104_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		IF ABL:受虐属性 < 2
			IF TALENT:恋慕 || TALENT:愛欲
				PRINTFORML 「好痛！　等等、%CALLNAME:MASTER%！？」
				PRINTFORMW 「嗯～，如果是%CALLNAME:MASTER%的兴趣的话，也可以……不要那么用力哦？」
			ELSE
				PRINTFORML 「哇超痛！」
				PRINTFORMW 「我没有那方面的兴趣啦！　高抬贵手啊～」
			ENDIF
		ELSE
			IF TALENT:恋慕 || TALENT:愛欲
				PRINTFORML 「啊、哈啊%UNICODE(0x2665) *1%　好痛……%UNICODE(0x2665) *1%」
				PRINTFORMW 「……再、更用力一点也可以哦%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORML 「呜呜%UNICODE(0x2665) *1%　喂，你在敲哪里啊！」
				PRINTFORMW 「嘛……因为有点舒服，倒也是没什么呢～」
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

