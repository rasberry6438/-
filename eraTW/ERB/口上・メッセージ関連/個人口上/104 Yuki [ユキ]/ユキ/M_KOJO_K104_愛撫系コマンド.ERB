;20,接吻
;==================================================
@M_KOJO_MESSAGE_COM_K104_20
CALL M_KOJO_MESSAGE_COM_K104_20_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K104_20_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		PRINTFORML 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		IF TALENT:恋慕 || TALENT:愛欲
			PRINTFORML 「哇噗……嗯呼呼～%UNICODE(0x2665) *1%」
			PRINTFORMW 「……哈啊……唔姆、感觉不错呢%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORML 「呼、唔姆……呜呜」
			PRINTFORMW 「噗、哈啊……干什么啊、突然这样」
		ENDIF
	ELSEIF EXP:TARGET:接吻経験 == 1
		PRINTFORML 「啊～……接吻还是第一次啊」
		PRINTFORMW 「好像，说不定有点不可思议的感觉%UNICODE(0x2665) *1%」
			SIF TALENT:恋慕 || TALENT:愛欲
				PRINTFORMW 「……是和%CALLNAME:MASTER%一起的真是太好了%UNICODE(0x2665) *1%」
	ELSEIF TALENT:恋慕 || TALENT:愛欲
		PRINTFORML 「嗯%UNICODE(0x2665) *1%　嗯……（舔）、啾」
		PRINTFORMW 「哈啊—……嗯、嗯……%UNICODE(0x2665) *1%」
	ELSE
		PRINTFORML 「嗯……呜……噗啊」
		PRINTFORMW 「啊～，总感觉……好像认真起来了」
	ENDIF
ENDIF
RETURN 1


