;==================================================
@M_KOJO_MESSAGE_SCOM_K33_1
CALL M_KOJO_MESSAGE_SCOM_K33_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K33_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		CALL K33_TATARI_COUNTER(3)
		RETURN 1
	ELSEIF NOWEX:TARGET:Ｃ絶頂
		PRINTDATAW
			DATAFORM 「嗯%UNICODE(0x2764)%　不行、我要去了%UNICODE(0x2764)%」
			DATAFORM 「啾……嗯噗……！　呀啊……要先、先去了……呼啊啊啊%UNICODE(0x2764)%」
			DATAFORM 「好、再激烈一点……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF NOWEX:MASTER:射精
		PRINTDATAW
			DATAFORM 「嗯啾、啾噗……诶嘿嘿、要去了？　好啊、射出来吧射出来吧♪」
			DATAFORM 「嗯、手停下了哦ー？　看上去要射了呢……呼呼%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF NOWEX:MASTER:射精 && NOWEX:TARGET:Ｃ絶頂
		PRINTDATAW
			DATAFORM 「呀……我也、要去了……%UNICODE(0x2764)%　呐、一起去吧……嗯、啾……呼啊啊%UNICODE(0x2764)%」
			DATAFORM 「嗯ー、rero……呼呀%UNICODE(0x2764)%　真是的、不要再忍了、快点射出来吧……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:炮友
		PRINTDATAW
			DATAFORM 「呀呀啊嗯%UNICODE(0x2764)%　嗯……、我不会输的♪　嗯啾、呜咕……啾呜呜%UNICODE(0x2764)%」
			DATAFORM 「呀嗯、会是谁先、去呢ー%UNICODE(0x2764)%」
			DATAFORM 「嗯、啾……哈咿……好舒服啊、再多做点……%UNICODE(0x2764)%」
		ENDDATA
		RETURN 1
	ELSE
		PRINTFORMW 「呀呀啊嗯%UNICODE(0x2764)%　嗯……、我不会输的♪　嗯啾、呜咕……啾呜呜%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
ENDIF
