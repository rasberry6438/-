LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		CALL K33_TATARI_COUNTER(2)
	ELSEIF CFLAG:睡眠
		CALL K33_TATARI_COUNTER(2)
	ELSEIF CFLAG:诶嘿嘿 == 2
		SELECTCASE TCVAR:20
			CASE 35
				PRINTDATAW
					DATAFORM 「怎么样？　虽然我觉得给你看没什么好害羞的……呀嗯、不要把手指插进去咕咻咕咻地搅动啊%UNICODE(0x2764)%」
					DATAFORM 「来吧……再用手指叭滋叭滋地抽动%UNICODE(0x2764)%」
					DATAFORM 「呼呼、完全入迷了呢……做更多也是可以的哦%UNICODE(0x2764)%」
				ENDDATA
				RETURN 1
			CASE 70
				IF TALENT:恋慕 || TALENT:愛欲 || TALENT:炮友
					IF NOWEX:Ｃ絶頂 || NOWEX:Ｖ絶頂
						PRINTDATAW
							DATAFORM 「呀%UNICODE(0x2764)%　再来、再来」
							DATAFORM 「%CALLNAME:MASTER%的舌头和手指、好灵活、要去了……%UNICODE(0x2764)%」
							DATAFORM 「没关系、那里、再激烈一点……%UNICODE(0x2764)%」
						ENDDATA
						RETURN 1
					ELSE
						PRINTDATAW
							DATAFORM 「%CALLNAME:MASTER%……很熟练哦%UNICODE(0x2764)%」
							DATAFORM 「呼呼……我的确感受到了、你在全神贯注地进攻呢%UNICODE(0x2764)%」
							DATAFORM 「呀嗯%UNICODE(0x2764)%　真是毫不留情……呼呼%UNICODE(0x2764)%」
						ENDDATA
						RETURN 1
					ENDIF
				;それ以外
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%的舌头、淫荡地到处乱爬呢……%UNICODE(0x2764)%」
					RETURN 1
				ENDIF
			CASE 87, 89
				CALL INSERT_KOJO_K33(1, 0, 1)
			CASEELSE
				IF NOWEX:TARGET:Ｖ絶頂
					PRINTDATAW
						DATAFORM 「呀%UNICODE(0x2764)%……已经、要去了、所以、不要停哦%UNICODE(0x2764)%」
						DATAFORM 「呼啊……大概是%CALLNAME:MASTER%的缘故吧、所以这么有感觉……呐……明白了吗？」
						DATAFORM 「啊啊%UNICODE(0x2764)%　快要去了、呀……♪」
					ENDDATA
				ELSE
					PRINTDATAW
						DATAFORM 「呀呜……%CALLNAME:MASTER%　手指、好H%UNICODE(0x2764)%」
						DATAFORM 「不要让我发出下流的声音呀……是故意的吧、真是的%UNICODE(0x2764)%」
						DATAFORM 「啊……不要啊……要变得乱七八糟了哟……%UNICODE(0x2764)%」
					ENDDATA
				ENDIF
				RETURN 1
		ENDSELECT
	ELSE
		IF TALENT:恋慕 || TALENT:愛欲 || TALENT:炮友
			IF NOWEX:TARGET:Ｖ絶頂
				PRINTDATAW
					DATAFORM 「不行%UNICODE(0x2764)%……已经、要去了……啊啊%UNICODE(0x2764)%」
					DATAFORM 「呼啊……大概是%CALLNAME:MASTER%的缘故吧、所以这么有感觉……呐……明白了吗？」
					DATAFORM 「啊啊%UNICODE(0x2764)%　要在这种地方、去了……♪」
				ENDDATA
			ELSE
				PRINTDATAW
					DATAFORM 「呀呜……　真是的、竟然在这种地方做%UNICODE(0x2764)%」
					DATAFORM 「不要让我发出下流的声音呀……是故意的吧、真是的%UNICODE(0x2764)%」
					DATAFORM 「啊呜……你是想把我搞得黏糊糊的一团糟吧%UNICODE(0x2764)%」
				ENDDATA
			ENDIF
			RETURN 1
		;それ以外
		ELSE
			IF NOWEX:TARGET:Ｖ絶頂
				PRINTFORMW 「呀%UNICODE(0x2764)%……已经、要去了、所以、不要停哦%UNICODE(0x2764)%」
			ELSE
				SELECTCASE ABL:Ｖ感覚
					CASE IS >= 5
