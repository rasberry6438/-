;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K100_140
CALL M_KOJO_MESSAGE_COM_K100_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K100_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;舌技術５以上
	IF ABL:舌 >= 5
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL %CALLNAME:MASTER%没有做任何预告、便在%CALLNAME:TARGET%的喉咙深处射精了
					CALL HPH_PRINT,@"「咕噗、呜嗯HPH 咕嘟……咕嘟……HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%露出了淫荡的表情、将灌进去的精液全部收到了胃中……
				CASE 1
					CALL HPH_PRINT,@"「呜、呜HPH 啾噗HPH 咕啵HPH」","L"
					PRINTFORMDL 就像以后背位侵犯的那样、抓住%CALLNAME:TARGET%的脸用腰叩打着
					PRINTFORMDW %CALLNAME:TARGET%没有一句抱怨的话语、顺从地接受了那个行为……
				CASE 2
					CALL HPH_PRINT,@"「咳噗、呼HPH 欸HPH 唔嗯HPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%的喉咙深处、被注入了大量滚烫的精液
					PRINTFORMDW %CALLNAME:TARGET%将手环抱在%CALLNAME:MASTER%的腰上、将精液一滴不剩地喝尽了……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「呼呜HPH 呼呜HPH 唔卟、唔嗯嗯HPH」","L"
					PRINTFORMDL 稍微粗暴地对待也不会坏掉也是%CALLNAME:TARGET%的一个优点
					PRINTFORMDW %CALLNAME:MASTER%按照自己的欲望、『使用』了%CALLNAME:TARGET%的喉咙
				CASE 1
					CALL HPH_PRINT,@"「哈、咳咳……哈啊……哈啊……HPH」","L"
					PRINTFORMDL 即使被泪水与鼻涕、以及唾液一起把脸弄得乱七八糟也好、%CALLNAME:TARGET%依旧向着这边微笑着
					PRINTFORMDW 如果把她卖做奴隶的话、一定能卖个好价格的吧……
				CASE 2
					CALL HPH_PRINT,@"「嗯呼、啾噜HPH 呼呜、咕噗HPH」","L"
					PRINTFORMDL 即使被粗暴地侵犯了喉咙、%CALLNAME:TARGET%也不忘侍奉
					PRINTFORMDW 为了让%CALLNAME:MASTER%能舒服起来、用上了她所掌握的所有技巧照顾着阴茎
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「呼！？ 哦、嗯、咕……呜！」
					PRINTFORMDL 喉咙深处的阴茎痉挛着、直接向食道内注入了精液
					PRINTFORMDW 因缺氧而无力的%CALLNAME:TARGET%、被%CALLNAME:MASTER%当做飞机杯使用着
				CASE 1
					PRINTFORML 「嗞卟！ 呜嗯！ 唔嗯嗯！ ――呜呜！！！」
					PRINTFORMDL 仅仅只是为了让自己能够舒服起来、%CALLNAME:MASTER%便强奸着%CALLNAME:TARGET%的喉咙
					PRINTFORMDW %CALLNAME:TARGET%放弃了抵抗、只是等待着结束……
				CASE 2
					PRINTFORMDL 到达极限的%CALLNAME:MASTER%把%CALLNAME:TARGET%的头拉了过来、就这样向着喉咙深处射精了
					PRINTFORML 「哈噗、呜！ 嗯、咕、咳咳！ 咳咳！」
					PRINTFORMDW 拔出阴茎后、从鼻子里流出精液的%CALLNAME:TARGET%激烈地咳嗽着……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「唔卟！？ 呴！ 呼咕！？」
					PRINTFORMDL 紧紧抓住%CALLNAME:TARGET%的头、完全不考虑她的負担叩动着腰
					PRINTFORMDW %CALLNAME:TARGET%陷入了完全的混乱、眼睛翻起了白眼……
				CASE 1
					PRINTFORML 「哦咕、欸！？ 诶呜！ 呜呜！」
					PRINTFORMDL 无视了因为痛苦而拼命摇动着脚的%CALLNAME:TARGET%、遵从着欲望叩动着腰
					PRINTFORMDW %CALLNAME:TARGET%的脸被泪水与鼻涕弄得乱七八糟……
				CASE 2
					PRINTFORML 「哈、哈、咳咳！ 咳咳！ 哈啊……哈啊……」
					PRINTFORML 「为、什么……要这样……咳咳……」
					PRINTFORMDW %CALLNAME:TARGET%就像被虐待的宠物一样仰望着%CALLNAME:MASTER%……
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

