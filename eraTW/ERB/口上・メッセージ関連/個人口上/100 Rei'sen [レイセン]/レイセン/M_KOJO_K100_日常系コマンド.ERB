#DIM 正解
#DIM 経験値
#DIM 制限時間
#DIM 倍率
#DIM FEINT
#DIM イカサマ
VARSET LOCAL
イカサマ = 0
制限時間 = 2000 - (ABL:100:戦闘能力 - ABL:MASTER:戦闘能力) * 100

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;３分の１の確率で特別な稽古
	IF !RAND:3
		;特訓デイリーからお借りしました
		PRINTFORMDL %CALLNAME:MASTER%和%CALLNAME:TARGET%决定练习如何在战场上生存下来
		PRINTFORML 「欸欸、你要让我開枪吗……那样会很危险\@ !TALENT:恋慕 ? 的说 # \@……」
		PRINTFORMDW %CALLNAME:TARGET%似乎不太愿意……
		RESETCOLOR
		PRINTFORML 
		CALL ASK_YN("没关系！做吧","还是算了吧")
		IF RESULT
			CALL M_KOJO_COLOR_K100
			PRINTFORML 「呼……」
			PRINTFORMDW 因为%CALLNAME:MASTER%打消了做这件危险的事的念头、%CALLNAME:TARGET%松了一口气
			RESETCOLOR
		ELSE
			正解 = RAND:4
			経験値 = MIN(ABL:100:戦闘能力 * 5, 5)
			SELECTCASE RAND:20
				;漢字
				CASE IS <= 11
					LOCALS:0 = 上
					LOCALS:1 = 右
					LOCALS:2 = 左
					LOCALS:3 = 下
					倍率 = 100
				;ひらがな
				CASE 12 TO 15
					LOCALS:0 = うえ
					LOCALS:1 = みぎ
					LOCALS:2 = ひだり
					LOCALS:3 = した
					倍率 = 100
				;カタ喀娜
				CASE 16,17,18
					LOCALS:0 = ウエ
					LOCALS:1 = ミギ
					LOCALS:2 = ヒダリ
					LOCALS:3 = シタ
					倍率 = 100
				;スワヒリ語
				CASE 19
					LOCALS:0 = juu
					LOCALS:1 = kulia
					LOCALS:2 = kushoto
					LOCALS:3 = chini
					倍率 = 320
			ENDSELECT
			;你の戦闘能力が高いと５分の一の確率でフェイントをかけてくる
			IF ABL:MASTER:戦闘能力 >= 5 && !RAND:5
				FEINT = 1
				制限時間 += 400
				倍率 += 80
			ELSE
				FEINT = 0
			ENDIF
			CALL FISHER_YATES_SHAFFLE(4)
			PRINTFORMW 躲避%CALLNAME:TARGET%的攻击（点击開始）
			PRINTFORM %CALLNAME:TARGET%的
			SETCOLOR C_YELLOW
			IF FEINT
				PRINTFORM %LOCALS:(RAND:4)%
				RESETCOLOR
				PRINTFORM 假象%LOCALS:(3 - 正解)%
			ELSE
				PRINTFORM %LOCALS:(3 - 正解)%
				RESETCOLOR
			ENDIF
			PRINTFORML 枪击！
			$LOOP_IKASAMA
			FOR LOCAL:10,0,4
				LOCAL = SHAFFLE_ARRAY:(LOCAL:10)
				PRINTFORML [{LOCAL}] - 避開%LOCALS:LOCAL%
			NEXT
			SIF !イカサマ && BASE:MASTER:TSP > 500
				PRINTFORML [{4}] - 時間停止
			TINPUT 制限時間, -1, 1
			IF RESULT == 正解
				CALL M_KOJO_COLOR_K100
				IF イカサマ
					PRINTFORMDL %CALLNAME:MASTER%靠耍滑头躲过了攻击
				ELSE
					PRINTFORMDL %CALLNAME:MASTER%躲过了枪击！
				ENDIF
				PRINTFORML 「好、好厉害……我真的是想打中你才開枪的」
				PRINTFORMDL %CALLNAME:TARGET%说着非常可怕的话
				PRINTFORMDW ……难道是被讨厌了吗
				RESETCOLOR
				BASE:MASTER:体力 -= 200 * (1 + ABL:100:戦闘能力)
				経験値 = 経験値 * 倍率 / 100
				EXP:MASTER:戦闘経験 += 経験値
				EXP:100:戦闘経験 += 経験値 * 2
				CFLAG:100:好感度 += 経験値 * 2
				CFLAG:100:信賴度 += 経験値 * 2
				PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:MASTER%）
				PRINTFORML 戦闘経験 ＋{経験値 * 2}（%CALLNAME:TARGET%）
				PRINTFORML 好感度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
				PRINTFORML 信賴度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			ELSEIF RESULT == 4 && !イカサマ && BASE:MASTER:TSP > 500
				イカサマ = 1
				倍率 /= 5
				PRINTFORMW %CALLNAME:MASTER%通过时间停止能力看清了射线！
				GOTO LOOP_IKASAMA
			ELSE
				SETCOLOR C_RED
				FONTBOLD
				PRINTFORML 
				PRINTFORML 砰
				PRINTFORMW 
				FONTREGULAR
				RESETCOLOR
				CALL M_KOJO_COLOR_K100
				PRINTFORML 「诶诶诶诶诶诶诶！！？」
				PRINTFORMDL 没有躲開来自%CALLNAME:TARGET%的攻击
				PRINTFORMDL 灼烧般的疼痛让%CALLNAME:MASTER%连站都站不起来……头痛……又…想吐……
				PRINTFORML 「血、血流出来了！ 必须要快点急救！」
				PRINTFORMDL %CALLNAME:TARGET%为了救%CALLNAME:MASTER%的命而拼命地抢救着
				PRINTFORMDW 剩下的就交给她吧。%CALLNAME:MASTER%闭上眼睛、就这样失去了意识……
				RESETCOLOR
				BASE:MASTER:体力 -= 1000 * (1 + ABL:100:戦闘能力)
				PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:TARGET%）
				EXP:100:戦闘経験 += 経験値
				CFLAG:100:好感度 += 経験値 / 2
				CFLAG:100:信賴度 += 経験値 / 2
				PRINTFORML 好感度 ＋{経験値}（%CALLNAME:TARGET%）
				PRINTFORML 信賴度 ＋{経験値}（%CALLNAME:TARGET%）
			ENDIF
		ENDIF
	;通常
	ELSE
