;-------------------------------------------------
;名前を教えるイベント（関数）
;納茲琳口上からお借りしました
;-------------------------------------------------
@NAME_SET_K100
 PRINTL 请输入你的名字(敬称を付けたい場合はそれ込みで入力してください)
 IF NAME:MASTER != "你"
	LOCALS = %CALLNAME:MASTER%
ELSE
	LOCALS = %NAME:MASTER%
ENDIF
PRINTBUTTON @"[%LOCALS%と名乗る]", @"%LOCALS%"
PRINTL 　
;名前を入力するループ
$INPUT_LOOP_ONAMAE
INPUTS
; 行頭行尾の空白文字削除
LOCALS = %RESULTS%
REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
;10文字以上は受け付けない
IF STRLENS(LOCALS) > 10
	CLEARLINE 1
	REUSELASTLINE 名前が長すぎます
	GOTO INPUT_LOOP_ONAMAE
;空白は受け付けない
ELSEIF RESULTS == ""
	PRINTL 请输入你的名字
	GOTO INPUT_LOOP_ONAMAE
ELSE
	PRINTFORML 就决定是%RESULTS%了吗？
	PRINTL [0] 是的
	PRINTL [1] 不是
	;入力した名前を確定するループ
	$INPUT_LOOP_KAKUTEI_1
	INPUT
	IF RESULT == 0
		;你の名前を%MASTERNAME:100%に格納
		MASTERNAME:100 = %RESULTS%
		PRINTFORML 
	ELSEIF RESULT == 1
		PRINTL 请输入你的名字
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTL 请输入你的名字
		GOTO INPUT_LOOP_ONAMAE
	ENDIF
ENDIF

