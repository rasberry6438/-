@M_KOJO_MESSAGE_COM_K100_330_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:時間停止
		PRINTFORML 
		RETURN 1
	;被逆推
	ELSEIF CFLAG:诶嘿嘿 == 2
		PRINTFORML
		CALL AUTO_AEGI
		RETURN 1
	;失敗
	ELSEIF TFLAG:193 <= 0
		PRINTFORMDL %CALLNAME:MASTER%大步走向了%CALLNAME:TARGET%、毫不犹豫地卷起了裙子
		PRINTFORML 「Kya啊啊！？」
		;穿いてる
		IF TEQUIP:0 > 1
			PRINTFORMDW 因为看到了穿着可爱的内裤、%CALLNAME:MASTER%感到十分满足
		;穿いてない
		ELSE
			PRINTFORMDW 没看到应该在这里的内裤、能清清楚楚地看见一丝不挂的秘所……
		ENDIF
	;膣内精液1000以上
	ELSEIF 充填率(TARGET,1) >= 100
		PRINTFORML 「\@ TALENT:恋慕 ? 呼呼、说不定会怀上小宝宝呢？ # 呜呜…要是怀上了小宝宝该怎么办啊…… \@」
		;穿いてる
		IF TEQUIP:0 > 1
			PRINTFORMDL %CALLNAME:MASTER%将裙子卷起来后、已经完全湿透的%CALLNAME:TARGET%的内裤暴露了出来
			PRINTFORMDW 每当内裤没法承受的种子汁顺着大腿往下滴的时候、%CALLNAME:TARGET%的身体都会颤抖着……
		;穿いてない
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%将裙子卷起来后、因爱液和种子汁而黏糊糊地濡透的秘所暴露了出来
			PRINTFORMDW 潺潺作响不断溢出的精液拉出了粘稠的丝、在脚下形成了白浊的水洼……
		ENDIF
	;通常
	ELSE
		IF ABL:露出癖 <= 2
			PRINTFORML 「\@ TALENT:恋慕 ? 会被谁看到的啦、只能看一会儿哟……？ # 果、果然还是很害羞的说…… \@」
			;穿いてる
			IF TEQUIP:0 > 1
				PRINTFORMDW %CALLNAME:MASTER%抑制住兴奋的同时、仔细地鉴赏着%CALLNAME:TARGET%可爱的内裤
			;穿いてない
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%将裙子卷起来后、没想到%CALLNAME:TARGET%竟然是真空状态
				PRINTFORMDW 『为什么你没有穿』%CALLNAME:MASTER%这么说着后、%CALLNAME:TARGET%满脸通红的没法说话反驳
			ENDIF
		ELSEIF ABL:露出癖 <= 4
			PRINTFORML 「ぅぅ……」
			PRINTFORMDL 被%CALLNAME:MASTER%拜托后、%CALLNAME:TARGET%双手微微颤抖着露出了裙子内部的风景
			;穿いてる
			IF TEQUIP:0 > 1
				PRINTFORMDL %CALLNAME:MASTER%蹲下后、充分地视奸着%CALLNAME:TARGET%可爱的内裤……
			;穿いてない
			ELSE
				PRINTFORMDL %CALLNAME:MASTER%说着『是忘记穿内裤了吗』揶揄着、%CALLNAME:TARGET%因为过于羞耻羞得低下了头
			ENDIF
			PRINTFORMW 「这样子我…就只是个变态了……」
		ELSE
			CALL HPH_PRINT,@"「哈啊…哈啊……HPH」","L"
			PRINTFORMDL 在%CALLNAME:MASTER%的命令下、%CALLNAME:TARGET%粗重地呼吸着、自己掀起了裙子
			;穿いてる
			IF TEQUIP:0 > 1
				PRINTFORMDL 是因为露出而感觉到兴奋了吗、\@ HAS_PENIS(TARGET) ? 硬硬地勃起的阴茎从内裤中探出头、垂流着先走汁的口水 # 暴露在外的内裤的股间接触部分染上了爱液的污渍 \@……
			;穿いてない
			ELSE
				PRINTFORMDL \@ HAS_PENIS(TARGET) ? 由于露出的兴奋令%CALLNAME:TARGET%的阴茎已经勃起到足以掀起裙子的程度 # 明明是没穿内裤的真空状态、%CALLNAME:TARGET%非但不害羞还因为被看到而兴奋得股间湿润了 \@……
			ENDIF
			CALL HPH_PRINT,@"「我…因为被看着而兴奋了……HPH」","W"
			PALAM:潤滑 += 500
		ENDIF
	ENDIF
ENDIF
RETURN 1
