;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K53_80
CALL M_KOJO_MESSAGE_COM_K53_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K53_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;２４時間以上風呂に入っていない場合咎められる
		IF DAY * 1440 + TIME - EX:MASTER:上次的入浴時间 > 1440
			PRINTFORMW 「嗚哇！等下這個、這個好好洗過了沒？讓我握住這樣的東西真是低級呢ー」
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		CALL K53_AUTO_PVOICE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

