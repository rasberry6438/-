RETURN RESULT

@M_KOJO_MESSAGE_COM_K103_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
		PRINTFORMW 「想用手做是么？」
		PRINTFORMW 「唔…是、这样么…？」
		PRINTFORML 
		PRINTFORMDL 说再稍微用力一点也没关系，梦子就用力地搓弄了起来
		PRINTFORMW 
		PRINTFORMW 「唔…感觉怎样…？ 舒服么…？」
		PRINTFORML
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE M_KOJO_K103_RANDNR(4,80)
		CASE 1
			PRINTFORMW 「嗯咻…嗯咻……感觉怎样？ 还舒服么？%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「呼呼…%M_KOJO_K103_GET("阴茎")%好暖和%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「上上下下…上上下下……%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「用手掌这样套弄龟头也很舒服吧…？%M_KOJO_K103_HEART()%」
		ENDSELECT 
		SIF NOWEX:MASTER:射精
			PRINTFORMW 「啊…膨胀起来了…要出来了么？可以哦…让我看看呐……%M_KOJO_K103_HEART()%」
		PRINTFORML
		RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		SELECTCASE M_KOJO_K103_RANDNR(4,80)
		CASE 1
			PRINTFORMW 「嘿哟…嘿哟……怎样？ 我做得好吗…」
		CASE 2
			PRINTFORMW 「挺温暖的呢…这个…气味也……」
		CASE 3
			PRINTFORMW 「嗯…哟………手臂有点累了…」
		CASEELSE
			PRINTFORMW 「顶端的部分也想要被玩弄吗…？ 在、这样…？」
		ENDSELECT 
		SIF NOWEX:MASTER:射精
			PRINTFORMW 「啊…胀起来了…要射了吗？ 诶…诶…」
		PRINTFORML
		RETURN 1
	ENDIF
ENDIF
RETURN 1

