;==================================================
;150,雙乳間抽插
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K103_150
RETURN 1

@M_KOJO_MESSAGE_COM_K103_150
CALL M_KOJO_MESSAGE_COM_K103_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K103_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;谷間潤滑チェック
	LOCAL:2 = M_KOJO_K103_B_WET()
	;地の文を移動させているため口上色を変更
	SIF FLAG:口上色
		CALL M_KOJO_COLOR_K103
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
		PRINTFORML 「啊,轻点轻点,不要那么粗暴地对待我的胸部!」
		PRINTFORMW 「啊,啊~不行了!!」
		PRINTFORMW 「呀...不要揉我的乳头...啊%M_KOJO_K103_HEART()%」
		PRINTFORML
		RETURN 1
	ENDIF
	;地の文 意図に沿わないので持ってきた
	IF SELECTCOM == PREVCOM
		PRINTFORMD %CALLNAME:PLAYER%は
		SIF FLAG:70 == 1
			PRINTFORMD 時間停止了
		SIF CFLAG:MASTER:恶作剧 == 2
			PRINTFORMD 沉浸在夢鄉中
		PRINTFORMD %CALLNAME:TARGET%の%OPPAI_DESCRIPTION(TARGET,1)%
		IF RAND:2
			IF RAND:3 == 0
				PRINTFORMD 用拇指挠弄她的乳晕
			ELSEIF RAND:2
				PRINTFORMD 用手指抚弄她的%NIPPLE_DESCRIPTION(TARGET,1)%
			ELSE
				PRINTFORMD 开始揉捏起来并
			ENDIF
		ELSE
			IF RAND:4 == 0
				PRINTFORMD 将其向内推挤
			ELSEIF RAND:3 == 0
				PRINTFORMD 紧贴着它们
			ELSEIF RAND:2
				PRINTFORMD 紧紧缠绕住并
			ELSE
				PRINTFORMD 按住并
			ENDIF
		ENDIF

		IF EXP:PLAYER:Ｖ性交経験 + EXP:PLAYER:Ａ性交経験 && RAND:2
			IF RAND:3 == 0
				PRINTD 就像在做性交一样
			ELSEIF RAND:2
				PRINTFORMD 想要使她怀孕般地
			ELSE
				PRINTD 就像在交媾一样
			ENDIF
		ENDIF
		SIF RAND:2
			PRINTFORMD 只要有体力持续下去
		IF ABL:PLAYER:施虐属性 && RAND:3 == 0
			PRINTFORMDL 粗暴地摆动起腰
		ELSEIF RAND:2
			PRINTFORMDL 激烈地动起了腰
		ELSE
			PRINTFORMDL 前后动起了腰
		ENDIF
		
		IF RAND:3
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET)%
		ELSE
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET)%的乳沟
		ENDIF
		
		IF RAND:2 == 0
			PRINTFORMDL 持续侵犯起来…
		ELSE
			PRINTFORMDL 持续侵犯着…
		ENDIF
	ELSE
		PRINTFORMD %CALLNAME:PLAYER%は
		SIF FLAG:70 == 1
			PRINTFORMD 時間停止了
		SIF CFLAG:MASTER:恶作剧 == 2
			PRINTFORMD 沉浸在夢鄉中
		PRINTFORMD %CALLNAME:TARGET%的
		;貧乳
		IF TALENT:胸圍 == -1
			PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%上蹭动%TINKO(PLAYER)%
		;巨乳
		ELSEIF TALENT:胸圍 == 1
			PRINTFORMD 将%TINKO(PLAYER)%夹在%OPPAI_DESCRIPTION(TARGET,1)%之间上下摩擦
		;爆乳
		ELSEIF TALENT:胸圍 == 2
			PRINTFORMD 将%TINKO(PLAYER)%夹在%OPPAI_DESCRIPTION(TARGET,1)%之间上下摩擦
		;超乳
		ELSEIF TALENT:胸圍 == 3
			PRINTFORMD 将%TINKO(PLAYER)%插入%OPPAI_DESCRIPTION(TARGET,1)%上下摩擦
		ELSE
			PRINTFORMD 将%TINKO(PLAYER)%夹在%OPPAI_DESCRIPTION(TARGET,1)%之间上下摩擦
		ENDIF
		PRINTDL 着…

		PRINTFORMD %CALLNAME:PLAYER%は%CALLNAME:TARGET%の%OPPAI_DESCRIPTION(TARGET)%
		IF RAND:2
			IF RAND:3 == 0
				PRINTFORMD の乳輪を親指でくすぐりながら
			ELSEIF RAND:2
				PRINTFORMD の%NIPPLE_DESCRIPTION(TARGET,1)%を指で扱きながら
			ELSE
				PRINTFORMD 揉捏起来并
			ENDIF
		ELSE
			IF RAND:4 == 0
				PRINTFORMD を内側に寄せながら
			ELSEIF RAND:3 == 0
				PRINTFORMD を密着させながら
			ELSEIF RAND:2
				PRINTFORMD 紧缠住并
			ELSE
				PRINTFORMD 按住并
			ENDIF
		ENDIF

		IF EXP:PLAYER:Ｖ性交経験 + EXP:PLAYER:Ａ性交経験 && RAND:2
			IF RAND:3 == 0
				PRINTD 性交でもするかのように
			ELSEIF RAND:2
				PRINTFORMD 乳を孕ませようと
			ELSE
				PRINTD まるで交尾をしているかの如く
			ENDIF
		ENDIF

		IF ABL:PLAYER:施虐属性 && RAND:3 == 0
			PRINTFORMDW 粗暴的腰を動かした…
		ELSEIF RAND:2
			PRINTFORMDW 激烈的动起了腰た…
		ELSE
			PRINTFORMDW 前後动起了腰た…
		ENDIF

		IF !FLAG:70 && CFLAG:MASTER:恶作剧 != 2
			PRINTFORMD %CALLNAME:TARGET%是胸の谷間を犯され
			;恋慕か親愛（愛欲の有無で稍微分岐）
			IF TALENT:愛欲
				PRINTFORMDL \@ TALENT:恋慕 ? うっとりとした # 蕩けた \@表情をしている…
			ELSE
				PRINTFORMDL ている…
			ENDIF
			IF ABL:PLAYER:技巧 > 2
			;プレイヤーの技巧3以上
				;胸で分岐させて尺寸はSELECTCASEで分けてみる。大きさは違ってもその先に突起はあるんだよ
				SELECTCASE TALENT:胸圍
				;きょにゅー以上
				CASE IS > 0
					IF RAND:2 == 0
						PRINTFORMDL 把丰满的双峰凑上来、用%OPPAI_DESCRIPTION(TARGET,1)%间的峡谷将%TINKO(PLAYER)%夹住、撸动起来。
						PRINTDL 紧密包裹,柔软却坚硬的肉棒被完全吞没,
						PRINTDL 令人沉沦的舒适感,就好像沉浸在肉海之中一般...
					ELSE
						PRINTFORMDL 将%TINKO(PLAYER)%压在那%OPPAI_DESCRIPTION(TARGET,1)%上,充分感受到%CALLNAME:TARGET%丰满胸部的重量,
						PRINTFORMDL %CALLNAME:PLAYER%的%TINKO(PLAYER)%深深插入其中...
						PRINTFORMDL 在%CALLNAME:TARGET%柔软丰满的胸部包裹下,%CALLNAME:PLAYER%沉沦其中...
					ENDIF
						SIF LOCAL:2 >= 3 && LOCAL:2 < 5
							PRINTFORMDL 稍微湿润的%TINKO(PLAYER)%,每次撞击在胸部上都发出咕啾咕啾的声音。
						SIF LOCAL:2 >= 3
							PRINTFORMDL 湿漉漉的%TINKO(PLAYER)%,每次撞击在胸部上都发出滋滋的声音。
					PRINTDL 光滑柔软的双峰不断摩擦着坚硬的阴茎,有着令人沉迷的柔软触感...
					SIF EX:Ｂ絶頂 * 10000 + PALAM:快Ｂ + PALAM:欲情 >= 10000
						PRINTFORMDL 其中粉嫩的%OPPAI_DESCRIPTION(TARGET,1)%前端,%NIPPLE_DESCRIPTION(TARGET,1)%不时地刮蹭着肉棒和龟头...
				;小胸以下
				CASE IS < 0
					PRINTFORMDL 尽管有着体格上的劣势,但正在用经验和技巧来弥补,%OPPAI_DESCRIPTION(TARGET,1)%正在为%TINKO(PLAYER)%服务。
					SIF EX:Ｂ絶頂 * 10000 + PALAM:快Ｂ + PALAM:欲情 >= 10000
						PRINTFORMDL 通过微微发红的胸部前端,%NIPPLE_DESCRIPTION(TARGET,1)%正小心翼翼地引导着肉棒和冠状沟。
				;中等胸围
				CASEELSE
					PRINTFORMDL 用%OPPAI_DESCRIPTION(TARGET,1)%夹住%TINKO(PLAYER)%,上下撸动。
					SIF EX:Ｂ絶頂 * 10000 + PALAM:快Ｂ + PALAM:欲情 >= 10000
						PRINTFORMDL 偶尔会将微微出汗的%OPPAI_DESCRIPTION(TARGET,1)%顶端,%NIPPLE_DESCRIPTION(TARGET,1)%贴在肉棒和冠状沟上,发出甜美的呻吟。
				ENDSELECT
			ELSEIF ABL:PLAYER:技巧 <= 2
			;胸2以下
				;貧乳
				IF TALENT:胸圍 == -1
					PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%で懸命に%TINKO(PLAYER)%を
				;巨乳
				ELSEIF TALENT:胸圍 == 1
					PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%で%TINKO(PLAYER)%を包み込んで
				;爆乳、超乳
				ELSEIF TALENT:胸圍 >= 2
					PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%で%TINKO(PLAYER)%を包み隠すようにして
				ELSE
					PRINTFORMD %OPPAI_DESCRIPTION(TARGET,1)%で%TINKO(PLAYER)%をはさんで
				ENDIF
				PRINTDL 摩擦了起来…
			ELSE

			ENDIF
		ENDIF
	ENDIF
	IF FLAG:70 == 1 || CFLAG:MASTER:恶作剧 == 2
		RETURN 1
	ENDIF
	PRINTDW 
	;基本セット
	;恋慕
	IF TALENT:恋慕
		SELECTCASE M_KOJO_K103_RANDNR(4,150)
		CASE 1
			PRINTFORMW 「啊,不要这么用力啦!」
			PRINTL
			PRINTFORMDL %CALLNAME:TARGET%虽然这么说,但还是把%OPPAI_DESCRIPTION(TARGET,1)%紧紧贴合在一起
			PRINTDW 与肌肤更加亲密接触,快感也相应增强,一种麻痹感漫上心头
			PRINTL
			PRINTFORMW 「呵呵,你看起来好舒服的样子...%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「被抓住乳头晃动,我也,啊...%M_KOJO_K103_HEART()%」
			PRINTFORMW 「%CALLNAME:MASTER%的%M_KOJO_K103_GET("阴茎")%在我胸前乱撞...!」
			PRINTL
			PRINTFORMDL %CALLNAME:TARGET%身体缩紧,%OPPAI_DESCRIPTION(TARGET,1)%的压力也随之增大
			PRINTFORMDW 感觉越来越好,我更加用力地揉捏抓住的%OPPAI_DESCRIPTION(TARGET,1)%乳头
			PRINTL
			PRINTFORMW 「啊,啊%M_KOJO_K103_HEART()%不要,不要啊%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「嗯...唔...%M_KOJO_K103_HEART()%呵呵...胸部舒服吗?」
			PRINTFORMW 「你看起来好认真,表情都乱掉了」
			PRINTFORMW 「啊,轻一点,不要再捏乳头了%M_KOJO_K103_HEART(2)%」
		CASEELSE
			PRINTFORMW 「嗯,啊...%M_KOJO_K103_HEART()%你,觉得舒服吗?」
			PRINTFORMW 「这样做,...怎么样?」
			PRINTL
			PRINTFORMDL %CALLNAME:TARGET%一边说着,一边用手抓住胸部两侧,
			PRINTFORMDW 配合我腰部的动作,上下摇晃着%OPPAI_DESCRIPTION(TARGET,1)%
			PRINTFORMDW 撞击的%TINKO(PLAYER)%根部发出噼啪作响的声音
			PRINTL
			PRINTFORMW 「随时都可以高潮哦,随时都可以%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		SELECTCASE M_KOJO_K103_RANDNR(4,150)
		CASE 1
			PRINTFORMW 「啊,不要这么用力啊!」
			PRINTL
			PRINTFORMDL %CALLNAME:TARGET%微微扭动身体,%OPPAI_DESCRIPTION(TARGET,1)%的压力增加了
			PRINTFORMDW 作为回报,我更加用力地拧动抓住的%OPPAI_DESCRIPTION(TARGET,1)%乳头
			PRINTL
			PRINTFORMW 「啊,轻一点...啊%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「不要拽着乳头晃来晃去,啊,啊%M_KOJO_K103_HEART()%」
			PRINTFORMW 「不要这样啦...啊!」
		CASE 3
			PRINTFORMW 「嗯...唔...」
