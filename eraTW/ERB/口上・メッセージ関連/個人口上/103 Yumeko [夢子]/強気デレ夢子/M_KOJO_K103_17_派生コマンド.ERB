;==================================================
;6,挿入Ｇスポ責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K103_6
CALL M_KOJO_MESSAGE_SCOM_K103_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K103_6_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	;ELSEIF TALENT:恋慕
	;	PRINTFORMW 
	;	RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
			LOCAL:2 = M_KOJO_K103_KANKEI()
			SELECTCASE LOCAL:2
			CASE 10, 11, 12, 13, 21, 22, 23, 24
				;特別な感情は抱いていない時
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「呜嗯……那里、这样搅动的话…」
				CASE 2
					PRINTFORMW 「感觉像是从肚子里被顶起来一样…っ」
				CASE 3
					PRINTFORMW 「…%M_KOJO_K103_AEGI()%…」
				CASEELSE
					PRINTFORMW 「在里面搅动着……发出奇怪的声音了……っ」
				ENDSELECT
				PRINTFORML
			CASE 31,32,33,34,61,62,63,64
				;思慕OR炮友
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「哈啊…っ…呜嗯………」
				CASE 2
					PRINTFORMW 「啊、好深、不行…不行…！」
				CASE 3
					PRINTFORMW 「啊…啊…腰自己动起来了……っ」
				CASEELSE
					PRINTFORMW 「不要在肚子里这么闹腾呀……！」
				ENDSELECT
				PRINTFORML
			CASE 41,42,43,44,51,52,53,54,71,72,73,74
				;恋慕OR愛欲
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
				CASE 2
					PRINTFORMW 「呜嗯嗯嗯…到深处了……っ…%M_KOJO_K103_HEART()%」
				CASE 3
					PRINTFORMW 「…啊、那里、那里啊……！%M_KOJO_K103_HEART()%」
				CASEELSE
					PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()% 这样搅着…腰、腰停不下来……っ…！%M_KOJO_K103_HEART()%」
				ENDSELECT
				PRINTFORML
			ENDSELECT
		RETURN 1
	ENDIF
ENDIF
