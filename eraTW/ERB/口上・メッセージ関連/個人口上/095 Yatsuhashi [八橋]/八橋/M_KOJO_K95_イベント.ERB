	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「…哼！」
			PRINTFORMW 「我还在生气呢！」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW 的事我可不会那么轻易原谅的！」
		ELSE
			PRINTFORMW 「…还是很讨厌」
			PRINTFORM 「但是，
			CALL MASTER_CALLNAME_K95
			PRINTFORMW ，我就稍微原谅你…」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「…还是很讨厌」
			PRINTFORM 「但是，
			CALL MASTER_CALLNAME_K95
			PRINTFORMW ，我就稍微原谅你…」
			PRINTFORMW 「…另外！」
			PRINTFORMW 「我不会一直生气的…」
		ELSE
			PRINTFORMW 「…好啊」
			PRINTFORMW 「只有一点点…原谅你！」
		ENDIF
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
