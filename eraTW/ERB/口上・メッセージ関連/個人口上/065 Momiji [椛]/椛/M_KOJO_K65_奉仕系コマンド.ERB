@M_KOJO_MESSAGE_COM_K65_80

CALL M_KOJO_MESSAGE_COM_K65_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「哇、好热……」
		PRINTFORMDW 椛轻轻握着%CALLNAME:MASTER%的阴茎、有些惊讶地盯着。
		PRINTFORMDL 
		PRINTFORML 「嗯…只要上下摩擦就可以了吧？」
		PRINTFORMW 「那个…我再稍稍用点力可以吗？第一次做、我不知道怎么控制力度…」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;発情
	ELSEIF TCVAR:発情
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「嗯…是这样吗？」
					PRINTFORMDL 椛一边抬眸眼望着%CALLNAME:MASTER%、一边张開嘴伸出了舌头。
					CALL HPH_PRINT,@"「唔嗯……这里…请射在这里……HPH」","W"
					PRINTFORMDL
					CALL HPH_PRINT,@"「精液、请多射一些出来给我HPH」","L"
					PRINTFORMDW 椛像是在故意展示给你看一样地来回摇晃着舌头、索求着精液…
				CASE 1
					PRINTFORML 「啊…一抖一抖的…要射出来了吗…？」
					CALL HPH_PRINT,@"「虽然有点浪费…就这样、在手里咻咻地射出来也可以哟…HPH」", "L"
					PRINTFORMDW 椛加快了手的速度、催促着%CALLNAME:MASTER%射精…
			ENDSELECT
		ELSEIF ABL:精液中毒 > 3 && RAND:2
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「呼哧呼呜、哈啊……HPH%CALLNAME:MASTER%桑的气味好厉害…HPH」","L"
					PRINTFORMDL 椛捧着%CALLNAME:MASTER%的阴茎、怜爱地将其贴在脸上…
					CALL HPH_PRINT,@"「呼呼、随时都可以射出来哦？」","L"
					CALL HPH_PRINT,@"「我、无论是被%CALLNAME:MASTER%桑射到身上也好、还是让我喝下去也好我都非常喜欢……HPH」","W"
				CASE 1
					PRINTFORMDL 椛把脸靠在%CALLNAME:MASTER%的胸前、用柔软的肢体抚摸着他的阴茎…
					CALL HPH_PRINT,@"「撸一撸、再摸一摸…HPH呵呵、接下来、包皮系带…我揉我揉我揉HPH」","L"
					PRINTFORMDL 用手掌来回抚摸龟头、抑或是伸出手指刺激龟头的里筋。
					PRINTFORMDW 椛一边哼着歌一边玩弄着%CALLNAME:MASTER%的阴茎、给人一种连贯性的快感…
					PRINTFORMDL
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%桑、觉得舒服吗？…呼呼HPH看起来很舒服呢HPH」","L"
					CALL HPH_PRINT,@"「其实、我是一边感受着%CALLNAME:MASTER%桑的心音和气味一边慢慢地给你做的哦…」","L"
					CALL HPH_PRINT,@"「像这样用舒服的方式触摸的话、%CALLNAME:MASTER%桑的整个身体都会有敏感的反应呢HPH」","L"
					CALL HPH_PRINT,@"「而且、和%CALLNAME:MASTER%桑靠得这么近、我也開始兴奋起来了呢…、嘻嘻…HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「握着如此\@ TALENT:MASTER:形状 >= 3 ? 炙热魁梧的 # 炙热\@的东西、连我都開始心脏猛跳个不停了…」","L"
					PRINTFORMDW 椛一边扭扭捏捏地交换着盘起的腿、一边用手来回摩擦着%CALLNAME:MASTER%的%TINKO(MASTER)%。
				CASE 1
					CALL HPH_PRINT,@"「撸啊撸啊、撸啊撸啊…HPH啊哈、開始不断硬起来了…HPH」","L"
					PRINTFORMDL 椛紧靠着%CALLNAME:MASTER%、一边在他的耳边不断低语、一边做着手淫。
					CALL HPH_PRINT,@"「欧金金、再变大一点吧…HPH多多地射出来给我吧…HPH」","W"
			ENDSELECT
		ENDIF
		RETURN 1
	;非発情
	ELSE
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			IF ABL:精液中毒 > 3 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「啊…好像要射出来了呢…」
						PRINTFORML 「那么、就多帮你蹭一蹭龟头部位…要射得多一些哦…」
						PRINTFORMDL 椛用期待的眼神不断玩弄着%CALLNAME:MASTER%的阴茎。
						PRINTFORMDW 虽然手淫并不激烈、但都恰到好处地控制在要点上。%CALLNAME:MASTER%很干脆地就达到了高潮…
					CASE 1
						PRINTFORML 「啊哈、看来快要射了呢…」
						PRINTFORMDL 椛跪在%CALLNAME:MASTER%面前、双手握住肉竿、開始刺激起来。
						PRINTFORML 「我会全部、接住的…请把我沾上、%CALLNAME:MASTER%桑的味道吧…」
						PRINTFORMDW 收到了下流请求的%CALLNAME:MASTER%、心中那股想用精液把她做上标记的射精欲一下子高涨起来。
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「诶？要高潮了吗…？」
						IF EXP:精液経験 < 20
							PRINTFORML 「就这样用手摩擦就可以了吗？」
							PRINTFORMW 「明白了。请用我的手舒服起来吧…」
						ELSE
							PRINTFORML 「我知道了、那就这样用手继续摩擦吧？」
							PRINTFORMW 「呵呵、舒服起来了呢、请多多地射出来吧…」
						ENDIF
						IF ABL:膣射中毒 > 2 && RAND:3 == 0
							PRINTFORMDL
							PRINTFORML （其实是想让你射在肉膣里的…）
								PRINTFORMW （不过如果%CALLNAME:MASTER%桑能觉得舒服的话、那就是最好的吧）
						ENDIF 
					CASE 1
						PRINTFORML 「啊、啊啊…膨胀起来了……」
						PRINTFORMDW 椛一边抬眸眼望着因高潮感而身体颤抖的%CALLNAME:MASTER%、一边继续玩弄着%CALLNAME:MASTER%的阴茎…
				ENDSELECT
			ENDIF
		ELSEIF ABL:精液中毒 > 3 && RAND:2
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"（……每撸动一下、气味都会变得更浓…）","L"
					CALL HPH_PRINT,@"（头晕乎乎的……HPH）","L"
					PRINTFORMDW 椛几乎是无意识地把鼻尖凑近铃口、仔细地嗅着%CALLNAME:MASTER%的雄性气息、脸上绽放出笑容…
				CASE 1
					CALL HPH_PRINT,@"「呼啊、呼啊…嗯……HPH」","L"
					PRINTFORMDL 椛小心翼翼地撸动着%CALLNAME:MASTER%的阴茎、尾巴随意的晃来晃去、小巧的屁股也好像有些难掩兴奋地晃动着。
					PRINTFORMDW 微微喷吐出的气息轻抚着阴茎、带来了一种痒痒的快感。
					PRINTFORMDL
					IF RAND:3 == 0
						CALL HPH_PRINT,@"「嗯……只用手来做就能兴奋起来吗什么的、就算你问我这种事情…」","L"
						CALL HPH_PRINT,@"「也、是呢…在这之后该怎么帮你做、之类的…在这之后该你会为我做什么事、之类的」","L"
						CALL HPH_PRINT,@"「……有着很多的期待、然后就兴奋起来了也说不定……HPH」","W"
					ELSE
						PRINTFORMDL 已经无数次记住%CALLNAME:MASTER%的气味的椛、看来光是摩擦阴茎就会使其兴奋起来。
						CALL HPH_PRINT,@"「哈啊…嗯呵呵、%CALLNAME:MASTER%桑…」","L"
						PRINTFORMDW %CALLNAME:MASTER%摸了摸椛的头、椛高兴地眯起眼睛并加快了用手奉仕的速度…
					ENDIF
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「唔嗯、呼呼……%CALLNAME:MASTER%桑、舒服吗？」
					PRINTFORMDW 椛一边仔细地观察着%CALLNAME:MASTER%的反应、一边仔细地抚摸着他的阴茎…
				CASE 1
					PRINTFORML 「呼呼、好热啊…%CALLNAME:MASTER%桑…」
					PRINTFORMDW 椛紧贴着%CALLNAME:MASTER%、一边抚摸着柔软的肢体一边为肉竿奉仕着。
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

