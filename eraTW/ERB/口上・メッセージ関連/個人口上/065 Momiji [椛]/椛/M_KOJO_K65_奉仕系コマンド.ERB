			;それ以外
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「嗯嗯HPH好啊、就这样…就这样、射进我的嘴里…HPH」", "L"
						PRINTFORMDL 椛感觉到了射精的前兆、就紧紧地抿住嘴唇、捋着阴茎。
						CALL HPH_PRINT,@"「嗯姆、呼姆、啾、啾呜呜呜～～～～HPH」", "W"
					CASE 1
