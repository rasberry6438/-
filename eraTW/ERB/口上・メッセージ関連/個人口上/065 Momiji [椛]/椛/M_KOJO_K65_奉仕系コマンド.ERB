	ELSEIF TCVAR:発情
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			;慣れてない
			IF EXP:精飲経験 < 20
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「嗯、姆、啾…要射了吗…？」
						CALL HPH_PRINT,@"「可以、就这样射在嘴里吧…HPH」", "W"
					CASE 1
						CALL HPH_PRINT,@"「啾、呼姆呼姆……要好好地、在我的嘴里射出来…HPH」", "L"
						CALL HPH_PRINT,@"「我会、努力喝下去的、所以…HPH」", "W"
				ENDSELECT
			;精液中毒高め
			ELSEIF ABL:精液中毒 > 3 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「……！！啾姆、嗯呜呜呜呜～～～HPH」", "L"
						PRINTFORMDL 感觉到射精的预兆之后、椛按住了%CALLNAME:MASTER%的腰、将阴茎吞到喉咙深处。
						CALL HPH_PRINT,@"（射出来HPH射出来HPHせ啊、快点…HPH）", "L"
						PRINTFORMDL 椛赤红的眼睛里闪烁着期待的光芒、一边用眼睛向上看、渴求着射精。
						PRINTFORMDW 在强烈的快感的驱使下、%CALLNAME:MASTER%射出了炽热的欲望之块…
					CASE 1
						CALL HPH_PRINT,@"（肉棒不停跳着、快射出来吧…HPH好像很难受呢…HPH）", "L"
						CALL HPH_PRINT,@"「啾啵HPH咕啾HPH嗯姆呜呜HPH嗯呜呜～HPH」", "L"
						PRINTFORMDL 椛一边发出极其下流的声音、一边责备着%CALLNAME:MASTER%的阴茎。
						PRINTFORMDL 每当小脑袋激烈地前后摆动时、银白色的美丽头发就会闪闪发光地摇曳。
						CALL HPH_PRINT,@"（%CALLNAME:MASTER%桑的精液HPH真想喝啊…HPH）", "L"
						PRINTFORMDW 从%CALLNAME:MASTER%的下腹涌出的甘甜麻痹感达到顶点后、精液从怒张的前端迸出…
