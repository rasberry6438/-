;一人目口上
@M_KOJO_MESSAGE_SCOM_K65_9_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;二人目口上中止フラグ設定
	TCVAR:352 = 0
	;極、映姫口上と同じくLOCALに二人目を入れてしまう
	LOCAL = MASTER_POSE(4,1,1)
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMDW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;二人目が極
	ELSEIF LOCAL == 42
		;二人目口上呼ばないフラグON
		TCVAR:352 = 1
		;極・椛ともに百合属性がある
		;PRINTFORML {GETBIT(STAIN:口,污渍_精液)}{GETBIT(STAIN:42:0,4)}　{GETBIT(STAIN:65:0,4)}
		IF ABL:42:百合属性> 0 && ABL:65:百合属性 > 0 && RAND:3 == 0
			;二人とも口内精液が付いてる
			;うまく動かんな
			IF GETBIT(STAIN:42:0,4) && GETBIT(STAIN:65:0,4) && RAND:2
				SETCOLOR 0xA260BF
				CALL HPH_PRINT,@"『舔…我说椛啊、嘴周围还沾着%MASTERNAME:42%的精子…唔、啾』","L"
				RESETCOLOR
				CALL HPH_PRINT,@"「极不也是、还沾在上面哦…舔、啾、啾咕……」","L"
				PRINTFORMDW
				PRINTFORML 极和椛一边交替吮吸阴茎、一边将飞溅到彼此脸上的精液舔食干净
				PRINTFORML 把脸上残留的东西舔干净后、又開始交缠着舌头互相分享着嘴里的白浊液。
				PRINTFORMDW 
				PRINTFORML 「舔、啾、啾呣……%CALLNAME:MASTER%桑…精液、请再多来点……」
				SETCOLOR 0xA260BF
				PRINTFORMW 『要这样、友好的平均分配才对嘛…』
				RESETCOLOR
				STAIN:42:0 = 0
				STAIN:65:0 = 0
			ELSEIF ABL:65:精液中毒 > 3 && ABL:42:精液中毒 > 3 && RAND:2
				CALL HPH_PRINT,@"「嗯……极小姐的那里、\@ EX:42:膣内精液 >= 300 ? 混杂着%CALLNAME:MASTER%桑的那个都黏糊糊的了 # 因为舔着鸡鸡已经变得哔啾哔啾的全是水了 \@呢…」","L"
				PRINTFORMDL 椛的手在极光滑的大腿上抚摸着、又用指尖搔弄起极位于大腿根旁的秘部。
				SETCOLOR 0xA260BF
				PRINTFORML 『嗯呣…椛不也是、只是吞下了%MASTERNAME:42%的精液就已经湿得不成样子了…』
				RESETCOLOR
				PRINTFORMDW 两位少女一边相竞舔舐着%CALLNAME:MASTER%的阴茎、一边用手指在彼此的裂缝上来回扫动…
				PRINTFORMDL
				SETCOLOR 0xA260BF
				PRINTFORML 『哈呣、舔舔、溜噜噜…%MASTERNAME:42%听见了吗？椛的那里响起了咕啾咕啾的声音。真ー淫乱ー』
				RESETCOLOR
				PRINTFORML 「极也没好多少、从刚才開始腰就颤个不停…舔舔、叻噜噜噜、啾溜」
				PRINTFORMDW 美丽的少女们虽然一边在相互摩擦着对方的秘裂、却也没忘记继续拼命地侍奉着你的阴茎…
				PRINTFORMDL
				PRINTFORML 「嗯噗、啾噜、咕啪啊…我们俩之间的谁、或者是%CALLNAME:MASTER%桑…谁会最先高潮呢…？」
				SETCOLOR 0xA260BF
				PRINTFORMW 『呵呵、不会输的、唔、所以…我舔、噜啾…！』
				RESETCOLOR
				;SOURCE:快Ｃとか入れてもいいけどなんか処理が変になりそうなので保留
			ELSE
				CALL HPH_PRINT,@"「啾、叻噜啾、啾噜噜……哈呼啊、咿呀唔……HPH」","L"
				SETCOLOR 0xA260BF
				CALL HPH_PRINT,@"『舔舔、噜溜……呣咪咿……HPH』","L"
				CALL HPH_PRINT,@"『れるれるっ、ぬりゅっ……もみひぃ……HPH』","L"
				RESETCOLOR
