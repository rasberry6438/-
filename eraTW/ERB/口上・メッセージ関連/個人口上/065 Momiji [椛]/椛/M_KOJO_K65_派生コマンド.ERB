;==================================================
;7,挿入子宮口玩弄
;==================================================
@M_KOJO_MESSAGE_SCOM_K65_7
CALL M_KOJO_MESSAGE_SCOM_K65_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K65_7_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF EXP:子宮口開発経験 > 50
			CALL HPH_PRINT,@"「啊啊HPH嗯嗯…？」","L"
			CALL HPH_PRINT,@"「嘿、噢HPH%CALLNAME:MASTER%先生，请等…额嗯啊啊HPH」","L"
			PRINTFORMDW 被已经充分开发的最深处猛烈顶击所带来的强烈快感，椛的眼中充满了白色和黑色…
		ELSE
			PRINTFORML 「咕、嗯…」
			PRINTFORML 「%CALLNAME:MASTER%先生，请、请稍微，不要那么深，顶得太猛了…！」
			PRINTFORMDW 子宫口被推上去的顶撞，让椛发出了一些略显痛苦的声音。
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	ELSE
		SELECTCASE EXP:子宮口開発経験
			;気持ちいい
			CASE IS > 50
				IF TALENT:恋慕 && RAND:2 == 0
					IF TCVAR:発情
						SELECTCASE RAND:2
							CASE 0
								CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
								PRINTFORMDL 椛\@ TEQUIP:65:口接触部位 == 4 ? 一边热切地吻着你的嘴唇，一边 # \@以欣喜的表情自己主动陷入深深的快感，让子宫口和铃口亲吻起来。
								CALL HPH_PRINT,@"「哈啊啊…HPH%CALLNAME:MASTER%先生的小弟弟，太厉害了…HPH一直连接着，好痛快…HPH」","L"
								PRINTFORMDW 被柔软的肉体热度、甜美的呼吸、娇媚的声音所包围。所有的一切都融为一体，椛让%CALLNAME:MASTER%更加兴奋…
							CASE 1
								CALL HPH_PRINT,@"「好、好棒…HPH这、这简直让人发疯啊啊…HPH好舒服嗯HPH%CALLNAME:MASTER%先生，呀HPH」","L"
								PRINTFORMDL 在像是将子宫口撑开一样有力的抽插中，椛发出了半迷糊的快感呼唤…
								IF TALENT:妊娠
									CALL HPH_PRINT,@"「已、已经怀上宝宝了，但还要…想要更多…哼呜呜呜HPH」","W"
								ELSE
									CALL HPH_PRINT,@"「这个HPH呀HPH就这样HPH留着，让我HPH满足，喔呜呜呜HPH」","W"
								ENDIF
						ENDSELECT
					ELSE
						SELECTCASE RAND:2
							CASE 0
								;骑乘位
								IF TEQUIP:体位２ == 3
									CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
									PRINTFORML （\@ TEQUIP:65:口接触部位 == 4 ? 就像在请求更多爱一样热吻 # 就像在请求最深处一样 \@，自己摇动着腰部…）
									PRINTFORML （我，从什么时候开始变成这么淫荡的女孩了呢…）
								ELSE
									CALL HPH_PRINT,@"「%CALLNAME:MASTER%先生，%CALLNAME:MASTER%先生啊HPH噢HPH…呜，嗯啊啊HPH」","L"
									PRINTFORML （被最深处激烈抚弄，\@ TEQUIP:吸乳头 ? 连乳房也被吮吸， # \@发出像野兽一样淫荡的声音…）
								ENDIF
								CALL HPH_PRINT,@"（不过，好舒服…HPH真实的子宫创造之爱，停不下来了呢…HPH）","W"
							CASE 1
								CALL HPH_PRINT,@"「啊啊啊…HPH啊HPH啊HPH啊啊啊～～～～～HPH」","L"
								PRINTFORMDL 椛发出着狂喜的喘息声，颤栗在被深深爱抚的愉悦中…
								CALL HPH_PRINT,@"（被心爱的%CALLNAME:MASTER%先生这样激烈地爱抚着…）","L"
								CALL HPH_PRINT,@"（我，好幸福…HPH）","W"
						ENDSELECT
					ENDIF
				ELSE
				IF TCVAR:発情
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「哈啊啊HPH啊啊啊HPH哦、呼呼呼HPH嗯、呜呜啊啊HPH噢、呼呼HPH」","L"
							PRINTFORMDL \@ 充填率(TARGET,1) >= 100 ? 腹部被填满精液， # \@紧贴着肉棒的子宫在强烈的快感中摇摆，椛发出不可描述的声音…
							CALL HPH_PRINT,@"「%K65_AEGI()%…好舒服、嗯嗯嗯…HPH」","W"
						CASE 1
							CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
							PRINTFORML 「啊啊啊HPH嗯～HPH嗯嗯～HPH」
							PRINTFORMDW 发情中的阴道壁因热而涨红，每次轻推都引导着你的阴茎深入更深…
						ENDSELECT
					ELSE
						SELECTCASE RAND:2
							CASE 0
								CALL HPH_PRINT,@"「哈HPH啊啊HPH%K65_AEGI()%」","L"
								PRINTFORMDL 骑在%CALLNAME:MASTER%身上的椛深深地降低腰部，随着%CALLNAME:MASTER%的抽送，椛摆动着臀部，主动将阴茎的顶端压在子宫口，寻求更强烈的快感…
								CALL HPH_PRINT,@"「噢、噢嗯…%K65_AEGI(,2)%…好、好舒服…HPH」","W"
							CASE 1
								CALL HPH_PRINT,@"「%K65_AEGI(,3)%…啊呜呜HPH呀、呀啊HPH停下来、停下来、快停下来！！！」","L"
								PRINTFORMW 「咳、哈啊…嗯、这么猛烈，一直在深处啊…」
								PRINTFORML
								PRINTFORML 「头都要糊涂了，只能想着%CALLNAME:MASTER%先生的小弟弟了…」
								CALL HPH_PRINT,@"「啊啊啊HPH啊HPH噢啊HPH嘶、嘶嘶、噢、不要啊HPH」","L"
								IF RAND:5 == 1
									CALL HPH_PRINT,@"「这、这样说着，是、是什么啊HPH啊啊啊HPH」","W"
								ELSE
									PRINTFORMDW 被椛无意中引诱的话语，%CALLNAME:MASTER%更深地将肉棒刺入阴道深处…
								ENDIF
						ENDSELECT
					ENDIF
				ENDIF
			;松弛中
			CASE IS > 25
				IF TCVAR:発情
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「嗯呜呜啊HPH嗯、嗯～～～～HPH」","L"
							CALL HPH_PRINT,@"「这、这个、啊啊啊…HPH好像、要变得…HPH啊啊HPH」","L"
							PRINTFORMDW 被刺穿的子宫口的快感，椛不禁使劲地跃动着身体…
						CASE 1
							CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
							PRINTFORMDL 发情中感觉加强的椛，因被最深处反复搅拌而在愉悦中抬起了臀部…
							CALL HPH_PRINT,@"「嘶、好舒服…深、太深了…HPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「哈噢HPH…啊啊HPH嗯、嗯嗯…HPH什么、这是什么…HPH」","L"
							PRINTFORMDL 在被开发为敏感带的最深处轻推时，尖锐的快感袭击椛。
							PRINTFORMDW 这与以往完全不同的感觉，使椛的眼睛变得黑白交错…
						CASE 1
							CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
							CALL HPH_PRINT,@"「停、停下来HPH%CALLNAME:MASTER%先生…这、这样…HPH」","L"
							CALL HPH_PRINT,@"（学到这么舒服的性爱，会上瘾的…HPH会上瘾的…嗯）","W"
					ENDSELECT
				ENDIF
			;紧致，即使发情也变得清醒
			CASEELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「呜、咕…这、这么深、好痛苦……」
							PRINTFORMDW 椛发出低沉的声音…
						CASE 1
							PRINTFORML 「咳、哈啊…太、太过分了…」
							PRINTFORMW 「%CALLNAME:MASTER%先生…稍微、有点痛苦…」
					ENDSELECT
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
RETURN 1

