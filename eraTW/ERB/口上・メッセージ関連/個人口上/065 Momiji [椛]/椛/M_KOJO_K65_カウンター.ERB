				CALL HPH_PRINT,@"「…呵呵。%CALLNAME:MASTER%的手、好温暖…」","L"
				PRINTFORMDL 紧紧握住和恋人牵在一起的手，椛再次開始抽送…
				CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
		ENDSELECT
	ENDIF
ENDIF
