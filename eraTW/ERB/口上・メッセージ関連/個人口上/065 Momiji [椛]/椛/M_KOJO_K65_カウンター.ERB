@M_KOJO_MESSAGE_COUNTER_K65_85_1

	;初めて
IF TALENT:処女
	PRINTFORML 「我说、%CALLNAME:MASTER%桑…」
	PRINTFORMDL 椛轻柔地握着%CALLNAME:MASTER%的阴茎、红着脸小声说着。
	PRINTFORML 「我的第一次、不来取走吗…」
	PRINTFORML 「啊…那个、请不要那么深思熟虑！」
	IF TALENT:恋慕
		PRINTFORML 「我、要是第一次能是%CALLNAME:MASTER%就好了、我是这么想的！」
		PRINTFORML 「……～～～」
		PRINTFORMDL 随着气势大声说了出来、但似乎是无法忍耐这种羞耻感、
		PRINTFORMDL 椛低着头浑身颤抖着…
	ELSE
		PRINTFORML 「…我、要是是和%CALLNAME:MASTER%桑的话就好了、我是这么想的……」
	ENDIF
	RETURN 1
ENDIF
IF TCVAR:発情
	SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「哈啊啊…HPH」", "L"
			CALL HPH_PRINT,@"「%CALLNAME:MASTER%桑的肉棒进来啦啊啊……HPH」", "L"
		CASE 1
			CALL HPH_PRINT,@"「诶嘿…HPH进来了呢……HPH」", "L"
			PRINTFORMDL 椛一脸恶作剧般的微笑用膣口顶住了%CALLNAME:MASTER%的阴茎、
			PRINTFORMDL 一边像是在向你展示秘部一般一边慢慢地沉下了腰…
		CASE 2
			PRINTFORML 「嗯嗯……！！」
			PRINTFORMDL 跨坐在%CALLNAME:MASTER%身上的椛沉下了腰、把因为发情而整个湿透的小小蜜穴尽全力扩開、
			PRINTFORMDL 然后把狰狞膨胀的%CALLNAME:MASTER%的那个东西整个吞了进去…
			CALL HPH_PRINT,@"「啊、哈……HPH进来了哦、%CALLNAME:MASTER%桑…HPH」", "L"
	ENDSELECT
ELSE
	SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「进来了、呢…」
		CASE 1
			PRINTFORML 「已、已经忍不下去了…」
	ENDSELECT
ENDIF
RETURN 1

