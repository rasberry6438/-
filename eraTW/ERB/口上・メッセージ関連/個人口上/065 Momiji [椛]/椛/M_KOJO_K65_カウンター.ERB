				CALL HPH_PRINT,@"「胸部、请来舔吧…HPH」", "L"
			CASE 1
				PRINTFORMDL 每当用力吮吸那可爱的鼓起着的先端、椛的身体就会因为快感而大幅颤抖…
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「胸部、请来舔吧…HPH」", "L"
			CASE 1
				PRINTFORMDL 椛用发情湿润的眼瞳注视着用力吸住椛的乳头的%CALLNAME:MASTER%…
		ENDSELECT
	ENDIF
ELSE
	IF TALENT:胸圍 > 0
		SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「那个…请欺负我的、乳尖…HPH」", "L"
			CASE 1
				PRINTFORMDL 丰盈膨大着的乳头被%CALLNAME:MASTER%用嘴唇用力磨蹭着、椛发出了甜美的喘息…
			ENDSELECT
	ELSEIF TALENT:胸圍 < 0
		SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「那个、虽然不怎么打…还请、多多地疼爱这里吧…HPH」", "L"
			CASE 1
				CALL HPH_PRINT,@"「那个…请欺负我的、乳尖…HPH」", "L"
			ENDSELECT
	ELSE
		SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「那个…请欺负我的、乳尖…HPH」", "L"
			CASE 1
				PRINTFORMDL 椛用欲望和慈爱混杂的表情注视着用力吸住椛的乳头的%CALLNAME:MASTER%…
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

