		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
			CASE 1
				CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
