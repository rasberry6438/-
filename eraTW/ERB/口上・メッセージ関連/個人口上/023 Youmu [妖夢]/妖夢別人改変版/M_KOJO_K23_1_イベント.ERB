	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;诶嘿嘿中断
		IF TFLAG:102 > 1
			IF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「啊，诶？……对、对不起，我要去采购……」
					CASE 1
						PRINTFORMW 「呀，呀，啊……不赶快的话，家里的食物……」
					CASE 2
						PRINTFORMW 「抱歉，时间……我……嗯嗯……」
				ENDSELECT
			ELSE
				PRINTFORMW 「……！　我、我差不多该去采购了就先告辞了……」
			ENDIF
			PRINTFORMW 妖梦急忙整理了仪容、\@ TALENT:恋慕 ? 拖着因欲望而发痛的身体朝着商业街走去 # 匆匆忙忙地朝商业街走去 \@
		
		;勃然大怒
		ELSEIF CFLAG:318 == 1
			PRINTFORML 「......」
			PRINTFORMW 妖梦默不作声，快步走向商业街……
		
		;反発刻印有
		ELSEIF (MARK:23:反発刻印 == 1 && RAND:2 == 0) || MARK:23:反発刻印 > 1
			PRINTFORML 「……就这样了」
			PRINTFORMW 妖梦仅此一句，推着拉货车向商业街走去
		
		;荒天时に分岐できるだろうか
		ELSEIF GROUPMATCH(TIME:5,5,9,13)
			PRINTFORML 「唔，这天气……如果把路上的艰难当作锻炼的话……！」
			PRINTFORMW 妖梦扑哧一声拍打脸颊，鼓舞精神，奋力走向商业街
		ELSEIF TALENT:恋慕
			PRINTFORML 「啊、都这个时间了……抱歉、我得去采购了……」
			PRINTFORMW 妖梦抱歉地低下了头、然后推着大型手推车朝商业街走去
		ELSE
			PRINTFORML 「差不多该去购物了……告辞了」
			PRINTFORMW 妖梦不知从哪里弄来的拉货车，推着它朝商业街走去
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;业务中
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE RAND:7
			CASE 0
				PRINTFORMW 载货的半灵来回匆忙地穿梭着
			CASE 1
				PRINTFORMW 「大米买完了、接下来是%TEXTR("肉/鸡肉/")%和%TEXTR("蔬菜/水果/")%还有%TEXTR("茶点/酒/")%――」
			CASE 2
				PRINTFORMW 「是的、付款的方式就和往常一样――」
			CASE 3
				PRINTFORMW 「剩下的货物请放在货车上」
			CASE 4
				PRINTFORMW 「是的、剩下的部分我改天再来取――」
			CASE 5
				PRINTFORMW 「啊咧？　数目对不上……对不起、请让我再确认一下――」
			CASEELSE
				PRINTFORMW 「诶、给我的慰问品？　非常感谢您、请先交给那边的半灵吧――」
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;工作结束
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTDATAW
			DATAFORM 「好了，全部搞定……」
			DATAFORM 「清单没有漏项……」
			DATAFORM 「货物固定得……没有问题」
			DATAFORM 「支、支付这样就……没问题了」
		ENDDATA
		;手传った
		IF TCVAR:23:帮忙工作量
			;勃然大怒
			IF CFLAG:318 == 1
				PRINTFORML 「......」
			
			;反発刻印有
			ELSEIF (MARK:23:反発刻印 == 1 && RAND:2 == 0) || MARK:23:反発刻印 > 1
				PRINTFORML 「……谢谢你」
			
			;早く终わった
			ELSEIF TIME < 960
				IF TALENT:恋慕 && RAND:2 == 0
					PRINTFORM 「有%CALLNAME:MASTER%先生一起的话、这点杂务根本不在话下
				ELSEIF TALENT:思慕 && RAND:2 == 0
					PRINTFORM 「比一个人的时候轻松多了呢
				ELSE
					PRINTFORM 「拖您的福顺利完成了呢
				ENDIF
				PRINTFORML 、非常感谢您的帮助！」
			ELSE
				PRINTFORML 「谢谢您的%TEXTR("帮忙/协助/")%！」
			ENDIF
			PRINTFORMW 妖梦深深地向%CALLNAME:MASTER%鞠了一躬
		;手传ってない
		ELSE
			PRINTFORMW 完成工作后，妖梦轻松地舒展了一下身体
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

