					ELSE
						CALL HPH_PRINT,@"「呼呜，热热的，在里面……积攒起来了……HPH」","L"
						CALL HPH_PRINT,@"「啊哈……%CALLNAME:MASTER%大人，似乎还能进去哦……？HPH」","L"
					ENDIF
				CASE 5
					CALL HPH_PRINT,@"「啊啊啊，这个……HPH 精液，还要更多……HPH」","L"
					IF EX:膣内精液 >= 1200
						PRINTFORML 尽管已经装不下子宫了，妖梦仍然不断地索求着子种
						PRINTFORMW %CALLNAME:MASTER%感动地挺动腰部，将精液涂抹到更深处
					ELSE
						PRINTFORMW 每次%CALLNAME:MASTER%的刚硬之物脉动着射精，妖梦的身体都因欢喜而颤抖
					ENDIF
				CASE 6
					CALL HPH_PRINT,@"「呼，哈……好厉害……出来了……HPH」","L"
					CALL HPH_PRINT,@"「请更多地使用我，把我变成%CALLNAME:MASTER%大人喜欢的样子吧……HPH」","W"
				CASE 7
					CALL HPH_PRINT,@"「哈，哈啊……啊，嗯……HPH 真不可思议呢……」","L"
					CALL HPH_PRINT,@"「这样被对待的话，就能清楚地感觉到“美味”呢……HPH」","L"
					PRINTFORMW 妖梦用手指舀起从结合部\@ EX:膣内精液 >= 1200 ? 溢出 # 漏出 \@的子种，眯起眼睛用舌头舔舐干净……
			ENDSELECT
		ELSEIF ABL:膣射中毒 > 0
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「啊啊……这种热度，渐渐地记住了……HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「哈，嗯……%CALLNAME:MASTER%大人渗透进来了……HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「呼啊……HPH 肚子深处，更加热了……HPH」","W"
			ENDSELECT
		ELSE
			PRINTDATAW
				DATAFORM 「嗯嗯……奇怪的感觉，呢……」
				DATAFORM 「啊……现在，出来了……」
			ENDDATA
		ENDIF
	ELSE
		IF ABL:膣射中毒 > 3 && RAND:2 == 0
			PRINTDATAW
				DATAFORM 「啊，呜……为什么，这么热……」
				DATAFORM 「呼，哈啊……还在，出来着……」
			ENDDATA
		ELSE
			PRINTDATAW
				DATAFORM 「呜啊……感觉好恶心……」
				DATAFORM 「咕，呜……这种东西，好奇怪……」
			ENDDATA
		ENDIF
	ENDIF
	RETURN 1
ENDIF
