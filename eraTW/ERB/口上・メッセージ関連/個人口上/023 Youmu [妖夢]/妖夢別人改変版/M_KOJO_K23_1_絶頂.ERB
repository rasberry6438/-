								CALL HPH_PRINT,@"「……能好好地为您服务真是太好了HPH」","W"
							CASE 1
								PRINTFORML 「嗯呗……哦哦，好多……」
								PRINTFORMW 「视觉上能看到的话，成就感会增加呢……」
						ENDSELECT
					ENDIF
				ELSE
				;从嘴里滴滴答答地流出来
					SELECTCASE RAND:(2 + (ABL:精液中毒 >= 5) * 3)
						CASE 0
							PRINTFORML 「嗯啊，啊噗……这、这么多……」
							CALL HPH_PRINT,@"「对不起，我失态了……HPH」","W"
						CASE 1
							PRINTFORML 「嗯，嗯呜……，噗啊……！」
							PRINTFORMW 「没、没办法一口气喝完呢……咳」
						CASE 2
							PRINTFORML 「啊啊，洒了很多……」
							CALL HPH_PRINT,@"「好不容易才射出来的，太浪费了……啾噗，咧啰，啾噜HPH」","W"
						CASE 3
							CALL HPH_PRINT,@"「呼，哈……洒出来的是我，所以必须打扫干净……HPH」","L"
							PRINTFORMW 妖梦用指尖舀起弄脏身体的乳白色液体，仔细品味般地舔舐干净……
						CASE 4
							PRINTFORML 「哈啊……明明知道又臭又难吃，还会粘在喉咙上……」
							CALL HPH_PRINT,@"「看到这个，就觉得非常渴望……咧啰，嗯……啾噜噜噜HPH」","W"
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ELSE
		IF (EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3) && EXP:精飲経験 >= 20
			IF ABL:精液中毒 >= 5 && RAND:3 == 0
				PRINTFORML 「嗯……嗯库，呼……咕……」
				PRINTFORMW （明明很难喝……却离不开……）
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「嗯咕……呜欸……还是一如既往的难喝……」
						PRINTFORMW （已经习惯味道本身了……）
					CASE 1
						PRINTFORML 「呜……姆咕……，哈啊……」
						PRINTFORMW 「……洒出来的话，周围会更脏的吧」
				ENDSELECT
			ENDIF
		ELSE
			IF TCVAR:TARGET:精飲経験 == 0
				PRINTFORML 「嗯咕，姆！？　……呜欸，咳，咳咳」
				PRINTFORMW 「这是什么啊……想让我喝下去，你不是疯了吧……」
			ELSEIF TCVAR:TARGET:精飲経験 < 20
				PRINTFORML 「咳，呜欸……好难喝……」
				PRINTDATAW
					DATAFORM （唔，只能用惨烈来形容的余味……）
					DATAFORM （真想用口对口的方式给他也尝尝……！）
				ENDDATA
			ELSE
				IF ABL:精液中毒 >= 5 && RAND:3 == 0
					PRINTFORML 「咕……啾噜……怎、怎么了，那是什么眼神」
					PRINTFORMW 「都被弄得那么黏糊糊的了，厌恶感也会麻痹的……」
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「呜噗……这、这量是从哪里来的……？」
						CASE 1
							PRINTFORML 「姆，嗯……呜，咳，等、等等……！」
							PRINTFORMW （只、只用嘴根本装不下……）
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF
RETURN 1

