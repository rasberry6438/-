@M_KOJO_SUCCESS_COM_K30_181
TFLAG:192 = TCVAR:30:媚薬
SIF TCVAR:30:385 || TCVAR:30:强行 || TCVAR:30:烂醉
	RETURN
IF TCVAR:30:367
	TFLAG:194 = 3
	RETURN
ENDIF
SELECTCASE TFLAG:192
	CASE 0
	CASE 1 TO 270
		TFLAG:194 = 1
	CASEELSE
		TFLAG:194 = 2
ENDSELECT
;媚薬効果時間が累積する映姫様のEXTRASOURCE関数
@M_KOJO_EXTRASOURCE_COM_K30_181
IF TFLAG:194 < 3
	TCVAR:30:媚薬 = TFLAG:192 + 180
;もう飲まない処理
;ソース変動と媚薬タイマーをコマンド実行前に戻す
ELSE
	TCVAR:30:媚薬 = TFLAG:192
	IF TALENT:PLAYER:調合知識
		DOWNBASE:体力 -= 150
		DOWNBASE:気力 -= 150
	ELSE
		DOWNBASE:体力 -= 300
		DOWNBASE:気力 -= 300
	ENDIF
	UP:欲情 = 0
	SOURCE:逸脱 = 0
	IF HETEROSEX(30, MASTER)
		EXP:(50 + HETEROSEX(30, MASTER)) -= 1
		EXP:PLAYER:(50 + HETEROSEX(30, MASTER)) -= 1
	ENDIF
	ITEM:媚薬 ++
ENDIF

