;==================================================
;300,会話
;TFLAG:193(1=大成功 0=成功 -1=失敗 -2=大失敗（TARGETがMASTERより教養が4以上高い)
;TCVAR:302(0=会話可能 1>=非恋慕時会話不能)
;MARK:反発刻印(3以上=会話不能)(4以上はバグ
;TCVAR:烂醉(1=通常成功or大失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K30_300
;成否判定
;TFLAG:192(-2=指令終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
;裁判中会話は強制失敗扱い。代わりに会話累積値を打ち消して「気まずい空気」を発生させない
IF 仕事中チェック(30) && BETWEENTIME(540, 719) && (CFLAG:30:現在位置 == CFLAG:30:職場) && CFLAG:30:職種 == 43
	TFLAG:192 = -1
	TCVAR:30:301 = 0
ELSE
	TFLAG:192 = 0
ENDIF

;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K30_300
RETURN 1

@M_KOJO_MESSAGE_COM_K30_300
;TRAIN_MESSAGEを消す条件並べる。TFLAG:193を借りてフラグ立てる
;初回or変Ｔで初対面日
IF !STRCOUNT(CSTR:30:0, "300/") || (CFLAG:30:衣装一時変更 == 62 && STRCOUNT(CSTR:30:1, "初対面"))
	TFLAG:193 = 4
;口交抜き
ELSEIF TFLAG:193 == -999
;変Ｔ
ELSEIF K30_CHECK_HEN_T() && !GROUPMATCH(CFLAG:現在位置, 情人用宿屋, 情人旅館, OMANEKIBEYA())
	TFLAG:193 = 2
;振動棒约会中
ELSEIF CHK_DATENOW(CFLAG:30:约会中) && TCVAR:30:364 > 0
	TFLAG:193 = 5
;事後の会話
ELSEIF TIME_PROGRESS(CFLAG:1504) <= 10 && TALENT:恋慕 && !K30_BE_SEEN() && TALENT:心情 > -1 && !MARK:反発刻印 && !K30_CHECK_HEN_T()
	CALLF ONCE("会話＠記念日")
	TFLAG:193 = 6
;裁判中
ELSEIF (仕事中チェック(30) && BETWEENTIME(540, 719) && (CFLAG:30:現在位置 == CFLAG:30:職場) && CFLAG:30:職種 == 43) 
;接吻魔取得時確率
ELSEIF TALENT:接吻魔 && TALENT:恋慕 && RAND:(50 - TALENT:心情 * 25 - (CFLAG:积攒度 / 50)) == 0 && TALENT:心情 != -1 && MARK:30:反発刻印 == 0 && (K30_BE_SEEN() == 0 || RAND:2 == 1)
	TFLAG:193 = 3
;他は通常どおり
ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K30_300_1
	RETURN RESULT
ENDIF
;TRAIN_MESSAGE抜きで口上メイン呼出
CALL K30_D_LINE
CALL M_KOJO_MESSAGE_COM_K30_300_1
RETURN RESULT

