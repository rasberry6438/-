@M_KOJO_MESSAGE_COM_K30_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;-------------------------------------------------
	;時姦中
	IF FLAG:70
		RETURN 1
	ELSEIF FIRSTTIME(SELECTCOM)
		PRINTFORML 「！？」
		PRINTFORML 「诶~…突然要干什么啊！？」
		PRINTFORMDW 比起疼痛，突然被打让映姬更受打击…
		RETURN 1
	;マグロスイッチオン
	ELSEIF TCVAR:30:385 || TCVAR:30:强行 || TCVAR:30:烂醉
		CALL K30_GASPING_MAGURO
		RETURN 1
	;ABL:受虐属性はちゃんと機能してない？結局お尻を使ったプレイになったし随意Ａ感覚で代用
	;不慣れ
	ELSEIF ABL:Ａ感覚 < 3
		PRINTFORML 「%TEXTR("/唔唔~/不要~/为什么")%……从没人这样对我！？」
		PRINTFORML 「被打的……应该是你…」
		PRINTFORMDW 映姬摸着发红的屁股，发出不满的声音…
		RETURN 1
	;非挿入時
	ELSEIF ALLSAMES(TEQUIP:50, TEQUIP:51, -1)
		PRINTDATAL
			DATAFORM 「嗯！…♥　这样随性地打过来…」\n「你不会觉得、我这样就会满足的吧♥」
			DATAFORM 「这个样子也太过分啦~…♥」\n「我有强烈的预感…这里……很快就需要你来帮助了啊♥」
			DATAFORM 「屁股…火辣辣的…♥」\n「请快点…安慰我…♥」
		ENDDATA
		PRINTFORMDW 映姬左右摇晃着刚被打过的雪臀，在期待着什么。…
		IF RAND:2 == 1
			PRINTFORMDL %CALLNAME:MASTER%像狗一样舔着发红的屁股
			IF ABL:Ａ感覚 < 5 || RAND:2
				PRINTFORML 「恰啊~♥　等下…%K30_C_NAME(MASTER)%！？」
				PRINTFORML 「总觉得…酥麻的、奇怪的…感觉……嗯嗯~♥」
				PRINTFORMDW 映姬对%CALLNAME:MASTER%的倒错式虐待困扰地发出声音…
			ELSE
				CALL HPH_PRINT, @"「咿呀HPH　那里HPH　就这样，别看我…HPH」", "L"
				CALL HPH_PRINT, @"「啊…啊啊HPH　咿呀、湿漉漉的…咿啊HPH」", "L"
				PRINTFORMDW 被用舌头舔舐勾描着灼热的臀部，映姬发出急不可耐的娇声…
			ENDIF
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%用手温柔地抚摸着映姬的屁股
			IF ABL:Ａ感覚 < 5 || RAND:2
				PRINTFORML 「啊呼~♥　那个，好喜欢…火辣辣的，飞走了…♥」
				PRINTFORML 「%K30_C_NAME(MASTER)%、我的屁股…希望得到你更多更多…的疼爱呀。♥」
				PRINTFORMDW 这种由刺痛转变为快感的感觉，映姬完全沉浸入了…
			ELSE
				CALL HPH_PRINT, @"「嗯啊…HPH　%K30_C_NAME(MASTER)%的手、很温柔，舒服…但是…」", "L"
				PRINTFORMDL 随着抚摸着火辣的屁股，胯间的爱液会进一步分泌
				CALL HPH_PRINT, @"「那个……有点太不够……唔咿咿嗯HPHHPH」", "L"
				PRINTFORMDW 轻轻触碰着因期待而发抖的花蕾，顿时响彻起阎魔大人不成声断断续续的娇泣…
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;挿入時
	IF PALAM:欲情 < PALAMLV:10 || !RAND:4
		STRDATA LOCALS
			DATAFORM 「啊呃~…♥」\n「现，现在…里面，非常的……」\n「哼~噫~♥」
			DATAFORM 「恰~♥」\n「…被打了，但是怎么这么舒服」\n「为什么，为什么…这样…？」
			DATAFORM 「啊呼~…♥」\n「…屁股，发出的声音…好羞耻…」\n「比起痛……我更感到不好意思…♥」
		ENDDATA
		SIF TEQUIP:51 == MASTER
			LOCALS '= REPLACE(LOCALS, "♥", "HPH")
		CALL HPH_PRINT, LOCALS, "L"
		PRINTFORMDL 控制场面，故意拍着屁股只发出声音，煽动映姬的羞耻感、
		PRINTFORMDW 映姬感到困惑的同时，\@ !TEQUIP:50 ? 雌 # 菊 \@穴的紧致度表明了她的兴奋…
	ELSE
		STRDATA LOCALS
			DATAFORM 「哼呦♥　不…不行~、那个，不可以♥」\n「两边都被、砰砰的拍打着、不要~哦~♥」
			DATAFORM 「啊~！　啊~♥　啊咿~♥」\n「屁股，被打了♥」\n「深♥　深处，感觉到了。♥」
			DATAFORM 「呼——~♥　呼——~♥　呼——~…」\n「…哼噫♥♥」\n「对不起~♥　屁股这么下流、对不起~♥」
		ENDDATA
		SIF TEQUIP:51 == MASTER
			LOCALS '= REPLACE(LOCALS, "♥", "HPH")
		CALL HPH_PRINT, LOCALS, "L"
		PRINTFORMDW 打屁股已经变成了提高\@ TEQUIP:51 == MASTER ? 肛门 # \@性交快感的催化剂、映姫已陷入忘乎所以的迷乱癫狂之中了…
	ENDIF
	PRINTFORML 
	;Ｖ挿入/Ａ挿入:感覚-低
	IF TEQUIP:50 == MASTER || ABL:Ａ感覚 < 5 || PALAM:欲情 < PALAMLV:10 || RAND:2
		PRINTFORMDL 突然%CALLNAME:MASTER%停止抽插温柔地抚摸映姬的屁股…
		IF TEQUIP:50 == MASTER
			PRINTFORML 「对，那里…火辣辣的…♥嗯啊♥」
			PRINTFORML 「啊啊…♥　好热…♥」
			PRINTFORML 「屁股…又来了，好热啊…♥」
		ELSE
			CALL HPH_PRINT, @"「啊啊啊HPH　不，不行！　屁股，好难过…好难受啊HPH」", "L"
			CALL HPH_PRINT, @"「%K30_C_NAME(MASTER)%的肉棒！　请HPH　快一点、插进来啊HPHHPH」", "L"
		ENDIF
		PRINTFORMDW 因为刺激而变得敏感的美臀被爱抚，\@ !TEQUIP:50 ? 肉棒被含住了 # 湿透了 \@从腔口溢出了新的蜜…
	;Ａ挿入:感覚-高
	ELSE
		PRINTFORMDL 这次%CALLNAME:MASTER%将阴茎挤进直肠深处，然后用力拍打了映姬的屁股
		CALL HPH_PRINT, @"「哈唏HPHHPH　屁股HPH　哈、哈HPH　好激烈HPH」", "L"
		CALL HPH_PRINT, @"「要坏掉了HPH　屁股、坏掉了唔唔呜HPH」", "W"
		PRINTFORMDW 沉浸在被%CALLNAME:MASTER%支配的感觉中，映姬像痴女一样不断淌口水喘息着…
	ENDIF
ENDIF
RETURN 1

;地の文改変
@K30_TRAIN_MESSAGE_100
CALL K30_D_LINE
IF GROUPMATCH(MASTER, TEQUIP:50, TEQUIP:51)
	PRINTFORMD 一边痛快享受映姫的\@ !TEQUIP:50 ? 雌 # 菊 \@穴
ELSEIF TEQUIP:0 & 1 && (EQUIP:裙子 || EQUIP:下身衣服 || EQUIP:連衣裙 || EQUIP:和服)
	PRINTFORMD 映姫的%SHOW_BOTTOM(TARGET,1)%被翻开
ELSE
	PRINTFORMD 映姫的
ENDIF
SIF EQUIP:TARGET:緊身内衣 || EQUIP:TARGET:連体内衣
	PRINTFORMD %SHOW_下半身内衣(TARGET,2)%包裹着
PRINTFORMDL 一巴掌拍了一下%HIP(TARGET)%
PRINTFORMD %CALLNAME:MASTER%轻重交加地拍了几次后、
IF ABL:Ａ感覚 < 3
	PRINTFORMDL 映姬的屁股稍微透红了…
ELSEIF ALLSAMES(TEQUIP:50, TEQUIP:51, -1)
	PRINTFORMDL 映姬的胯间潮水泛滥了…
ELSE
	PRINTFORMDL 映姫的\@ !TEQUIP:50 ? 女阴 # 肛门 \@期待着进一步的欺负而怦然心动…
ENDIF
PRINTFORMW 
RETURN

