	SIF STRLENS(RESULTS)
		CLEARLINE 1
	REUSELASTLINE 「…请多珍惜父母给的姓名」
	GOTO INPUT_LOOP
;12文字(全角6文字)超えた
ELSEIF STRLENS(C_NAM) > 12
	CLEARLINE 1
