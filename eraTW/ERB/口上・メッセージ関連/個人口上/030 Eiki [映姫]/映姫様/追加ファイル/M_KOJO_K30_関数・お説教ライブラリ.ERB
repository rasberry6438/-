;==================================================
;酔っぱらい度を数字で返す式中関数
;=0 清醒 =1 微醺 =2 酔酒 3=不省人事
;-------------------------------------------------
@K30_DRUNK()
#FUNCTION
SIF BASE:30:酒気 <= 2
	RETURNF 0
SELECTCASE BASE:30:酒気
	;不省人事
	CASE IS > (MAXBASE:30:酒気 / 10) * 8
		RETURNF 3
	;酔酒
	CASE IS > (MAXBASE:30:酒気 / 10) * 5
		RETURNF 2
	;微醺
	CASE IS <= (MAXBASE:30:酒気 / 10) * 5
		RETURNF 1
ENDSELECT
