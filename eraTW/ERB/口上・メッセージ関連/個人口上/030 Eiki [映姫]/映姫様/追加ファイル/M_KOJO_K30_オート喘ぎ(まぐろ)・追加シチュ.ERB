;部位・感度等無視して矛盾しない喘ぎ声をつくるのを目標に
@K30_GASPING_MAGURO
;--------------------------------------------------
#DIM 状況
#DIM 心神喪失
#DIM ひどい
#DIM TEMP
#DIM TEMP2
心神喪失 = 0
ひどい = 0
TEMP = 0
TEMP2 = 0
SIF !CFLAG:30:诶嘿嘿
	RETURN 0
;ノーマルまぐろ
状況 = 0 
;炮友
SIF TALENT:愛欲 || TALENT:炮友 || TALENT:風騷
	状況 = 4
;烂醉まぐろ
IF TCVAR:烂醉
	状況 = 5
	心神喪失 =1
ENDIF
;睡眠まぐろ
IF CFLAG:睡眠
	状況 = 9
	心神喪失 =1
ENDIF
;接吻時
SIF GROUPMATCH(SELECTCOM, 19, 20)
	状況 = 1
;口交時
SIF GROUPMATCH(SELECTCOM, 81, 88, 89, 140, 501, 505, 508, 509, 512)
	状況 = 2
;個別処理時(薬
SIF GROUPMATCH(SELECTCOM, 50, 51, 180, 181, 182, 192, 490)
	状況 = 6
;SM系(SOURCE入らない)時
SIF GROUPMATCH(SELECTCOM, 100, 104, 105, 107)
	状況 = 7
;奉仕(ちゅぱ音無し)時
SIF GROUPMATCH(SELECTCOM, 80, 82, 83, 84, 85, 510, 511)
	状況 = 8
;Ａ関連
SIF GROUPMATCH(SELECTCOM, 4, 5, 13, 45, 62, 63, 66, 69, 70, 71, 74, 75, 514, 515)
	状況 = 3
PRINTFORM 「
SIF TCVAR:强行
	PRINTFORM %TEXTR("やだ/いや/だめ/やめて")%っ！　…
;炮友、心神喪失、薬、ＳＭは必ず最初に一言
IF (RAND:2 == 0 || GROUPMATCH(状況, 4, 6, 7) || 心神喪失) && !TCVAR:强行 
	CALL K30_GASPING_RAND(状況)
	;ＳＭは一言で処理終了
	SIF 状況 == 7
		RETURN 1
	TEMP = 1
ENDIF
;行中の処理
REPEAT 5 + RAND:2
	SELECTCASE 状況
	;接吻・口交はちゅぱ音
	;心神喪失・强行時は頻度半分
		CASE 1, 2
			IF (RAND:2 == 1 && !(心神喪失 || TCVAR:强行)) || (RAND:4 == 1 && (心神喪失 || TCVAR:强行))
				IF 状況 == 1
					PRINTFORM …%TEXTR("嗯唔/呸咯/哈姆/啾噜/唔姆")%
				ELSE
					PRINTFORM …%TEXTR("唔噗/啾啵/啾噜/滋噜/啾噗")%
				ENDIF
				SIF PALAM:30:欲情 > PALAMLV:7 && !TCVAR:强行 && RAND:3 != 1
					PRINTFORM ♥　
			ELSE
				PRINT …
			ENDIF
	;通常は拒否っぽい声	、準強制わいせつは寝言っぽいのを、炮友・恋人同伴は肯定をそれぞれ2回まで
		CASE 0, 4, 5, 9
			IF TEMP2 < 3 && !TCVAR:强行
				IF RAND:3 == 1
					TEMP2 ++
					IF TCVAR:烂醉
						PRINTFORM ……%TEXTR("唔呼呼/唔嗯～/啊唔/唔唔/咿咕")%
					ELSEIF CFLAG:睡眠
						PRINTFORM ……%TEXTR("呼喵/啊呼/唔/啊嗯/嗯唔")%
					ELSEIF 状況 == 4 || TALENT:恋慕
						PRINTFORM ……%TEXTR("好舒服/那里/啊啊/好棒/啊嗯/呀唔")%
						IF (PALAM:30:欲情 > PALAMLV:3 && RAND:3 == 1) || (PALAM:30:欲情 > PALAMLV:5 && RAND:3 != 1)
							PRINTFORM っ
						ENDIF
						IF PALAM:30:欲情 > PALAMLV:6
							SIF RAND:2 == 1
								CALL HPH_PRINT(@"HPH　")
						ELSEIF PALAM:30:欲情 > PALAMLV:3 && RAND:3 != 1
							PRINTFORM ♥　
						ENDIF
					ELSE
						PRINTFORM ……%TEXTR("这样不行/不行了/感觉好奇怪/感觉有什么……/不要/要变奇怪了")%
						IF (PALAM:30:欲情 > PALAMLV:2 && RAND:3 == 1) || PALAM:30:欲情 > PALAMLV:6
							PRINTFORM っ
						ENDIF
						IF PALAM:30:欲情 > PALAMLV:9
							SIF RAND:4 == 1
								CALL HPH_PRINT(@"HPH　")
						ELSEIF PALAM:30:欲情 > PALAMLV:5 && RAND:3 == 1
							PRINTFORM ♥　
						ENDIF
					ENDIF
				ELSE
					PRINT …
				ENDIF
			ELSE
				PRINT …
			ENDIF
	;他は無言
		CASEELSE
			PRINT …
	ENDSELECT
REND
IF TEMP == 0
	;强行・薬時は語尾に一言だけ、後はどっちかに一言だけ TCVAR:30:386 强行口上反映フラグ
	IF TCVAR:强行
		TCVAR:30:386 = 1
	;强行時に避妊
		SIF (SELECTCOM == 51 || TEQUIP:PLAYER:避孕套) && TCVAR:30:不耐烦 == 1
			PRINTFORM %TEXTR("你想干什么…？/根本搞不懂你的想法…/别開玩笑了…/你到底在想什么呢…")%
		PRINTFORM %TEXTR("…够了…住手/…太过分了/…已经够了吧/…明明这么信任着你/…给我适可而止/…不要碰我")%
	ELSE
		CALL K30_GASPING_RAND(状況)
	ENDIF
ENDIF
PRINTFORMW 」
PRINTFORML 
;MASTER射精直前
IF NOWEX:MASTER:11 && !心神喪失 
	;强行避妊無しで中出し
	IF TCVAR:强行 && !TCVAR:30:口服避孕薬 && TEQUIP:PLAYER:避孕套 == 0 && TCVAR:PLAYER:2 == 1
		;閻魔様の罰は準備中
		SETBIT CFLAG:1800, 0
		ひどい ++
		PRINTDATA
			DATAFORM 「等一下…拜托了……
			DATAFORM 「不要,不要啊……
			DATAFORM 「住手！！拔出来吧！！　……
			DATAFORM 「里面是！里面不行！！　……
		ENDDATA
		SELECTCASE ひどい
			CASE 1
				PRINTFORM %TEXTR("不要再…了/这样做…不要这样/真的…不要这样")%………
			CASEELSE
				PRINTFORM %TEXTR("已经不…饶过我吧…/…真的是…真讨厌呀…/拜托了…别这样…")%………
		ENDSELECT
		PRINTFORML %TEXTR("不要啊啊——/咿咦呀————！！/不行啊啊———！！/啊啊啊啊——！")%！」
	ELSEIF 状況 == 2
		SIF EXP:30:精飲経験 < 20 && !TALENT:恋慕
			PRINTFORMW （%TEXTR("等会就吐出来…/真不想咽下去啊…/请射在外面啊…")%）
	ENDIF
