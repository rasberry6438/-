;==================================================
@M_KOJO_MESSAGE_COM_K132_23
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K132_23_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K132_23_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;失敗
	IF TFLAG:193
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋人
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 袿姬靠近%CALLNAME:MASTER%的耳边，轻声细语道
			PRINTFORMW 「你这个雜魚♡」
			PRINTFORMW 随后说道
			PRINTFORMW 「这是我从外界书上新学的呢，喜欢吗?」
		CASE 1
			PRINTFORMW 「和%CALLNAME:MASTER%睡在一起有一种莫名的安心感呢……」
		CASE 2
			PRINTFORMW 袿姬如同宝石般的眼睛看着你，没有说一句话，只是注视着你
			PRINTFORMW 仿佛要将你的灵魂看穿
		ENDSELECT
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「真想有一天把%CALLNAME:MASTER%变成属于我的东西呢……」
		CASE 1
			PRINTFORMW 「真好呢，和%CALLNAME:MASTER%在一起……」
		CASE 2
			PRINTFORMW 「真想把%CALLNAME:MASTER%的灵魂抽出来紧紧地抱在怀里睡觉呢……」
		ENDSELECT
		RETURN 1
	;思慕
	ELSEIF TALENT:思慕
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1