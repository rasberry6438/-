;==================================================
@M_KOJO_MESSAGE_COM_K132_22
;CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K132_22_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K132_22_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋人
		SELECTCASE RAND:3
			CASE 0
			PRINTFORMW 袿姬主动掀开自己带有花纹的黄色裙子，露出里面的胖次
			PRINTFORMW 见到脸已经通红但不为所动的%CALLNAME:MASTER%，袿姬有些愉悦
			PRINTFORMW 「真是可爱呢，明明已经支起小帐篷了却还在抵抗呢♡」
			PRINTFORMW 「真是没办法呢♡」
			CASE 1
			PRINTFORMW 袿姬故意将自己的胸部展露给%CALLNAME:MASTER%看，但是对方宁愿脸憋的通红也还是不为所动
			PRINTFORMW 「真是没办法 真是没办法」
			PRINTFORMW 袿姬对着%CALLNAME:MASTER%的耳边轻语道
			PRINTFORMW 「如果想要的话就直说吧?♡」
			CASE 2
			PRINTFORMW 故意将自己完美的身材展现的袿姬看到不为所动的%CALLNAME:MASTER%
			PRINTFORMW 便绕到身后，对着%CALLNAME:MASTER%赤红的耳根轻轻地吹了一口气
			PRINTFORMW 随后轻语道
			PRINTFORMW 「想要的话就直说吧？到时候憋坏了可不好哦♡」
		ENDSELECT
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1
