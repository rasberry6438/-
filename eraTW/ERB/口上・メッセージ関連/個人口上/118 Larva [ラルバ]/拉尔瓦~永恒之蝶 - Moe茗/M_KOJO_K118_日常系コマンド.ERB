;执行什么命令。 描述经验和SOURCE波动
;RETURN 1=执行命令 / RETURN 0=取消（跳过源计算） / RETURN -1=取消命令
;コマンド実行内容。経験・SOURCE変動を記述
;RETURN 1=コマンド実行 / RETURN 0=キャンセル(SOURCE計算SKIP) / RETURN -1=コマンド取り消し
@KOJO_Moe_COM_K118_0
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 10
ELSE
	IF  ((((ABL:TARGET:9 * 10) + (ABL:TARGET:10 * 15) + ((CFLAG:TARGET:2 + 1)/ 12 ) + (CFLAG:TARGET:4 / 2))) < 300 ) && !CFLAG:睡眠
		TFLAG:193 = 99
		LOCAL = 0
		;固定獲得ソース
		SOURCE:反感 = 500 + RAND:50
		SOURCE:恐怖 = 750 + RAND:50
		IF CFLAG:2 <= 100
			LOCAL -= 50
		ELSEIF CFLAG:2 <= 300
			LOCAL -= 30
		ELSEIF CFLAG:2 <= 500
			LOCAL -= 10
		ENDIF
		;好感度変化
		TFLAG:97 += LOCAL
		TIME += 10
		RETURN 1
	ENDIF 
	SELECTCASE TFLAG:192
		;通常
		CASE 0
			LOCAL = RAND:100
			LOCAL:1 = 80 + GET_SUCCESS_RATE() / 7
			SIF LOCAL:1 > 109
				LOCAL:1 = 109
			IF LOCAL <= 9
				;大成功
				TFLAG:193 = 1
				TFLAG:98 = 2
			ELSEIF (LOCAL >= LOCAL:1) && !(TALENT:TARGET:3 || TALENT:TARGET:7)	;恋人或恋慕不可能失败
				;失敗
				TFLAG:193 = -1
				TFLAG:98 = -2
			ELSE
				;残りは成功
				TFLAG:193 = 0
				TFLAG:98 = 0
			ENDIF
		;強制成功or大成功
		CASE 1
			IF RAND:20 + 70 > GET_SUCCESS_RATE() / 7
				TFLAG:193 = 0
				TFLAG:98 = 0
			ELSE 
				TFLAG:193 = 1
				TFLAG:98 = 2
			ENDIF 
		;強制失敗
		CASE -1
			TFLAG:193 = -1
			TFLAG:98 = -2
		;指令終了
		CASE -2
			TFLAG:193 = -1
			RETURN -1
	ENDSELECT

	;先制発生時は失敗しない
	SIF TFLAG:102 == 3 && TFLAG:192 != -1
		TFLAG:193 = MAX(TFLAG:193,0)
	IF CFLAG:334		;陪睡
		DOWNBASE:MASTER:気力 += 20
	ELSEIF CHK_DATENOW(CFLAG:TARGET:12)		;约会中
		DOWNBASE:気力 += 30
		DOWNBASE:MASTER:気力 += 30
	ELSE
		DOWNBASE:気力 += 60
		DOWNBASE:MASTER:気力 += 40
	ENDIF
	
	;-------------------------------------------------
	;ソース追加処理
	;-------------------------------------------------
	;固定で獲得するソース
	;最低获得的一些SOURCE

	SOURCE:歓楽 = 200
	SOURCE:情愛 = 100
	SOURCE:苦痛 = 100
	SOURCE:欲情 = 50
	SOURCE:逸脱 = 150
	SOURCE:反感 = 750
	SOURCE:受動 = 200 

	SELECTCASE ABL:親密
		CASE IS < 5
			SOURCE:歓楽 +=  (ABL:親密 * 10)
			SOURCE:反感 += ((5 - ABL:親密) * 10)
		CASE IS < 10 
			SOURCE:歓楽 +=  50 +  (ABL:親密 * 20)
			SOURCE:反感 -= ((ABL:親密 - 5) * 50 + 50) 
			SOURCE:苦痛 -= ((ABL:親密 - 5) * 10 ) 
		CASEELSE
			SOURCE:歓楽 +=  100 +  (ABL:親密 * 50)
			SOURCE:反感 -= ((ABL:親密 - 5) * 75)
			SOURCE:苦痛 -= ((ABL:親密 - 5) + 50) 
	ENDSELECT

	SELECTCASE CFLAG:TARGET:4 
		CASE IS < 100 
			SOURCE:歓楽 += MIN(CFLAG:TARGET:2 , 200 )
			SOURCE:反感 += (100 - CFLAG:TARGET:4)
		CASE IS < 500 
			SOURCE:歓楽 += 100 + MAX((CFLAG:TARGET:2 - 100) / 4 , 100 )
			SOURCE:情愛 += 100 + (CFLAG:TARGET:4  / 3)
			SOURCE:反感 -= CFLAG:TARGET:4 
			SOURCE:受動 += (CFLAG:TARGET:4  / 5)
		CASE IS < 1500 
			SOURCE:歓楽 += 500 + MAX((CFLAG:TARGET:2 -500) / 5 ,200 )
			SOURCE:情愛 += 350 + (CFLAG:TARGET:4  / 4)
			SOURCE:受動 += 100 + ((CFLAG:TARGET:4 - 500) / 10 )
			SOURCE:反感 = 0
		CASEELSE 
			SOURCE:歓楽 += 1000 + MAX((CFLAG:TARGET:2 - 1000) / 5 ,300 )
			SOURCE:情愛 += 700
			SOURCE:受動 += 200 + ((CFLAG:TARGET:4 - 1500) / 30 )
			SOURCE:反感 = 0
	ENDSELECT

	SIF SOURCE:反感 < 0
		SOURCE:反感 = 0
	SIF SOURCE:苦痛 < 0
		SOURCE:苦痛 = 0

	;寝てる場合効果半額
	IF CFLAG:313 && SOURCE:歓楽 > 0
		SOURCE:歓楽 = SOURCE:歓楽 / 2
		SOURCE:情愛 = SOURCE:情愛 / 2
	ENDIF

	;好感度变化判别
	LOCAL = 0
	IF !CFLAG:睡眠
		;親密が低いと好感度低下
		IF ABL:9 == 0
			LOCAL -= 7
		ELSEIF ABL:9 < 3
			LOCAL -= 5
		ELSEIF ABL:9 < 5
			LOCAL -= 2
		ENDIF
		;好感度が低いと好感度低下
		IF CFLAG:2 <= 200
			LOCAL -= 5
		ELSEIF CFLAG:2 <= 500
			LOCAL -= 3
		ELSEIF CFLAG:2 <= 1000
			LOCAL -= 1
		ENDIF
	ELSE 
		TFLAG:97 = 0 
	ENDIF

	;好感度変化
	TFLAG:97 += LOCAL

	;信赖度变化判别
	SIF TFLAG:193 >= 0 
	TFLAG:98 += 1
	
	IF TFLAG:193 == -1
		TIMES SOURCE:歓楽 , 0.50
		TIMES SOURCE:情愛 , 0.50
		TIMES SOURCE:受動 , 0.50
		TIMES SOURCE:欲情 , 0.50
		TIMES SOURCE:反感 , 1.25
	ELSEIF TFLAG:193 == 0
		TIMES SOURCE:歓楽 , 1.00
		TIMES SOURCE:情愛 , 1.00
		TIMES SOURCE:受動 , 1.00
		TIMES SOURCE:欲情 , 1.00
	ELSE
		TIMES SOURCE:歓楽 , 2.00
		TIMES SOURCE:情愛 , 2.00
		TIMES SOURCE:受動 , 2.00
		TIMES SOURCE:欲情 , 2.00
		TIMES SOURCE:反感 , 0.75
	ENDIF

	;最后的清0与随机数判定
	LOCAL = 0 
	FOR LOCAL , 0 , 56
		SIF SOURCE:LOCAL < 0 
			SOURCE:LOCAL = 0 
		SIF SOURCE:LOCAL == 0
			CONTINUE
		SELECTCASE SOURCE:LOCAL 
			CASE IS < 50 
				SOURCE:LOCAL += 5 
				SOURCE:LOCAL -= RAND:10 
			CASE IS < 200 
				SOURCE:LOCAL += 10
				SOURCE:LOCAL -= RAND:20
			CASE IS < 500 
				SOURCE:LOCAL += 20
				SOURCE:LOCAL -= RAND:40
			CASEELSE
				SOURCE:LOCAL += 40
				SOURCE:LOCAL -= RAND:80
		ENDSELECT
	NEXT
	;时间判定
	TIME += 10
	;定义自己的370是摸翅膀时刻
	TCVAR:370 = 1440 * DAY + TIME
	IF CHK_DATENOW(CFLAG:TARGET:12)		;约会中	
		;約會経験
		EXP:MASTER:77 ++
		EXP:TARGET:77 ++
	ENDIF
	IF TALENT:TARGET:7 
		EXP:TARGET:41 += RAND:3	;爱情经验
	ENDIF 
ENDIF 
RETURN 1
