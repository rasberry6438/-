;81,フェラチオ
;==================================================
@M_KOJO_Moe_MESSAGE_COM_K118_81
CALL LARVA_MOE_SET_COLOR(C_MOE_LARVA)
CALL M_KOJO_Moe_MESSAGE_COM_K118_81_1
RETURN RESULT

@M_KOJO_Moe_MESSAGE_COM_K118_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「要%MASTERNAME:118%的这个，放进嘴里吗？」
		PRINTL 
		PRINTFORMDL 拉尔瓦饶有兴致地盯着眼前的怒张……
		PRINTL 
		IF GROUPMATCH(TALENT:MASTER:形状,6,10)
			PRINTFORML 「好大哇……可能放不进去哇……」
		ELSEIF GROUPMATCH(TALENT:MASTER:形状,3,4,5)
			PRINTFORML 「可能会有些难做好……」
		ELSE
			PRINTFORML 「好像没问题的说……」
		ENDIF
		PRINTL 
		PRINTFORMW 「不过，让妖精做这样的事……%MASTERNAME:118%真变态——」
		PRINTL 
		PRINTFORMDW 还没开始就被无情地嘲笑了……
		PRINTL 
		IF TALENT:118:7 || TALENT:118:3 
			IF TALENT:118:27 
				PRINTFORML 「不过，这样做会舒服的话，那就没法拒绝了呢……」
				PRINTFORMW 「所以呢，像这样……啊呜——」
			ELSE 
				PRINTFORML 「这么变态的%MASTERNAME:118%，不好好地让你舒服起来是不行的～」
				PRINTFORMW 「所以呢，像这样……啊呜——」
			ENDIF 
			PRINTL 
			PRINTFORMDW 忽而被那幼小口腔包覆住整个尖端的触感，刺激得你几乎要跳起来……
			PRINTL 
			PRINTFORML 「诶？弄疼了吗……是要轻一点吗？」
			PRINTFORMW 「因为，碰到上面的时候，%MASTERNAME:118%就会变得舒服，所以就这样……」
			PRINTL 
			PRINTFORMDL 你指导着拉尔瓦，先舔一舔前端，再去慢慢地进入下一步……
			PRINTFORMDW 就这样享受起了蝴蝶妖精的口交奉仕……
		ELSE 
			PRINTFORML 「只是舔一舔噢……」
			PRINTFORMW 「再怎么说，%CSTR:118:20%都不喜欢那个味道啦！呜……只是舔一下……」
			PRINTL 
			PRINTFORMDW 只是被幼女一样的蝴蝶妖精舔弄着阴茎，不由自主地便因为背德感而兴奋了起来……！
		ENDIF 
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:3
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

