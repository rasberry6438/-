					PRINTFORML 「啊、%CALLNAME:MASTER%ー、睡过头太多了吧ー」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「啊、%CALLNAME:MASTER%、%M_KOJO_EVENT_K111_1_HELLO()%」
				CASE 2
					PRINTFORML 「%M_KOJO_EVENT_K111_1_HELLO()%、%CALLNAME:MASTER%！」
				CASEELSE
					PRINTFORML 「啊、%CALLNAME:MASTER%、晚上好♪」
				ENDSELECT
			ENDIF
		ENDSELECT
	;来訪
	ELSE
		PRINTFORML 「啊、%CALLNAME:MASTER%！」
