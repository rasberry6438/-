	;8,贈り物を貰った
	ELSEIF !GETBIT(TCVAR:111:359, 8) && TCVAR:[[克勞恩皮絲]]:今天的礼物 && FLAG:约会的对象 == [[克勞恩皮絲]]
		CALL M_KOJO_K111_TALKSET("状況会話", 8)
