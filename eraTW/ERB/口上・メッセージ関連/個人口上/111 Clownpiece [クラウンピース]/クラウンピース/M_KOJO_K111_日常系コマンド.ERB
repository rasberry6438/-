;==================================================
;621,釣魚
;TFLAG:193(0=釣れない 1～4=魚 11～15=好吃的魚 610=木材 131=守矢之守札 21=跳蛋 609=氷之鱗 606=龍之爪)
;TFLAG:194(1000以上ならTARGETが釣った)
;==================================================
@M_KOJO_MESSAGE_COM_K111_621
DRAWLINE
CALL M_KOJO_MESSAGE_COM_K111_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K111_621_1
#DIM CHARA
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;地の文カットは危ないので廃止
IF LOCAL
	SELECTCASE TFLAG:193
	;釣れない
	CASE 0
		IF TALENT:111:恋慕
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「啊～～没钓到～～っ！」
			CASE 2
				PRINTFORMW 「是不是这里没有鱼啊～♪」
			CASEELSE
				PRINTFORMW 「%CALLNAME:MASTER%、不太行嘛～」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「……好无聊ー！！」
			CASE 2
				PRINTFORMW 「……这个很有趣吗？」
			CASEELSE
				PRINTFORMW 「……什么都钓不到嘛……」
			ENDSELECT
		ENDIF
	;木片
	CASE 610
		IF TFLAG:194 > 1000
			PRINTFORMW 「快看！ 好像调到什么了！！」
			PRINTFORMW 「……姆～～！」
		ELSE
			PRINTFORMW 「呀哈哈哈！ 居然是木片～！」
		ENDIF
	;紙切れ
	CASE 131
		PRINTFORMW 「……？ 这是什么？？ 垃圾吗？」
	;青蛙
	CASE 700
		PRINTFORMW 「呀哈哈哈！ 青蛙ー！ 呱呱！ 」
	;跳蛋
	CASE 21
		IF TFLAG:194 > 1000
			PRINTFORMW 「嗯？ 这是……\/\/\/」
		ELSE
			PRINTFORML 「嗯？ 这是……」
			PRINTFORMW 「%CALLNAME:MASTER%是有多好色才能钓到这种东西啊～！」
		ENDIF
	CASEELSE
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			LOCALS = %FISH_NAME(TFLAG:193)%
		CASEELSE
			LOCALS = %ITEMNAME:(TFLAG:193)%
		ENDSELECT
		SELECTCASE LOCALS
		CASE "氷之鱗", "龍之爪"
			PRINTFORMW 「这是什么ー！？ 好好看～～っ！！」
		CASE "龍涎香"
			PRINTFORMW 「这是什么、垃圾吗」
		CASE "食人魚"
			PRINTFORMW 「哦哦～食人鱼欸！ 」
		CASE "鰻魚", "八目鰻", "鲶魚"
			PRINTFORMW 「呀哈哈哈！滑溜溜的～！」
		CASEELSE
			IF LOCAL:1 >= 80
				PRINTFORMW 「哇哦！ 好大！！」
			ELSEIF INRANGE(TFLAG:193, 11, 20) || INRANGE(TFLAG:193, 24, 60)
				PRINTFORMW 「哦ー！ 好厉害！ 这可以吃吗？」
			ELSEIF TALENT:111:恋慕 || TALENT:111:思慕
				PRINTFORMW 「哇っ！ 好厉害！ 这是什么？？」
			ELSE
				PRINTFORMW 「哦ー！ 很能干吗！」
			ENDIF
		ENDSELECT
	ENDSELECT
ENDIF
RETURN 1
