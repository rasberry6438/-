@M_KOJO_MESSAGE_COM_K111_300_1
#DIM POINT
#DIM 约会会話, 120
;日が変わってたら约会会話リセット
SIF !TCVAR:111:353
	VARSET 约会会話
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	RESETCOLOR
	DRAWLINE
	;ここに口上書いて下さい
	IF ALCOHOL_FACE(111) == "酔酒" || ALCOHOL_FACE(111) == "不省人事"
		PRINTFORML %CALLNAME:MASTER%尝试向%CALLNAME:TARGET%搭话、可%CALLNAME:TARGET%口齿不清完全听不懂在说些什么
	;会話不能の地の文は恋慕でない場合は省略できず、恋慕の時は表示されないので、ここではどちらの場合も同じように表示されるようにする
	ELSEIF TCVAR:111:会話不能
		IF TALENT:111:恋慕
			PRINTFORML 虽然想向%CALLNAME:TARGET%搭话、可是不知道该说些什么
			PRINTFORMW 四周弥漫着尴尬的气氛…
		ENDIF
	ELSE
		;约会会話
		IF FLAG:约会的对象 == [[克勞恩皮絲]] && DATENAME_SPOT() != "道中" && !约会会話:(CFLAG:111:現在位置 / 10)
			CLEARLINE 2
			PRINTFORML 在%DATENAME_SPOT()%约会
			DRAWLINE
		ENDIF
		PRINTFORM %CALLNAME:MASTER%
		IF TALENT:111:恋慕 && !M_KOJO_K111_FUKIGEN(111)
			PRINTDATA
				DATAFORM 坐到了%CALLNAME:TARGET%身旁
				DATAFORM 有些害羞的和%CALLNAME:TARGET%
				DATAFORM 在相互微笑着的同时
				DATAFORM 在一片和睦的气氛中
				DATAFORM 简短地寒暄了一番后
				DATAFORM 一邊和%CALLNAME:TARGET%玩鬧一邊
			ENDDATA
		ELSE
			PRINTDATA
				DATAFORM 有些无聊地
				DATAFORM 无言地
				DATAFORM 客套地
				DATAFORM 平和地
				DATAFORM 一如往常地
				DATAFORM 稍稍地
				DATAFORM 滑稽地
			ENDDATA
		ENDIF
		IF TFLAG:SELECTCOM的分岐 > 0
			PRINTFORML 谈性十足地交流着
		ELSEIF TFLAG:SELECTCOM的分岐 < 0
			PRINTFORML 话说到一半……
		ELSE
			PRINTFORML 說着話
		ENDIF
	ENDIF
	PRINTW 
	CALL M_KOJO_COLOR_K111
	IF ALCOHOL_FACE(111) == "酔酒" || ALCOHOL_FACE(111) == "不省人事"
		IF !RAND:2
			PRINTFORML 「欸嘿嘿♪ 喝酒超棒的♪ 还有，同样喜欢喝酒的%CALLNAME:MASTER%也是我的最爱哦❤」
			PRINTFORMW 「～～ッ！！＠＃！＄％！？＊＊＊＊ッ！！」
		ELSE
			PRINTFORML 「然后～～、打開瓶子～～～一下子喝掉～～～～～！」
			PRINTFORMW 「很快就会～～醉过去、哈～～～～哈哈哈、嘿呀呀呀呀！！」
		ENDIF
		PRINTFORML 
		PRINTFORMDW 完全无法交谈的样子...
	ELSEIF M_KOJO_K111_FUKIGEN(111)
		PRINTFORML 「什么鬼！？」
		PRINTFORMW 「准备就这样丢下我去和别的女孩子说话？」
	;会話不能
	ELSEIF TCVAR:111:会話不能
		PRINTFORML 「还来...？」
		PRINTFORMW 「这种没劲的话题要说几次啊..」
	;失敗
	ELSEIF TFLAG:SELECTCOM的分岐 == -1
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「嗯ー、啊嗯唔唔ー」
			PRINTFORMW 「好没劲的话题啊啊啊！」
		CASE 2
			PRINTFORML 「%CALLNAME:MASTER%喜欢说我听不懂的话题」
			PRINTFORMW 「这样的男孩子会被女孩子讨厌哦ー」
		CASEELSE
			PRINTFORML 「嗯...」
			PRINTFORMW 「皮斯不是很懂你在说什么」
		ENDSELECT
	;夏,野外,汗フラグ無,昼間,地上,晴天で確定
	ELSEIF DAY:2 == 2 && OUTROOF(CFLAG:111:現在位置) && !TCVAR:111:350 && INRANGE(TIME, 600, 960) && M_KOJO_K111_EARTH(CFLAG:111:現在位置) && 天候 < 2
		;暑いの大好きだった
		SELECTCASE RAND:5
		CASE 1
			PRINTFORML 「%CALLNAME:MASTER%、热不热？ 是不是热的受不了？」
			PRINTFORMW 「哈哈！真棒！ 看人被晒得晕头转向的样子可是我的兴趣之一♪」
			TALENT:111:心情 = 1
;			PRINTFORML 「毎日暑いね……」
;			PRINTFORMW 「灼熱地獄に比べたらこれでも涼しい方なんだろうケド」
		CASE 2
			PRINTFORML 「居然，每天都能被如此阳光照射……」
			PRINTFORMW 「体内的热情、动力都被燃烧起来了，就是所谓热血沸腾吧！！」
;			PRINTFORMW 「地上もこんなに暑いなんて……」
		CASE 3
			PRINTFORML 「对于生在地狱长在地狱的我来说」
			PRINTFORMW 「没有什么比夏日的阳光直射更棒的了～♪」
		CASE 4
			PRINTFORML 「沐浴在阳光下的我，浑身充满了力量！」
			PRINTFORMW 「呜哇哇ー！ 燃起来了ー！！」
			TALENT:111:心情 = 1
		CASEELSE
			PRINTFORML 「和灼热地狱比起来只是毛毛雨啦」
			PRINTFORMW 「但可以感觉到太阳光蕴含的能量，大概和灼热地狱是不同的原理～」
;			PRINTFORML 「ぐでー……あついー……」
;			PRINTFORMW 「%CALLNAME:MASTER%、この暑さなんとかならないのー？」
		ENDSELECT
		PRINTL 
		PRINTFORMDL 紧贴克劳恩皮斯大腿的裤袜，细看之下散发着丝缕热气
		PRINTFORMDW 总觉得…很热吧…
		TCVAR:111:350 = 1
	;禮物会話１（禮物の礼物）,フラグ発生から5日以上
	ELSEIF CFLAG:[[克勞恩皮絲]]:1005 && DAY - CFLAG:[[克勞恩皮絲]]:1005 >= 5
		PRINTFORMW 「啊、想起来了！ 今天要把这个给%CALLNAME:MASTER%」
		;12日以上
		SIF DAY - CFLAG:[[克勞恩皮絲]]:1005 >= 12
			PRINTFORML 「%CALLNAME:MASTER%是忘记了吗……」
		PRINTFORMW 「是之前说的礼物哦！」
		SELECTCASE RAND:8
		CASE 1
			LOCALS:1 = 狂気的
		CASE 2
			LOCALS:1 = 地獄的
		CASE 3
			LOCALS:1 = 滑稽的
		CASE 4
			LOCALS:1 = 亲自做的
		CASEELSE
			LOCALS:1 = 星星与条纹的
		ENDSELECT
		SELECTCASE RAND:8
		CASE 1
			LOCALS:2 = 手帕
		CASE 2
			LOCALS:2 = 头巾
		CASE 3
			LOCALS:2 = 首饰
		CASE 4
			LOCALS:2 = 马克杯
		CASE 5
			LOCALS:2 = 小灯
		CASE 6
			LOCALS:2 = 宝石
		CASE 7
			LOCALS:2 = 戒指
		CASEELSE
			IF TALENT:MASTER:性別 == 1
				LOCALS:2 = 丝带
			ELSE
				LOCALS:2 = 领带
			ENDIF
		ENDSELECT
		CALL GIFT_ORDER_MADE_FROM_CHARA([[克勞恩皮絲]], LOCALS:1, LOCALS:2)
		PRINTL 
		CALL COLORMESSAGE(@"克勞恩皮絲から【%RESULTS%】を貰った！", C_AQUA, 2)
		PRINTL 
		PRINTFORMW 「毕竟是我送给你的礼物、不需要你感谢的话了哦！」
		CFLAG:[[克勞恩皮絲]]:1005 = 0
	;禮物会話２（圣诞节禮物）,フラグ発生から1日以上12日以内
	ELSEIF CFLAG:[[克勞恩皮絲]]:1006 && INRANGE(DAY - CFLAG:[[克勞恩皮絲]]:1006, 1, 12)
		PRINTFORMW 「嗯……%CALLNAME:MASTER%……给你这个……」
		;年越してる
		SIF DAY - CFLAG:[[克勞恩皮絲]]:1006 > 7
			PRINTFORML 「虽说过了很久才给你……」
		PRINTFORMW 「圣诞节礼物……一直都很想要对吧！」
		PRINTFORMW 「所以，这个给你！」
		SELECTCASE RAND:8
		CASE 1
			LOCALS:1 = 圣诞节的
		CASE 2
			LOCALS:1 = 幽灵的
		CASE 3
			LOCALS:1 = 圣诞老人的
		CASE 4
			LOCALS:1 = 驯鹿的
		CASE 5
			LOCALS:1 = 圣诞夜的
		CASE 6
			LOCALS:1 = 下雪的
		CASE 7
			LOCALS:1 = 星星与条纹的
		CASEELSE
			LOCALS:1 = 雪与星的
		ENDSELECT
		SELECTCASE RAND:8
		CASE 1
			LOCALS:2 = 怀表
		CASE 2
			LOCALS:2 = 小灯
		CASE 3
			LOCALS:2 = 手套
		CASE 4
			LOCALS:2 = 卡片
		CASEELSE
			LOCALS:2 = スノードーム
		ENDSELECT
		CALL GIFT_ORDER_MADE_FROM_CHARA([[克勞恩皮絲]], LOCALS:1, LOCALS:2)
		PRINTL 
		CALL COLORMESSAGE(@"克勞恩皮絲から【%RESULTS%】を貰った！", C_AQUA, 2)
		PRINTL 
		PRINTFORMW 「明年的话，该轮到%CALLNAME:MASTER%送给我了哦！！」
		CFLAG:[[克勞恩皮絲]]:1006 = 0
	;约会会話
	ELSEIF FLAG:约会的对象 == [[克勞恩皮絲]] && DATENAME_SPOT() != "道中" && !约会会話:(CFLAG:111:現在位置 / 10)
		POINT = CFLAG:111:現在位置 / 10
		;▼博麗神社
		SELECTCASE STR:(POINT + 6000)
		CASE "桜花道的鳥居"
			;春
			IF DAY:2 == 1
				PRINTFORML 「这里的樱花很美吧！」
				PRINTFORMW 「第一次看到的时候，连心底都被震撼到了！」
			;春以外
			ELSE
				PRINTFORML 「这里的樱花很棒的！」
				PRINTFORMW 「记不清已经多少年春天都来这里赏花了ー♪」
			ENDIF
		CASE "神社境内"
			PRINTFORML 「要注意一下博丽的巫女！」
			PRINTFORMW 「每次恶作剧她总会第一个跑来干扰」
		CASE "神社本堂"
			PRINTFORML 「既然是神社，那肯定有神明住在里面吧」
			PRINTFORMW 「会不会比主人更强呢？」
		CASE "神社走廊"
			;TW内の設定では別荘ということにしておく
			PRINTFORML 「这里的地下是我的大别墅哦ー」
			PRINTFORMW 「虽然不知为什么，但神社的地下我呆着相当惬意」
		CASE "妖精的大樹"
			;誰かいる
			IF GROUPMATCH(CFLAG:[[克勞恩皮絲]]:現在位置, CFLAG:[[桑尼]]:現在位置, CFLAG:[[露娜]]:現在位置, CFLAG:[[斯塔]]:現在位置)
				PRINTFORML 「这是我妖精朋友的住处」
				PRINTFORMW 「一起去玩玩吧ー！」
			;留守
			ELSE
				PRINTFORML 「这里是我妖精朋友的住处……」
				PRINTFORMW 「嗯……怎么没人在啊」
			ENDIF
		CASE "夢幻遺跡"
			PRINTFORML 「哇哦ー？ 这里好大……」
			PRINTFORMW 「感觉……好神奇？ 怎么会有这种地方」
		;▼命蓮寺
		CASE "地蔵小路"
			PRINTFORML 「我不是常来这边」
			PRINTFORMW 「忘了到底是怎么回事了，不过这所寺庙对妖精不是很友好」
		CASE "倒垂的柳樹下"
			PRINTFORML 「哦！真是棵其貌不扬的树！」
			PRINTFORMW 「总觉得能化成厉害的妖怪不是吗？」
		CASE "命蓮寺境内"
			PRINTFORML 「这里就是传闻中妖怪的寺庙～」
			PRINTFORMW 「唔……确实感受得到妖力在涌动」
		CASE "命蓮寺方丈"
			PRINTFORML 「%CALLNAME:MASTER%、这里的人都在干什么呀？」
			PRINTFORMW 「总觉得一直很忙的样子」
		CASE "墓地"
			PRINTFORML 「哇～！ 是墓地诶～♪」
			PRINTFORMW 「大家一起去地狱玩吧～♪」
		CASE "神霊廟広場"
			PRINTFORML 「哇哦ー！ 好气派的大门！」
			PRINTFORMW 「想必住在这里的是位不得了的神明大人吧～」
		CASE "神霊廟道場"
			PRINTFORML 「仙人的道场……？」
			PRINTFORMW 「之前我有挺友人大人说过关于仙人的事情～」
		;▼人里
		CASE "広場"
			PRINTFORML 「哦！ 发现了合适的猎物」
			PRINTFORMW 「看我去捉弄一下那些人！」
		CASE "商家町"
			PRINTFORML 「%CALLNAME:MASTER%！ 我想去那里想去哪里！」
			PRINTFORMW 「啊！ 给我买那个！ 买那个！ 好不好嘛！ 给我买嘛！」
		CASE "呑屋小道"
			PRINTFORML 「那边是有居酒屋吗」
			PRINTFORMW 「空气中的酒香告诉我有这种可能！」
		CASE "寺子屋"
			PRINTFORML 「人类就是在这种地方学习的啊？ 嗯哼~~」
			PRINTFORMW 「我也可以在这里画画吗？」
		CASE "大衆浴場"
			IF DAY * 1440 + TIME - EX:MASTER:上次的入浴時间 < 360
				PRINTFORML 「呼呼～！ 清爽了～！」
				PRINTFORMW 「不过为什么不能和%CALLNAME:MASTER%一起洗嘛，没劲」
			ELSE
				PRINTFORML 「哇～！ 这有浴室～♪」
				PRINTFORMW 「釜茹で風呂はないのかー？」
			ENDIF
		CASE "情人用宿屋"
			;入った時点で合意取得するのね…,诶嘿嘿前か後かは絶頂したかで判断するので事前に絶頂させてると稍微齟齬
			IF EX:111:Ｃ絶頂 + EX:111:Ｖ絶頂 + EX:111:Ａ絶頂 + EX:111:Ｂ絶頂 + EX:111:Ｍ絶頂
				IF TALENT:111:処女
					PRINTFORMW 「……\/\/\/」
				ELSE
					PRINTFORMW 「……❤」
				ENDIF
			ELSE
				IF TALENT:111:処女
					PRINTFORMW 「%CALLNAME:MASTER%……就在这儿做吧……\/\/\/」
				ELSE
					PRINTFORMW 「快点%UNICODE(0x2764)% 快点来嘛%UNICODE(0x2764)%」
				ENDIF
			ENDIF
		CASE "長屋街"
			PRINTFORML 「第一次见到，原来人类每天都在做这样的事情啊，好有趣」
			PRINTFORMW 「%CALLNAME:MASTER%为什么不住在这里啊？」
		;▼霧之湖
		CASE "霧之湖"
			IF DAY:2 == 2
				PRINTFORML 「呀哈哈ー♪ %CALLNAME:MASTER%快，和我一起去游泳吧♪」
				PRINTFORMW 「只是靠近就已经能感受到清爽的湿气了！」
			ELSE
				PRINTFORML 「实在是了不得的厚重雾气」
				PRINTFORMW 「呼呼呼，没猜错的话，里面肯定有什么不可告人的秘密！」
			ENDIF
		CASE "冰造的小雪屋"
			PRINTFORML 「啊ー！ 这是那个蓝色笨蛋的家ー！」
