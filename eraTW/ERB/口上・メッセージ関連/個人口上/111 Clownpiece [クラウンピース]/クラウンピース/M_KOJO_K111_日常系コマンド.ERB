		CALL M_KOJO_K111_TALKSET("状況会話", 27)
	;28,微醺
	ELSEIF !GETBIT(TCVAR:111:359, 28) && ALCOHOL_FACE(111) == "微醺"
		CALL M_KOJO_K111_TALKSET("状況会話", 28)

[SKIPSTART]
	ELSEIF !GETBIT(TCVAR:111:359, ) && 
		CALL M_KOJO_K111_TALKSET("状況会話", )
