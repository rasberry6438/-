		CALL M_KOJO_K111_TALKSET("状況会話", 12, ABL:[[斯塔]]:親密)
	;13,琪露諾の話
	ELSEIF !GETBIT(TCVAR:111:359, 13) && CFLAG:[[克勞恩皮絲]]:現在位置 == CFLAG:[[琪露諾]]:現在位置
		CALL M_KOJO_K111_TALKSET("状況会話", 13, ABL:[[琪露諾]]:親密)
	;14,拉瓦爾の話
	ELSEIF !GETBIT(TCVAR:111:359, 14) && CFLAG:[[克勞恩皮絲]]:現在位置 == CFLAG:[[拉瓦爾]]:現在位置
		CALL M_KOJO_K111_TALKSET("状況会話", 14, ABL:[[拉瓦爾]]:親密)
