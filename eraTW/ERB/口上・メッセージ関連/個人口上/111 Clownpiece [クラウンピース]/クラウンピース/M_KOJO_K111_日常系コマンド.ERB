	;▼ここから感想＝＝＝＝＝＝＝＝＝＝
	IF STRCOUNT(DISH_TASTE, "心跳味/") || STRCOUNT(DISH_TASTE, "未知の味/")
		PRINTFORML 「嗯嗯っ！？」
		PRINTFORMW 「什么嘛，有股神秘的味道！？」
		IF TCVAR:[[克勞恩皮絲]]:料理評価値 >= 650
			PRINTFORMW 「嗯ー…这个味道不错哦～♪」
		ELSEIF TCVAR:[[克勞恩皮絲]]:料理評価値 >= 500
			PRINTFORMW 「嗯嗯…算不上讨厌……」
		ELSE
			PRINTFORMW 「唔っ……难吃死了……」
		ENDIF
	ELSEIF TCVAR:[[克勞恩皮絲]]:料理評価値 < 650
		VARSET LOCAL
		LOCAL:1 = STRCOUNT(DISH_TASTE, "野菜/")
		LOCAL:2 = STRCOUNT(DISH_TASTE, "简易/")
		LOCAL:3 = STRCOUNT(DISH_TASTE, "酸/")
		LOCAL:4 = STRCOUNT(DISH_TASTE, "健康/")
		LOCAL:5 = STRCOUNT(DISH_TASTE, "淡白/")
		FOR LOCAL, 1, 10
			SIF LOCAL:LOCAL
				LOCAL:10 ++
		NEXT
		;以上都不是
		IF !LOCAL:10
			IF TCVAR:[[克勞恩皮絲]]:料理評価値 < 500
				PRINTFORMW 「呸ー、我讨厌这个！」
			ELSE
				PRINTFORMW 「唔、还可以吧」
			ENDIF
		ELSE
			;シャッフルして最初にヒットしたやつ
			CALL FISHER_YATES_SHAFFLE(10)
			FOR LOCAL, 1, 10
				SIF !SHAFFLE_ARRAY:LOCAL
					CONTINUE
				IF LOCAL:(SHAFFLE_ARRAY:LOCAL)
					SELECTCASE SHAFFLE_ARRAY:LOCAL
					;野菜
					CASE 1
						PRINTFORML 「呸呸呸ー！ 怎么加了蔬菜啊……」
						PRINTFORMW 「我不需要这个、%CALLNAME:MASTER%自己吃吧！」
					;简易
					CASE 2
						PRINTFORMW 「唔嗯、太清淡了，不合胃口…」
					;酸
					CASE 3
						PRINTFORML 「唔，太酸了……」
						PRINTFORMW 「不喜欢这个味道……」
					;健康
					CASE 4
						PRINTFORML 「唔嗯、过于健康了吧！」
						PRINTFORMW 「多来点刺激味蕾的味道啊！」
					;淡白
					CASE 5
						PRINTFORMW 「……无聊的味道，没劲……」
					ENDSELECT
					BREAK
				ENDIF
			NEXT
		ENDIF
	ELSE
		VARSET LOCAL
		LOCAL:1 = STRCOUNT(DISH_TASTE, "香料/")
		LOCAL:2 = STRCOUNT(DISH_TASTE, "乳酪/")
		LOCAL:3 = STRCOUNT(DISH_TASTE, "甜/")
		LOCAL:4 = STRCOUNT(DISH_TASTE, "辣/")
		LOCAL:5 = STRCOUNT(DISH_TASTE, "垃圾/")
		LOCAL:6 = STRCOUNT(DISH_TASTE, "濃厚/")
		LOCAL:7 = STRCOUNT(DISH_TASTE, "软糯的/")
		FOR LOCAL, 1, 10
			SIF LOCAL:LOCAL
				LOCAL:10 ++
		NEXT
		;以上都不是
		IF !LOCAL:10
			IF TCVAR:[[克勞恩皮絲]]:料理評価値 >= 850
				PRINTFORMW 「嗯～～！ 好吃！！ 再来点再来点♪」
			ELSEIF TCVAR:[[克勞恩皮絲]]:料理評価値 >= 750
				PRINTFORMW 「嗯～～！ 这个味道超棒！」
			ELSE
				PRINTFORMW 「嗯～！ 很好吃哦～！」
			ENDIF
		ELSE
			;シャッフルして最初にヒットしたやつ
			CALL FISHER_YATES_SHAFFLE(10)
			FOR LOCAL, 1, 10
				SIF !SHAFFLE_ARRAY:LOCAL
					CONTINUE
				IF LOCAL:(SHAFFLE_ARRAY:LOCAL)
					SELECTCASE SHAFFLE_ARRAY:LOCAL
					;香料
					CASE 1
						PRINTFORMW 「嗯～♪ 辣辣的味道真棒！」
