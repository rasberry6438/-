@NAME_SET_K64
PRINTL 请输入姓名
IF NAME:MASTER != "你"
	LOCALS = %CALLNAME:MASTER%
ELSE
	LOCALS = %NAME:MASTER%
ENDIF
PRINTBUTTON @"[自称%LOCALS%]", @"%LOCALS%"
PRINTL 　
;名前を入力するループ
$INPUT_LOOP_ONAMAE
INPUTS
; 行頭行尾の空白文字削除
LOCALS = %RESULTS%
REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
;10文字以上は受け付けない
IF STRLENS(LOCALS) > 10
	CLEARLINE 1
	REUSELASTLINE 名字太长了
	GOTO INPUT_LOOP_ONAMAE
;空白は受け付けない
ELSEIF RESULTS == ""
	PRINTL 请输入姓名
	GOTO INPUT_LOOP_ONAMAE
ELSE
	PRINTFORML 就决定是%RESULTS%了吗？
	PRINTL [0] 是的
	PRINTL [1] 不
	;入力した名前を確定するループ
	$INPUT_LOOP_KAKUTEI_1
	INPUT
	IF RESULT == 0
		;你の名前を%MASTERNAME:64%に格納
		MASTERNAME:64 = %RESULTS%
		PRINTFORML 
	ELSEIF RESULT == 1
		PRINTL 请输入姓名
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTL 请输入姓名
		GOTO INPUT_LOOP_ONAMAE
	ENDIF
ENDIF
