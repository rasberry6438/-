LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「啊、嗯HPH ――嗯嗯HPH」","L"
				CALL HPH_PRINT,@"「――啊、那里HPH 嗯、哈啊……HPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%浮起臀部，享受着指尖在秘密之地游走的触感…
			CASE 1
				CALL HPH_PRINT,@"「――可爱的声音？ 呵呵……HPH」","L"
				CALL HPH_PRINT,@"「被你触摸着呢……发出声音也是正常的……HPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%这么说着，将手环绕到%CALLNAME:MASTER%的背后…
			CASE 2
				CALL HPH_PRINT,@"「也别忘了这边哦……HPH」","L"
				CALL HPH_PRINT,@"「嗯……啾……HPH 哈啊……啾……HPH」","L"
				PRINTFORMDW %CALLNAME:MASTER%停下爱抚，与%CALLNAME:TARGET%共同缠绕舌头…
		ENDSELECT
		RETURN 1
	;其他情况
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「嗯、嗯……再来个狠的吧」
				PRINTFORML 「这样子，只是有点痒……嗯！」
				PRINTFORMDW 突然捏住阴蒂，%CALLNAME:TARGET%的腰部猛地跃动了一下…
			CASE 1
				PRINTFORML 「嗯嗯……啾……啾啪……」
				PRINTFORML 「哈啊……这就是你的『鬼退治』吗？」
				PRINTFORMDW %CALLNAME:MASTER%一边玩弄%CALLNAME:TARGET%的身体，一边点头，再次贴上嘴唇
			CASE 2
				PRINTFORML 「为什么呢……和你拥抱在一起的时候……」
				PRINTFORML 「就好像喝了特别的酒一样，身体变得热热的……」
				PRINTFORMDW %CALLNAME:TARGET%松弛下来，接受着%CALLNAME:MASTER%的爱抚…
		ENDSELECT
		RETURN 1
	ENDIF
