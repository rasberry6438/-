					CASE 2
						PRINTFORMDL %CALLNAME:TARGET%的阴道舔起来，自然而然地带有精液的味道
						PRINTFORML 「喂，别自己舔得反胃吧？」
						PRINTFORML 「啊啊算了，来，用这个漱口液漱口」
						PRINTFORMDW %CALLNAME:TARGET%递过一瓶酒。现在这个也算是感激了…
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「哈啊……啊、嗯HPH 啊啊HPH」","L"
				PRINTFORMDL %CALLNAME:TARGET%的阴道中插入舌头，像挖出蜜汁一样前后摇动舌头
				PRINTFORMDW %CALLNAME:TARGET%的阴道，紧紧地夹住%CALLNAME:MASTER%的舌头，渴望着更多的快感……
			CASE 1
				CALL HPH_PRINT,@"「不仅是外面，里面也……HPH」","L"
				CALL HPH_PRINT,@"「喔、啊啊……HPH 来了……HPH」","L"
				PRINTFORMDW 被膣壁舔动的触感让%CALLNAME:TARGET%的脸颊放松了…
			CASE 2
				PRINTFORMDL %CALLNAME:MASTER%用舌尖转动阴蒂，然后贴上嘴唇，轻吸一口
				CALL HPH_PRINT,@"「嗯HPH 嗯嗯HPH ――哈啊HPH」","L"
				CALL HPH_PRINT,@"「哈啊……HPH 刚才的有效果了……HPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%在强烈快感的余韵中，断断续续地摇动着腰部…
		ENDSELECT
		RETURN 1
	;其他情况
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「啊、哈♪ 好吃吧？ 我的穴好吃吗」
				PRINTFORML 「看着吧，想要更多就好好舔吧♪」
				PRINTFORMDW %CALLNAME:TARGET%这样说着，抬起臀部将私处压在%CALLNAME:MASTER%的脸上…
			CASE 1
				PRINTFORML 「呵呵，想舔吗？」
				PRINTFORMDL %CALLNAME:TARGET%大大张开私处…
				PRINTFORMW 「好吧，来吧……嗯♪」
			CASE 2
				PRINTFORML 「嗯、咕、哈哈！ 好痒！」
				PRINTFORML 「咕、呵呵！ 不用这么急，反正也跑不了嘛」
				PRINTFORMDW %CALLNAME:TARGET%这样说着，用头轻拍%CALLNAME:MASTER%埋在股间的头
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
