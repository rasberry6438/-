	;膣内射精直後
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			;恋慕
			IF TALENT:恋慕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMDL %CALLNAME:MASTER%俯身在注满精液的阴道上亲吻，顿时吸入
						CALL HPH_PRINT,@"「留点给我也好吗……？HPH」","L"
						CALL HPH_PRINT,@"「毕竟你这么用心地注入进来的……嗯HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「嘻嘻……这么多被灌进来，就算是我也喝不完啊……HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%张开阴唇，精液噗嗤一声流了出来
						PRINTFORMDW %CALLNAME:MASTER%舔掉后，%CALLNAME:TARGET%微微扭动着腰部
					CASE 2
						CALL HPH_PRINT,@"「哈啊……HPH 这可真是射得很足啊……HPH」","L"
						PRINTFORMDL %CALLNAME:MASTER%不停地舔，但是从%CALLNAME:TARGET%的阴道中涌出的精液仍然源源不断
						CALL HPH_PRINT,@"「还不满足吗？来，快点再插进去HPH」","W"
				ENDSELECT
				RETURN 1
			;其他情况
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「居然真的舔了啊……味道怎么样？」
						PRINTFORML 「――难喝？哈哈，当然咯！」
						PRINTFORMDW %CALLNAME:MASTER%有点后悔舔刚射过精液的阴道
					CASE 1
						PRINTFORML 「那是你射的东西。好好地清理一下吧？」
