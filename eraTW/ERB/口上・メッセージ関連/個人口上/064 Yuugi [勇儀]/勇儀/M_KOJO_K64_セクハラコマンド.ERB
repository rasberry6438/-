;立絵リソースに合わせて裙子の下に运动短裤を穿いている解釈にしました
;なので胖次じゃないから恥ずかしくないもん
@M_KOJO_EXTRASOURCE_COM_K64_330
SOURCE:反感 = 0

@M_KOJO_MESSAGE_COM_K64_330_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K64
	IF FLAG:時間停止
		PRINTFORML 
		RETURN 1
	;被逆推
	ELSEIF CFLAG:诶嘿嘿 == 2
		RETURN 1
	;時間停止状態で胖次を剥ぎ取り済
	ELSEIF TCVAR:64:357 == 1
		PRINTFORMDL %CALLNAME:MASTER%将裙子朝上掀起、理所当然的%CALLNAME:TARGET%什么都没穿
		PRINTFORMDL 当然因为没有运动短裤之类的遮盖的原因，秘密的私处一览无遗
		PRINTFORMDL 
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「我记得早上应该是有穿上的啊……是我记错了吗」
				PRINTFORMW 「是说、你要掀到什么时候？」
			CASE 1
				PRINTFORML 「嗯？喝醉了之后脱了忘记穿上了吗……」
				PRINTFORML 「还是说、被谁偷走了吗……」
				PRINTFORMW 「嘛不管怎么说、如果被谁偷走了的话不能不给他回礼呐」
			CASE 2
				PRINTFORML 「下面什么都没穿凉飕飕的呐……对了」
				PRINTFORML 「%MASTERNAME:64%、之后会还给你的先把你的借给我穿一下呗」
				PRINTFORMW 「总比没有好……欸？不行？ 别说那么小气的话ー」
		ENDSELECT
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMDL %CALLNAME:MASTER%准备将%CALLNAME:TARGET%的裙子掀起来的时候
			PRINTFORMDL %CALLNAME:TARGET%自己主动掀起来了裙子、将那之下的景色展示给了%CALLNAME:MASTER%观赏……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「呼呼呼、你想看的吧？ ……」
					PRINTFORMDL %CALLNAME:TARGET%将手放在运动短裤边上、缓缓的向下脱下
					PRINTFORMW 「干脆直接把这个也脱掉吧……呐、你觉得呢？」
				CASE 1
					CALL HPH_PRINT,@"「再贴近点看也可以哟……HPH」","L"
					CALL HPH_PRINT,@"「啊啊、实话实说……这样被人看着有点兴奋呐……HPH」","W"
					PALAM:潤滑 += 1000
				CASE 2
					CALL HPH_PRINT,@"「看呐……能看到的吧……？HPH」","L"
					CALL HPH_PRINT,@"「我的这里、只是被看到了就变得这么湿了……HPH」","L"
					CALL HPH_PRINT,@"「%MASTERNAME:64%的、好想让它赶紧进来啊……HPH」","W"
					PALAM:潤滑 += 2000
			ENDSELECT
		;思慕
		ELSEIF TALENT:思慕
			PRINTFORMDL %CALLNAME:MASTER%把%CALLNAME:TARGET%的裙子从正面掀了起来
			PRINTFORMDL %CALLNAME:TARGET%抱着胳膊、堂堂正正的站着……
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「话说回来、掀别人裙子有那么好玩吗？」
					PRINTFORMW 「如果想听女孩尖叫的话找错对象了吧？」
				CASE 1
					PRINTFORML 「为什么裙子是半透明的？」
					PRINTFORML 「啊啊、这是我唯一的一件好衣服……」
					PRINTFORML 「洗了不知道多少遍了就变成这样了……」
					PRINTFORMDW %CALLNAME:TARGET%一边掀着裙子一边诉说着关于衣服的看法
				CASE 2
					PRINTFORML 「嘛反正也不会少块肉、随便你看吧」
					PRINTFORMDL 真是感谢。%CALLNAME:MASTER%将手搭在了%CALLNAME:TARGET%的运动短裤上
					PRINTFORMW 「喂、那边不行的吧！」
			ENDSELECT
		;それ以外
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%把%CALLNAME:TARGET%的裙子掀了起来
			PRINTFORMDL 令人震惊、%CALLNAME:TARGET%的下边居然穿着类似运动短裤之类的东西
			PRINTFORMDW 
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「好像是从外界流进来的东西的样子、这东西穿起来很适合运动呐」
					PRINTFORML 「现在的话、没有一天不穿这个的哟！」
					PRINTFORMDW 这样的吗……
				CASE 1
					PRINTFORML 「就觉得你要搞什么恶作剧、结果居然是掀裙子呐」
					PRINTFORML 「你这种坚韧不拔的精神、真是个了不起的家伙啊」
					PRINTFORMDW 虽然没有看到想看的东西、但是%CALLNAME:MASTER%被%CALLNAME:TARGET%表扬了
				CASE 2
					PRINTFORML 「哈哈哈哈！ 一看就知道的吧！」
					PRINTFORMDL 确实，%CALLNAME:TARGET%的裙子是半透明的，有没有穿胖次之类的东西一目了然
					PRINTFORMDW 但是即便如此也想看看会不会有别样的世界。这样的探求心促使了%CALLNAME:MASTER%的行动
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
