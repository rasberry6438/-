;330,スカートめくり
;TFLAG:193(
;	-5=拒絶（ビンタ）
;	-3=拒絶（つねり）
;	-1=いやがる
;	0 =いやがる（嬉しげ）
;	1 =受け入れ
;)
;### TFLAG:193 = 1 のとき、更に下記のように分岐 ###
;TFLAG:193 = 1 && ABL:露出癖(
;	6以上 = 人目のある場所で自らスカートをまくり上げる 
;	5以下 = 人目のある場所でめくられるさいに弱い抵抗
;	3以上 = 人目のない場所で自らスカートをまくり上げる
;	3未満 = 弱い抵抗
;)
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K9_330
CALL M_KOJO_MESSAGE_COM_K9_330_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K9_330_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		;時間停止時
		PRINTFORMDW 在停止的时间里对梦美的%SHOW_BOTTOM(TARGET,1)%下手。
		PRINTFORMDW 当然、由于没法反抗、很容易就看到了里面的内容。
		PRINTFORMDW 但另一方面、由于时间静止、没有任何反应、多少有些空虚。
	ELSE
		SELECTCASE TFLAG:193
			CASE 1
				;めくれますが夢美ちゃん、はいてます？
				IF CFLAG:291
					;はいてないよ
					IF ABL:14 >= 8
						;露出癖Lv8以上の夢美ちゃんはノーパンでお出かけも出来ちゃうえっちな大学教授です
						IF PALAM:9 >= 5000
							;びちょびちょじゃないかよオイ
							CALL HPH_PRINT,@"「%CALLNAME:MASTER%看、我的那里、现在变成这样了唷……HPH」","W"
							PRINTFORMDW 梦美高兴的撩起了自己的%SHOW_BOTTOM(TARGET,1)%、向我展示光滑无毛、湿漉漉的私处。
							PRINTFORMDW 从衣服中解放出来的雌性气味刺激着%CALLNAME:MASTER%的鼻腔
							PRINTFORML 「馁……难不成只想这样看着……吗？」
							PRINTFORMDW 浮起淫靡笑容的梦美在%CALLNAME:MASTER%的耳边低语
							PRINTFORMW 
							CALL HPH_PRINT,@"「――來・做・吧HPH」","W"
						ELSE
							CALL HPH_PRINT,@"「嗯哼哼……餒%CALLNAME:MASTER%你看、我今天、没穿唷……HPH」","W"
							PRINTFORMDW 露出淫荡笑容的梦美自己撩起%SHOW_BOTTOM(TARGET,1)%、露出光滑无毛的私处
							PRINTFORMDW 眼前的绝景让%CALLNAME:MASTER%不由得屏住呼吸
							CALL HPH_PRINT,@"「讨厌……HPH　被%CALLNAME:MASTER%看着的话……都湿漉漉的了HPH」","W"
							PRINTFORMDW 正如梦美所说、蠢蠢欲动的腔口已经渗出爱液、带着淫靡的热气
							PALAM:9 += 500
						ENDIF
					ELSEIF ABL:14 < 8 && ABL:14 >= 6
						;露出癖Lv6以上、どこでもまくってくれる
						IF PALAM:9 >= 5000
							;びちょびちょじゃないか
							CALL HPH_PRINT,@"「嘿嘿……我的湿漉漉的淫荡小穴、想看吗……？HPH」","W"
							CALL HPH_PRINT,@"「好啊、看好啰……HPH　嗯哼、怎么样啊……？」","W"
							PRINTFORMDW 梦美用引诱的眼神撩起%SHOW_BOTTOM(TARGET,1)%、露出光滑无毛的湿漉漉的小穴
							PRINTFORMDW 从衣服中解放出来的雌性气味刺激着%CALLNAME:MASTER%的鼻腔……
						ELSE
							CALL HPH_PRINT,@"「等等……%CALLNAME:MASTER%、今天、没有穿……HPH」","W"
							PRINTFORMDW 说着、梦美自己撩起%SHOW_BOTTOM(TARGET,1)%、露出光滑无毛的私处
							CALL HPH_PRINT,@"「%CALLNAME:MASTER%真是个充满好奇心的人啊……HPH」","W"
						ENDIF
					ELSEIF ABL:14 < 6 && ABL:14 >= 3
						;露出癖Lv3以上6未満
						IF GET_TARGETNUM() >= 2
							;他に誰か居る
							PRINTFORML 「等等%CALLNAME:MASTER%、这里不行啊……」
							PRINTFORML 「如果……是在没人的地方、那个……」
							PRINTFORML 「再、再给你看…………好吗？」
						ELSE
							PRINTFORML 「真是的、那么想看吗？%CALLNAME:MASTER%充满好奇心呢」
							PRINTFORML 「你看……今天……那个……」
							PRINTFORML 「没有穿……内裤……唷、别这么盯着看啊……\/\/\/\/」
						ENDIF
					ELSE
						;露出癖Lv3未満
						PRINTFORML 「等 、不、不行啊……现、现在那个……！」
						PRINTFORML （没有穿啊ーーーー啊！！\/\/\/\/\/）
					ENDIF
				;はいてるよ
				ELSE
					IF ABL:14 >= 6
						;露出癖Lv6以上、どこでもまくってくれる
						CALL HPH_PRINT,@"「嘿嘿……想看……？好啊……HPH」","W"
						PRINTFORMDW 说完、梦美毫不犹豫地撩起%SHOW_BOTTOM(TARGET,1)%、露出了内裤
						CALL HPH_PRINT,@"「今天是%PANTSNAME(EQUIP:TARGET:下半身内衣２, TARGET)%、如何啊HPH」","W"
						PRINTFORML 「餒、这样看着就可以了吗……？」
					ELSEIF ABL:14 < 3
						;露出癖Lv3未満
						PRINTFORML 「不行、不行掀啊！那样的话内裤会被看见的……\/\/\/」
					ELSE
						;露出癖Lv3以上6未満
						IF GET_TARGETNUM() >= 2
							;人が居る
							PRINTFORML 「等等、这里实在不行……这里实在不行要是被人看到了怎么办……\/\/\/」
							PRINTFORML 「如果是没人的地方、随便你怎么看都可以……吧？」
							PRINTFORML 「啊、等等、所以、不是说了因为会被看见所以不行吗……\/\/\/」
							PRINTFORMDW 不是因为想看才翻的。
							PRINTFORML 「真、真是的！笨蛋……\/\/\/」
						ELSE
							;人が居ない
							PRINTFORML 「真是的、真是的那么想看吗？%CALLNAME:MASTER%充满好奇心呢」
							PRINTFORMDW 梦美害羞地撩起%SHOW_BOTTOM(TARGET,1)%、给我看了内裤。
							PRINTFORML 「你看……今天我试着穿了……%PANTSNAME(EQUIP:TARGET:下半身内衣２, TARGET)%、怎……怎么样？\/\/\/」
						ENDIF
					ENDIF
				ENDIF
			CASE 0
				;やだもー(嬉しげ)
				PRINTFORML 「真是的、%CALLNAME:MASTER%、别这样啦……\/\/\/\/」
			CASE -1
				;やだもー(嫌)
				PRINTFORML 「喂、不要做这种蠢事」
			CASE -3
				;つねるぞ痴漢痴女たち
				PRINTFORML 「突然把手伸向哪里啊变态」
				PRINTFORMDW 痛痛痛痛痛！不要这样、皮会被扯下来的！
				PRINTFORML 「蛤？没被毆打一頓就很好了」
			CASE -5
				;最大の怒りと嫌悪を込めたボカッ！ボカッ！
				PRINTFORML 「咿啊！？突然做什么啊变态！！」
				PRINTFORMDW 笨蛋！笨蛋！
				PRINTFORMDW 不仅是耳光、脑袋还被梦美狠狠的揍了一拳！冒出了白煙！
				PRINTFORML 「到那种程度就结束了、所以请心怀感激」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
