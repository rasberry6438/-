@M_KOJO_MESSAGE_MARKCNG_K9
;このままだと地の文が表示されません
;地の文を表示したい方は、下記のコメントアウトを削除して下さい
;苦痛刻印
IF TFLAG:22 == 1
	PRINTFORMW 
ELSEIF TFLAG:22 == 2
	PRINTFORMW 
ELSEIF TFLAG:22 == 3
	PRINTFORMW 
ENDIF

;時姦刻印
IF TFLAG:時姦刻印取得 == 1
	PRINTFORML （诶、什麼？这是什么……？）
ELSEIF TFLAG:時姦刻印取得 == 2
	CALL HPH_PRINT,@"（討厭、這是怎麼回事……身体……好像在尋求什麼……HPH）","W"
ELSEIF TFLAG:時姦刻印取得 == 3
	CALL HPH_PRINT,@"（怎麼回事、為什麼突然變得這麼舒服HPHHPHHPH）","W"
ENDIF

;快楽刻印
IF TFLAG:23 == 1
	CALL HPH_PRINT,@"「唔～～～HPH」","W"
	PRINTFORML （討、討厭、剛才的……舒服極了……）
ELSEIF TFLAG:23 == 2
	CALL HPH_PRINT,@"「……呼啊……HPH」","W"
	CALL HPH_PRINT,@"（感觉好舒服啊……滿腦子都是……要變成笨蛋了……HPH）","W"
ELSEIF TFLAG:23 == 3
	CALL HPH_PRINT,@"「啊ー……HPH　哈ーHPH　哈HPH」","W"
	CALL HPH_PRINT,@"（做愛居然這麼舒服……HPH　如果知道能這麼舒服的話……HPH）","W"
	CALL HPH_PRINT,@"（已經只能喜歡上做愛了、不是吗……HPHHPHHPH）","W"
ENDIF

;不埒刻印
;快楽による刻印
;LV1
IF TFLAG:24 == 11
	PRINTFORML （怎麼回事、剛才的……舒服極了……？）
	PRINTFORML 「餒、能再……做一点吧？」
;LV2
ELSEIF TFLAG:24 == 12
	PRINTFORML （清楚的感覺到……這種行為是很舒服的事情……）
	PRINTFORML 「想要……感到更多舒服……和%CALLNAME:MASTER%」
;LV3
ELSEIF TFLAG:24 == 13
	PRINTFORML （……真是太好了、這麼舒服的事……不用忍耐了）
	PRINTFORML 「%CALLNAME:MASTER%……餒、再来点吧……？HPH」

;屈服、恭順による刻印
;LV1
ELSEIF TFLAG:24 == 31
	PRINTFORML （這个人的話……也許可以做這樣、的事……？）
	PRINTFORML 「啊、没什麼、没什麼唷？嗯」
;LV2
ELSEIF TFLAG:24 == 32
	PRINTFORML （總覺得我……想和這个人做更多這樣、的事……）
	PRINTFORML 「你問我怎麼沉默了？我不会生气的、没關系的」
;LV3
ELSEIF TFLAG:24 == 33
	PRINTFORML （嗯、這个人挺好的、对吧……即使做了、很多……）
	PRINTFORML 「你說怎麼了？嗯、没什麼、%CALLNAME:MASTER%♪」
