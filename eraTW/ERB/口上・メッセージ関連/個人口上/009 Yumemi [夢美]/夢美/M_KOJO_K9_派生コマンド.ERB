				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSE
				CALL HPH_PRINT,@"「咕……等等、突然在里面射出來也太无礼了吧……！？HPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ENDIF
			RETURN 1
		ELSE
			;子作りOKしているか(CFLAG:14==2),危険日以外OKか(CFLAG:14==1),ピル服用済か(TCVAR:109),それ以外
			IF CFLAG:14 == 2
				CALL HPH_PRINT,@"「呼啊HPH　%CALLNAME:MASTER%的种子进来了HPH　进到子宫裡了HPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSEIF CFLAG:14 == 1
				CALL HPH_PRINT,@"「啊哈、好棒啊HPH　今天是安全日……所、以、射进来吧HPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSEIF TCVAR:109 == 1
				CALL HPH_PRINT,@"「唔啊、等等、你那里、有、多少、药丸？！HPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSE
				CALL HPH_PRINT,@"「唔啊、等等、%CALLNAME:MASTER%想、去、哪、啊HPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;時姦中
	;IF FLAG:70
	;	PRINTFORMW 
