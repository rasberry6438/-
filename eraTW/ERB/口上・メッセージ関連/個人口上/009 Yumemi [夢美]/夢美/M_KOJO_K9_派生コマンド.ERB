				ENDIF
			ELSEIF CFLAG:14 == 1
				CALL HPH_PRINT,@"「啊哈HPH　更深处、一股热热的进来了HPH」","W"
				CALL M_KOJO_MESSAGE_SIKYUKO_K9
			ELSEIF TCVAR:109
