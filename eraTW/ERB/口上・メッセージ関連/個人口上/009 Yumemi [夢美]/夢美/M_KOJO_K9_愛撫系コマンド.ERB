@M_KOJO_MESSAGE_COM_K9_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「诶……？你、你要舔……我、我的、那个地方……？」
		PRINTFORMDW %CALLNAME:MASTER%点头
		PRINTFORML 「等、等一下！还没有心理准备――呼啊啊啊啊！？」
		PRINTFORMDW 无视语无伦次的梦美、%CALLNAME:MASTER%吸附在了梦美的私处。
		CALL HPH_PRINT,@"「唔啊、等、等一下HPH　说、说了让你等的」","W"
		PRINTFORMDW 感受着梦美光滑无毛的私处在脸上的触感的%CALLNAME:MASTER%用舌头摩擦着阴蒂、并舔了舔阴唇。
		PRINTFORMDW 渐渐的、梦美的抵触情绪消失了、声音里也开始夹杂着悦耳的甜美声音。
		CALL HPH_PRINT,@"「啊唔、啊、啊啊……HPH讨厌、什么、这……这种……感觉……」","W"
		PRINTFORML （难道……这就是、快感、嗎……？）
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;膣内射精直後
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			IF TALENT:恋慕 || TALENT:愛欲
				CALL HPH_PRINT,@"「啊……HPH现在、如果被舔小穴的话……哈啊、%CALLNAME:MASTER%的会流出来的……」","W"
				CALL HPH_PRINT,@"「好浪费啊……还是、唔啊、%CALLNAME:MASTER%也想尝尝自己的精液、呢……？HPH」","W"
				CALL HPH_PRINT,@"「啊、好舒服啊……那样的话、那就尽情舔吧、对、对了、那里、啊哈HPH」","W"
			ELSE
				PRINTFORML 「唔、啊、呀、现在舔那里的话……\/\/\/」
				PRINTFORML 「从里面、出来了、流出来了、啊、对了……啊哈啊\/\/\/」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		IF ABL:0 > 5
			IF TALENT:73 == 1
				CALL HPH_PRINT,@"「啊哈HPH咿啊HPH差不多要去了HPH","W"
				CALL HPH_PRINT,@"「是啊、再多一点、再多舔一点HPH啊、啊、啊、啊……」","W"
				CALL HPH_PRINT,@"「去、去了、去了、啊、去了、啊、啊ーーーーーっHPH」","W"
			ELSE
				CALL HPH_PRINT,@"「哈啊、好舒服啊HPH、%CALLNAME:MASTER%HPH」","W"
				CALL HPH_PRINT,@"「我、差不多、要去了HPH」","W"
				CALL HPH_PRINT,@"「所以、哈啊、再多一点、再多舔一点HPH」","W"
			ENDIF
		ELSE
			PRINTFORML 「啊、啊、不行、不行呀、不能在舔了！要去了、要去了啊！」
			PRINTFORML 「不行、不行了、要去、去了、啊、啊、啊、去了、呀、去了！」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 在停止的时间里、%CALLNAME:MASTER%把梦美的私处含在嘴里。
		PRINTFORMDW 光滑无毛的触感在脸上很舒服、而梦美虽然溢出了爱液、却纹丝不动。
		PRINTFORMDW %CALLNAME:MASTER%以解除时间停止时的反应为乐、不断舔梦美的私处。
		RETURN 1
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「啊哈、啊啊、不要……发出那种舔舐的声音啊……\/\/\/」
				PRINTFORMDW 看到梦美言不由衷的溢满了爱液的私处、%CALLNAME:MASTER%更大声地舔了舔。
				CALL HPH_PRINT,@"「呼啊、就说了、声音、不行啊HPH」","W"
			CASE 1
				PRINTFORMDW %CALLNAME:MASTER%故意发出啾噜啾噜的声音、啜了一口梦美的爱液。
				PRINTFORML 「呼啊啊啊、啊啊、好舒服啊、可是……别弄的那么大声啊……っ\/\/\/」
				PRINTFORML （明明很害羞……我的小穴、却更加搔痒难耐了……）
			CASE 2
				PRINTFORMDW %CALLNAME:MASTER%、把梦美的私处掰开、用舌头侵入梦美的私处。
				PRINTFORML 「呀啊啊、啊哈！别那样掰开啊……\/\/\/」
				PRINTFORMDW 梦美的膣肉缠绕在侵入私处的%CALLNAME:MASTER%舌头上。
				PRINTFORMDW 与之相对的%CALLNAME:MASTER%也用舌头来回摩擦着、梦美浑身颤抖着。
				CALL HPH_PRINT,@"「啊啊啊啊啊！那里、咿啊！别那样、啊啊、舔啊HPH」","W"
			CASE 3
				PRINTFORMDW %CALLNAME:MASTER%没有舔私处、而是在梦美私处周遭挑逗着。
				PRINTFORML 「等、为什么……要舔那种地方啊……」
				PRINTFORMDW 梦美原本期待着能舔自己的私处、期望落空的她开始扭动着腰。
				PRINTFORMDW 挑逗她一阵的%CALLNAME:MASTER%は、扑向期待而滴汁的私处。
				CALL HPH_PRINT,@"「啊？！啊啊ーーーーーHPH」","W"
			CASEELSE
				PRINTFORML 「啊啊、不行、小豆豆、别那么舔啊……\/\/\/」
				PRINTFORMDW 虽然嘴上拒绝、但受着刺激的阴蒂、身体颤抖着溢出了更多爱液。
				CALL HPH_PRINT,@"「呼啊啊啊啊啊啊HPH不、不要、这样舔啊……！因为、很敏感的、啊……」","W"
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
