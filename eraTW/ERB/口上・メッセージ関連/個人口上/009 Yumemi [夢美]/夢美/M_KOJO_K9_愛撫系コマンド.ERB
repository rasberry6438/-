;9,自慰
; 我が実装に、一片の悔い無しィ……ッ!!ぬわーーっっ！！
;========================================================
@M_KOJO_MESSAGE_COM_K9_9
CALL M_KOJO_MESSAGE_COM_K9_9_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K9_9_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		; オナニーを覗いたことがあったらちょっとMSG変わるよ
		IF CFLAG:11 == 1
			PRINTFORML 「等……诶、想、想看我自慰……你、在这之前、看过了吧……？那还不满意吗……？」
			PRINTFORML 「而且、那也不是给你看的东西……吧？重新考虑一下？\/\/\/」
			PRINTFORML 「诶、正因为是这样的状况、才想再仔细看看？」
			PRINTFORML 「有时候会觉得%CALLNAME:MASTER%、是不是真的脑子有问题呢……」
			PRINTFORML 「哀……没办法啊……」
			PRINTFORMDW 梦美深深的叹了口气、手指朝着胸部和私处缓缓移动。
			PRINTFORML 「唔……唔……哈……\/\/\/」
			PRINTFORMDW 可能是兴致慢慢上来了、为了寻求快感改变着手的动作。
			PRINTFORML （%CALLNAME:MASTER%在看着……不用那么仔细看啊……）
			PRINTFORMDW 但是%CALLNAME:MASTER%看着的事多少也提高了梦美的兴奋的程度。
			PRINTFORML 「啊、唔、啊呼……不、为什么比平时……不要那么认真的看啊……\/\/\/」
		ELSE
			PRINTFORML 「咦！？等、等一下%CALLNAME:MASTER%、你是认真的吗？！」
			PRINTFORMDW 我一直都很认真！
			PRINTFORML 「因因因因因因为、自、自慰ー……？不、不应该给人看的、对吧……？\/\/\/」
			PRINTFORMDW %CALLNAME:MASTER%问其实你没做过吗
			PRINTFORML 「笨……！不可能吧？！自慰过一两次吧！……啊……\/\/\/」
			PRINTFORML 「啊、啊ーー……有是有啦！这不应该给你看的吧！\/\/\/」
			PRINTFORMDW %CALLNAME:MASTER%拜倒在地说着因为是梦美所以想看、
			PRINTFORML 「哀～～～……真的不懂你在想什么\/\/\/」
			PRINTFORML 「我、我知道了……做、做就行了吧……！\/\/\/」
			PRINTFORMDW 脸都涨红的梦美说着、开始用不稳的手自慰着。
		ENDIF
		RETURN 1
	ENDIF
	;通常の処理
	;ここには各ステータスの状態に応じた口上を表示
	IF TALENT:73 == 1
		;[自慰狂い]を持っている場合(TALENT:73 == 1)
		CALL HPH_PRINT,@"「诶？可、可以……自慰吗？HPHHPH」","W"
		CALL HPH_PRINT,@"眼睛里浮现出HPH的梦美马上开始用陶醉的表情抚摸自己的身体","W"
		PRINTFORMDW 私处开始发出咕啾咕啾的水声。
		CALL HPH_PRINT,@"「啊、好厉害HPH好舒服HPH好像在做梦、自慰ー最喜欢了啊HPH」","W"
		CALL HPH_PRINT,@"「每天都这样咕啾咕啾的……HPH自慰着ー、完全忍不住啊HPH」","W"
		CALL HPH_PRINT,@"「所以啊、多看一点……我自慰的地方、%CALLNAME:MASTER%HPHHPHHPH」","W"
		CALL M_KOJO_MESSAGE_MASTERBATION_K9
	ELSEIF ABL:14 >= 6 && ABL:30 >= 4
		;露出癖と自慰中毒が高い(Lv>=6 & Lv>=4)場合
		PRINTFORMDW 梦美采取能让%CALLNAME:MASTER%清楚看到自己玩弄着的地方的姿势、正在激烈的自慰着。
		PRINTFORMDW 那表情充满了给%CALLNAME:MASTER%看的兴奋和手淫的快感。
		CALL HPH_PRINT,@"「哈啊HPH咿呀、好棒呀、好舒服啊、%CALLNAME:MASTER%！HPH」","W"
		CALL HPH_PRINT,@"「馁、啊啊、馁、唔嗯、在看吗？看到了吗？我的自慰、怎么样啊HPH」","W"
		CALL HPH_PRINT,@"「啊啊、不行、太舒服了、手指都停不下来了HPH%CALLNAME:MASTER%HPH」","W"
		CALL M_KOJO_MESSAGE_MASTERBATION_K9
	ELSEIF ABL:30 >= 4
		;自慰中毒のみ高めの(Lv>=4)場合
		CALL HPH_PRINT,@"「唔啊、好棒呀、好舒服啊！明明被、这样、看着HPH」","W"
		PRINTFORMDW 尽管被%CALLNAME:MASTER%看着、梦美依然沉浸在自慰的快乐中
		PRINTFORMDW 双手继续激烈的手淫着。
		PRINTFORMDW 从她的私处传来咕啾咕啾的水声、潮水和爱液四处飞溅。
		CALL HPH_PRINT,@"「啊哈啊啊、啊、啊、不行不行、太舒服忍受不了了HPH」","W"
		CALL M_KOJO_MESSAGE_MASTERBATION_K9
	ELSE
		;その他
		PRINTFORML 「下、下次再做……？真是没办法啊……唔啊……\/\/\/」
		PRINTFORMDW 梦美害羞的开始自慰、不一会。声音里开始夹杂着娇喘。
		PRINTFORML 「啊、啊、啊唔、唔……啊呼……怎么会……」
		PRINTFORML （被看到了……明明被人看到了这么羞耻的地方……感觉比平时都舒服\/\/\/）
		PRINTFORMDW 不知不觉间、梦美的手动得越来越厉害。
		CALL M_KOJO_MESSAGE_MASTERBATION_K9
	ENDIF
ENDIF
RETURN 1

