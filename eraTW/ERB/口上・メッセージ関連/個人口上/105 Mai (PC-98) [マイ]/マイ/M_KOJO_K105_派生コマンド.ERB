;6,挿入Ｇスポ責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K105_6
CALL M_KOJO_MESSAGE_SCOM_K105_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K105_6_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORML 
		RETURN 1
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		PRINTFORML 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		PRINTDATAW
			DATAFORM 「！　嗯呜呜呜%UNICODE(0x2665) *1%　啊啊啊嗯%UNICODE(0x2665) *1%　ki、咿咕、呜%UNICODE(0x2665) *1%」
			DATAFORM 「嗯咿！　不行不行、那里、是%UNICODE(0x2665) *1%　呀啊、啊啊啊！」
		ENDDATA
		RETURN 1
	ELSEIF TCVAR:媚薬 || TCVAR:発情
		IF TCVAR:105:351 >= 4
			PRINTFORML 「啊、呀啊゛啊啊啊！！　咿゛っ、咕呜呜%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　那里好糟糕゛%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			PRINTFORMW 「已゛已经゛不゛行了%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　要゛っ要、变得奇怪゛了%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
		ELSEIF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「啊啊嗯%UNICODE(0x2665) *1%　那里%UNICODE(0x2665) *1%　击中好地方了%UNICODE(0x2665) *1%」
			PRINTFORMW 「咿、咿嘻%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　不行、太有感觉要去了%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORML 「呀啊%UNICODE(0x2665) *1%　等等、呀……っ%UNICODE(0x2665) *1%　那里、很不妙……%UNICODE(0x2665) *1%」
			PRINTFORMW 「等、等等%UNICODE(0x2665) *1%　等一下、啊%UNICODE(0x2665) *1%」
		ENDIF
	ELSE
		IF TALENT:恋人 || TALENT:恋慕
			PRINTDATAW
				DATAFORM 「嗯、那里、不行、啊……等！　等下、噫咕、嗯嗯%UNICODE(0x2665) *1%」
				DATAFORM 「没错、那里……呀、呜啊啊啊！　咿咿、嗯咿咿%UNICODE(0x2665) *1%」
			ENDDATA
		ELSE
			PRINTFORMW 「！？　等、那里是……啊%UNICODE(0x2665) *1%」
			SIF TALENT:思慕
				PRINTFORMW （舒服的地方、已经、暴露了啊……库呜%UNICODE(0x2665) *1%）
		ENDIF
	ENDIF
ENDIF
