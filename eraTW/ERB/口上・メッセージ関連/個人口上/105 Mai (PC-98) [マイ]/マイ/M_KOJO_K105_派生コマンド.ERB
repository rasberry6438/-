;==================================================
;7,挿入子宮口玩弄
;==================================================
@M_KOJO_MESSAGE_SCOM_K105_7
CALL M_KOJO_MESSAGE_SCOM_K105_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K105_7_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORML 
		RETURN 1
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		PRINTFORML 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		PRINTDATAW
			DATAFORM 「嗯噢゛%UNICODE(0x2665) *1%　呜咕、呜唉……不、行……要、去……呜%UNICODE(0x2665) *1%」
			DATAFORM 「唉呜、咕……那里、啊……子、好难受……的……噢、噢%UNICODE(0x2665) *1%」
		ENDDATA
		RETURN 1
	ELSEIF TCVAR:媚薬 || TCVAR:発情
		IF TCVAR:105:351 >= 4
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「哦゛っ%UNICODE(0x2665) *1%　哦咯……咕、呜%UNICODE(0x2665) *1%　伸处、好腻……害%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「嗯゛呜、再……深、一……点%UNICODE(0x2665) *1%　啊诶、嘿嘿嘿……」
			CASEELSE
				PRINTFORML 「嗯゛咕……子宫、要被压碎了……%UNICODE(0x2665) *1%　咿、咕……%UNICODE(0x2665) *1%」
				PRINTFORMW 「啊゛っ、我……已经……啊、哈%UNICODE(0x2665) *1%」
			ENDSELECT
		ELSEIF TALENT:恋人 || TALENT:恋慕
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「嗯哦゛っ%UNICODE(0x2665) *1%　深处……好、难过%UNICODE(0x2665) *1%」
				PRINTFORMW 「想要侵犯……子宫、吗？　呜呼、可以哦……%UNICODE(0x2665) *1%　再……深、一点……%UNICODE(0x2665) *1%」
			CASEELSE
				PRINTFORML 「咕%UNICODE(0x2665) *1%　突然……插进来、太多……了%UNICODE(0x2665) *1%」
				PRINTFORMW 「啊゛っ啊……转动着……好舒、服……%UNICODE(0x2665) *1%」
			ENDSELECT
		ELSE
			PRINTFORML 「嗯゛っ……有点、难受……%UNICODE(0x2665) *1%」
			PRINTFORMW 「但、是……这、个……っ%UNICODE(0x2665) *1%　啊、啊啊……%UNICODE(0x2665) *1%」
		ENDIF
	ELSEIF EXP:子宮口開発経験 > 25
		IF TALENT:恋人 || TALENT:恋慕
			PRINTDATAW
				DATAFORM 「嗯咕、呜唉……深处、呜……这个、意识要飞走了呜……呜、啊……%UNICODE(0x2665) *1%」
				DATAFORM 「深、处……被突刺、着……%UNICODE(0x2665) *1%　%CALLNAME:MASTER%っ的、全部……都、哈」
			ENDDATA
		ELSE
			PRINTFORMW 「呜、呜、啊……好、深……」
			SIF TALENT:思慕
				PRINTFORMW （这个、或需要不行了……最深的地方、被%CALLNAME:MASTER%的那个给、不停地……）
		ENDIF
	ELSE
		IF TALENT:恋人 || TALENT:恋慕
			PRINTDATAW
				DATAFORM 「呜、好、紧……不行、等下……呜」
				DATAFORM 「咕、唉……插得、太深了……原谅我、呜唉……」
			ENDDATA
		ELSE
			PRINTFORMW 「咕、噫……快、住手……」
			SIF TALENT:思慕
				PRINTFORMW （这也太乱来了吧……还是说、我对这方面太弱了呢……）
		ENDIF
	ENDIF
ENDIF
