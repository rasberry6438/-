;181,媚薬,
;==================================================
@M_KOJO_MESSAGE_COM_K105_181
CALL M_KOJO_MESSAGE_COM_K105_181_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K105_181_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;こういうのが好き、というのがあるんだけど上手いこと書けてる気がまったくしない
IF LOCAL
	IF TCVAR:烂醉 == 1
		PRINTFORML 
	ELSEIF CFLAG:诶嘿嘿
		IF FIRSTTIME("诶嘿嘿中媚薬")
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「媚药呢……虽然去找的话可能会找到各种各样的媚药，但这些东西真的有效吗？」
				PRINTFORML 「顶多，可能只是提高血液循环之类的程度吧」
				PRINTFORMW 「就算不起作用，也不是我的错哦」
				PRINTFORML 
				PRINTFORML 「嗯……」
				PRINTFORMW （嘛，破坏了他的幻想也不好……就陪他一起玩吧）
				PRINTFORMW （如果真的有效，也挺有趣的）
				PRINTFORMW （说不定会有安慰剂效应呢……）
				PRINTFORML 
				PRINTFORMW 「……啊、那个？　感觉有点……啊」
				PRINTFORMW 「%CALLNAME:MASTER%……这个、薬」
				PRINTFORMW 「啊、啊啊……%UNICODE(0x2665) *1%　这个……应该有效……%UNICODE(0x2665) *1%」
				PRINTFORML 
				PRINTFORMW 「总之……我们继续吧……%UNICODE(0x2665) *1%」
				CALL RECOVER(TARGET,300,"気力")
				TCVAR:105:351 += 1
			ELSE
				PRINTFORML 「媚薬……？」
				PRINTFORMW 「……是吗，那种东西」
				PRINTFORMW 「大概，没什么效果吧」
				PRINTFORMW 「我是这么觉得的……」
				PRINTFORMW 「但、是……？」
				PRINTFORML 
				PRINTFORML 「这……这、这个……难道」
				PRINTFORMW 「啊啊啊啊……竟然、这么……%UNICODE(0x2665) *1%」
				CALL RECOVER(TARGET,300,"気力")
				TCVAR:105:351 += 1
			ENDIF
		ELSE
			IF TALENT:恋人 || TALENT:恋慕
				SELECTCASE TCVAR:105:351
				CASE 0
					PRINTFORML 「呜、媚药吗……肯定，会很舒服吧」
					PRINTFORMW 「记忆有点模糊……我没有做奇怪的事情吧？」
					PRINTFORMW 「嘛，无所谓了，总之……嗯……」
					PRINTFORML 
					PRINTFORMW 「……呼呜、开始起效了……%UNICODE(0x2665) *1%」
					PRINTFORMW 「那，继续吧%UNICODE(0x2665) *1%」
					CALL RECOVER(TARGET,300,"気力")
					TCVAR:105:351 += 1
				CASE 1
					IF CFLAG:105:1002 == 0
						PRINTFORML 「呼呼、药啊……%UNICODE(0x2665) *1%　要、再来一瓶吗？」
						PRINTFORML 「……嗯……%UNICODE(0x2665) *1%」
						PRINTFORML 
						PRINTFORMW 「啊啊啊啊、起作用了……%UNICODE(0x2665) *1%　%CALLNAME:MASTER%……快点，继续吧……%UNICODE(0x2665) *1%」
						CALL RECOVER(TARGET,200,"気力")
						TCVAR:105:351 += 1
					ELSEIF CFLAG:105:1002 == 1
						PRINTFORML 「药效，不够了……%UNICODE(0x2665) *1%　啊啊、已经……昏昏欲睡了」
						PRINTFORML 「我也带了一些……%UNICODE(0x2665) *1%　和%CALLNAME:MASTER%的媚药……相性不太好的那种%UNICODE(0x2665) *1%」
						PRINTFORMW 「来吧、来做个实验吧……呼呼、呼呼呼……%UNICODE(0x2665) *1%」
						PRINTFORMDW ​舞从怀里拿出一个小瓶子，一口气喝下了里面的液体……
						PRINTFORML 
						TCVAR:105:351 += 3
						CFLAG:105:1002 = 2
						SETCOLOR 0xFF99EE
						PRINTFORMW 「啊、啊、啊……来、来了啊%UNICODE(0x2665) *1%」
						PRINTFORMW 「这、这个……比之前的……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						PRINTFORMW 「啊诶嘿嘿……%CALLNAME:MASTER%……继、续、做爱吧……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						CALL RECOVER(TARGET,200,"気力")
					ELSEIF CFLAG:105:1002 == 2
						PRINTFORML 「药，还要加一点吗？那，今天也要飞起来吗%UNICODE(0x2665) *1%」
						PRINTFORML 「我可是好好改良了的……绝对有效……%UNICODE(0x2665) *1%」
						PRINTFORMW 「不对、应该是改坏才对……呼呼呼呼、呼呼……%UNICODE(0x2665) *1%」
						PRINTFORMDW ​舞从怀里拿出一个小瓶子，一口气喝下了里面的液体……
						PRINTFORML 
						TCVAR:105:351 += 3
						SETCOLOR 0xFF99EE
						PRINTFORML 
						PRINTFORMW 「……啊啊啊，来了，来了啊……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						CALL RECOVER(TARGET,200,"気力")
					ELSE
						PRINTFORML
						TCVAR:105:351 += 1
					ENDIF
				CASE 2
					PRINTFORML 「诶，又要吗？这样不太好吧，再继续下去……可能会……」
					PRINTFORMW 「稍微有点害怕」
					PRINTFORMW 「但是……好吧，感觉很舒服……嗯」
					PRINTFORML 
					SETCOLOR 0xFF99EE
					PRINTFORMW 「嗯゛っ、啊啊……%UNICODE(0x2665) *1%　很好……%UNICODE(0x2665) *1%　还能继续%UNICODE(0x2665) *1%」
					PRINTFORMW 「更、更多……更多的媚药、呜……」
					SETCOLOR 0xFFDDEE
					PRINTFORML 
					PRINTFORMW 「っ……呼、呼呼呼呼呼……%UNICODE(0x2665) *1%」
					PRINTFORMDW ​舞的情况有些奇怪……最好不要再让她喝媚药了。
					CALL RECOVER(TARGET,200,"気力")
					TCVAR:105:351 += 1
				CASE 3
					IF FIRSTTIME("媚薬OD")
						PRINTFORML 「药……还有吗？　那就再来一瓶……%UNICODE(0x2665) *1%」
						PRINTFORMW 「这个……啊哈哈……喝，喝得越多，效果就越明显」
						PRINTFORML 
						TCVAR:105:351 += 1
						CFLAG:105:1002 = 1
						SETCOLOR 0xFF99EE
						PRINTFORMW 「奇、怪……这个、糟糕、啊゛……っ」
						PRINTFORMW 「嗯哦゛哦……这是、什么%UNICODE(0x2665) *1%　脑子、变得奇怪了……呜%UNICODE(0x2665) *1%」
						PRINTFORMW 「好舒服゛……%CALLNAME:MASTER%……啊诶、呜呼呼%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					ELSE
						PRINTDATAW
							DATAFORM 「啊啊啊、再喝下去的话……会、会死的……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
							DATAFORM 「什、什么……？　还能、变得、更舒服吗……那、那么……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
							DATAFORM 「啊哈啊啊、又……%UNICODE(0x2665) *1%　喝了……这个、停不下来%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						ENDDATA
						CALL RECOVER(TARGET,100,"気力")
						TCVAR:105:351 += 1
					ENDIF
				CASE IS >= 4
					PRINTDATAW
						DATAFORM 「啊啊啊、再喝下去的话……会、会死的……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						DATAFORM 「什、什么……？　还能、变得、更舒服吗……那、那么……%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
						DATAFORM 「啊哈啊啊、又……%UNICODE(0x2665) *1%　喝了……这个、停不下来%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					ENDDATA
					CALL RECOVER(TARGET,100,"気力")
					TCVAR:105:351 += 1
				CASEELSE
					PRINTFORML 
				ENDSELECT
				RETURN 1
			ELSE
				SELECTCASE TCVAR:105:351
					CASE 0
						PRINTFORML 「啊啊、媚药啊……」
						PRINTFORMW 「……嘛、稍微用一点的话……会很舒服……」
						TCVAR:105:351 += 1
					CASEELSE
						PRINTFORML 「不、不行……再这样下去、的话……」
