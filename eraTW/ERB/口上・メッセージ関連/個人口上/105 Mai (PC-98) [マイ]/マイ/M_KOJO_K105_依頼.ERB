		;このお悩みは、夏之月に発生し、正解が随机で変わります
		SIF DAY:2 == 2
			RESULT = 1
		RESULTS:0 = どの水着を買おうかな？
		RESULTS:1 = 可愛的連衣裙
		RESULTS:2 = クールなセパレート
		RESULTS:3 = 色っぽい比基尼
		RESULT:1 = RAND:3 + 1
	CASE 3
		;このお悩みは、無条件で発生し、1番と3番が正解になります（5 - 3 = 2番がハズレになります）
		SIF 1
			RESULT = 1
		RESULTS:0 = ワンちゃんの名前は何にしよう？
		RESULTS:1 = ボロンゴ
		RESULTS:2 = ゲレゲレ
		RESULTS:3 = プックル
		RESULT:1 = 5
	CASE 4
		;このお悩みは、無条件で発生し、どれを選んでも正解になります
		SIF 1
			RESULT = 1
		RESULTS:0 = 夕飯は何がいい？
		RESULTS:1 = 土豆炖肉
		RESULTS:2 = 炸丸子
		RESULTS:3 = 焼鱼
		RESULT:1 = 7
	ENDSELECT
[SKIPEND]

	SIF !RESULT
		RESTART
	RETURN 1
ENDSELECT

