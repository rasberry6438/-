;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_假酒自制_EVENT_K66_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF TALENT:恋慕
				SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「%CALLNAME:MASTER%熟睡的样子可真是可爱呐～」
					PRINTFORMW	幽幽子温柔地将手臂搭在了%CALLNAME:MASTER%的肩膀上
					PRINTFORMW 「一起做点什么吧？」
				CASE 1
					PRINTFORMW 「早上好哟～」
					PRINTFORMW  幽幽子早已穿好衣装，手上好像拿着什么
					PRINTFORMW 「没有恶作剧哟～」
				ENDSELECT
			ELSEIF TALENT:思慕
				PRINTFORMW 「醒了呀？%CALLNAME:MASTER%～」
				PRINTFORMW	迷迷糊糊地醒来，幽幽子正调皮地揉着%CALLNAME:MASTER%的脸颊
				PRINTFORMW 「来喝杯茶吧？嘿嘿～」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%桑醒了嘛？」
				PRINTFORMW	幽幽子就躺在旁边，看来也刚刚睡醒不久
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 「在嘛？打扰一下～」
			RETURN 1
		ENDIF
		PRINTFORMW 「♪～～」
		RETURN 1
	ENDIF
ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE RAND:2
			case 0
				PRINTFORMW 「♪～～啊啦啦？早上好哦%CALLNAME:MASTER%～」
				PRINTFORMW	一睁眼便是在幽幽子的怀里，虽是幽灵但还是感受到了一丝温度
				PRINTFORMW 「该起来了吧？今天有很多事情要做呢～」
			case 1
				PRINTFORMW 「♪～」
				PRINTFORMW	在朦胧中醒来，发现躺在幽幽子柔软的膝上
				PRINTFORMW 「如果想再睡一会儿的话是可以的哦～」
				PRINTFORMW  柔软到不想起床
			ENDSELECT 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋慕
			PRINTFORMW 「%CALLNAME:MASTER%熟睡的样子可真是可爱呐～」
			PRINTFORMW	幽幽子温柔地将手臂搭在了%CALLNAME:MASTER%的肩膀上
			PRINTFORMW 「一起做点什么吧？」
