;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			SELECTCASE RAND:2
				case 0
					PRINTFORML 「啊啦啦～%CALLNAME:MASTER%～」
					PRINTFORML	幽幽子挽着%CALLNAME:MASTER%的手臂
					PRINTFORMW 「要来一起做点什么嘛？一起做饭～还是一起喝茶呢～」
				case 1
					PRINTFORML 「%CALLNAME:MASTER%～」
					PRINTFORML	幽幽子看到你就缠在了你身边
					PRINTFORMW 「今天又有什么计划呢～」
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:2
				case 0
					PRINTFORML 「啊%CALLNAME:MASTER%君～」
					PRINTFORML	幽幽子热情地跟%CALLNAME:MASTER%打着招呼
					PRINTFORMW 「妖梦最近有点忙呢，要不来帮忙打扫一下庭院吧？」
				case 1
					PRINTFORML 「%CALLNAME:MASTER%桑～」
					PRINTFORML	幽幽子看到%CALLNAME:MASTER%便亲切地打招呼
					PRINTFORMW 「今天来做什么好呢？」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:2
				case 0
					PRINTFORML 「嗯～%CALLNAME:MASTER%桑来啦？」
					PRINTFORML	幽幽子看起来很清闲的样子
					PRINTFORMW 「肚子有点饿了呢，是不是该吃饭了？」
				case 1
					PRINTFORML 「♪～是%CALLNAME:MASTER%桑～」
					PRINTFORML	幽幽子正哼着不知名的曲调
					PRINTFORMW 「最近都能遇见呢～」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				case 0
					PRINTFORML 「♪～～」
					PRINTFORML  幽幽子看了%CALLNAME:MASTER%一眼，继续忙自己的事情了
				case 1
					PRINTFORML	 %CALLNAME:MASTER%向幽幽子打了声招呼
					PRINTFORML	 幽幽子点了点头
					PRINTFORML	「有什么事情嘛～」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
			CALL SPECIALDAY_EVENT_1_K66
		ELSEIF TALENT:恋人
			PRINTFORML 「♪～～啊，%CALLNAME:MASTER%在啊～」
			PRINTFORML	幽幽子看到%CALLNAME:MASTER%后便靠在了%CALLNAME:MASTER%的身上
			PRINTFORMW 「♪♫～」
		ELSEIF TALENT:恋慕
			PRINTFORML 「%CALLNAME:MASTER%桑～」
			PRINTFORML	幽幽子看到%CALLNAME:MASTER%便亲切地打招呼
			PRINTFORMW 「今天来做什么好呢？」
		ELSEIF TALENT:思慕
			PRINTFORMW 「啊啦啦～%CALLNAME:MASTER%桑～在做什么有趣的事情？」
		ELSE
			PRINTFORMW 「嗯？%CALLNAME:MASTER%桑在啊～」
		RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋慕
			PRINTFORML 「%CALLNAME:MASTER%？一起入浴吧～～」
			PRINTFORML	幽幽子拿掉了浴巾，很亲昵地和%CALLNAME:MASTER%泡在一起
			PRINTFORML 「唔……脸红了呢，要不要来做点什么呢，%CALLNAME:MASTER%？」
			PRINTFORMW	幽幽子边说着边往%CALLNAME:MASTER%的身边挪了挪
		ELSE
			PRINTFORML 「想一起洗澡吗？」
			PRINTFORML 「啊咧？就这样轻易地答应了啊……不像妖梦，到现在还恐惧鬼魂呢～」
			PRINTFORML 「嗯？幽灵需要洗澡很惊讶嘛？我也是有实体的哦，保持自身清洁也是很重要呢～」
			PRINTFORMW 「那就来一起洗澡吧～」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「♪～啊……%CALLNAME:MASTER%桑正在洗澡啊，失礼了～」
		PRINTFORMW	裹着浴巾的幽幽子看到了正在洗澡的%CALLNAME:MASTER%，立刻将浴室门关上了
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「♪～啊……%CALLNAME:MASTER%桑正在洗澡啊，可是我现在想要洗澡呢」
		PRINTFORML	裹着浴巾的幽幽子看到了正在洗澡的%CALLNAME:MASTER%
		PRINTFORMW	惹恼白玉楼的主人可不是一个明智的选择，还是老老实实地离开了浴室……
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		;正月、バレンタイン、萬聖節、聖誕節の場合
		IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
			CALL SPECIALDAY_EVENT_1_K66
		ELSEIF TALENT:恋人
			SELECTCASE RAND:2
				case 0
					PRINTFORML 「啊啦啦～%CALLNAME:MASTER%～」
					PRINTFORML	幽幽子挽着%CALLNAME:MASTER%的手臂
					PRINTFORMW 「要来一起做点什么嘛？一起做饭～还是一起喝茶呢～」
				case 1
					PRINTFORML 「%CALLNAME:MASTER%～」
					PRINTFORML	幽幽子看到你就缠在了你身边
					PRINTFORMW 「今天又有什么计划呢～」
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:2
				case 0
					PRINTFORML 「啊%CALLNAME:MASTER%君～」
					PRINTFORML	幽幽子热情地跟%CALLNAME:MASTER%打着招呼
					PRINTFORMW 「妖梦最近有点忙呢，要不来帮忙打扫一下庭院吧？」
				case 1
					PRINTFORML 「%CALLNAME:MASTER%桑～」
					PRINTFORML	幽幽子看到%CALLNAME:MASTER%便亲切地打招呼
					PRINTFORMW 「今天来做什么好呢？」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:2
				case 0
					PRINTFORML 「嗯～%CALLNAME:MASTER%桑来啦？」
					PRINTFORML	幽幽子看起来很清闲的样子
					PRINTFORMW 「肚子有点饿了呢，是不是该吃饭了？」
				case 1
					PRINTFORML 「♪～是%CALLNAME:MASTER%桑～」
					PRINTFORML	幽幽子正哼着不知名的曲调
					PRINTFORMW 「最近都能遇见呢～」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				case 0
					PRINTFORML 「♪～～」
					PRINTFORML  幽幽子看了%CALLNAME:MASTER%一眼，继续忙自己的事情了
				case 1
					PRINTFORML	 %CALLNAME:MASTER%向幽幽子打了声招呼
					PRINTFORML	 幽幽子点了点头
					PRINTFORML	「有什么事情嘛～」
			ENDSELECT
		RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:恋人
			;和妖梦约会时遇到
			IF FLAG:约会的对象 == 23
				PRINTFORML 「%CALLNAME:MASTER%～嗯？妖梦？」
				PRINTFORML  在和%CALLNAME:MASTER%约会的妖梦看到幽幽子后畏畏缩缩
				PRINTFORML 「%CALLNAME:MASTER%～约会结束后是否要和我解释解释呢～」
				PRINTFORML  幽幽子的话语间充满了火药味
				CFLAG:ARG:心情不快 = 3
				CFLAG:ARG:ブチギレ = 1
				TALENT:心情 = -1
			;和紫约会时遇到
			ELSEIF FLAG:约会的对象 == 26
				PRINTFORML 「%CALLNAME:MASTER%真是花心啊～」
				PRINTFORML  幽幽子微笑着看着自己的好友和自己的恋人
				PRINTFORML 「那么，%CALLNAME:MASTER%更喜欢我还是紫呢～约会完后告诉我哦～」
				PRINTFORMW  需要好好道个歉了
				CFLAG:ARG:心情不快 = 3
				CFLAG:ARG:ブチギレ = 1
				TALENT:心情 = -1
			ELSE
				PRINTFORML 「有了我还想得到别人嘛～」
				PRINTFORML  幽幽子“和善”地望着%CALLNAME:MASTER%和%CALLNAME:ARG%
				PRINTFORMW 「出轨可是不太好的事哦～」
				CFLAG:ARG:心情不快 = 3
				CFLAG:ARG:ブチギレ = 1
				TALENT:心情 = -1
			ENDIF
		ELSEIF TALENT:恋慕
			;和妖梦约会时遇到
			IF FLAG:约会的对象 == 23
				PRINTFORML 「嗯？在约会？」
				PRINTFORML 「还是更喜欢妖梦嘛～」
				PRINTFORML 「要好好照顾妖梦哦～」
				PRINTFORMW  幽幽子直接飘走了
			;和紫约会时遇到
			ELSEIF FLAG:约会的对象 == 26
				PRINTFORML 「什么嘛……和紫关系也很好啊～」
				PRINTFORML  看到紫和%CALLNAME:MASTER%约会的幽幽子撅起小嘴
				PRINTFORMW 「我可不会输给你哦～紫～」
			ELSE
				PRINTFORML 「真可爱呢～」
				PRINTFORML  直接从%CALLNAME:MASTER%的身边飘走了
				PRINTFORMW  感觉到了幽幽子的目光从后面传来
			ENDIF
		ELSE
			PRINTFORML 「♪～～」
			PRINTFORMW  幽幽子忙着自己的事情
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
	;正月、バレンタイン、萬聖節、聖誕節の場合
		IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
			CALL SPECIALDAY_EVENT_1_K66
		ELSE
		PRINTFORML 「♪～～今天吃什么呢？」
		RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
	;ARG:1にデート相手、ARG:1が0の場合は非デート
	;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1とデート中
		IF ARG:1
		;一人で外出
		PRINTFORML 【1】
		ELSE
		PRINTFORML 【2】
		ENDIF
		PRINTFORML 【3】
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;特别的日子打招呼
