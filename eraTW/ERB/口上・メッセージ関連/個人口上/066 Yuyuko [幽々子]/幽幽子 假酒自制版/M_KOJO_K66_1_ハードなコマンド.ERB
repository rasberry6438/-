;==================================================
;144,放尿
;==================================================
@M_KOJO_假酒自制_MESSAGE_COM_K66_144
CALL TRAIN_MESSAGE
CALL M_KOJO_假酒自制_MESSAGE_COM_K66_144_1
RETURN RESULT

@M_KOJO_假酒自制_MESSAGE_COM_K66_144_1
