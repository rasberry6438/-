;40,跳蛋
;==================================================
@M_KOJO_假酒自制_MESSAGE_COM_K90_40
CALL TRAIN_MESSAGE
CALL M_KOJO_假酒自制_MESSAGE_COM_K90_40_1
RETURN RESULT

@M_KOJO_假酒自制_MESSAGE_COM_K90_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「要用这种小玩具？」
		PRINTFORML 「看起来也不太……啊啊啊？！唔～」
		PRINTFORML 「速度好快～❤嗯、姆啊～～」
		PRINTFORMW  刚接触跳蛋的瞬间，刺激感瞬间冲向幽幽子的身体
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORML  跳蛋不断嗡嗡摩擦着幽幽子的阴蒂
		PRINTFORMW  这么多的快感堆杂在一起，解除后应该会很有趣
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:恋人
		IF ABL:Ｃ感覚 >= 5
			PRINTFORML 「唔啊啊～那里……嗯～呜～❤」
			PRINTFORMW 「好舒服…要去……啊……、❤」
		ELSEIF ABL:Ｃ感覚 >= 3
			PRINTFORML 「太刺激了……哇、咿～～❤」
			PRINTFORMW 「一抖一抖地……哈、嗯啊～～❤」
		ELSEIF ABL:Ｃ感覚 >= 1
			PRINTFORML 「嗯、唔啊啊～好痒～」
			PRINTFORMW 「小豆豆……嗯～❤好奇怪的感觉～❤」
		ELSE
			PRINTFORML 「啊啊啊～那里……好快～❤」
			PRINTFORMW 「麻酥酥的…唔～很厉害呢❤」
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「咦？姆啊～」
			PRINTFORMW 「好快～唔、麻麻的呀～❤」
		CASEELSE
			PRINTFORML 「这种东西……嗯～～」
			PRINTFORMW 「只靠玩具可也…唔啊～等…唔姆～❤」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

