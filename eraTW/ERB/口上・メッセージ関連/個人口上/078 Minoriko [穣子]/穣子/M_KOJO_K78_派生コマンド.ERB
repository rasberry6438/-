;==================================================
;5,手淫口交
;==================================================
@M_KOJO_MESSAGE_SCOM_K78_5
CALL M_KOJO_MESSAGE_SCOM_K78_5_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K78_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	IF K78_BASIC_CONDITION_IN_MATING()
		$HANDJOB_AND_FELLATIO_LOOP
		IF TALENT:恋慕 || TALENT:愛欲
			IF NOWEX:MASTER:射精 && !TEQUIP:MASTER:避孕套 && EXP:精飲経験 > 0
				SELECTCASE RAND:3
				CASE 1
					;咥えたまま
					IF EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3
						CALL K78_HPH_PRINT,@"「嗯？ 要出来了吗？HPH 可以哟HPH 在我的嘴里尽情的射出来吧HPH」","W"
					ELSE
						GOTO HANDJOB_AND_FELLATIO_LOOP
					ENDIF
				CASE 2
					PRINTFORMDL %CALLNAME:TARGET%感觉到马上就要射精了，就咯吱咯吱的用手激烈的摩擦起来。
					CALL K78_HPH_PRINT,@"「撒，出来吧HPH 在我的手和嘴里尽情的射出来吧HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「啊，黏糊糊的HPH 那么，就这样HPH 阿姆…啾…吸溜HPH」","W"
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「不用忍耐哦HPH 想出来的时候就尽管射出来吧HPH」","W"
				CASE 2
					PRINTFORMDL %CALLNAME:TARGET%用舌头妖艳的舔着下面各个角落
					CALL K78_HPH_PRINT,@"「嗯啾、吸溜、嗯唔HPH 这里舒服吗？HPH 还是说这里呢？ 嗯唔，嗯啾HPH」","W"
				CASEELSE
					PRINTFORMDL %CALLNAME:TARGET%发出咕啾咕啾的声音激烈的吸吮着龟头
					CALL K78_HPH_PRINT,@"「嗯、嗯唔HPH 啾、咕啵HPH 啾噜、啾噜噜HPH」","W"
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE RAND:3
			CASE 1
				IF TALENT:思慕 || TALENT:炮友
					PRINTFORMW 「嗯唔…啾…啾噜…哈啊… 渐渐烫起来了呢，这个」
				ELSE
					PRINTFORMW 「嗯…嗯唔、嗯啾…… 呼，嘴巴和手都有点累了」
				ENDIF
			CASE 2
				IF ABL:指 + ABL:舌 >= 3
					PRINTFORMW 「嗯唔…啾…嗯啾… 如何？ 是不是相当熟练了呢？」
				ELSE
					PRINTFORMW 「没，没有弄疼吧？我不是太懂…」
				ENDIF
			CASEELSE
				PRINTFORMW 「快要出来的时候就说一声。突然就出来的话还是会吓一跳的」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K78_{SELECTCOM}_1
RETURN 1

