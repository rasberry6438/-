		ELSEIF CHK_DATENOW(CFLAG:LOCAL:约会中) && CFLAG:MASTER:現在位置 != OMANEKIBEYA()
			
		ELSE
			IF BASE:情緒 >= 500 && BASE:理性 < 975
				;開心的回抱过来…
				IF TALENT:恋慕
					SELECTCASE RAND:2
