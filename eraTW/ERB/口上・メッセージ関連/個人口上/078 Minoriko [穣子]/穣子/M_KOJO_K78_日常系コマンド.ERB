				LOCALS:0 = 在出产前后变化する
			CASE 2
				LOCALS:0 = 无论是否妊娠，根据母乳体质发生变化する
			CASEELSE
				LOCALS:0 = ※CFLAG:78:1001の値が不正※
			ENDSELECT

			SELECTCASE CFLAG:78:1002
			CASE 0
				LOCALS:1 = スカトロ无
			CASE 1
				LOCALS:1 = 有脱粪场景
			CASEELSE
				LOCALS:1 = ※CFLAG:78:1002の値が不正※
			ENDSELECT

			PRINTL
			PRINTFORMDL 現在的乳头変化的設定：%LOCALS:0%　　　現在的屎尿屁表現的設定：%LOCALS:1%
			PRINTL
			CALL ASK_M("変更乳头変化的設定",1,"変更屎尿屁表現的設定",1,"这样就好",1)
			SELECTCASE RESULT
			CASE 0
				IF CFLAG:78:1001 >= 2
					CFLAG:78:1001 = 0
				ELSE
					CFLAG:78:1001 += 1
				ENDIF
				PRINTL
				GOTO PRAYER_LOOP
			CASE 1
				CFLAG:78:1002 = !CFLAG:78:1002
				PRINTL
				GOTO PRAYER_LOOP
			CASE 2
				PRINTFORMDL %CALLNAME:TARGET%的乳头が%LOCALS:0%ようになった気がする。
				PRINTFORMD %CALLNAME:TARGET%用
				IF CFLAG:78:1002 == 0
					PRINTFORMD お尻をきれいにして肛门性交の準備を怠らない
				ELSE
					PRINTFORMD 肛门性交の前でもお尻を直接住下にしておく
				ENDIF
				PRINTFORMDW ようになった気がする。
				PRINTL
			ENDSELECT
		ENDIF
		SETCOLOR LOCAL:1
	ENDIF
	IF K78_CONSCIOUS_AND_TOLERANT()
		SELECTCASE RAND:9
		CASE 1 TO 4
			PRINTFORMW 「许了什么愿望呢？」
		CASE 5 TO 8
			PRINTFORMW 「看起来很虔诚的样子呢」
		CASEELSE
			PRINTFORMW 「嗯唔… 你所拜的那个神，总感觉很可疑的样子…」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

