			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「早上好%CALLNAME:MASTER%。那么，今天我们来打理一下田地吧」
			CASE 2
				PRINTFORMDL 一睁開眼睛%CALLNAME:TARGET%的脸庞就立刻出现在了%CALLNAME:MASTER%面前。
				CALL K78_HPH_PRINT,@"「埃嘿嘿♪看到%CALLNAME:MASTER%可爱的睡脸了HPH」","W"
			CASEELSE
				;TARGETと一緒に起きた
				IF TIME:0 == CFLAG:起床時間
					PRINTFORMW 「早上好。今天做什么好呢」
				;TARGETより遅く起きた
				ELSE
					PRINTFORMW 「终于起床了？睡了个懒觉呢」
				ENDIF
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or陪睡中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;其他
	PRINTFORML  
ENDIF
RETURN 1

