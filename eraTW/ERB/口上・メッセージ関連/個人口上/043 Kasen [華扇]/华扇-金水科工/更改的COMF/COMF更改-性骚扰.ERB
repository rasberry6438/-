				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 像摩擦乳沟似的愛撫起来……
					CASE 1
						PRINTFORMW 细心地揉捏起来……
					CASE 2
						PRINTFORMW 感受着体温并揉捏起来……
				ENDSELECT
			;胸を揉ませる
			CASE 33
				PRINTFORMW %CALLNAME:PLAYER%不甘示弱的动起了手、揉捏起%CALLNAME:TARGET%的%OPPAI_DESCRIPTION(TARGET)%……
			;乳交
			CASE 53
				IF TALENT:TARGET:献身的
					PRINTFORM %CALLNAME:PLAYER%对沉浸在乳交中而忘乎所以的%CALLNAME:TARGET%的
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML %OPPAI_DESCRIPTION(TARGET)%伸出手、
							PRINTFORML 从両側使其挟住陰茎、就这様动起腰侵犯着乳沟……
							SELECTCASE ABL:TARGET:Ｂ感覚
								CASE 0 TO 3
									PRINTFORMW %CALLNAME:TARGET%微笑着并避開%CALLNAME:PLAYER%的手、重新開始奉仕起来……
								CASE 4 TO 7
									PRINTFORMW %CALLNAME:TARGET%用嘴含着手并承受着快感……
								CASEELSE
									PRINTFORMW %CALLNAME:TARGET%似乎按捺不住快感、面泛紅潮向%CALLNAME:PLAYER%提出了抗議……
							ENDSELECT
						CASE 1
							PRINTFORML %NIPPLE_DESCRIPTION(TARGET)%捏住提拉
							SELECTCASE ABL:TARGET:Ｂ感覚
								CASE 0 TO 3
