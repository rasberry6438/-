@COMF_K43_TPP_X_310

IF FLAG:70 == 1
;时停
	PRINTFORML %CALLNAME:PLAYER%从背后把手伸向了%CALLNAME:TARGET%的%HIP(TARGET)%
	PRINTFORMW 時停中的%CALLNAME:TARGET%的屁股就这様毫无抵抗地被抚摸了...
ELSEIF CFLAG:TARGET:睡眠 == 1
;睡觉
	PRINTFORMW %CALLNAME:PLAYER%把手伸向了侧躺着入睡的%CALLNAME:TARGET%的%HIP(TARGET)%...
ELSEIF CFLAG:318 == 1 || MARK:3 >= 2
;反发+愤怒
	PRINTFORMW 恬不知耻地执着于摸屁股的%CALLNAME:PLAYER%根本没料到迎接自己的是什么——
ELSEIF CFLAG:诶嘿嘿 == 2
;爱爱
	PRINTFORM %CALLNAME:PLAYER%
	SELECTCASE TCVAR:20
	CASE 70
		IF !TEQUIP:0
			PRINTFORMW 揉着%CALLNAME:TARGET%的%HIP(TARGET)%，同时用舌头奉仕着%SLIT(TARGET, 1)%...
		ELSE
			GOTO NOMALSIRINADE
		ENDIF
	CASE 72
		IF !TEQUIP:0
			PRINTFORMW 扒開%CALLNAME:TARGET%的臀瓣，用舌尖侵入到肛門之中...
		ELSE
			GOTO NOMALSIRINADE
		ENDIF
	CASE 35
		PRINTFORMW 把%CALLNAME:TARGET%的屁股转向自己，然后来回抚摸起来...
	CASE 52
		PRINTFORMW 揉起跨坐在腰上的%CALLNAME:TARGET%的%HIP(TARGET)%...
	CASEELSE
		$NOMALSIRINADE
		SIF TEQUIP:六九式
			PRINTFORMW 対跨坐在臉上的
		PRINTFORMW 对%CALLNAME:TARGET%的%HIP(TARGET)%揉搓起來...
	ENDSELECT
ELSE
;一般
	PRINTFORML %CALLNAME:PLAYER%\@ PREVCOM != 305 ? 从背后 #  \@把手伸向了\@ PREVCOM == 305 ? 向躺在膝盖上的 #  \@%CALLNAME:TARGET%的%HIP(TARGET)%
	IF TALENT:恋人 
		IF CFLAG:43:1011 == 1
			PRINTFORMW 但在触及屁股之前，%CALLNAME:TARGET%却缓缓地把%CALLNAME:PLAYER%的手挪开了...
		ELSE
			PRINTFORMW %CALLNAME:TARGET%微笑着接受起%CALLNAME:PLAYER%的亲昵来...
		ENDIF
	ELSEIF TALENT:恋慕 
		IF CFLAG:43:1011 == 1
			PRINTFORMW 但%CALLNAME:TARGET%选择轻轻抓住%CALLNAME:PLAYER%的手，将其扳开。
		ELSE
			PRINTFORMW %CALLNAME:TARGET%脸颊微红，身子微微颤抖着接受起爱抚...
		ENDIF
	ELSE
		IF CFLAG:43:1011 == 1
			PRINTFORMW 但手还没贴上去，就被%CALLNAME:TARGET%一把甩开了...
		ELSE
			IF TALENT:思慕 || ABL:親密 >= 5
				PRINTFORMW %CALLNAME:TARGET%眼神微闭，尽力忍受着来自屁股的不适感...
			ELSE
				PRINTFORMW %CALLNAME:TARGET%嘴唇紧闭着接受了朝向屁股的侵犯...
			ENDIF
		ENDIF
	ENDIF
	TFLAG:400 = RAND:2
	SIF TFLAG:400 && (DATE_HITOGOMI() || WITH_MOB() == 9)
		CALL DATE_HITOGOMI_MESSAGE(CFLAG:PLAYER:現在位置)
ENDIF

IF FLAG:70 == 1
	BASE:MASTER:TSP -= 10
ELSE
	IF FLAG:甲斐性無 && !CFLAG:诶嘿嘿
		DRAWLINE
		PRINTFORMW %CALLNAME:MASTER%沒有那樣的膽量
		RETURN -1
	ENDIF
	LOCAL = RAND:110
	LOCAL:1 = MIN(109, 90 + GET_SUCCESS_RATE() / 5)
	CALL KOJO_FORCED_FAILURE(LOCAL, LOCAL:1)
	SIF TFLAG:193 == -2
		RETURN -1
	DOWNBASE:気力 += 50
	DOWNBASE:MASTER:気力 += 50
	TFLAG:理性削り = 1
	;信赖度
	TFLAG:98 = -3
	;各数值的基础获取量
	SOURCE:歓楽 = 100
	SOURCE:情愛 = 100
	SOURCE:露出 = 200
	SOURCE:反感 = 500
	SOURCE:欲情 = 100
	SOURCE:受動 = 100
	;考虑到亲密时的计算
	IF TALENT:恋人 
		SOURCE:歓楽 += 200 + (ABL:親密 * 20)
		SOURCE:情愛 += 1600 + (ABL:親密 * 30)
		SOURCE:欲情 += 1000 + 300 * ABL:欲望
		SOURCE:受動 += 900 + 200 * ABL:従順
		SOURCE:反感 -= (ABL:親密 * 50)
		TFLAG:193 = RAND:2 + 3
	ELSEIF TALENT:恋慕
		SOURCE:歓楽 += 1000 + (ABL:親密 * 10)
		SOURCE:情愛 += 700 + (ABL:親密 * 20)
		SOURCE:欲情 += 300 * ABL:欲望
		SOURCE:受動 += 200 * ABL:従順
		SOURCE:反感 -= (ABL:親密 * 20)
		TFLAG:193 = RAND:2 + 2
	ELSEIF TALENT:思慕 || ABL:親密 >= 5
		SOURCE:歓楽 += 300 + (ABL:親密 * 5)
		SOURCE:情愛 += 200 + (ABL:親密 * 10)
		SOURCE:欲情 += 300 * ABL:欲望
		SOURCE:受動 += 200 * ABL:従順
		SOURCE:反感 -= (ABL:親密 * 15)
		TFLAG:193 = RAND:2 + 2
	ELSE
		SOURCE:反感 -= (ABL:親密 * 5)
	ENDIF
	SIF SOURCE:反感 < 0
		SOURCE:反感 = 0
	SIF SOURCE:歓楽 < 0
		SOURCE:歓楽 = 0
	;先制発生時は失敗しない
	SIF TFLAG:102 == 3 && TFLAG:192 != -1
		TFLAG:193 = MAX(TFLAG:193,2)
	LOCAL = 0
	;亲密低时对应降低的好感度
	IF ABL:親密 <= 5
		LOCAL -= 10
	ELSEIF ABL:親密 <= 7
		LOCAL -= 5
	ELSEIF ABL:親密 <= 9
		LOCAL -= 3
	ELSEIF TALENT:恋慕
		LOCAL -= 1
	ENDIF
	;好感度变化
	TFLAG:97 += LOCAL
	IF HITOKE(1)
		TIMES SOURCE:露出 , 2.00
		TIMES SOURCE:反感 , 2.00
	ENDIF
	IF TFLAG:193 == 1
		TIMES SOURCE:歓楽 , 0.10
		TIMES SOURCE:情愛 , 0.50
		TIMES SOURCE:受動 , 0.50
		TIMES SOURCE:欲情 , 0.50
		TIMES SOURCE:反感 , 1.50
	ELSEIF TFLAG:193 == 2
		TIMES SOURCE:歓楽 , 1.00
		TIMES SOURCE:情愛 , 1.00
		TIMES SOURCE:受動 , 1.00
		TIMES SOURCE:欲情 , 1.00
	ELSEIF TFLAG:193 == 3
		TIMES SOURCE:歓楽 , 1.50
		TIMES SOURCE:情愛 , 1.50
		TIMES SOURCE:受動 , 1.50
		TIMES SOURCE:欲情 , 1.50
	ELSE
		TIMES SOURCE:歓楽 , 2.00
		TIMES SOURCE:情愛 , 2.00
		TIMES SOURCE:受動 , 2.00
		TIMES SOURCE:欲情 , 2.00
		TIMES SOURCE:反感 , 0.75
	ENDIF
	TIME += 10
ENDIF
RETURN 1

