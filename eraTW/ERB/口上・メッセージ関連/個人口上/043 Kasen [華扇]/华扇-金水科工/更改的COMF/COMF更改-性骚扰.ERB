ENDIF

IF FLAG:70 == 1
	BASE:MASTER:TSP -= 10
	IF HETEROSEX(TARGET,PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
	ENDIF
ELSE
	IF FLAG:甲斐性無 && !CFLAG:诶嘿嘿
		DRAWLINE
		PRINTFORMW %CALLNAME:MASTER%沒有那樣的膽量
		RETURN -1
	ENDIF
	TFLAG:理性削り = 1
	SELECTCASE TFLAG:192
		;通常
		CASE 0
			LOCAL = RAND:110
			LOCAL:1 = 90 + GET_SUCCESS_RATE() / 5
			SIF LOCAL:1 > 109
				LOCAL:1 = 109
			IF LOCAL <= 9
				;10％で大成功
				TFLAG:193 = 1
				IF CHK_DATENOW(CFLAG:TARGET:约会中)
					EXP:MASTER:約會経験 ++
					EXP:TARGET:約會経験 ++
				ENDIF
			ELSEIF LOCAL >= LOCAL:1
				;10～1％で失敗
				TFLAG:193 = -1
			ELSE
				;残りは成功
				TFLAG:193 = 0
			ENDIF
		;強制成功or大成功
		CASE 1
			IF RAND:110 <= 9
				;10％で大成功
				TFLAG:193 = 1
				IF CHK_DATENOW(CFLAG:TARGET:约会中)
					EXP:MASTER:約會経験 ++
					EXP:TARGET:約會経験 ++
				ENDIF
			ELSE
				;残りは成功
				TFLAG:193 = 0
			ENDIF
		;強制失敗
		CASE -1
			TFLAG:193 = -1
		;指令終了
		CASE -2
			TFLAG:193 = -1
			RETURN 0
	ENDSELECT
