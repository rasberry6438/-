		PRINTFORMW 刚一分开嘴唇，%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%就露出淡淡的微笑…
	ELSEIF ABL:親密 >= 3
		PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%有点难受地皱起眉头…
	ELSE
		PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%神情扭曲并无意识地转过了脸…
	ENDIF
ELSEIF TFLAG:193 == 99
	PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%刚要接吻，%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%就厌恶地拉开了距离…
ELSEIF CFLAG:TARGET:陪睡中
	PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%用自己的嘴堵上了陪睡中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的嘴…
	IF TALENT:恋人 || TALENT:恋慕
		PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%靠到%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%身边轻轻吻了上去…
	ELSEIF ABL:親密 >= 5
		PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%红着脸并有些迟疑地把嘴唇迎了上来…
	ELSE
		PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%害羞地红了脸…
	ENDIF
ELSE
	IF CFLAG:诶嘿嘿 == 2
		SELECTCASE TCVAR:20
			;前から抱きつく
			CASE 32
				PRINTFORM %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%和%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%
				IF TALENT:恋慕 || TALENT:恋人
					PRINTW 抱在一起的同时，热情且贪婪地接吻起来……
				ELSE
					PRINTW 抱在一起的同时接吻起来……
				ENDIF
			;手淫
			CASE 50
				IF TALENT:PLAYER:2 & 2
					PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%搂住撸动着肉棒的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%并亲吻起来…
				ELSE
					PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%搂住玩弄着身体的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%并亲吻起来…
				ENDIF
			;接吻
			CASE 60
				SELECTCASE RAND:3
				CASE 0
					PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%与%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%热烈地湿吻着、互相索求着唾液……
				CASE 1
					PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%將舌头伸入%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%口中，与%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%自己主动伸出的舌头交缠在一起……
				CASEELSE
					PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%将舌头侵入%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的口腔、不落死角地来回舔舐起来……
				ENDSELECT
			;85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ
			CASE 85,87,88,89
				IF TALENT:恋慕 || TALENT:恋人
					PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%亲吻着\@ TCVAR:20 == 85 || TCVAR:20 == 88 ? 膣道 # 腸道 \@正被插入的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%
					PRINTFORMW 而%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%也激烈的晃動著腰、貪求著%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的\@ TCVAR:20 == 85 || TCVAR:20 == 88 ? 蜜穴 # 肛門 \@……
				ELSE
					PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%亲吻着\@ TCVAR:20 == 85 || TCVAR:20 == 88 ? 膣道 # 腸道 \@正被插入的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%
				ENDIF
			;だいしゅき大堂ド
			CASE 86
				SELECTCASE BASE:PLAYER:2
				CASE 0 TO 2999
					PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%保持着与%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%深入相连的状态并紧紧抱在一起、热吻起来
					PRINTFORMW 是想增加与%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%結合的時間吗、慢慢地开始有节奏的扭动起来……
				CASE 3000 TO 9999
					PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%保持着与%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%深入相连的状态并紧紧抱在一起、热吻起来
					PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%似是按捺不住，开始激烈地摇动着腰
					PRINTFORMW 爱液从接合部位不停地迸射出来、周围响起了咕啾咕啾的水声……
				CASEELSE
					PRINTFORML 快射精的%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%保持着与%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%深入相连的状态并紧紧抱在一起、热吻起来
					PRINTFORML 同时%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%也为了与%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%同時高潮而激烈地摇动着腰
					PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%在与%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%互相表达了一会儿爱意之后、把即将爆发的阴茎插进了最深处……
				ENDSELECT
			CASEELSE
				PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%直起身嘬弄起%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的嘴唇…
		ENDSELECT
	ELSE
		;前回膝枕
		IF PREVCOM == 305
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%从膝盖上撑起身体轻吻了%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的嘴唇…
		ELSE
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%用自己的嘴堵住了%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的嘴…
		ENDIF
		IF TALENT:恋人 
			IF CFLAG:43:1011 == 1
				PRINTFORMW 但只停留了几秒，%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%便又缓缓地将两人的唇瓣分离开来...
			ELSE
				PRINTFORMW 而%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%也叩开%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%的嘴唇，把舌头缠绕过去…
			ENDIF
		ELSEIF TALENT:恋慕 
			IF CFLAG:43:1011 == 1
				PRINTFORMW 但很快就在%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%推动%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%身子的动作中缓缓地分开了两人的嘴唇...
			ELSE
				PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%于亲吻中不禁染红了脸颊...
			ENDIF
		ELSEIF TALENT:思慕 || ABL:親密 >= 5
			IF CFLAG:43:1011 == 1
				PRINTFORMW 当然，这只是%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%被%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%一把推开前的幻想罢了。
			ELSE
				PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%眼神微闭，尽力忍受着来自嘴唇处的冒犯...
			ENDIF
		ENDIF
		TFLAG:400 = RAND:2
		SIF TFLAG:400 && (DATE_HITOGOMI() || WITH_MOB() == 9)
			CALL DATE_HITOGOMI_MESSAGE(CFLAG:PLAYER:現在位置)
	ENDIF
ENDIF

IF FLAG:70 == 1
	BASE:MASTER:TSP -= 10
	SOURCE:快Ｍ = ABL:PLAYER:舌 * 2 + 10
	EXP:MASTER:接吻経験 += 1
	IF HETEROSEX(TARGET,PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 2
	ENDIF
ELSE
	;甲斐性無しの場合、その場にいる全員が関係を公認してないと勇気がでない
	IF FLAG:甲斐性無
		FOR LOCAL, 1, GET_TARGETNUM() + 1
			SIF TARGET:LOCAL == TARGET:0
				CONTINUE
			IF GET_UWAKI_HISTORY(TARGET:LOCAL, TARGET:0) < 2
				DRAWLINE
				; PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%は%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%に口づけをしようとしたが
				PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%想亲吻%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%
				; PRINTFORMW 視界の端に%NAME_OUTPUT_TRANSLATION("CALLNAME", (TARGET:LOCAL))%の姿が映ると思わず顔を離してしまった…
				PRINTFORMW 但视野的边缘出现%NAME_OUTPUT_TRANSLATION("CALLNAME", (TARGET:LOCAL))%的身影时，%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%不由得将脸移开了
				RETURN -1
			ENDIF
		NEXT
	ENDIF
	TFLAG:理性削り = 1
	;拒絶判定
	IF (REJECT_CHECK(SELECTCOM) || CFLAG:318 == 1 || MARK:3 >= 2  || ABL:親密 < 5) && !CFLAG:睡眠
		TFLAG:193 = 99
		LOCAL = 0
		;固定獲得ソース
		SOURCE:反感 = 500
		SOURCE:恐怖 = 300
		;好感度が低いと好感度低下
		IF CFLAG:2 <= 500
			LOCAL -= 50
		ELSEIF CFLAG:2 <= 1000
			LOCAL -= 30
		ELSEIF CFLAG:2 <= 1500
			LOCAL -= 10
		ENDIF
		;好感度変化
		TFLAG:97 += LOCAL
		TIME += 10
		RETURN 1
	ENDIF

	SELECTCASE TFLAG:192
		;通常
		CASE 0
			LOCAL = RAND:110
			LOCAL:1 = 90 + GET_SUCCESS_RATE() / 5
			SIF LOCAL:1 > 109
				LOCAL:1 = 109
			IF LOCAL <= 29
				;10％で大成功
				TFLAG:193 = 1
				IF CHK_DATENOW(CFLAG:TARGET:约会中)
					EXP:MASTER:約會経験 ++
					EXP:TARGET:約會経験 ++
				ENDIF
			ELSEIF LOCAL >= LOCAL:1
				;10～1％で失敗
				TFLAG:193 = -1
			ELSE
				;残りは成功
				TFLAG:193 = 0
			ENDIF
		;強制成功or大成功
		CASE 1
			IF RAND:110 <= 9
				TFLAG:193 = 1
				IF CHK_DATENOW(CFLAG:TARGET:约会中)
					EXP:MASTER:約會経験 ++
					EXP:TARGET:約會経験 ++
				ENDIF
			ELSE
				TFLAG:193 = 0
			ENDIF
		;強制失敗
		CASE -1
			TFLAG:193 = -1
		;指令終了
		CASE -2
			TFLAG:102 = 0
			TFLAG:193 = -1
			RETURN 0
	ENDSELECT

	;先制発生時は失敗しない
	SIF TFLAG:102 == 3 && TFLAG:192 != -1
		TFLAG:193 = MAX(TFLAG:193,0)


	IF CFLAG:TARGET:陪睡中
		DOWNBASE:MASTER:気力 += 50
	ELSEIF CHK_DATENOW(CFLAG:TARGET:约会中)
		DOWNBASE:気力 += 30
		DOWNBASE:MASTER:気力 += 50
	ELSE
		DOWNBASE:気力 += 70
		DOWNBASE:MASTER:気力 += 100
	ENDIF

	;固定で獲得するソース
	SOURCE:快Ｍ = ABL:PLAYER:舌 * 2 + 10
	SOURCE:PLAYER:快Ｍ = ABL:舌 * 2 + 10
	SOURCE:歓楽 = 150
	SOURCE:情愛 = 150
	SOURCE:露出 = 80
	SOURCE:反感 = 300
	SIF CFLAG:睡眠
		SOURCE:反感 = 200

	;ABL:親密をみる
	IF ABL:親密 <= 1
		SOURCE:情愛 += (ABL:親密 * 30)
	ELSEIF ABL:親密 <= 3
		SOURCE:歓楽 += 150 + (ABL:親密 * 30)
		SOURCE:情愛 += 150 + (ABL:親密 * 30)
		SOURCE:欲情 += 65 + (ABL:親密 * 20)
	ELSEIF ABL:親密 <= 5
		SOURCE:歓楽 += 300 + (ABL:親密 * 40)
		SOURCE:情愛 += 280 + (ABL:親密 * 30)
		SOURCE:欲情 += 130 + (ABL:親密 * 26)
		SOURCE:露出 += 80 + (ABL:親密 * 15)
	ELSEIF ABL:親密 <= 8
		SOURCE:歓楽 += 450 + (ABL:親密 * 70)
		SOURCE:情愛 += 380 + (ABL:親密 * 40)
		SOURCE:欲情 += 170 + (ABL:親密 * 30)
		SOURCE:露出 += 110 + (ABL:親密 * 18)
	ELSEIF ABL:親密 <= 11
		SOURCE:歓楽 += 500 + (ABL:親密 * 85)
		SOURCE:情愛 += 480 + (ABL:親密 * 50)
		SOURCE:欲情 += 230 + (ABL:親密 * 35)
		SOURCE:露出 += 160 + (ABL:親密 * 20)
	ELSE
		SOURCE:歓楽 += 800 + (ABL:親密 * 80)
		SOURCE:情愛 += 760 + (ABL:親密 * 45)
		SOURCE:欲情 += 350 + (ABL:親密 * 30)
		SOURCE:露出 += 260 + (ABL:親密 * 17)
	ENDIF

	;好感度をみる
	IF CFLAG:2 <= 500
		SOURCE:情愛 += CFLAG:2
	ELSEIF CFLAG:2 <= 5000
		SOURCE:情愛 += 400 + (CFLAG:2 - 500) / 5
		SOURCE:歓楽 += 400 + (CFLAG:2 - 500) / 4
	ELSE
		SOURCE:情愛 += 1200 + (CFLAG:2 - 5000) / 7
		SOURCE:歓楽 += 1200 + (CFLAG:2 - 5000) / 4
	ENDIF
	SIF SOURCE:情愛 < 0
		SOURCE:情愛 = 0

	LOCAL = 0
	;好感度が低いと好感度低下
	IF CFLAG:2 <= 100
		LOCAL -= 3
	ELSEIF CFLAG:2 <= 300
		LOCAL -= 2
	ELSEIF CFLAG:2 <= 500
		LOCAL -= 1
	ENDIF

SIF CFLAG:体目当て
	SOURCE:情愛 *= 3

	SOURCE:受動 = 150 + 300 * ABL:従順
	SOURCE:征服 = 150 + 300 * ABL:施虐属性

	;好感度変化
	SIF !CFLAG:睡眠
		TFLAG:97 += LOCAL

	;二人きりだと効果UP、そうでないと露出以外ダウン=============
	FOR LOCAL,1,CHARANUM
		SIF CFLAG:LOCAL:同室
			LOCAL:2 ++
	NEXT
	IF LOCAL:2 == 1
		TIMES SOURCE:歓楽 , 1.20
		TIMES SOURCE:情愛 , 1.20
		TIMES SOURCE:征服 , 1.20
		TIMES SOURCE:受動 , 1.20
	ELSE
		TIMES SOURCE:歓楽 , 0.70
		TIMES SOURCE:情愛 , 0.80
		TIMES SOURCE:征服 , 0.80
		TIMES SOURCE:受動 , 0.90
		TIMES SOURCE:露出 , 1.10
	ENDIF
	;情緒の影響=====================================================
	IF BASE:情緒 == MAXBASE:情緒
		TIMES SOURCE:歓楽 , 1.30
		TIMES SOURCE:情愛 , 1.30
		TIMES SOURCE:征服 , 1.20
		TIMES SOURCE:受動 , 1.20
		TIMES SOURCE:露出 , 1.30
	ELSEIF BASE:情緒 < 600 && BASE:情緒 > 200 && !TALENT:接吻魔
		TIMES SOURCE:歓楽 , 0.70
		TIMES SOURCE:情愛 , 0.75
		TIMES SOURCE:征服 , 0.70
		TIMES SOURCE:受動 , 0.60
		TIMES SOURCE:露出 , 0.50
	ELSEIF BASE:情緒 <= 200 && !TALENT:接吻魔
		TIMES SOURCE:歓楽 , 0.40
		TIMES SOURCE:情愛 , 0.40
		TIMES SOURCE:征服 , 0.40
		TIMES SOURCE:受動 , 0.40
		TIMES SOURCE:露出 , 0.35
	ENDIF
	;接吻魔===============================================================
	IF TALENT:接吻魔
		TIMES SOURCE:歓楽 , 1.40
		TIMES SOURCE:情愛 , 1.40
	ENDIF
	;=====================================================================
	IF TFLAG:193 == -1
		TIMES SOURCE:歓楽 , 0.10
		TIMES SOURCE:情愛 , 0.50
		TIMES SOURCE:征服 , 0.50
		TIMES SOURCE:受動 , 0.50
		TIMES SOURCE:露出 , 0.40
	ELSEIF TFLAG:193 == 0
		TIMES SOURCE:歓楽 , 1.00
		TIMES SOURCE:情愛 , 1.00
		TIMES SOURCE:征服 , 1.00
		TIMES SOURCE:受動 , 1.00
		TIMES SOURCE:露出 , 0.90
		TFLAG:情緒BONUS = 50
	ELSE
		TIMES SOURCE:歓楽 , 2.00
		TIMES SOURCE:情愛 , 2.00
		TIMES SOURCE:征服 , 2.00
		TIMES SOURCE:受動 , 2.00
		TIMES SOURCE:露出 , 1.80
		TFLAG:情緒BONUS = 100
	ENDIF
	TIME += 5
	SIF !CFLAG:睡眠
		EXP:接吻経験 += 1
	;SOURCE設定してると、SOURCE処理でCVABM経験はプラスされるのでコメントアウト
	;EXP:Ｍ経験 += 1
	;同上
	;EXP:MASTER:Ｍ経験 += 1
	EXP:MASTER:接吻経験 += 1
	IF CHK_DATENOW(CFLAG:TARGET:约会中)
		EXP:MASTER:約會経験 ++
		EXP:TARGET:約會経験 ++
	ENDIF
	IF HETEROSEX(TARGET,PLAYER)
		EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 2
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 2
	ENDIF
	;接吻したフラグ
	SIF !TALENT:恋慕 && !TALENT:恋人 && !TALENT:愛欲 && !TALENT:炮友
		TCVAR:314 = 1440 * DAY + TIME
ENDIF
IF CFLAG:43:1011 != 1
	TCVAR:ＦＫ喪失 = 1
ENDIF

RETURN 1

