		DOWNBASE:MASTER:気力 += 50
	ELSE
		DOWNBASE:気力 += 100
		DOWNBASE:MASTER:気力 += 50
	ENDIF

	;固定で獲得するソース
	SOURCE:歓楽 = 120
	SOURCE:情愛 = 160
	SOURCE:露出 = 100
	SOURCE:反感 = 250

	;ABL:親密をみる
	IF ABL:親密 <= 1
		SOURCE:情愛 += (ABL:親密 * 30)
	ELSEIF ABL:親密 <= 3
		SOURCE:情愛 += 200 + (ABL:親密 * 30)
	ELSEIF ABL:親密 <= 5
		SOURCE:情愛 += 400 + (ABL:親密 * 30)
	ELSEIF ABL:親密 <= 8
		SOURCE:情愛 += 600 + (ABL:親密 * 40)
		SOURCE:露出 += 200 + (ABL:欲望 * 15)
	ELSEIF ABL:親密 <= 11
		SOURCE:情愛 += 800 + (ABL:親密 * 40)
		SOURCE:露出 += 300 + (ABL:欲望 * 15)
	ELSE
		SOURCE:情愛 += 1800 + (ABL:親密 * 20)
		SOURCE:露出 += 700 + (ABL:欲望 * 10)
	ENDIF

	;好感度をみる
	IF CFLAG:2 <= 1000
	ELSEIF CFLAG:2 <= 5000
		SOURCE:恭順 = CFLAG:2 / 5
	ELSEIF CFLAG:2 <= 10000
		SOURCE:恭順 = 100 + CFLAG:2 / 20
	ELSE
		SOURCE:恭順 = 350 + CFLAG:2 / 100
	ENDIF
	SIF SOURCE:恭順 < 0
		SOURCE:恭順 = 0

	LOCAL = 0
	;好感度が低いと好感度低下
	IF CFLAG:2 <= 50
		LOCAL -= 3
	ELSEIF CFLAG:2 <= 100
		LOCAL -= 2
	ELSEIF CFLAG:2 <= 250
		LOCAL -= 1
	ENDIF


	SOURCE:受動 = 120 + 240 * ABL:従順
	SOURCE:征服 = 120 + 240 * ABL:施虐属性

	;好感度変化
	TFLAG:97 += LOCAL

	IF TFLAG:193 == -1
		TIMES SOURCE:歓楽 , 0.10
		TIMES SOURCE:情愛 , 0.50
		TIMES SOURCE:征服 , 0.50
		TIMES SOURCE:受動 , 0.50
		TIMES SOURCE:露出 , 0.40
	ELSEIF TFLAG:193 == 0
		TIMES SOURCE:歓楽 , 1.00
		TIMES SOURCE:情愛 , 1.00
		TIMES SOURCE:征服 , 1.00
		TIMES SOURCE:受動 , 1.00
		TIMES SOURCE:露出 , 0.90
		TFLAG:情緒BONUS = 30
	ELSE
		TIMES SOURCE:歓楽 , 2.00
		TIMES SOURCE:情愛 , 2.00
		TIMES SOURCE:征服 , 2.00
		TIMES SOURCE:受動 , 2.00
		TIMES SOURCE:露出 , 1.80
		TFLAG:情緒BONUS = 50
	ENDIF
	TIME += 10

	IF CHK_DATENOW(CFLAG:TARGET:约会中)
		EXP:MASTER:約會経験 ++
		EXP:TARGET:約會経験 ++
	ENDIF

	IF HETEROSEX(TARGET,PLAYER)
		EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 1
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
	ENDIF
ENDIF
RETURN 1

