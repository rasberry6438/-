;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K101_140
CALL M_KOJO_MESSAGE_COM_K101_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K101_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯！？嗯～～～～」
		PRINTFORMW 朱鹭子痛苦地叼着%CALLNAME:MASTER%的阴茎。
		RETURN 1
	;基本セット
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「嗯～～～～……嗯……噗哈哈…噗」
			PRINTFORMW 朱鹭子流着眼泪看%CALLNAME:MASTER%。
		CASE 0
			PRINTFORMW 「嗯！？ ～～～～～～～っ！！！」
			PRINTFORMW 朱鹭子突然被强行插入口内而张大了眼。
		ENDSELECT
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 「啊，啊…嗯…嗯～～～～～」
		PRINTFORMW 朱鹭子虽然看起来很痛苦，但还是想办法让%CALLNAME:MASTER%心情变好。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「唔唔～～～～～」
		PRINTFORMW 「呵呵……好过分啊…」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;141,拳交
;==================================================
