				PRINTW 
				PRINTFORML 如她所愿,从后方犯入她的脸,当成屁股一般狠狠拉近
				PRINTFORML 然后一口气插入那双盈着甜美唾液的淫唇
				PRINTL
				CALL HPH_PRINT,@"「呃噢HPH 嗯哼HPH 嗯呃HPHHPH」","L"
				PRINTFORML 自主摆动腰部进行口交的感觉非常舒服,只想直奔高潮
				CALL HPH_PRINT,@"（这就是HPH 用嘴被真正交配HPH 嘴里会受孕的HPHHPH）","L"
				PRINTW
			CASE 2
				PRINTFORML 咕滋咕滋咕滋噢~~~~...
				CALL HPH_PRINT,@"「嗯HPH 嗯HPH 嗯HPH 嗯HPH 嗯HPH 嗯HPH」","L"
				CALL HPH_PRINT,@"（来了来了来了HPHHPH）","L"
				PRINTL
				PRINTFORML 啪叽!!
				CALL HPH_PRINT,@"「呃噢!?HPHHPH」","L"
				CALL HPH_PRINT,@"（来了HPH 那么猛烈的摆腰HPH 被当成洞使用HPH）","L"
				PRINTW
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
