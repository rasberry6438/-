							CALL HPH_PRINT,@"「会怀孕的说,啊啊HPH」","L"
						CASE 1
							CALL HPH_PRINT,@"「啊HPH 典的子宫HPH 变成了精液袋HPH」","L"
						CASE 2
							CALL HPH_PRINT,@"(今天没关系吧...算了,很舒服就好...HPH)","L"
						CASE 3
							CALL HPH_PRINT,@"(呵呵,我是不允许内射的,但如果生殖器内射的话,那也没办法呢HPH)","L"
						CASE 4
