;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL K139_AMANURE
	IF TALENT:139:心情 == -1 || CFLAG:139:ブチギレ == 1
		SELECTCASE TFLAG:193
		CASE 1
			PRINTFORML 「…即使在這種時候、也能泡出美味的茶啊」
			PRINTW 
		CASE 0
			PRINTFORML 「…這茶不壞呢」
		CASE -1
			PRINTFORML 「怎麼才能泡出這麼難喝的茶呢、真是不可思議啊」
			PRINTW 
		ENDSELECT
	ELSE
		SELECTCASE TFLAG:193
		CASE 1
			IF TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行
				PRINTFORML 「泡的非常不錯啊、好棒好棒」
				PRINTFORML 典輕輕地撫摸著頭
				PRINTW 
			ELSE
				PRINTFORML 「這茶非常好喝。謝謝啦」
				PRINTW 
			ENDIF
			
			CALL K139_TEA

		CASE 0
			IF TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行
				PRINTFORML 「嗯、味道和溫度都相當不錯呢」
