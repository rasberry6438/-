				CALL HPH_PRINT,@"「雜魚HPH　雜魚HPH　被區區管狐給難看的被弄到射精了呢HPHHPH」","L"
			ENDIF
		ELSE
			CALL HPH_PRINT,@"「啊HPH　再更HPH　用力點頂進來吧HPHHPH」","L"
			CALL HPH_PRINT,@"「%CALLNAME:MASTER%大人的肉棒HPH　肉棒、好喜歡～HPHHPH」","L"
		ENDIF
	ENDIF
ENDIF
