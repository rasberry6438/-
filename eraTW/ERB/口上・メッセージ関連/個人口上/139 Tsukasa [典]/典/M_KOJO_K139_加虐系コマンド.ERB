;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K139_100
CALL M_KOJO_MESSAGE_COM_K139_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:発情
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「哦嗯HPH」","L"
				CALL HPH_PRINT,@"「不要～HPH　不要打姐姐的屁股啦～HPH」","L"
				RESETCOLOR
				PRINTL 
				PRINTFORML 啪！　啪！　啪！
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「啊嗚HPH　噫嘶HPH　哎咿HPHHPH」","L"
				CALL HPH_PRINT,@"「好過分…HPH　不要欺負姐姐嘛…HPH」","L"
				PRINTL 
				RESETCOLOR
				PRINTFORML 典因發情而性感度高到極致的情況下、身體的感覺甚至強制性地將疼痛變為快樂
				PRINTFORML 屁股受到拍打形成的衝擊被感覺化為一股快感的波浪、一直傳達到子宮
				PRINTW 
			ELSE
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「唔啊～HPH　咿呀～HPH　噫～HPHHPH」","L"
				RESETCOLOR
				PRINTFORML 每當啪～啪～的聲音響起時、都會聽到典的叫聲
				PRINTW 
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「明明很痛的HPH　可是感覺卻好舒服啊～HPH　為什麼呀HPH」","L"
				RESETCOLOR
				PRINTFORML 典因發情而性感度高到極致的情況下、身體的感覺甚至強制性地將疼痛變為快樂
				PRINTFORML 屁股受到拍打形成的衝擊被感覺化為一股快感的波浪、一直傳達到子宮
				PRINTW 
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「不要～HPH　不要打了～HPH　好疼～HPH　啊咕～HPH　啊啊ーーHPH　啊啊ーーHPH」","L"
				RESETCOLOR
				PRINTW 
			ENDIF
		ELSE
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
				IF ABL:受虐属性 >= 2
					CALL HPH_PRINT,@"「ＫＯＯＯＮHPH」","W"
					PRINTFORML 「啊、剛剛那個…不是那樣的… 姐姐、才沒有很舒服哦…」
					PRINTW 
					PRINTFORML %CALLNAME:MASTER%無言地將手揮下
					CALL HPH_PRINT,@"「咿噫咿咿HPH」","W"
					CALL HPH_PRINT,@"「不、不要HPH　很痛啦、停下、停下來、嘻咿嗯HPH」","L"
					PRINTW 
				ELSE
					PRINTFORML 「Kon！！？」
					PRINTFORML 「等、等一下…打屁股是怎…ＫＯＯＯＮ！！」
					PRINTFORML 「姐姐、會生氣、的喔！」
					PRINTFORML 「好痛、很痛啊、不要再打了啦、啊！」
					PRINTW 
				ENDIF
			ELSE
				IF ABL:受虐属性 >= 2
					CALL HPH_PRINT,@"「ＫＯＯＯＮHPH」","W"
					PRINTFORML 「啊、感覺不一樣了、變得舒服了、怎麼會ＫＯＯＯＮ！！？」
					PRINTW 
					PRINTFORML 兩次、三次的拍在典的屁股上、紅暈一次次浮現在那圓滾滾的臀部上
					CALL HPH_PRINT,@"「不行～HPH　不可以在打屁股了～HPH　好痛的不行呀～HPH　ＫＯＯＯＮ！！」","L"
					PRINTW 
				ELSE
					PRINTFORML 「Kon！！？」
					IF GROUPMATCH(TALENT:MASTER:体型,2,3)
						PRINTFORML （好痛好痛好痛！！？　認真的好痛啊！！！）
						PRINTW 
						PRINTFORML 啪啪啪啪啪啪！
						PRINTFORML 「噫噫！！？」
						PRINTW 
						PRINTFORML 「啊、啊…%CALLNAME:MASTER%大人、拜託停下來吧…」
						PRINTFORML 典的眼角溢出了淚水、全身都在顫抖著真的很害怕
						PRINTW 
					ELSE
						PRINTFORML 「等、等一下…打屁股是怎…ＫＯＯＯＮ！！」
						PRINTFORML 「好痛、拜託停下來吧、不要、ＫＯＯＯＮ！」
						PRINTW 
					ENDIF
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
			IF ABL:受虐属性 >= 2
				SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「ＫＯＮHPH　再來HPH」","L"
					CALL HPH_PRINT,@"「姐姐…被打太多次、好像覺醒什麼了呢…HPH」","L"
					PRINTW 
				CASE 1
					CALL HPH_PRINT,@"「ＫＯＯＮHPH」","L"
					PRINTFORML 「喂、喂、不應該打姐姐的屁股…才對吧…」
					PRINTL 
					PRINTFORML 啪！
					CALL HPH_PRINT,@"「ＫＯＯＯＮHPH」","L"
					PRINTL 
					PRINTFORML 「哦、姐姐的…」
					PRINTL 
					PRINTFORML 啪！
					CALL HPH_PRINT,@"「屁股HPH　呀HPH」","L"
					PRINTL 
					PRINTFORML 啪！
					CALL HPH_PRINT,@"「別帶這樣玩的啊HPH　呀啊HPH」","L"
					PRINTW 
				ENDSELECT
			ELSE
				PRINTFORML 啪！
				SELECTCASE RAND:6
				CASE 0
					PRINTFORML 「姐姐、很痛啊！　真的、很痛啊」
				CASE 1
					PRINTFORML 「現在我會原諒你的、所以停下來吧、ＫＯＮ！」
				CASE 2
					PRINTFORML 「喂、喂、不要把姐姐的屁股當太鼓打啊！」
				CASE 3
					PRINTFORML 「啊、之後你就曉得了、ＫＯＮ！」
				CASE 4
					PRINTFORML 「再、再這樣下去我真的會生氣哦！」
				CASE 5
					IF (TALENT:MASTER:応答 == 1 || TALENT:MASTER:態度 == 1)
						PRINTFORML 「你、你這可惡的小鬼！　之後就給我記住了！　ＫＯＮ！」
					ELSE
						PRINTFORML 「這、這到底是怎麼了啊！？　你不是會做這種事的孩子啊！？　ＫＯＮ！」
					ENDIF
				ENDSELECT
				PRINTW 
			ENDIF
		ELSE
			IF ABL:受虐属性 >= 2
				CALL HPH_PRINT,@"「ＫＯＯＮHPH」","L"
				PRINTFORML 「啊、剛剛那個、不是的…並不是因為很舒服…」
				PRINTL 
				PRINTFORML 啪！
				CALL HPH_PRINT,@"「ＫＯＯＯＮHPH」","L"
				PRINTFORML 明顯不同於痛苦的音色在悲鳴中交織
				PRINTFORML 而且更重要的是、從秘處流出的愛液量正在增加這一點、已經無言地說明了一切
				PRINTW 
			ELSE
				PRINTFORML 啪！
				PRINTFORML 「ＫＯＯＯＮ！？」
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「對我做這種事、噫！？　好、好痛！」
					PRINTW 
				CASE 1
					PRINTFORML 「啊咕嗚！　就、就說好痛了啊…」
					PRINTW 
				CASE 2
					PRINTFORML 「好、好痛！　好痛！！　求求你停下來吧！」
					PRINTW 
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF
RETURN 1

