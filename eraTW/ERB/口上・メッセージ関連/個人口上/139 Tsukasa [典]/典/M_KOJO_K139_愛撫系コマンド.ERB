					CALL HPH_PRINT,@"「%CALLNAME:MASTER%大人是那種會因為陰毛從內衣外露而感到興奮的人嗎…HPH」","L"
				ENDSELECT
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
