								CALL HPH_PRINT,@"「噫、嘿、誒嘿嘿…這…這麼的短小ー…HPH　連手指都不如的肉棒…HPH」","L"
								PRINTW 
								CALL HPH_PRINT,@"「呀～HPH　呀～HPH　呀～HPH　啊～HPH　才沒有到呢HPH　根本沒到這麼裡面～HPHHPH」","L"
								CALL HPH_PRINT,@"「頂到的只不過是陰道皺褶而已啦～HPH　才不是什麼子宮口啦～HPHHPH　哦哦哦～HPHHPH」","L"
								PRINTW 
							ENDSELECT
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		;気持ちよくなってくる
		CASE IS > 150
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
				IF TEQUIP:子宮
					SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「你看、已經完美匹配在一起了呢HPH」","L"
						CALL HPH_PRINT,@"「姐姐子宮口的形狀、%CALLNAME:MASTER%大人龜頭的形狀HPH」","L"
						PRINTW 
						CALL HPH_PRINT,@"「毫無疑問的%CALLNAME:MASTER%大人的雞雞、就是為了回到姐姐陰道內而存在的喔HPH」","L"
						CALL HPH_PRINT,@"「來ー吧、趕快回來吧HPH　啾HPH　啾HPH」","L"
						CALL HPH_PRINT,@"「嘸嘸HPH　被子宮口吸住了的雞雞在一抖一抖的呢るHPH　好可愛HPH」","L"
						PRINTW 
					CASE 1
						CALL HPH_PRINT,@"「你看、感覺到了吧HPH」","L"
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%大人的雞雞、正被姐姐的子宮口吸吮著呢HPH」","L"
						PRINTW 
						CALL HPH_PRINT,@"「給我精子HPH　給我精子HPH　的、如此懇求著哦～HPH」","L"
						CALL HPH_PRINT,@"「所以請在姐姐的最深處將精液噗～噗～地射進去吧HPH」","L"
						PRINTW 
					ENDSELECT
				ELSE
					CALL HPH_PRINT,@"「真是的HPH　我可不記得我有教過你這孩子玩弄子宮口的哦HPH」","L"
					CALL HPH_PRINT,@"「對於如此壞壞的雞雞、必須用姐姐的陰道一直抓著不放才行呢HPH」","L"
					CALL HPH_PRINT,@"「你看、緊緊地ーHPH　姐姐的小穴、就要將你給抓住了哦HPH」","L"
					PRINTW 
				ENDIF
			ELSE
				IF TEQUIP:子宮
					CALL HPH_PRINT,@"「啊～～HPH　啊啊啊～～～HPH　啊～HPH　啊～HPH　啊～HPH」","L"
					CALL HPH_PRINT,@"（這樣子…HPH　將龜頭緊貼著子宮口溫柔地戳動著、下半身都快要融化了HPH）","L"
					CALL HPH_PRINT,@"（正是因為沒有粗暴的對待、我的子宮才會永遠認%CALLNAME:MASTER%大人為主人的HPH）","L"
					PRINTW 
				ELSE
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%大人、感覺到了嗎HPH」","L"
					CALL HPH_PRINT,@"「子宮口正緊緊吸吮著您的龜頭呢HPH」","L"
					PRINTW 
					CALL HPH_PRINT,@"「多半直到%CALLNAME:MASTER%大人射精之前、它都不會放開呢HPH」","L"
					CALL HPH_PRINT,@"「該怎麼辦呢？HPH　該做什麼呢？HPH　%CALLNAME:MASTER%大人HPH」","L"
					PRINTW 
				ENDIF
			ENDIF
		;苦痛のソース半減。開発完了（地の描写）
		CASE IS > 50
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
				CALL HPH_PRINT,@"「瞧啊HPH　你的雞雞正在與姐姐的子宮親親哦～HPH」","L"
				CALL HPH_PRINT,@"「感覺到了嗎HPH　它一直在催促著%CALLNAME:MASTER%大人讓自己懷孕呢HPH」","L"
				PRINTW 
			ELSE
				CALL HPH_PRINT,@"「都是因為%CALLNAME:MASTER%大人一再地刺激子宮口HPH　這樣要我怎麼體會不到啊～HPH」","L"
				CALL HPH_PRINT,@"「繼續HPH　咕哩咕哩的HPH　猛力的進攻過來吧HPHHPH」","L"
				PRINTW 
			ENDIF
		;快Ｖ半減、25以下なら苦悶、25超でこなれてくる
		CASEELSE
			CALL HPH_PRINT,@"「唔、咕嗚嗚HPH」","L"
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
				SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「感、感覺得出來嗎…HPH　這裡就是子宮口哦…就是小寶寶的房間入口哦…HPH」","L"
				CASE 1
