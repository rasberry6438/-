				CASE 0
					CALL HPH_PRINT,@"「啊啊、進到這麼深的地方…HPH」","L"
				CASE 1
					CALL HPH_PRINT,@"「啊啊啊…呃、好深呀…HPH」","L"
				ENDSELECT
			ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
