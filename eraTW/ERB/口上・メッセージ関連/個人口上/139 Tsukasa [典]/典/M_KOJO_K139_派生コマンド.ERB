								CALL HPH_PRINT,@"（進、進去了呀HPH　子宮、被龜頭侵入了呀HPHHPH）","L"
								CALL HPH_PRINT,@"（子宮HPH　變得笨蛋過頭太舒服了～～HPHHPH）","L"
