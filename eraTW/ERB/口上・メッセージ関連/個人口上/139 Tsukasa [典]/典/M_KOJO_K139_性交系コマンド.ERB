CASE 501 TO 1500
	IF ABL:Ｖ感覚 >= 5
		SELECTCASE RAND:2
		CASE 0
			CALL HPH_PRINT,@"「嗯…HPH　啊…HPH　啊啊嗯…HPH」","L"
		CASE 1
			CALL HPH_PRINT,@"「呼唔HPH　呼唔HPH　呼ーーーHPH　呼ーーーHPH」","L"
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「啊……　嗯……啊、哈啊…」
		CASE 1
			PRINTFORML 「呼呼嗯、嗯、嗯……」
		ENDSELECT
	ENDIF
;睡眠深度: 眠っている～すやすやと眠っている
CASE 1501 TO 5000
	IF ABL:Ｖ感覚 >= 5
		CALL HPH_PRINT,@"「啊…HPH　嗯…HPH」","L"
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「啊……　嗯……」
		CASE 1
			PRINTFORML 「嗯、嗯……」
			CALL HPH_PRINT,@"「嗯、嗯…」","L"
		ENDSELECT
	ENDIF
;睡眠深度: ぐっすりと眠っている
CASE IS > 5000
	IF ABL:Ｖ感覚 >= 5
