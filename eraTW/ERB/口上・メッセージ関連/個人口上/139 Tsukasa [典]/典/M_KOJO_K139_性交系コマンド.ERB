CASE 0 TO 500
	IF ABL:Ｖ感覚 >= 5
		SELECTCASE RAND:4
		CASE 0
			CALL HPH_PRINT,@"「啊゛～…HPH　哈啊…HPH　嗯…？」","L"
		CASE 1
			CALL HPH_PRINT,@"「嗯HPH　啊HPH　啊゛啊～…HPH」","L"
		CASE 2
			CALL HPH_PRINT,@"「嗯゛哦゛～HPH　嗯゛哦゛～HPH　哦゛哦゛～…HPH」","L"
		CASE 3
			CALL HPH_PRINT,@"「哦哦゛～HPH　哦哦゛～HPH　嗯゛哦哦゛～HPH」","L"
		CASE 4
			CALL HPH_PRINT,@"「啊゛～HPH　啊゛～HPH　啊゛～HPH　啊゛～HPH」","L"
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「哈啊、哈啊、哈啊…」
		CASE 1
			CALL HPH_PRINT,@"「嗯、嗯、嗯、嗯HPH」","L"
		ENDSELECT
	ENDIF
