	IF TALENT:恋慕
		CALL HPH_PRINT("「呼、唔、嗯…嗯……HPH」","L")
		PRINTFORMW 咲夜接受了阴道里的拳头。似乎多多少少觉醒了一些快乐……
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		CALL HPH_PRINT("「弄坏我HPH 再多一点HPH 把我弄得乱七八糟HPH」","L")
		PRINTFORMW 咲夜的阴道被拳头持续突入、喉咙发出宛如野兽般的喘息……
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「嘎啊啊啊啊啊啊啊！！？」
		PRINTFORMW 咲夜的阴道遭到拳头的突入、难以想象的苦痛让背部极大限度的反仰，狂乱的发丝四散飞舞、如野兽一般尖叫着……
		RETURN 1
	ENDIF
ENDIF
RETURN 1

