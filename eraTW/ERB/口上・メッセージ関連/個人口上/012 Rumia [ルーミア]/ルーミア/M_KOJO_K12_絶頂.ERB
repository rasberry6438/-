					CASE 2
						CALL HPH_PRINT("「一喷一喷…的HPH　欸嘿嘿HPH」","W")
					CASE 3
						CALL HPH_PRINT("「还想要更多…HPH　看…还在不断往外流HPH」","W")
					CASEELSE
						CALL HPH_PRINT("「爽的快要飞了…HPH　还想要做更多…HPH」","W")
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						CALL HPH_PRINT("「讨厌…胸部都停不下来了…HPH」","W")
					CASE 2
						CALL HPH_PRINT("「哈啊啊…HPH　奶水出来了…」","W")
					CASEELSE
						CALL HPH_PRINT("「又、又出来了…HPH」","W")
				ENDSELECT
			ENDIF
		;ELSEIF TALENT:思慕
		
		ELSE
			SELECTCASE RAND:3
				CASE 1
					CALL HPH_PRINT("「讨厌…停不下来了…HPH」","W")
				CASE 2
					CALL HPH_PRINT("「胸部好痒…嗯、嗯嗯HPH」","W")
