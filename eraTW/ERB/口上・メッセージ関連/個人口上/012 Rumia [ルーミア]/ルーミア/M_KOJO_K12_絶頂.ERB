					CASEELSE
						CALL HPH_PRINT("「呜呜…忍不住了嘛HPH」","W")
				ENDSELECT
			ENDIF
		ELSE
			;IF TALENT:恋慕
			;	SELECTCASE RAND:5
			;		CASE 1
			;			PRINTFORMW 「」
			;		CASE 2
			;			PRINTFORMW 「」
			;		CASE 3
			;			PRINTFORMW 「」
			;		CASE 4
			;			PRINTFORMW 「」
			;		CASEELSE
			;			PRINTFORMW 「」
			;	ENDSELECT
			IF TALENT:思慕
				SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「不好吃对吧？不用勉强哦ー」
					CASE 2
						PRINTFORMW 「射到嘴里了…抱歉」
					CASEELSE
						PRINTFORMW 「哈啊…肉棒、好厉害…」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「那个、不好吃对吧ー？」
					CASE 2
						PRINTFORMW 「让我做到射精为止什么的…真是恶趣味」
					CASEELSE
						PRINTFORMW 「居然喝精液什么的…变态」
				ENDSELECT
			ENDIF
		ENDIF
	ELSEIF TCVAR:2 == 1
	;PRINTFORMW 阴道
	
	ELSE
	;PRINTFORMW 其它
	
	ENDIF
	
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
;合計絶頂数で分岐
;これだけではどの部位が絶頂したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			PRINTFORMW 
		;中
		CASE 2 TO 4
			PRINTFORMW 
		;弱
		CASE 1
			PRINTFORMW 
	ENDSELECT
ENDIF

