IF TALENT:12:3 && ( CFLAG:12:300 == CFLAG:0:300 ) && SHIRAHU(12) 
	CALL M_KOJO_Moe_MESSAGE_COM_K12_406_2 
	CALL RUMIA_Moe_K12_COM406
	CALL M_KOJO_Moe_MESSAGE_COM_K12_406_1
	TARGET = 0 
	CALL DOKU_COMF_FAIL_FOR_Moe_K12
ELSEIF  ( CFLAG:12:300 == CFLAG:0:300 ) && SHIRAHU(12)
	CALL M_KOJO_Moe_MESSAGE_COM_K12_406_2 
	RETURN RESULT
	TARGET = 0 
	CALL DOKU_COMF_FAIL_FOR_Moe_K12
ELSE 
	CALL RUMIA_Moe_K12_COM406
	TARGET = 0 
	CALL DOKU_COMF_FAIL_FOR_Moe_K12
ENDIF 
