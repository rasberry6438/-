;=============================
;日記発生処理の一括管理関数
;ARG	;日记的编号
;ARG:1	为1时设置为大喇叭，
;ARG:2	为正值时设置为不可重复触发（暂时无用）
;ARG:3	为返回至对应页数的判别参数
;=============================
@M_KOJO_Moe_DIARYSETTING_K12(ARG,ARG:1=0,ARG:2=0,ARG:3=0)
	;日记大喇叭变量设置
	IF ARG:1
		RUMIA_DIARY:12:ARG = 2
	ELSE 
		RUMIA_DIARY:12:ARG = 3
	ENDIF 
	;避免日常系的重复日记
	SIF !CFLAG:0:313	;如果自机不在睡觉，说明是事件触发的
		CFLAG:12:1770 = 1 
	CALL K12_DIARY_PAGESETTING(12,ARG,ARG:3)		
;===========================================================================
;⑩⑩⑩⑩⑩⑩⑩⑩⑩⑩⑩⑩
; 日记页数设置的函数
; 将最新的日记页数指定为 CHARA 的日记编号 （NO_DIARY）
;⑩⑩⑩⑩⑩⑩⑩⑩⑩⑩⑩⑩
;===========================================================================
@K12_DIARY_PAGESETTING(CHARA, NO_DIARY,SETTING_NUMBER)
#DIM CHARA
#DIM NO_DIARY
#DIM SETTING_NUMBER
;RUMIA_PAGESET
;RUMIA_MAX_DIARY_PAGE
;均为自定义变量，最大数值没有上限
FOR LOCAL, 1, RUMIA_MAX_DIARY_PAGE:CHARA:0 + 1
	;这里是页数的区分……
	IF !RUMIA_PAGESET:CHARA:LOCAL
		;检测多少页是第多少号日记并记录
		RUMIA_PAGESET:CHARA:LOCAL = NO_DIARY
		;日记日期和日记页数绑定
		RUMIA_DIARY_DATE:CHARA:LOCAL = DAY 
		;在设置日记页数的时候，就记录好日记的差分格式……
		IF SETTING_NUMBER != 0	;此时有自定义参数
			RUMIA_DIARY_SETTING:CHARA:LOCAL = SETTING_NUMBER
			SETTING_NUMBER = 0 
		ELSE 	;此时需要日记设置自定义参数
			TRYCALLFORM DIARY_Moe_K{CHARA}_{NO_DIARY}_EDIT
			SIF RESULT
				RUMIA_DIARY_SETTING:CHARA:LOCAL = RESULT 
			RETURN LOCAL 
		ENDIF 
	ENDIF
NEXT
;===========================================================================
;日記计数用函数
;可以返回目前日记的页数，括号内填角色代号，只有露米娅版日记才会触发
;===========================================================================
@RUMIA_SEARCH_DIARY(ARG)
#FUNCTION
LOCAL = 0 
FOR LOCAL , 1 , RUMIA_MAX_DIARY_PAGE:ARG:0 + 1
	;检测到首页空页即可
	SIF !RUMIA_PAGESET:LOCAL
		BREAK
NEXT 
LOCAL -= 1 
RETURNF LOCAL 
