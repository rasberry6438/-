@M_KOJO_MESSAGE_COM_K11_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;ここに口上書いて下さい
	;霧雨魔理沙の幻想郷探訪？
	IF CHK_DATENOW(CFLAG:TARGET:约会中)
	SELECTCASE CFLAG:11:現在位置
		;道中
		CASE ROAD_TO(CFLAG:MASTER:约会中)
			IF TALENT:恋慕
				SELECTCASE RAND:10
				CASE 0,1,2,3,4
					PRINTFORML 「和%CALLNAME:MASTER%的约会…嘿嘿，好期待哦♪」
					PRINTFORMW 魔理沙带着耀眼的笑顔望着这边。
				CASE 5,6,7
					PRINTFORMW 「喂、%CALLNAME:MASTER%、不快点的话就丢下你咯？」
				CASE 8,9
					PRINTFORML 「对了，到了的话做什么呢？」
					PRINTFORMW 「我和%CALLNAME:MASTER%在一起的话什么都可以哦♪」
				ENDSELECT
			ELSE
				SELECTCASE RAND:10
				CASE 0,1,2,3
					PRINTFORML 「和%CALLNAME:MASTER%在约会啊…」
					PRINTFORMW 「嘛，请尽情享受吧」
				CASE 4,5,6,7
					PRINTFORML 「哎呀，走得真快啊」
					PRINTFORMW 「不是没有什么着急的事吗？」
				CASE 8,9
					PRINTFORML 「对了，到了的话要做什么？」
					PRINTFORMW 「%CALLNAME:MASTER%就交给你了」
				ENDSELECT
			ENDIF
		;博麗神社…これでいいの？
		CASE 10 TO 90
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「还是那么热闹啊，明明这里全是妖怪…」
				PRINTFORML 「啊，虽说如此，也不是妖怪以外的人都不喜欢吧」
				PRINTFORML 「无论是人类还是妖怪，除此之外连妖精和神都能吸引，这就是灵梦的魅力所在」
				PRINTFORMW 「我觉得她真是个不可思议的家伙」
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%、好不容易来了不去参拜一下吗？」
				PRINTFORML 「嗯，我不知道有什么好处……也许会有什么好事发生哦？」
				PRINTFORMW 「啊对了，确实发生了好事，那就是灵梦的心情变好了」
			CASE 2
				PRINTFORML 「神社后面的水楢树是光之三妖精的住处、很亲近灵梦」
				PRINTFORML 「之前因为想被灵梦喜欢而吵架、果然挨了一顿毒打。」
				PRINTFORML 「「但是不知什么时候灵梦变得驯服了。」」
				PRINTFORMW 「你果然认为做得太过火了吗？灵梦就是那样，也有相当温柔的地方啊」
			CASE 3
				PRINTFORML 「我曾经在这里的结界打開的时候出过外面的世界……好厉害啊」
				PRINTFORMW 「到处都是闪闪发光的呢，虽然想再看一遍，但是不行吧」
			CASE 4
				PRINTFORML 「这么说来外面的世界不是有神前婚礼吗？」
				PRINTFORML 「要办的话，灵梦会主持婚礼吗……能做到吗？稍微想了一下那个画面」
