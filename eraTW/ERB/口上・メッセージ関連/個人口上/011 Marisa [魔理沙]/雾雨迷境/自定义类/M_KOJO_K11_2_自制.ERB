@Music_INSTRUMENT(ARG, ARG:1)
#FUNCTIONS
#DIMS INSTRUMENT


SELECTCASE TFLAG:使用楽器
    CASE 1
        INSTRUMENT = 电子琴
    CASE 2
        INSTRUMENT = 钢琴
    CASE 3
        INSTRUMENT = 吉他
    CASE 4
        INSTRUMENT = 小号
    CASE 5
        INSTRUMENT = 小提琴
ENDSELECT

RETURNF INSTRUMENT

