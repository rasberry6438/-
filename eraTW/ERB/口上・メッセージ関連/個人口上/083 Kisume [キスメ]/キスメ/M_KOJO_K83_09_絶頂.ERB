;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな変数などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:Ｃ絶頂 NOWEX:Ｖ絶頂 NOWEX:Ｂ絶頂 NOWEX:Ａ絶頂 NOWEX:Ｍ絶頂
;各部位の絶頂強度。まず0~3が割り当てられ、多重絶頂により最大16倍される。つまり単部位なら最大3、5重絶頂で最大48
;NOWEX:TotalEX
;多重絶頂補正を接受前の各部位絶頂強度の合計値。1~15。この貂プレにおける分岐の基準。

;TEQUIP:50
;Ｖに誰の竿が入ってるか。TEQUIP:50 = MASTERで你が挿入中となる。
;TEQUIP:51
;Ａに誰の竿が入ってるか。TEQUIP:51 = MASTERで你が挿入中となる。
;NOWEX:射精
;射精量、最大3。たとえばIF NOWEX:MASTER:射精 > 1だと、你が大量射精以上した場合となる。
;SYNCED_ORGASM(TARGET)
;相手と你が同時に絶頂した場合真となる。絶頂の部位は問わない。

;-------------------------------------------------
;絶頂時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K83
#DIM TotalEX
SIF FLAG:時間停止
	RETURN 1
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	PRINTFORMW 「讨、讨厌……\/\/\/ 胸部要……%M_KOJO_K83_AUTO("単刺激…！")%」
ENDIF
;;-------------------------------------------------
;;射精した
;;記入チェック（=0, 非表示、1, 表示）
;LOCAL = 0
;;-------------------------------------------------
;IF LOCAL && NOWEX:射精
;	PRINTFORMW 
;ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 「呜诶……要尿出来了……！」
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;合計絶頂数で分岐
;これだけではどの部位が絶頂したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	IF M_KOJO_K83_UFUFU([[琪斯美]])
		SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			SELECTCASE RAND:3
			CASE 1 
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")%、不行呀、%M_KOJO_K83_AUTO("単刺激")%！ 要去了要去了HPH 啊啊啊啊啊HPHHPH」", "W")
			CASE 2
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")%、要来了、%M_KOJO_K83_AUTO("単刺激")%！ 唔啊HPH 要去了～～HPHHPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")%、咿呀、%M_KOJO_K83_AUTO("単刺激")%！ 要去了～っHPH 对不起～～HPHHPH」", "W")
			ENDSELECT
		;中
		CASE 2 TO 4
			PRINTFORM 「%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("単刺激")% 
			SELECTCASE RAND:3
			CASE 1 
				CALL HPH_PRINT(@"要去了！ 去了HPH」", "W")
			CASE 2
				CALL HPH_PRINT(@"不、不行了！ 要去了HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"要去了！ 唔啊啊HPH」", "W")
			ENDSELECT
		;弱
		CASE 1
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 要去了…！」
			CASE 2
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 不、不行…！」
			CASEELSE
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 去、去了…！」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("単刺激")% 去了呀！」
		;中
		CASE 2 TO 4
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 去了！」
		;弱
		CASE 1
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激…！")% 去了…！」
		ENDSELECT
	ENDIF
ENDIF
