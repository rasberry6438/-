@M_KOJO_MESSAGE_COM_K83_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[琪斯美]])
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:時間停止
		;未成熟,粗チン
		IF TALENT:MASTER:形状 == 1 || TALENT:MASTER:形状 == 2
			PRINTFORMW 「要握着摩擦吗……？」
		ELSE
			PRINTFORMW 「好、好大……。要握着摩擦吗……？」
		ENDIF
		PRINTFORMW 「是、这样吗……？ 怎么样……？」
		RETURN 1
	ENDIF
	IF FLAG:時間停止
	;要去了
	ELSEIF NOWEX:MASTER:射精
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「涨的非常大……好像要射了呢……」
			CALL HPH_PRINT(@"「可以哟……噗噜地射出一大堆吧……HPH」", "W")
		CASEELSE
			PRINTFORML 「要射了吗？ 那、我要加快速度了……」
			CALL HPH_PRINT(@"「可以哟……噗噜地射出一大堆吧……HPH」", "W")
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
		CASE 1
			PRINTFORML 「感、感觉怎么样……？」
			PRINTFORMW 「我会让你舒服起来的……」
		CASE 2
			PRINTFORML 「那我来摸摸你吧……」
			PRINTFORMW 「呼呼……真可爱……」
		CASE 3
			PRINTFORML 「我做得很好吗？ 真的吗？」
			PRINTFORMW 「太好了……。我会更加努力的……」
		CASE 4
			PRINTFORML 「摩擦摩擦、感觉舒服吗……？」
			PRINTFORMW 「可以哟、更多地舒服起来吧……」
		CASEELSE
			;未成熟,粗チン
			IF TALENT:MASTER:形状 == 1 || TALENT:MASTER:形状 == 2
				PRINTFORML 「%CALLNAME:MASTER%的鸡鸡……小小的好可爱……」
				PRINTFORMW 「没关系的、这种尺寸对我的身体来说正合适哟……」
			;発光,长疣的
			ELSEIF TALENT:MASTER:形状 == 4 || TALENT:MASTER:形状 == 5
				PRINTFORML 「%CALLNAME:MASTER%的鸡鸡……那个……」
				PRINTFORMW 「感、感觉还是好奇怪……。果然……」
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%的鸡鸡……真大呀……」
				PRINTFORMW 「要把这么大的……放到我的里面……咕嘟」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

