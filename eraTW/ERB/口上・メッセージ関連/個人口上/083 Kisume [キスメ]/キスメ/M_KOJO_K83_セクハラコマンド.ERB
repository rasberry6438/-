;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[琪斯美]])
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	IF NOWEX:MASTER:射精
		CALL M_KOJO_K83_NAKADASHI
	ELSEIF ABL:[[琪斯美]]:Ｖ感覚 < 2
		PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")%太、太激烈了……！」
	ELSEIF ABL:[[琪斯美]]:Ｖ感覚 < 5
		PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")%唔、嗯咕、这、这样、摇动的话……不、不行了！」
	ELSE
		CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("二連刺激")%啊HPH」", "L")
		CALL HPH_PRINT(@"「不行HPH 这么激烈的话HPH 啊嗯HPH 要去了HPH」", "W")
	ENDIF
ENDIF
RETURN 1
