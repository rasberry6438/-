;ABL:Ｖ感覚(1未満=違和感に戸惑う 5未満=稍微気持ちいい 5以上＝カイカン)
;TFLAG:400(1=约会先or地底の路人が反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K83_316
CALL M_KOJO_MESSAGE_COM_K83_316_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_316_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	;時間停止中
	IF FLAG:時間停止
	;被逆推中
	ELSEIF CFLAG:[[琪斯美]]:诶嘿嘿 == 2
		SIF !M_KOJO_K83_UFUFU([[琪斯美]])
			RETURN 1
		SELECTCASE TCVAR:[[琪斯美]]:Counter行動
		;裙子の中を見せる
		CASE 35
			PRINTFORMW 「啊啊啊！咿！ 好舒服……！」
		;強制舐陰
		CASE 70
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 可以哟……更多地欺负我吧……！」
		;それ以外
		CASEELSE
			PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")%嗯唔、更多……！」
		ENDSELECT
	;酔ってる
	ELSEIF M_KOJO_K83_YOIDORE([[琪斯美]])
		CALL M_KOJO_K83_TALKSET("酔酒")
	;心情不快
	ELSEIF M_KOJO_K83_FUKIGEN([[琪斯美]])
		IF TALENT:[[琪斯美]]:恋慕
			PRINTFORMW 「别……别这样啦……」
		ELSE
			PRINTFORMW 「呜～……」
		ENDIF
	;通常
	ELSE
		IF TALENT:[[琪斯美]]:恋慕
			PRINTFORM 「%M_KOJO_K83_AUTO("単刺激")% 
			SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 那……那里是……」
			CASE 2
				PRINTFORMW 呼——…呼——！」
			CASE 3
				PRINTFORMW 哈啊、哈啊……！」
			CASE 4
				PRINTFORMW 变、变态……！」
			CASEELSE
				PRINTFORMW 为、为什么啊……」
			ENDSELECT
		ELSEIF TALENT:[[琪斯美]]:思慕
			PRINTFORM 「%M_KOJO_K83_AUTO("単刺激")%
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 不、不行……」
			CASE 2
				PRINTFORMW 不、不要……」
			CASEELSE
				PRINTFORMW 讨、讨厌……」
			ENDSELECT
		ELSE
			PRINTFORML 「%M_KOJO_K83_AUTO("悲鳴：強")%」
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「不要啊～！」
			CASEELSE
				PRINTFORMW 「住手……！！」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
