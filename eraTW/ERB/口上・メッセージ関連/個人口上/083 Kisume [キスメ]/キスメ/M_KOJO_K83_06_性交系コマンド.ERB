		SELECTCASE RAND:5
		CASE 1
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% 来了、精液要来了！ 会不会怀上小宝宝呢HPH」", "W")
		CASE 2
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% 好喜欢%CALLNAME:MASTER%HPH 射出来吧HPH 从我的最深处射出来吧HPH」", "W")
		CASE 3
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% %CALLNAME:MASTER%、%CALLNAME:MASTER%～HPH 要射了吗HPH」", "W")
		CASE 4
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% 要来了…！ %CALLNAME:MASTER%、全部射进来吧HPH」", "W")
		CASEELSE
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% 可以哟HPH 射进子宫里吧HPH」", "W")
		ENDSELECT
	ELSE
		PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 好、好难受…！ %M_KOJO_K83_AUTO("単刺激")%」
	ENDIF
ENDIF
