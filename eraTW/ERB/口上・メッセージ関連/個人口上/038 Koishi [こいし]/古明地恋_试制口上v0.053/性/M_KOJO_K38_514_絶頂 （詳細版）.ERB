			;Ａ＆Ｂ絶頂
			CASE 6
				PRINTFORML 
				RETURN 1
			;Ａ＆Ｍ絶頂
			CASE 7
				PRINTFORML 
				RETURN 1
			;Ｖ＆Ｍ絶頂
			CASE 8
				PRINTFORML 
				RETURN 1
			;Ｃ＆Ｍ絶頂
			CASE 9
				PRINTFORML 
				RETURN 1
			;Ｂ＆Ｍ絶頂
			CASE 10
				PRINTFORML 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ絶頂
		;超強絶頂Ｃ
		IF NOWEX:Ｃ絶頂 == 3
			PRINTFORML 「哇啊……意识……要飘走了……%UNICODE(0x2764)%」
		;強絶頂Ｃ
		ELSEIF NOWEX:Ｃ絶頂 == 2
			PRINTFORML 「啊啊啊%UNICODE(0x2764)%\@GETBIT(TALENT:性別,1)?肉棒#秘豆\@那里，酥酥麻麻的%UNICODE(0x2764)%」
		;絶頂Ｃ
		ELSE
			PRINTFORML 「呜哇…好厉害%UNICODE(0x2764)%」
		ENDIF
	ENDIF
	IF NOWEX:Ｖ絶頂
		;超強絶頂Ｖ
		IF NOWEX:Ｖ絶頂 == 3
			PRINTFORML 
		;強絶頂Ｖ
		ELSEIF NOWEX:Ｖ絶頂 == 2
			PRINTFORML 
		;絶頂Ｖ
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
	IF NOWEX:Ａ絶頂
		;超強絶頂Ａ
		IF NOWEX:Ａ絶頂 == 3
			PRINTFORML 
		;強絶頂Ａ
		ELSEIF NOWEX:Ａ絶頂 == 2
			PRINTFORML 
		;絶頂Ａ
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ絶頂
		;強絶頂Ｂ
		IF NOWEX:Ｂ絶頂 == 3
			PRINTFORML 
		;強絶頂Ｂ
		ELSEIF NOWEX:Ｂ絶頂 == 2
			PRINTFORML 
		;絶頂Ｂ
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
	IF NOWEX:Ｍ絶頂
		;超強絶頂Ｍ
		IF NOWEX:Ｍ絶頂 == 3
			PRINTFORML 
		;強絶頂Ｍ
		ELSEIF NOWEX:Ｍ絶頂 == 2
			PRINTFORML 
		;絶頂Ｍ
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
ENDIF
