;==================================================
;依頼口上・一般依頼04：想要学習料理
;==================================================
@M_KOJO_IRAI_4_K38(ROLE, SCENE, CLIENT_ID)
#DIMS ROLE     ;角色の立場、依頼者かそうでないか
#DIMS SCENE    ;セリフの表示されるタイミング
#DIM  CLIENT_ID;依頼者IDだけ抜き出したもの
;--------------------------------------------------
;RETURN 0 の部分は共通の口上が表示されます
;依頼ごとの口上を表示する部分は RETURN 0 を RETURN 1 にしてください
;--------------------------------------------------
SELECTCASE SCENE
CASE "依頼提示時"
	SELECTCASE RAND:3
	CASE 1
		PRINTFORMW 「这次我想给姐姐做一次菜呢♪」
	CASE 2
		PRINTFORMW 「拜托了%CALLNAME:MASTER%，想要让你教我怎么做料理啊♪」
	CASEELSE
		PRINTFORMW 「我也想学会怎么做饭啊」
	ENDSELECT
	RETURN 1
CASE "依頼非受託時"
	
	RETURN 0
CASE "依頼受託時"
	SELECTCASE RAND:3
	CASE 1
		PRINTFORMW 「嘿嘿～♪ 学料理♪ 做料理～♪ 好期待～♪」
	CASE 2
		PRINTFORMW 「太好了♪」
	CASEELSE
		PRINTFORMW 「真的吗？ 嘿嘿～、真開心啊～♪」
	ENDSELECT
	RETURN 1
CASE "依頼確認時"
	
	RETURN 0
CASE "依頼破棄時"
	
	RETURN 0
CASE "依頼実行直前"
;○この依頼は依頼主がTARGETです
;【◆口上】
;【L ［你］は［彼女］と一緒に料理をした】
;【W ［料理名］を作ります】
;【▼そのまま下の口上へ】
	PRINTFORMW 「那么，就请见证，我做出宛若苦相思十日不见丈夫的妻子所做的料理的时刻吧！」
	RETURN 1
CASE "依頼実行直後"
;○この依頼は必ず実行後達成します
;【▲上の口上から続き】
;【◆口上】
;【▼そのまま下の口上へ】
	SELECTCASE RAND:4
	CASE 1
		PRINTFORMW 「啊、这个不是砂糖，而是盐呢」
	CASE 2
		PRINTFORMW 「啊咧咧～？ 好像往菜里，无意识地放进去了什么…」
	CASE 3
		PRINTFORMW 「哦哦…炒菜锅里着火了」
	CASEELSE
		PRINTFORML 「这切野菜的方式、看起来有半灵庭师切菜的模样了吗？」
		PRINTFORMDW ……看着恋切菜的样子，真的让人担心会不会切到手……
	ENDSELECT
	RETURN 1
CASE "成功報告時"
;報酬：\500
;【▲上の口上から続き】
;【◆口上】
;【報酬獲得】
;【L できた料理は二人いっしょに頂いた】
;【吃飯効果（料理打分）】
	;料理口上のコピペ
	IF ABL:38:料理技能 >= 3
		SELECTCASE RAND:4
		CASE 1
			PRINTFORML 「嘿嘿♪ 完成啦♪」
			PRINTFORML 「唔嗯、看起来很好吃的样子～♪」
		CASE 2
			PRINTFORML 「唔嗯、做出来了好东西呢」
			PRINTFORML 「怎么样？ 好吃吗？」
		CASE 3
			PRINTFORML 「嘿嘿♪ 我，努力过了哦？」
			PRINTFORML 「快夸奖我夸奖我～♪」
		CASEELSE
			PRINTFORML 「唔嗯、完成了呢♪」
			PRINTFORML 「即使并非发自意识地去模仿，我也获得进步了呢」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 1
			PRINTFORML 「好累啊～……」
			PRINTFORML 「料理什么的，果然还是很难呢……」
		CASE 2
			PRINTFORML 「嗯～？」
			PRINTFORML 「感觉…就差一点，就完美了呢」
		CASE 3
			PRINTFORML 「唔嗯、做完了！」
			PRINTFORML 「……到底怎么做的……我忘了！」
		CASEELSE
			PRINTFORML 「啊？ 这样就算做完了吗？」
			PRINTFORML 「为什么不多放些刺激的调味料进去呢？」
		ENDSELECT
	ENDIF
	WAIT
	RETURN 1
CASE "失敗報告時"
	
	RETURN 0
;CASE "依頼報告不要"
;＜この依頼にはありません＞
;	RETURN 0
ENDSELECT

