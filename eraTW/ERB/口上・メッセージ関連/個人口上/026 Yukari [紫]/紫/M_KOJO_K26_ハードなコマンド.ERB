;==================================================
;150,雙乳間抽插
;==================================================
@M_KOJO_MESSAGE_COM_K26_150
CALL M_KOJO_MESSAGE_COM_K26_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K26_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K26
	;-------------------------------------------------
	;小分支
	;-------------------------------------------------
	;初次
	;记入检查
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「哎呀？这么挺着阴茎，是因为觉得我的胸部很有吸引力吗%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本设定
	;时姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;胸部大于3
	ELSEIF ABL:胸 > 2
		IF RAND:3 == 0
			PRINTFORMW 「被犯到胸部的感觉真是令人兴奋呢%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%不行，胸部都被搞得有点臭臭的%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%比起我，更喜欢我的胸部呢%UNICODE(0x2665) *1%」
		ENDIF
	;其他情况
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯……把阴茎擦在我的胸上，你倒是有眼光」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「快点射精吧，不然我的胸部可没法解放」
		ELSE
			PRINTFORMW 「相当壮观吧？我的胸部也是%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
ENDIF
RETURN 1

