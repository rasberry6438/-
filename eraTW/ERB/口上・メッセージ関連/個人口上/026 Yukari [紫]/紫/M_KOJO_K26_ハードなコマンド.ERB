;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K26_140
CALL M_KOJO_MESSAGE_COM_K26_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K26_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K26
	;-------------------------------------------------
	;小分支
	;-------------------------------------------------
	;初次
	;记入检查
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯！嗯嗯～～～！」
		PRINTFORMW 「不行，这样强迫的话……%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本设置
	;倾慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;即将射出
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%」
			PRINTFORMW 紫感受到%CALLNAME:MASTER%的射精，喉咙巧妙地引导着它。
			PRINTFORMW （快点……%UNICODE(0x2665) *1%快点……%UNICODE(0x2665) *1%）
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%嗯嗯%UNICODE(0x2665) *1%」
			PRINTFORMW 紫感受到%CALLNAME:MASTER%的射精，喉咙巧妙地引导着它。
		ELSE
			PRINTFORMW 「嗯嗯%UNICODE(0x2665) *1%嗯～嗯%UNICODE(0x2665) *1%」
			PRINTFORMW 紫感受到%CALLNAME:MASTER%的射精，喉咙巧妙地引导着它。
		ENDIF
	;其他情况
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯～！」
			PRINTFORMW 紫因为%CALLNAME:MASTER%的阴茎而呻吟着。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯嗯……嗯嗯！！！」
			PRINTFORMW 紫因为%CALLNAME:MASTER%的阴茎而呻吟着。
		ELSE
			PRINTFORMW 「～～～！！！」
			PRINTFORMW 紫因为%CALLNAME:MASTER%的阴茎而呻吟着。
		ENDIF
	ENDIF
ENDIF
RETURN 1

