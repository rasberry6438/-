					CALL HPH_PRINT,@" 「哈，啊……HPH 热热的东西在蔓延着……好舒服……HPH」","L"
					PRINTFORMW 
				ENDIF
			CASE 2
				CALL HPH_PRINT,@" 「啊HPH %CALLNAME:MASTER%の精液在我的里面……HPH」","L"
				CALL HPH_PRINT,@" 「嗯HPH 呀啊……HPH biubiu的…好厉害的样子…在里面HPH」","L"
				PRINTFORMW 
			CASE 3
				IF TEQUIP:体位 == 1;正常位
					CALL HPH_PRINT,@"「哈啊……HPH 哈啊……HPH 嗯HPH 这个体位，能看到你愉悦的表情…也不坏嘛HPH」","L"
					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「咕HPH 嗯HPH 好厉害…好，热……HPH」","L"
					PRINTFORMW 
				ENDIF
			CASE 4
				IF TEQUIP:体位 == 1;正常位
					CALL HPH_PRINT,@"「嗯HPH 哈啊…HPH 全部，这样抱着…HPH」","L"
					CALL HPH_PRINT,@"「里面外面，都能感受到你的温暖…HPH」","L"
					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「啊HPH 嗯HPH 呼，唔…HPH 你的精液在里面跳动…好厉害……HPH」","L"
					PRINTFORMW 
				ENDIF
			CASE 5
				IF TEQUIP:体位 == 2;後背位
					CALL HPH_PRINT,@"「嗯HPH 嗯HPH 哈……HPH 如此地，交尾…里面，全部出来了…HPH」","L"
					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「啊…HPH 里面，就这样强行的…全部，出来了HPH」","L"
					PRINTFORMW 
				ENDIF
			CASE 18
				IF TEQUIP:体位 == 2;後背位
					CALL HPH_PRINT,@"「呼……HPH 呼…HPH 后面被侵犯，射出来了………HPH」","L"
					CALL HPH_PRINT,@"「啊…热量在蔓延……好厉害…这个…HPH」","L"
					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「嗯……里面，热热的东西…出来了…HPH」","L"
					PRINTFORMW 
				ENDIF
			CASE 6
				IF TEQUIP:体位 == 3;騎乗位
					CALL HPH_PRINT,@"「嗯…HPH 下腹积蓄着温暖东西的感觉…也不坏嘛」","L"
					PRINTFORML 「我要用腰让你变得更加舒服」
					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「啊HPH 嗯嗯HPH 哈……HPH 哈……HPH 这个，好舒服…HPH」","L"
					PRINTFORMW 
				ENDIF
			CASE 7
				IF TEQUIP:体位 == 3;騎乗位
					CALL HPH_PRINT,@"「怎么啦？不行了吗%CALLNAME:MASTER%…HPH」","L"
					CALL HPH_PRINT,@"「嗯……HPH 里面还在咕噜咕噜地射出来…HPH 还是很有精神呢HPH」","L"
					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「嗯…HPH 啊…吚吚HPH 热热的东西，进来了……HPH」","L"
					PRINTFORMW 
				ENDIF
			CASE 8
				IF TALENT:妊娠
					CALL HPH_PRINT,@"「哈啊…哈啊…HPH 肚子那里，小宝宝还…还在……HPH」","L"
					CALL HPH_PRINT,@"「小宝宝，会受惊的……」","L"
					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「哈啊……HPH 哈啊……HPH 精液…弥散開来了……HPH」","L"
					CALL HPH_PRINT,@"「好喜欢……HPH」","L"
					PRINTFORMW 
				ENDIF
			CASE 9
				IF TALENT:妊娠
					CALL HPH_PRINT,@"「啊嗯HPH 嗯HPH 哈…HPH 哈……HPH 如，如此地…肚子里的小宝宝…还在…HPH」","L"
					PRINTFORMW 
				ELSE
					CALL HPH_PRINT,@"「嗯HPH 肚子里面…%CALLNAME:MASTER%的精液…进来了……HPH」","L"
					PRINTFORMW 
				ENDIF
			CASEELSE
				CALL HPH_PRINT,@" 「啊，哈啊…HPH 嗯……HPH 牢牢地被固定住，精液黏糊糊的……HPH」","L"
				CALL HPH_PRINT,@" 「……啊……子宮，装满了你的精液……HPH」","L"
				PRINTFORML 心以粘稠色情的表情接受了%CALLNAME:MASTER%的精液
				PRINTFORMW 
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:MASTER:濃厚精液
			IF RAND:3 == 0
				PRINTFORML 「啊……在……里面……如此的……」
				PRINTFORMW 
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORML 「哈……啊……这是，精液……」
				PRINTFORMW 
				RETURN 1
			ELSE
				PRINTFORML 「啊……啊……这个，好厉害……啊……」
				PRINTFORMW 
				RETURN 1
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORML 「啊，哈啊……里面，好热……」
				PRINTFORML 「……这就是，精液的感觉……」
				PRINTFORMW 
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORML 「哈……啊，这样……不行……」
				PRINTFORMW 
				RETURN 1
			ELSE
				PRINTFORML 「哈…哈…嗯……呼……」
				PRINTFORML 「还在，出来……里面……好痛……」
				PRINTFORMW 
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
