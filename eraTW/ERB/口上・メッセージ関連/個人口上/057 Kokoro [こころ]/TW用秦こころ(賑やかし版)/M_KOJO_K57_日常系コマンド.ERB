;301,泡茶
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K57_301
CALL M_KOJO_MESSAGE_COM_K57_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K57_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:ブチギレ > 0
		PRINTFORML 「……总之我收下了」
		PRINTFORMW 心取出了般若之面，面无表情地啜饮着茶
	;仕事中
	ELSEIF CFLAG:行動 == 5
		SELECTCASE RAND:2
			CASE 0
			PRINTFORMW 「谢谢了。正好有些渴——」
			CASE 1
			PRINTFORML 「喔喔，谢谢」
			PRINTFORMW 「呼姆……好不容易来一次，稍微休息一下吧」
		ENDSELECT
	ELSE
		;大成功
		IF TFLAG:193 == 1
			SELECTCASE RAND:2
				CASE 0
				PRINTFORML 「呼……这种香味，感觉能让人心情平静下来」
				CASE 1
				PRINTFORML 「喔喔，居然能泡出这么好喝的茶……」
				PRINTFORMW 「是有什么诀窍吗？」
			ENDSELECT
		;失敗
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「谢——非常之感谢。那我喝了——」
			PRINTFORML 「……呜呼呼！！？　哇……！！！？」
			PRINTFORMW 大概是味道不合，心含着泪狠狠地把茶喷了出来
		;成功
		ELSE
			SELECTCASE RAND:2
				CASE 0
				PRINTFORML 「喔喔、哪来的茶啊？」
				PRINTFORMW 「等一下……吃惊时的面具在……」
				CASE 1
				PRINTFORMW 「谢——非常之感谢。那我喝了——」 
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
