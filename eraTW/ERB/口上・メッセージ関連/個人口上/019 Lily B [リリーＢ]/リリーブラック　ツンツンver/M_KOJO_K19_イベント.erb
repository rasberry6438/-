				CASE 0
					PRINTFORML (我、我的春天来啦）
					PRINTFORML 
				CASEELSE
					PRINTFORML  (…难、难道说是来找我之类的吗？)
					PRINTFORML 
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「嗯、是来邀请我去玩的吗？」
				CASE 0
					PRINTFORML 「会不会发生些有趣的事情呢…」
				CASEELSE
					PRINTFORML 「啊咧、这不是%CALLNAME:MASTER%嘛」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「妖精可是为所欲为出现在各种地方的存在哟」
				CASE 0
					PRINTFORML 「嗯ー、也不在这里吗」
				CASEELSE
					PRINTFORML 「什么呀。就这么在意我在这里这件事么？」
			ENDSELECT
		ENDIF
		RETURN 1
