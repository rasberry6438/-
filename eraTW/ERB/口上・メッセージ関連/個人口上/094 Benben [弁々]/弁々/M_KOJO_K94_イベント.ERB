;==================================================
;约会帰りに告白（恋人が付く）
;==================================================
@M_KOJO_SPEVENT_K94_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示後に接受かどうかの選択分が表示されます
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
			;初回
			IF FIRSTTIME(1094)
				PRINTFORMW 「呐、%CALLNAME:MASTER%、」
				PRINTFORMW 「%CALLNAME:MASTER%有喜欢的人吗？」
				PRINTFORMW 「我、深深地喜欢、不对爱上%CALLNAME:MASTER%了」
				PRINTFORMW 「和%CALLNAME:MASTER%在一起、就觉得心脏像是要跳出来了一样開心」
				PRINTFORMW 「一直想着%CALLNAME:MASTER%的事情晚上都睡不着了」
				PRINTFORMW 「跟八橋和雷鼓姐一起演奏時也好、脑子里想的是%CALLNAME:MASTER%在不在场呢、会不会觉得好听呢？」
				PRINTFORMW 「看见%CALLNAME:MASTER%和其他女孩在一起的时候就会觉得嫉妒」
				PRINTFORMW 「%CALLNAME:MASTER%和其他人讲话时、我都会想比那个人长好几倍地与%CALLNAME:MASTER%聊天」
				PRINTFORMW 「“我想和%CALLNAME:MASTER%在一起”」
				PRINTFORMW 「“我想变成%CALLNAME:MASTER%的专属物”」
				PRINTFORMW 「“我还想独占%CALLNAME:MASTER%的一切”」
				PRINTFORMW 「每天晚上做梦时、这样的想法都会和%CALLNAME:MASTER%的样子一起出现在梦里呢」
				PRINTFORMW 「我……」
				PRINTFORMW 「我好想成为%CALLNAME:MASTER%的恋人」
				PRINTFORMW 弁弁拉着%CALLNAME:MASTER%的袖子、安静地向你告白。
			;二回目の告白
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、」
				PRINTFORMW 走在回家的路上、弁弁像是下了什么决心似的喊着你的名字。
				PRINTFORMW 「我、果然还是喜欢%CALLNAME:MASTER%」
				PRINTFORMW 弁弁向%CALLNAME:MASTER%做出了告白。
				PRINTFORMW 「我还是放不下%CALLNAME:MASTER%、也不想放下」
				PRINTFORMW 「别看我这样、我的占有欲意外的还蛮强的哦？」
				PRINTFORMW 「所以……能和我交往吗？」
				PRINTFORMW 「…还是说不行呢？」
			ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「…」
		PRINTFORMW 弁弁一言不发地凑到了你的脸上
		PRINTFORMW 「啾…」
		PRINTFORMW 在脸上温柔地落下一吻。
		PRINTFORMW 「呵呵、这样一来、我就成为%CALLNAME:MASTER%的东西了呀…」
		PRINTFORMW %CALLNAME:MASTER%像是要回应这温柔的吻一样也温柔地抱住了弁弁。
		PRINTFORMW （虽然说了要掀起下克上的革命…但像是这样的幸福…也很好啊）
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「…」
		PRINTFORMW 「我不会放弃的」
		PRINTFORMW 弁弁做出宣告后离開了。
		PRINTFORMW （如果就这样放弃的话…）
		RETURN 1
	ENDIF

ENDIF
RETURN 1

;==================================================
;约会帰り
;==================================================
@M_KOJO_SPEVENT_K94_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORMW 「哼っ…！」
			PRINTFORMW 弁弁生气地背过脸、看都不看你一眼地回去了。
		ELSEIF TALENT:恋人
			PRINTFORM 「再见了、
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 」
			PRINTFORMW 弁弁这么说着向%CALLNAME:MASTER%行了个礼。
			PRINTFORM 「如果、如果明天也能和
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 去哪里约会的话……我能期待一下吗？」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「今天过的很開心呢」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 也早点回去吧、路上小心」
			PRINTFORMW 「…再见、拜拜」
			PRINTFORMW 道完别、弁弁依依不舍地挥手离開了。
		ELSEIF TALENT:思慕
			PRINTFORM 「那就再见吧、明天见、
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 也要注意安全哦」
			PRINTFORMW 弁弁向你告别、慢慢地回去了。
			PRINTW
		ELSE
			PRINTFORMW 「差不多要回家了呢」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 也是、在外面玩太多的话小心挨骂哦？」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「…好吧、原谅你了」
			PRINTFORMW 「但是呢…再做这些不好的事情可是不行的哟？」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 的为人、我可是相信着的哟？」
		ELSE
			PRINTFORMW 「嗯、我知道了」
			PRINTFORMW 「不能惹人太生气哟」
			PRINTFORMW 「因为我很温柔、所以原谅你好了」
			PRINTFORMW 弁弁用仍带着怒気的笑容原谅了你。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「好吧、稍微原谅你一点也可以」
			PRINTFORMW 「嘛但是我可还没气消哦」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 、再稍微让我看看你的诚意吧」
		ELSE
			PRINTFORMW 「…」
			PRINTFORMW 「不能这么简单就原谅你呢」
			PRINTFORMW 「不过稍微原谅你一点点倒是可以…」
			PRINTFORMW 弁弁的脸上带着僵硬的笑容说道。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「…好吧」
			PRINTFORMW 「但我还是很生气呢」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K94
			PRINTFORMW 、请用更能让人信服的态度再来一次」
		ELSE
			PRINTFORMW 「…」
			PRINTFORMW 「我可还在气头上呢」
			PRINTFORMW 弁弁像是看脏东西似的斜睨着%CALLNAME:MASTER%。
		ENDIF
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

