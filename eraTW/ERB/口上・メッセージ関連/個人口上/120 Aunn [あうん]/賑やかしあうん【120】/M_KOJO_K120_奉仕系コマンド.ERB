;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K120_80
CALL M_KOJO_MESSAGE_COM_K120_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K120_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「……用手做就可以了吗？」
		PRINTFORMW 「好啊。如果这是%CALLNAME:MASTER%期望的话。」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW %CALLNAME:MASTER%用肉棒在一动不动的阿吽的脸上蹭来蹭去，直到满意为止……
		RETURN 1
	ELSE
		;この後射精する(射精寸前の描写)
		IF NOWEX:PLAYER:射精
			PRINTFORML 「……………………」
			PRINTFORMW 和平时的媚态完全不同，阿吽面无表情地凝视着即将射精的肉棒……。
		ELSE
			PRINTFORML 「………啊，说了什么？」
			PRINTFORMW 默默地继续手淫着的原村的眼睛，因黏稠的欲情而混浊……。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,口交
;==================================================
@M_KOJO_MESSAGE_COM_K120_81
CALL M_KOJO_MESSAGE_COM_K120_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K120_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「………………………………」
		PRINTFORMW 阿吽没有任何反应，把%CALLNAME:MASTER%的肉棒深深地含了进去。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORML 把肉棒塞进了沉默的阿吽的嘴里……。
		PRINTFORMW 不知为什么总觉得阿吽的舌头在动……。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「吸溜……吸溜………………」
		PRINTFORMW 阿吽像被附身了似的贪婪地舔弄着%CALLNAME:MASTER%的肉棒。
		IF NOWEX:PLAYER:射精
			CALL HPH_PRINT,@"「………………………HPH」","L"
			PRINTFORMW 阿吽那乌黑混浊的眼睛因征服感而扭曲。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

