;301,茶を淹れる
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K133_301
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K133_301
CALL M_KOJO_MESSAGE_COM_K133_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K133_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怒り
	IF CFLAG:318 == 1
		PRINTFORMW 「你的茶我不想喝！」
	ELSEIF TFLAG:193 == -1
		IF RAND:5 == 0
			PRINTFORMW 「……下次我教你如何泡茶吧」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「嗯！好難喝啊！」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「我泡的比較好喝啊！」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「以前喝的茶比這更美味啊？」
		ELSE
			PRINTFORMW 「……你泡茶技術還不太拿手呢！」
		ENDIF
	ELSE
		IF RAND:5 == 0
			PRINTFORMW 「%CALLNAME:MASTER%真厲害啊！我還沒喝過這麼好喝的茶呢！」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「噗哈……！你泡的茶真好喝啊！」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「嗯！味道格外的好！」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嘶嘶……味道不錯」
		ELSE
			PRINTFORMW 「嗚啊……喝到這樣的好茶真讓人舒服啊！」
		ENDIF
	ENDIF

ENDIF
RETURN 1
