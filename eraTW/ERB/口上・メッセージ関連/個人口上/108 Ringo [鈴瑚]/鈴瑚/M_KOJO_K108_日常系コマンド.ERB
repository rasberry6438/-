				CASEELSE
					SELECTCASE RAND:5
						CASE 0
							CALL RINGO_GENSAKU
						CASE 1
							CALL RINGO_RENBO
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_OTENKI
						CASE 4
							PRINTFORML 「%CALLNAME:MASTER%…今天、不去约会吗？」
							PRINTFORMDW 邀请鈴瑚去哪里玩吧……
					ENDSELECT
			ENDSELECT
		ENDIF
	ELSEIF TALENT:思慕
		IF FLAG:约会的对象 == 108
			SELECTCASE RAND:2
				CASE 0
					CALL RINGO_DATE
				CASE 1
					SELECTCASE RAND:4
						CASE 0
							CALL RINGO_SHIBO
						CASE 1
							CALL RINGO_OTENKI
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_GENSAKU
