LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FIRSTTIME(SELECTCOM)
		PRINTFORML 「嘿诶……这酒是%CALLNAME:MASTER%你酿的？」
		PRINTFORML 「那么就、让我见识见识你的手艺吧…♪」
		PRINTFORMDW 和鈴瑚干杯了。
		IF ALCOHOL_TASTE(TFLAG:194) >= 15
			PRINTFORML 「―！…非常好喝！」
			PRINTFORML 「说不定比店里卖的还要好喝……♪」
			PRINTFORMDW 鈴瑚似乎很喜欢%CALLNAME:MASTER%酿的酒。
		ELSEIF ALCOHOL_TASTE(TFLAG:194) < 10
			PRINTFORML 「……嘛、期待你以后的表现吧…」
			PRINTFORMDW 似乎不是很合鈴瑚的口味…
		ELSE
			PRINTFORML 「…还不错嘛、这个」
			PRINTFORMW 「私酿酒意外地不能小看嘛」
		ENDIF
	;酔ってる強
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 6
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「…已经～什么都无所谓了！喝到不能喝为止吧～♪」
			CASE 1
				PRINTFORML 「…我没醉啦…没问题的～」
				PRINTFORML 「来、再给我倒一杯～…！」
				PRINTFORMDW 按照她说的、给鈴瑚的杯子里倒了酒……
			CASE 2
				PRINTFORML 「……？已经没有了吗…再拿一瓶新的来」
				PRINTFORMDW 虽然已经摇摇晃晃的了、但鈴瑚还是给自己倒了酒……
		ENDSELECT
		IF ALCOHOL_TASTE(TFLAG:194) >= 15
			PRINTFORMW 「啊～！好好喝～♪最棒了～♪」
		ELSEIF ALCOHOL_TASTE(TFLAG:194) < 10
			PRINTFORML 「―好难喝！」
			PRINTFORMDW 好像稍微清醒了一点……
		ELSE
			PRINTFORML 「…………。」
			PRINTFORMDW 鈴瑚默默地喝着酒……
		ENDIF
	;酔ってる弱
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 3
		SELECTCASE TIME:2
			;朝
			CASE 1,2
				PRINTFORML 「哎呀～…大早起时喝的酒、有一种罪恶的味道呢…！」
				PRINTFORMDW 再一次、两人的手伸向了酒瓶……
			;昼
			CASE 3,4
				PRINTFORML 「中午的时候喝的酒、为什么会这么美味呢……」
				PRINTFORML 「%CALLNAME:MASTER%……再、再来一杯……！」
				PRINTFORMDW 给鈴瑚递过来的酒杯里倒满了酒。
			;夜
			CASE 5,6,7
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「%CALLNAME:MASTER%你、是不是喝太快了？」
						PRINTFORML 「慢慢地来吧…夜晚还很长呢…♪」
						PRINTFORMDW 给鈴瑚的酒杯里倒上了新的酒。
					CASE 1
						PRINTFORML 「―然后啊、那时清蘭她……啊咧？酒已经没了」
						PRINTFORML 「%CALLNAME:MASTER%、能再给我倒一杯吗？」
						PRINTFORMDW 给鈴瑚空掉的酒杯里倒上了新的酒。
					CASE 2
						PRINTFORML 「我还能喝、你呢？」
						PRINTFORMDW %CALLNAME:MASTER%回答说"当然可以"、拿出了新的酒。
				ENDSELECT
		ENDSELECT
		IF ALCOHOL_TASTE(TFLAG:194) >= 15
			PRINTFORML 「…嗯～♪好喝♪」
			PRINTFORMDW 鈴瑚高兴地微笑着。
		ELSEIF ALCOHOL_TASTE(TFLAG:194) < 10
			PRINTFORML 「……一般般吧」
			PRINTFORMDW 似乎不是很合鈴瑚的口味…
		ELSE
			PRINTFORML 「普通」
			PRINTFORMDW 鈴瑚认真地评判着……
		ENDIF
	;素面
	ELSE
		SELECTCASE TIME:2
			;朝
			CASE 1,2
				PRINTFORML 「一大早就喝酒什么的、%CALLNAME:MASTER%你还挺坏的嘛」
				PRINTFORML 「要是被依姫大人发现了、我们可要挨骂了……干杯♪」
				PRINTFORMDW 无以言表的背德感、衬托出了酒的美味…
			;昼
			CASE 3,4
				PRINTFORML 「诶～…是不是有点太早了？现在还是中午哦？」
				PRINTFORML 「真拿你没办法…就陪你喝一杯吧」
				PRINTFORMDW 把酒杯递给鈴瑚、倒上了酒。
			;夜
			CASE 5,6,7
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「今天就让我来给你倒酒吧。来、把酒杯递过来我」
						PRINTFORMDW 恭敬不如从命、让鈴瑚给自己倒了酒。
					CASE 1
						PRINTFORML 「…今天、遇到了一些不开心的事……」
						PRINTFORML 「可能会对你发发牢骚…开玩笑的♪」
						PRINTFORMDW 我一边说道"有多少牢骚都尽管跟我倾诉吧"、一边给鈴瑚的酒杯里倒上了酒。
					CASE 2
						PRINTFORML 「%CALLNAME:MASTER%。这次的酒怎么样？」
						PRINTFORMDW 我回答说"是满意的成果"、给鈴瑚的酒杯倒上了酒。
				ENDSELECT
				PRINTFORMW 「%TEXTR("今天也辛苦了/好、那么/今晚似乎会很开心/那么、味道如何呢")%…干杯♪」
		ENDSELECT
		IF ALCOHOL_TASTE(TFLAG:194) >= 15
			PRINTFORML 「…嗯～♪好喝♪」
			PRINTFORMDW 鈴瑚高兴地微笑着。
		ELSEIF ALCOHOL_TASTE(TFLAG:194) < 10
			PRINTFORML 「……一般般吧」
			PRINTFORMDW 似乎不是很合鈴瑚的口味…
		ELSE
			PRINTFORML 「普通」
			PRINTFORMDW 鈴瑚认真地评判着……
		ENDIF
	ENDIF
ENDIF
RETURN 1
