RETURN 0

@M_KOJO_MESSAGE_COM_K108_301
CALL M_KOJO_MESSAGE_COM_K108_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K108_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FIRSTTIME(SELECTCOM)
		PRINTFORML 「……嗯？要给我茶吗？…谢谢♪那我就不客气了…」
		PRINTFORML （总之、看起来应该没有下毒什么的……）
		PRINTFORMDW 虽然有些警惕、但鈴瑚还是喝了茶。
	ELSEIF CFLAG:318 == 1
		PRINTFORML 「……多谢ー」
		PRINTFORMDW 鈴瑚道了句谢、就一口气把茶喝光了…
	ELSEIF TFLAG:193 == 1
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「啊、茶梗立起来了！真幸运ー！感觉会有好事发生呢♪」
				PRINTFORMW 「…啊啊、好喝…♪」
			CASE 1
				CALL COLORMESSAGE("「不愧是你、很懂嘛…！果然配团子还是绿茶最棒了♪」", C_ORANGE, 0, 0)
				SETCOLOR 0xFFFF00
				PRINTFORML MGMG
				PRINTFORMDW 鈴瑚的口中发出了舌头搅拌茶与团子的声音。
			CASE 2
				CALL COLORMESSAGE("「这茶真的很好喝呢…！和团子也很搭…」", C_ORANGE, 0, 0)
				SETCOLOR 0xFFFF00
				PRINTFORML MGMG
				SETCOLOR 0xEE9900
				PRINTFORML 「…%CALLNAME:MASTER%。这茶、能不能在铃瑚屋卖呢？」
				PRINTFORMDW 鈴瑚好像很中意%CALLNAME:MASTER%泡的茶的样子。
			CASE 3
				PRINTFORML 「好好喝的茶啊…疲劳都一扫而空了…♪」
				PRINTFORMDW 疲劳的色彩、从鈴瑚脸上消失了……感觉是这样的。
			CASE 4
				IF TALENT:恋慕 || TALENT:思慕
					CALL HPH_PRINT,@"「总是泡好喝的茶给我谢谢你哦♪%CALLNAME:MASTER%HPH」","L"
					PRINTFORMDW 看着鈴瑚的笑容、%CALLNAME:MASTER%也高兴了起来。
				ELSE
					PRINTFORML 「很好喝哦。谢谢款待」
					PRINTFORMDW 鈴瑚把茶全部喝光了。
				ENDIF
		ENDSELECT
	ELSEIF TFLAG:193 == 0
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「茶梗…没有立起来。今天似乎也会是平凡的一天呢」
				PRINTFORMW 「滋滋……好喝♪」
			CASE 1
				PRINTFORML 「―嗯咕！？……咕…！」
				PRINTFORMDL 鈴瑚好像被团子噎住了…她从%CALLNAME:MASTER%手中抢过茶、一口气喝光了。
				PRINTFORMW 「%TEXTR("呼呜～…我还以为要死了…/咽下…呼、刚才真危险啊…/哎呀～…谢啦、得救了/")%」
			CASE 2
				PRINTFORML 「呜嗯、好喝。手艺不错呢」
				PRINTFORML 「……这么说就对了吧？地上那个叫"茶道"的东西」
				PRINTFORMDW 鈴瑚似乎对地上的饮食文化很了解。
			CASE 3
				PRINTFORML 「……！…%CALLNAME:MASTER%、这茶…」
				PRINTFORML 「―要不要试试看在我的店里卖？」
				PRINTFORMDW 鈴瑚罕见地露出了认真的表情……
			CASE 4
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「…嗯？%CALLNAME:MASTER%、你换了茶叶吗？」
					PRINTFORML 「不错嘛、这个。我可能挺喜欢的」
					PRINTFORMDW 看来这茶合鈴瑚的口味。
				ELSE
					PRINTFORML 「%CALLNAME:MASTER%…总而言之、你的能力应该是"可以随时随地泡茶程度的能力"对吧？」
					PRINTFORMDW 才不是。
				ENDIF
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「哦哦、是茶。多谢啦―」
				PRINTFORML 「好烫！！…呼ー…呼ー…」
				PRINTFORMDW 看来有些太烫了……
			CASE 1
				CALL COLORMESSAGE("「这茶…和团子不怎么搭啊…」", C_ORANGE, 0, 0)
				SETCOLOR 0xFFFF00
				PRINTFORML MGMG
				PRINTFORMDW 鈴瑚露出了苦涩的表情…
			CASE 2
				PRINTFORML 「%CALLNAME:MASTER%…这茶、不怎么好喝呢…」
				PRINTFORML 「虽然很抱歉、但这种程度的茶我可不能允许给我的客人们喝啊」
				PRINTFORMDW 什么时候变成讨论这个话题了……？
			CASE 3
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「…嗯？%CALLNAME:MASTER%、你换了茶叶吗？」
					PRINTFORML 「我很喜欢之前你泡的茶来着…」
					PRINTFORMDW 鈴瑚看起来有点遗憾……
				ELSE
					PRINTFORML 「呜嗯～…我可能、更喜欢浓一点的？」
					PRINTFORMDW 这茶、似乎不太合铃瑚的口味…
				ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
