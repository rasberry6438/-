@M_KOJO_MESSAGE_COM_K108_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます。また、削除した場合は自動で口上色が付きません。
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K108_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K108_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;初回会話
	IF FIRSTTIME(SELECTCOM)
		CALL COLORMESSAGE("「嗯～♪今天的团子也很好吃！好幸福啊～…♪」", C_ORANGE, 0, 0)
		SETCOLOR 0xFFFF00
		PRINTFORML MGMG
		CALL COLORMESSAGE("「―干嘛？直勾勾地盯着……难道你也想吃吗？」", C_ORANGE, 0, 0)
		SETCOLOR 0xFFFF00
		PRINTFORML MGMG
		SETCOLOR 0xEE9900
		PRINTFORMDW 鈴瑚经常…不对、应该说一直在吃着团子。真担心她把身体吃坏……
		PRINTFORML 「没事的没事的♪我可没有脆弱到吃这么点团子就把身体搞坏的地步」
		PRINTFORMW 「而且…这就是我的"能力"哦」
		PRINTFORMW 「我就告诉你吧%CALLNAME:MASTER%。我的能力是…"吃的团子越多就会变得越强程度的能力"」
		PRINTFORMDW 听到这像玩笑一样的能力、%CALLNAME:MASTER%不禁喷笑出来。
		PRINTFORML 「哎呀哎呀……看来你还没有意识到我能力的可怕之处啊」
		PRINTFORMW 「"吃的团子越多"…也就是说、这个能力是没有上限的…」
		CALL COLORMESSAGE("「也就是说总有一天、我会成为幻想乡最强的存在」", C_ORANGE, 0, 0)
		SETCOLOR 0xFFFF00
		PRINTFORML MGMG
		PRINTFORMDW 鈴瑚又拿出了数量多到只是看着就感觉要吃饱了的团子、开始吃了起来……
	;怒り
	ELSEIF CFLAG:318 == 1
		CALL COLORMESSAGE("「…………。」", C_ORANGE, 0, 0)
		SETCOLOR 0xFFFF00
		PRINTFORML MGMG
		PRINTFORMDW 是不想让我进入她的视野吗、鈴瑚背对着我吃着团子……
	;喋りすぎ
	ELSEIF TCVAR:302
		SELECTCASE RAND:3
			CASE 0
				IF TALENT:恋慕 || TALENT:思慕
					CALL COLORMESSAGE("「…啊啊、不好意思。刚才在说什么来着？」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 她好像对刚才的话题没什么兴趣……
				ELSE
					CALL COLORMESSAGE("「安静点。我现在正专注于品味团子呢」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 话被鈴瑚打断了……
				ENDIF
			CASE 1
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「……吃饱了去哪里走走吧。顺便换换心情」
					PRINTFORMDW 鈴瑚好像已经聊够了……
				ELSE
					CALL COLORMESSAGE("「这样啊这样啊。那还真是不容易啊」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 鈴瑚一边敷衍地附和着、一边吃着团子……
				ENDIF
			CASE 2
				IF TALENT:恋慕 || TALENT:思慕
					PRINTFORML 「%CALLNAME:MASTER%你话有点多了哦。吃个团子冷静一下吧」
					PRINTFORMDW 嘴里被塞了个团子……
				ELSE
					CALL COLORMESSAGE("「为什么团子、会这么好吃呢？」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 鈴瑚好像已经没兴趣和%CALLNAME:MASTER%聊天了……
				ENDIF
		ENDSELECT
	;酔っ払い時会話
	;ぐでぐで
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 8
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「真是的～……！和你一起喝酒的时候、总是会不知不觉喝太多呢～！」
					PRINTFORMW 「明明我以前不是这样子的……你这家伙、我真的是…！」
					CALL HPH_PRINT,@"「……最喜欢你了HPH」","W"
				CASE 1
					CALL COLORMESSAGE("「呜嗯～…下酒菜没有了……啊！对了」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					SETCOLOR 0xEE9900
					PRINTFORML 「那就用%CALLNAME:MASTER%的脸蛋来代替吧♪…噗尼～……」
					PRINTFORML 「啊哈哈哈！能拉的好长呀～♪」
					PRINTFORMDW 虽然不太明白是怎么回事、但捏着%CALLNAME:MASTER%脸颊的鈴瑚看起来很开心的样子……
				CASE 2
					PRINTFORML 「就像"酒心巧克力"一样、把酒加到团子里也应该很好吃吧」
					PRINTFORMDW 鈴瑚的眼神有些呆滞……
				CASE 3
					PRINTFORML 「……仔细一看、%CALLNAME:MASTER%看起来也很好吃呢……」
					PRINTFORML 「能让我尝尝味道吗？」
					PRINTFORMDW 鈴瑚看起来已经相当醉了……
				CASE 4
					PRINTFORML 「异变的时候也是这样ー！大家都不知道我有多辛苦！」
					CALL HPH_PRINT,@"「…但%CALLNAME:MASTER%你能理解我对吧？所以我喜欢你HPH」","L"
					PRINTFORMDW 鈴瑚好像积攒了相当多的压力……
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「喝、喝太多了啊～…！我先休息一下……」
					PRINTFORMDW 鈴瑚就这么躺了下来…
				CASE 1
					PRINTFORML 「…嗯？酒喝完了…」
					PRINTFORML 「我这有消毒用的酒精、要喝吗？」
					PRINTFORMDW 这可不能喝啊……
				CASE 2
					PRINTFORML 「星星在…转来转去的……」
					PRINTFORMDW 鈴瑚好像已经醉得不轻了……
				CASE 3
					PRINTFORML 「……Zzz」
					PRINTFORMW 「―哈！…差点睡着了…」
				CASE 4
					PRINTFORML 「不了…不要再给我倒酒了。还是给我水吧…」
					PRINTFORMDW 给鈴瑚喝了些水、她的气色好像稍微好了一点。
			ENDSELECT
		ENDIF
	;酔いどれ
	ELSEIF BASE:酒気 > (MAXBASE:酒気 / 10) * 5
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE RAND:5
				CASE 0
					CALL HPH_PRINT,@"「呼呼…♪呐…我今天想和你一起喝到天亮呢…HPH」","L"
					PRINTFORMDW 鈴瑚用湿润的眼眸凝视着我…。
				CASE 1
					PRINTFORML 「嗯―……呼呜…好喝…♪」
					PRINTFORMDW 被酒濡湿的鈴瑚的嘴唇、看起来格外妩媚……
				CASE 2
					PRINTFORML 「%CALLNAME:MASTER%的酒很好喝、一下子就喝完了…♪」
					PRINTFORMDW 我说了"要再来一杯吗"之后、鈴瑚高兴地点了点头。
				CASE 3
					CALL HPH_PRINT,@"「嗯？没事的……HPH我还没醉呢…HPH」","L"
					PRINTFORMDW 鈴瑚的脸红扑扑的……
				CASE 4
					PRINTFORMDL 向鈴瑚那边看去时和她的眼神对上了。
					PRINTFORML 「…？…呼呼♪怎么了～%CALLNAME:MASTER%？」
					PRINTFORMDW 鈴瑚脸颊微红、轻轻地笑了…
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「感觉不错……酒劲开始上来了…♪」
					PRINTFORMDW 鈴瑚的身体在摇摇晃晃的。
				CASE 1
					PRINTFORML 「这酒很好喝啊。%CALLNAME:MASTER%、还有吗？」
					PRINTFORMDW 鈴瑚似乎很喜欢%CALLNAME:MASTER%带来的酒。
				CASE 2
					PRINTFORML 「喝酒要适可而止呢。不过放心、我还没到极限呢」
					PRINTFORMDW 鈴瑚好像在某种程度上了解自己最多能喝多少。
				CASE 3
					CALL COLORMESSAGE("「意外的很适合当下酒菜呢」", C_ORANGE, 0, 0)
					SETCOLOR 0xFFFF00
					PRINTFORML MGMG
					PRINTFORMDW 鈴瑚一边喝着酒一边吃着被当做下酒菜的团子…
				CASE 4
					PRINTFORML 「姆……%CALLNAME:MASTER%、你杯子里的酒怎么没少啊」
					PRINTFORML 「―啊！难道说、你是想把我灌醉然后做些下流的事？」
					PRINTFORMDW 为了解开误会、%CALLNAME:MASTER%把杯中的酒一饮而尽了……
			ENDSELECT
		ENDIF
	ELSEIF TALENT:恋人
		IF FLAG:约会的对象 == 108
			SELECTCASE RAND:2
				CASE 0
					CALL RINGO_DATE
				CASE 1
					SELECTCASE RAND:4
						CASE 0
							CALL RINGO_KOIBITO
						CASE 1
							CALL RINGO_OTENKI
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_GENSAKU
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE RAND:5
				CASE 0
					CALL RINGO_GENSAKU
				CASE 1
					CALL RINGO_KOIBITO
				CASE 2
					CALL RINGO_SEASON
				CASE 3
					CALL RINGO_OTENKI
				CASE 4
					PRINTFORML 「%CALLNAME:MASTER%…今天、不去约会吗？」
					PRINTFORMDW 邀请鈴瑚去哪里玩吧……
			ENDSELECT
		ENDIF
	ELSEIF TALENT:恋慕
		IF FLAG:约会的对象 == 108
			SELECTCASE RAND:2
				CASE 0
					CALL RINGO_DATE
				CASE 1
					SELECTCASE RAND:4
						CASE 0
							CALL RINGO_RENBO
						CASE 1
							CALL RINGO_OTENKI
						CASE 2
							CALL RINGO_SEASON
						CASE 3
							CALL RINGO_GENSAKU
