			GOTO INPUT_LOOP_ONAMAE
		ELSEIF (FLAG:扮演 != 28 && STRCOUNT(LOCALS, "\(米斯蒂亜\)\*\\s\*蘿蕾拉")) || (FLAG:扮演 != 69 && STRCOUNT(LOCALS, "\(今泉\)\*\\s\*影狼"))
