			REUSELASTLINE 「这实在有点…」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF FIRSTTIME("U72-1") &&  (!TALENT:恋人 || !TALENT:恋慕) && STRCOUNT(LOCALS, "\(\(旦那\|相公\)\(样\大人\)\)\$")
			CLEARLINE 1
			REUSELASTLINE 「这实在有点…」
			GOTO INPUT_LOOP_ONAMAE
		ELSE
			PRINTFORML 「%RESULTS%…是么」
		ENDIF
		PRINTL [0] 是
		PRINTL [1] 否
		;入力した名前を確定するループ
		$INPUT_LOOP_KAKUTEI
		INPUT
		IF RESULT == 0
			;你の名前を%MASTERNAME:72%に格納
			MASTERNAME:72 = %RESULTS%
			PRINTFORML 
		ELSEIF RESULT == 1
			PRINTL 请输入名称
			GOTO INPUT_LOOP_ONAMAE
		ELSE
			PRINTL 请输入名称
			GOTO INPUT_LOOP_ONAMAE
		ENDIF
	ENDIF
ENDIF

RETURN 1

