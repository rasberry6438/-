		ELSEIF FIRSTTIME("U72-1") &&  ABL:親密 < 4 && STRCOUNT(LOCALS, "\(\(\(哥哥\|尼\)\|\(姐姐\|内\)\)\(桑\|酱\)\)\$")
			CLEARLINE 1
			REUSELASTLINE 「这实在有点…」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF FIRSTTIME("U72-1") &&  ABL:親密 < 7 && STRCOUNT(LOCALS, "\(\(\(哥哥\|尼\)\|\(姐姐\|内\)\)\(样\|大人\)\)\$")
