;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;合計絶頂数で分岐
;これだけではどの部位が絶頂したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	;-------------------------------------------------
	;五重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:五重絶頂 && LOCAL:1
		IF TCVAR:10:烂醉 == 1
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「呜呜呜……要飞起来了……飞起来了……」
				PRINTFORML 「飞的好高……要掉下来了……呜呜」
				PRINTFORML 「要死掉了……」
				PRINTFORML 灵梦含糊不清的说着醉话
			ELSEIF TALENT:炮友 || TALENT:愛欲
				PRINTFORML 「咕呜呜呜呜……」
				PRINTFORML 「咿呀……」
				PRINTFORML 「不要……再去了……」
			ELSE
				PRINTFORML 「咿呀……」
				PRINTFORML 「好痒……好麻……好难受……」
				PRINTFORML 「不要……好难受……」
				PRINTFORML 灵梦的脑袋有些空空
			ENDIF
		ELSE
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 
			ELSEIF TALENT:炮友 || TALENT:愛欲
				PRINTFORML 
			ELSE
				PRINTFORML 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:四重絶頂
		SELECTCASE NOWEX:四重絶頂
			;Ｃ＆Ｖ＆Ａ＆Ｂ絶頂
			CASE 1
			;Ｃ＆Ｖ＆Ａ＆Ｍ絶頂
			CASE 2
			;Ｃ＆Ｖ＆Ｂ＆Ｍ絶頂
			CASE 3
				RETURN 1
			;Ｃ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 4
			;Ｖ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 5
		ENDSELECT
		PRINTW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;三重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:三重絶頂
		SELECTCASE NOWEX:三重絶頂
			;Ｃ＆Ｖ＆Ａ絶頂
			CASE 1
			;Ｂ＆Ｖ＆Ａ絶頂
			CASE 2
			;Ｃ＆Ｂ＆Ａ絶頂
			CASE 3
			;Ｃ＆Ｖ＆Ｂ絶頂
			CASE 4
			;Ｃ＆Ｖ＆Ｍ絶頂
			CASE 5
			;Ｂ＆Ｖ＆Ｍ絶頂
			CASE 6
			;Ａ＆Ｖ＆Ｍ絶頂
			CASE 7
			;Ｃ＆Ｂ＆Ｍ絶頂
			CASE 8
			;Ａ＆Ｂ＆Ｍ絶頂
			CASE 9
			;Ｃ＆Ａ＆Ｍ絶頂
			CASE 10
		ENDSELECT
		PRINTW
		RETURN 1
		ENDIF
	;-------------------------------------------------
	;二重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:二重絶頂
		SELECTCASE NOWEX:二重絶頂
			;Ｃ＆Ｖ絶頂
			CASE 1
			;Ｃ＆Ａ絶頂
			CASE 2
			;Ｖ＆Ａ絶頂
			CASE 3
			;Ｃ＆Ｂ絶頂
			CASE 4
			;Ｖ＆Ｂ絶頂
			CASE 5
			;Ａ＆Ｂ絶頂
			CASE 6
			;Ａ＆Ｍ絶頂
			CASE 7
			;Ｖ＆Ｍ絶頂
			CASE 8
			;Ｃ＆Ｍ絶頂
			CASE 9
			;Ｂ＆Ｍ絶頂
			CASE 10
		ENDSELECT
	ELSEIF NOWEX:Ｃ絶頂
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ELSEIF NOWEX:Ｖ絶頂
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ELSEIF NOWEX:Ａ絶頂
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ELSEIF NOWEX:Ｂ絶頂
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ELSEIF NOWEX:Ｍ絶頂
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ENDIF
ENDIF
RETURN 1
