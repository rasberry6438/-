	ELSEIF TFLAG:193 == 2
		;ハズレの選択肢だったが逆に正解
	ELSEIF TFLAG:193 == -1
		;ハズレの選択肢
	ELSEIF TFLAG:193 == -2
		;正解の選択肢だったが逆にハズレ
