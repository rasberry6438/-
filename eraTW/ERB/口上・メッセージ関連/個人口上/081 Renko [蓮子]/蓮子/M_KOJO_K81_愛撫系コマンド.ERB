;==================================================
;23,枕邊話
;==================================================
@M_KOJO_MESSAGE_COM_K81_23
CALL M_KOJO_MESSAGE_COM_K81_23_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K81_23_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「这样说的话做爱也不错呢」
		RETURN 1
	ENDIF
	;失敗
	IF TFLAG:193
		PRINTFORMW 「……找点有趣的话题再来啊、笨蛋」
		RETURN 1
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「诶、在给我制造气氛吗？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……做爱真是费体力呢」
		ELSE
			PRINTFORMW 「……稍微休息一下吧」
		ENDIF
	ENDIF
ENDIF
RETURN 1