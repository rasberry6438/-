;==================================================
;19,接吻
;==================================================
@M_KOJO_MESSAGE_COM_K81_19
CALL M_KOJO_MESSAGE_COM_K81_19_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K81_19_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯啊……！」
		PRINTFORMW 「……喜欢、%CALLNAME:MASTER%、%UNICODE(0x2665) *1%」
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「嗯啊%UNICODE(0x2665) *1%啊……%UNICODE(0x2665) *1%」
			PRINTFORMW 「就这样……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯嗯啊%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「呜%UNICODE(0x2665) *1%」
			PRINTFORMW 蓮子保持着嘴唇被夺走的状态。
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯啊……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯嗯啊%UNICODE(0x2665) *1%嗯嗯～啊%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「呜%UNICODE(0x2665) *1%」
			PRINTFORMW 蓮子在亲吻结束之后又把嘴唇凑了过来。
		ENDIF
	ENDIF
ENDIF
RETURN 1

