	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「互相舔舐什么的简直是小丑剧场啊」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				PRINTFORMW 「呜%UNICODE(0x2665) *1%」
