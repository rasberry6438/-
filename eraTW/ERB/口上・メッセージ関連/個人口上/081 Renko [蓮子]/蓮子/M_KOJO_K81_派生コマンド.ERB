			IF RAND:3 == 0
				PRINTFORMW 「嗯……%UNICODE(0x2665) *1%嗯～……%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「嗯嗯嗯嗯%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「我的舌技也很厉害吧%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「啊啊%UNICODE(0x2665) *1%嗯啊%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「嗯嗯嗯？」
				PRINTFORMW 「之后也多舔舔我吧%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「嗯……嗯嗯%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「嗯嗯……啊%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯……嗯……%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……嗯……啊恩……」
		ELSE
