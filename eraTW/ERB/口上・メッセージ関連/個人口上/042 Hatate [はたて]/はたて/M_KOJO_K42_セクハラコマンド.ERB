IF CFLAG:K42C_結婚 >= 1 || (TALENT:恋慕 || TALENT:恋人 && EXP:愛情経験 >= 500 && CFLAG:信賴度 >= 5000)
	SOURCE:反感 = 0
	SOURCE:情愛 = 480
	SOURCE:歓楽 = 360
	CALL AddEXP("愛情経験", TARGET, 1)
	IF BASE:42:憤怒 >= MAXBASE:42:憤怒 / 2
		BASE:42:憤怒 = BASE:42:憤怒 / 2
	ENDIF
;女の子だと陥落前でも情愛、歓楽UP。反感減少
ELSEIF TALENT:MASTER:性別 == 1 || TALENT:MASTER:性別 == 3
	SOURCE:反感 = SOURCE:反感 / 5
	SOURCE:情愛 = SOURCE:情愛 * 12 / 10
	SOURCE:歓楽 = SOURCE:歓楽 * 12 / 10
ENDIF

