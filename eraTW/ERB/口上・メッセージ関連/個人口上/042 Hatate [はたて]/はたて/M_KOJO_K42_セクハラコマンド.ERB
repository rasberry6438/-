;扶她
IF TALENT:性別 == 3
	;無知な幼女相手におちんちん触られるといろいろ大幅プラス
	IF (TALENT:MASTER:年齢 < 1 && TALENT:MASTER:幼児／幼児退行)
		IF TALENT:MASTER:無知 == 1
			SOURCE:反感 = 0
			SOURCE:快Ｃ = 360
			SOURCE:歓楽 = 300
			SOURCE:欲情 = 300
			SOURCE:逸脱 = 300
			SOURCE:快Ｃ += ABL:PLAYER:指 * 50
			BASE:射精 += 500
			IF BASE:42:憤怒 >= MAXBASE:42:憤怒 / 2
				BASE:42:憤怒 = BASE:42:憤怒 / 2
			ENDIF
		ELSE
			SOURCE:反感 = 0
			SOURCE:快Ｃ = 240
			SOURCE:歓楽 = 200
			SOURCE:欲情 = 200
			SOURCE:逸脱 = 150
			SOURCE:快Ｃ += ABL:PLAYER:指 * 30
			IF BASE:42:憤怒 >= MAXBASE:42:憤怒 / 2
				BASE:42:憤怒 = BASE:42:憤怒 / 2
			ENDIF
		ENDIF
	;你が大人の女性なら反感減少
	ELSE
		SOURCE:反感 = SOURCE:反感 / 5
	ENDIF
;ふた無
ELSE
	;陥落＆高ステータスなら反感無
	IF CFLAG:K42C_結婚 >= 1 || ((TALENT:恋慕 || TALENT:恋人) && EXP:愛情経験 >= 500 && CFLAG:信賴度 >= 5000)
		SOURCE:反感 = 0
		SOURCE:快Ｃ = 240
		SOURCE:歓楽 = 150
		SOURCE:快Ｃ += ABL:PLAYER:指 * 30
		CALL AddEXP("愛情経験", TARGET, 1)
		IF BASE:42:憤怒 >= MAXBASE:42:憤怒 / 2
			BASE:42:憤怒 = BASE:42:憤怒 / 2
		ENDIF
	ENDIF
ENDIF

