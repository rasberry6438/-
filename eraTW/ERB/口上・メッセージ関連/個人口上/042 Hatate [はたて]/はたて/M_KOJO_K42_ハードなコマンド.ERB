;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K42_140
CALL M_KOJO_MESSAGE_COM_K42_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K42_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORML 「噢guo…噢、咕、嗯咕…！」
			PRINTFORML 「嘎哈、guo嚯、gei…」
			IF TALENT:恋慕 || TALENT:恋人
				PRINTFORML 「哈啊、哈啊…呐、不要这么强行地来做呀…！」
				PRINTFORML 「会用嘴帮你来做的…」
				PRINTW 
			ELSE
				PRINTFORML 「等下、乱暴快停下呀…！」
				PRINTW 
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		IF TALENT:恋慕 || TALENT:恋人
			IF ABL:Ｍ感覚 >= 5
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！　guo滋！　guo滋！
				PRINTFORML 「%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　mo゛%UNICODE(0x2665) *1%　mo゛%UNICODE(0x2665) *1%」
				PRINTFORML 即使蹭蹭地戳着极的喉咙深处、喉咙也很幸福地不断吸附着肉棒
				PRINTW 
			ELSEIF ABL:Ｍ感覚 >= 2
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！
				PRINTFORML 「噢゛%UNICODE(0x2665) *1%　guo%UNICODE(0x2665) *1%」
				PRINTFORML （明明很粗暴、很难受的、却好舒服%UNICODE(0x2665) *1%　为什么！？）
				PRINTW 
			ELSE
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！
				PRINTFORML 「噢゛啵！？　噢゛っ！？　噢゛っ！！　噢゛唉゛！」
				PRINTFORML 极虽然拍打着你的大腿抗议着、但是你无视之突入了喉咙深处
				PRINTW 
			ENDIF
			RETURN 1
		ELSEIF TALENT:思慕
			IF ABL:Ｍ感覚 >= 5
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！　guo滋！　guo滋！
				PRINTFORML 「噢゛、咕mo、　fuー%UNICODE(0x2665) *1%　fuー%UNICODE(0x2665) *1%」
				PRINTFORML 极浮现出痛苦和快乐的混合的表情吸附着肉棒
				PRINTW 
			ELSEIF ABL:Ｍ感覚 >= 2
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！
				PRINTFORML 「噢゛、咕mo、嗯卟、嗯卟」
				PRINTFORML 极的表情因为痛苦而扭曲着、但是嘴角却微微地翘起了
				PRINTW 
			ELSE
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！
				PRINTFORML 「噢゛啵！？　噢゛っ！？　噢゛っ！！　噢゛唉゛！」
				PRINTFORML 极虽然拍打着你的大腿抗议着、但是你无视之突入了喉咙深处
				PRINTW 
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

