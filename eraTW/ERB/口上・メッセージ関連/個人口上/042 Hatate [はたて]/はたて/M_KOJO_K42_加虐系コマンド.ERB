;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K42_100
CALL M_KOJO_MESSAGE_COM_K42_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K42_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML ぱぁんっ！
		IF TALENT:恋慕 || TALENT:恋人
			PRINTFORML 「好痛!%MASTERNAME:42%,你在干什么啊～～」
			PRINTW 
			PRINTFORML 屁股真的很漂亮,忍不住...
			PRINTFORML 「打女孩子的屁股,你也太过分了!」
			PRINTW 
		ELSE
			PRINTFORML 「好痛!你在干什么啊!」
			PRINTW 
			PRINTFORML 屁股真的很漂亮,忍不住...
			PRINTFORML 「不是'忍不住'吧!真是胡闹!」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:愛欲 || TALENT:炮友
		IF TALENT:TARGET:受虐狂 == 1 || ABL:TARGET:受虐属性 >= 2
			IF TEQUIP:Ｖ性交
					PRINTFORML 「啊%UNICODE(0x2665) *1% 哈%UNICODE(0x2665) *1% 好棒%UNICODE(0x2665) *1% 再多一些%UNICODE(0x2665) *1%」
					PRINTFORML 在极沉浸其中的性交中,我不由自主地揉捏着她的臀部
					PRINTW 
					PRINTFORML 触感真是太舒服了。她有一个很好的屁股...
					PRINTFORML 几乎是不经大脑,我用手掌拍打了一下极的臀部,发出清脆的响声
					PRINTW 
					PRINTFORML 「哇啊%UNICODE(0x2665) *1%」
					PRINTFORML 「啊...不,不是,刚才只是被插入的感觉太舒服了...」
					PRINTW 
					PRINTFORML 虽然极这么解释,但很明显那声音并不是因为抽插带来的
					PRINTFORML 看来极是个喜欢被打屁股的变态啊
					PRINTW 
			ELSEIF TEQUIP:Ａ性交
			ELSE
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 啪
				PRINTFORML 「好痛。不要那么随便就拍女孩子的屁股好不好」
				PRINTW 
			CASE 1
				PRINTFORML 啪!
				PRINTFORML 「好痛!你在干什么啊!」
				PRINTW 
				PRINTFORML 啪啪!
				PRINTFORML 「好痛!所以说,住手...」
				PRINTW 
				PRINTFORML 啪啪啪!
				PRINTFORML 「啊!?痛!?你再这样我可不会饶你!...」
				PRINTW 
				PRINTFORML 啪啪啪啪!
				PRINTFORML 「够了你这个笨蛋!!!!」
				PRINTW 
				PRINTFORML ばしぃぃぃぃぃん！
				PRINTFORML 极狠狠还了我一击
			ENDSELECT
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

