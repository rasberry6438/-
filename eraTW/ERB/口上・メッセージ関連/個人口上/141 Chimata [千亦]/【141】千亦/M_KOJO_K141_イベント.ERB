	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORML 「那、%CALLNAME:MASTER%、我要回去了哦」
			PRINTFORMW 「虽然还想多跟你待一会来着……」			
		ELSEIF TALENT:恋慕
			PRINTFORML 「再见啦」
			PRINTFORMW 直到消失在%CALLNAME:MASTER%面前、千亦一直时不时回头露出微笑。 			
		ELSEIF TALENT:思慕
			PRINTFORML 「那、%CALLNAME:MASTER%、就此别过」
			PRINTFORMW 千亦离去的时候向%CALLNAME:MASTER%挥了挥手。 			
		ELSE
			PRINTFORMW 「我要回去了」千亦告辞。 		
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;デート中帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORML 「我今天玩的很开心哦♪呼呼」
			PRINTFORMW 千亦临走时给%CALLNAME:MASTER%来了一记轻轻的拥抱。			
		ELSEIF TALENT:恋慕
			PRINTFORML 「今天……很开心哦」
			PRINTFORMW %CALLNAME:MASTER%在千亦含情脉脉的注视下与之告别。	
		ELSEIF TALENT:思慕
			PRINTFORMW 「嘛 过程先不论 结果今天还蛮开心的」 		
		ELSE
			PRINTFORMW 「都到这个时间了……再见哦」 		
		ENDIF	
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;玄関先でお見送り（キャラの自宅位置、またはキャラの部屋でデート終了）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%……谢谢你送我回家哦」
			PRINTFORMW 临别之际、千亦在%CALLNAME:MASTER%的脖子上轻吻了一口。			
		ELSEIF TALENT:恋慕
			PRINTFORML 「送到这就可以了」
			PRINTFORMW 「明天还要再见面喔」			
		ELSEIF TALENT:思慕
			PRINTFORMW 「……谢谢你送我到家了」 		
		ELSE
			PRINTFORMW 「居然特意送到这里啊……总之谢谢了」		
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;デート終了時に部屋に来ないかと誘う（～から部屋に来ないかと誘われた）
	;（来訪キャラのみ,自宅が無いキャラでも発生します）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;部屋に来ないか→「行く」を選択（～に誘われるまま、【MASTER】は【TARGET】の部屋へと向かった……）
	;（これが発生すると十中八九押し倒されます）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;部屋に来ないか→「やめとく」を選択（～は心なしか寂しそうだ……）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今夜は帰りたくないと言い出す（～は今夜は帰りたくないと言い出した…）
	;（来訪キャラの帰宅時に欲求不満と欲望が高いと発生）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今夜は帰りたくない→「泊める」を選択（～は【MASTERの自室】に泊まっていくことになりました）
	;記入チェック（=0, 非表示、1, 表示）
