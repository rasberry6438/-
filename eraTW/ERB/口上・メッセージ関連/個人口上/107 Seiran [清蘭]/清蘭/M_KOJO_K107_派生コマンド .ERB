@M_KOJO_MESSAGE_SCOM_K107_1_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;舌技术３以上
	ELSEIF ABL:舌 >= 3
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「嗯嗯~%CALLNAME:MASTER%…拜托了…已经快要去了…所以…呢…？」","L"
					PRINTFORMDL 清兰摆动着屁股，投来仿佛期待着什么的眼神。
					PRINTFORMDL %CALLNAME:MASTER%紧紧抓住清兰的大腿，将舌头伸进阴道，用力地吸吮着。
					CALL HPH_PRINT,@"「呼啊啊啊这、这个太舒服了嗯嗯呜呜」","W"
				CASE 1
					CALL HPH_PRINT,@"「嗯嗯嗯…嗯嗯…嗯啾…嗯…啾啾噜…」","L"
					PRINTFORMDL 当和清兰互相舔舐着彼此的性器时，阴道内开始震动起来。
					PRINTFORMDL 为了让她先去，%CALLNAME:MASTER%同时攻击着阴道和阴蒂。
					CALL HPH_PRINT,@"「嗯嗯嗯…嗯…嗯嗯…嗯」","L"
					CALL HPH_PRINT,@"「―嗯！哈噗啾滋溜啾噜噜滋…啾噜噜呜呜呜呜呜ー」","L"
					PRINTFORMDW 清兰也像是较上劲了一样，用力地吸吮着%CALLNAME:MASTER%的阴茎……！
				CASE 2
					CALL SEIRAN_FELLATIO
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「嗯嗯…嗯啾…啾…嗯嗯！啊啊啊啊啊」","L"
					PRINTFORMDL 因为%CALLNAME:MASTER%舔舐阴道所带来的快感，清兰从阴茎上松开了嘴…
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%…等等停下来」","L"
					PRINTFORMW （呜…！明明我想让%CALLNAME:MASTER%舒服的…！）
				CASE 1
					CALL HPH_PRINT,@"「呼啊啊啊啊入口的地方…不要那么舔啦…」","L"
					CALL HPH_PRINT,@"「嗯嗯…哈…啊―哈噗嗯啾啾啾嗯嗯啾啾噜噜噜」","L"
					PRINTFORMDW 为了不输给%CALLNAME:MASTER%，清兰一心一意地舔舐着阴茎…！
				CASE 2
					CALL SEIRAN_FELLATIO
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%…！啊…嗯…嗯嗯…等等…已经，快要去了…」","L"
					CALL HPH_PRINT,@"「啊啊！？啊啊啊啊不行不要那么激烈啊啊啊」","L"
					PRINTFORMDW 清兰发出娇声，身体瑟瑟发抖……
				CASE 1
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%…我…已经…！」","L"
					PRINTFORMDL 听到清兰那充满爱意的声音，%CALLNAME:MASTER%将嘴唇贴在蜜壶上，吸吮着爱液并一饮而尽。
					CALL HPH_PRINT,@"「嗯嗯嗯呜呜啊啊啊」","L"
					CALL HPH_PRINT,@"「…啊啊…什么，这个…头…晕晕乎乎的…」","W"
				CASE 2
					CALL SEIRAN_FELLATIO
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「来…！%CALLNAME:MASTER%也，让我让你舒服…」","L"
					PRINTFORMDL 清兰将阴户按在%CALLNAME:MASTER%的脸上…
					CALL HPH_PRINT,@"「啊嗯嗯舌头，进来了」","W"
				CASE 1
					CALL HPH_PRINT,@"「呜呜…啊嗯嗯…」","L"
					CALL HPH_PRINT,@"（啊…%CALLNAME:MASTER%，那么努力地舔着我的下面…）","L"
					PRINTFORML 「我也…必须努力！…啊姆―」
					CALL HPH_PRINT,@"「嗯…舔舔…啾噜啾啾呜…」","W"
				CASE 2
					CALL SEIRAN_FELLATIO
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
