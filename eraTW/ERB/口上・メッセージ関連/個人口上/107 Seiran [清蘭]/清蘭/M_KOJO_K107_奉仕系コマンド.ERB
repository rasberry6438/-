;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「用手…摩擦吗…？知道了…来吧！」
		PRINTFORMDL 顷刻间，%CALLNAME:MASTER%的肉棒传来剧痛…！
		PRINTFORML 「对、对不起！……疼吗？没事吧？」
		PRINTFORMW 「轻柔一点…慢慢地…慢慢地…！」
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;技巧３以上
	ELSEIF ABL:技巧 >= 3
		;今射精した
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「肉棒膨胀起来了HPH看来，快射了HPH」","L"
					PRINTFORMDL 察觉到射精即将来临，清兰加快了套动的速度。
					PRINTFORMDL 将自己的高潮交给他人，这种感觉实在难以言喻…！
					CALL HPH_PRINT,@"「快点快点HPH快射出来HPH射出来啊HPH」","W"
					
				CASE 1
					CALL HPH_PRINT,@"「啊…HPH嗯HPH嗯嗯…HPH精液…好多…HPH」","L"
					PRINTFORMDL 从%CALLNAME:MASTER%的肉棒射出的大量精液弄脏了清兰的手…！
					CALL HPH_PRINT,@"「呵呵…HPH有点可惜…HPH 嗯…吮吸吮吸HPH」","L"
					PRINTFORMDL 清兰毫无怨言地舔食着沾满手掌的精液…
					CALL HPH_PRINT,@"「没关系的…♪ 是%CALLNAME:MASTER%的嘛HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「啊…HPH 肉棒…要射了吗？…好的…♪」","L"
					PRINTFORMDL 清兰稍稍用力握住肉棒，加快动作以引发射精。
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%啊HPH快射出来HPH 射出满是污秽的精子HPH」","W"
			ENDSELECT
			RETURN 1
		;其他情况
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%…怎么样？舒服吗？」","L"
					CALL HPH_PRINT,@"「自己做，和用我的手做…你喜欢哪个…？」","L"
					PRINTFORMDL 恐怕即使自己用手自慰，也无法获得如此的快乐…
					PRINTFORMDW “清兰的手更好！”清兰高兴地颤抖着身体…！
					
				CASE 1
					CALL HPH_PRINT,@"「小弟弟，胀得这么鼓…到底积攒了多少污秽啊…」","L"
					CALL HPH_PRINT,@"「我会净化你的…我会净化你这罪恶的小弟弟…」","L"
					PRINTFORMDW 清兰用手撸动肉棒，进行着“净化活动”……
				CASE 2
					PRINTFORML 「呐，%CALLNAME:MASTER%…舒服吗？」
					CALL HPH_PRINT,@"「想射精的话，随时告诉我哦」","W"
			ENDSELECT
			RETURN 1
		ENDIF
	;其他情况
	ELSE
		;现在射精了
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「诶…要去了！？等、等等！纸巾—呀！！」
					PRINTFORMDL 想要忍住，但已经来不及了，精液溅到了清兰的脸和头发上……
					PRINTFORMW 「呜诶～…！精液，溅到了…！……好臭啊…！」
				CASE 1
					PRINTFORML 「小弟弟，膨胀起来了…！已经，要射精了吗？」
					PRINTFORML 「再快一点…？ 呜、嗯！我知道了…！」
					PRINTFORMDL 为了迎接高潮，清兰加快了撸动肉棒的速度…不久，%CALLNAME:MASTER%迎来了极限—
					PRINTFORMW 「哇、哇！好、好厉害…！精液是这样出来的啊……」
				CASE 2
					PRINTFORML 「啊…！射精了……嗯！呜…！…精液，沾到手上了……」
					PRINTFORMW 「…………。」
					CALL HPH_PRINT,@"「……舔舔…好奇怪的味道…」","W"
			ENDSELECT
			RETURN 1
		;其他情况
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「温柔地…温柔地…轻轻地…轻轻地…！」
					PRINTFORMDW 清兰非常小心地触摸着%CALLNAME:MASTER%的肉棒……好痒…！
				CASE 1
					PRINTFORML 「滴着口水……嗯啊……用手撸啊撸…撸啊撸…！」
					PRINTFORMDL 清兰用笨拙的手法撸动着%CALLNAME:MASTER%的肉棒…
					PRINTFORMDW 一边发出拟声词一边手淫的样子有些可爱，让人感到一种难以言喻的心情…！
				CASE 2
					PRINTFORML 「%CALLNAME:MASTER%，怎么样…？舒服吗？」
					CALL HPH_PRINT,@"「…太、太好了♪…那么我就这样继续了哦」","W"
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
