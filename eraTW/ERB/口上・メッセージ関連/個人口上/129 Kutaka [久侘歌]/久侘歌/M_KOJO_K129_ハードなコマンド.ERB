;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K129_140
CALL M_KOJO_MESSAGE_COM_K129_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K129_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯……！嗯嗯～～～！」
		PRINTFORMW 「不可能的……请不要把鸡鸡强行顶进来……%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「嗯嗯～～～%UNICODE(0x2665) *1%」
			PRINTFORMW 久侘歌感受到%CALLNAME:MASTER%的射精临近、用喉咙巧妙地催促着。
			PRINTFORMW （好想快点被%CALLNAME:MASTER%先生的精子填满啊……%UNICODE(0x2665) *1%）
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%嗯%UNICODE(0x2665) *1%」
			PRINTFORMW 久侘歌感受到%CALLNAME:MASTER%的射精临近、用喉咙巧妙地催促着。
		ELSE
			PRINTFORMW 「嗯嗯～%UNICODE(0x2665) *1%嗯%UNICODE(0x2665) *1%」
			PRINTFORMW 久侘歌感受到%CALLNAME:MASTER%的射精临近、用喉咙巧妙地催促着。
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯嗯嗯～！」
			PRINTFORMW 久侘歌因%CALLNAME:MASTER%刺入的阴茎发出呻吟。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯嗯……嗯～～～！！！」
			PRINTFORMW 久侘歌因%CALLNAME:MASTER%刺入的阴茎发出呻吟。
		ELSE
			PRINTFORMW 「嗯嗯～！！！」
			PRINTFORMW 久侘歌因%CALLNAME:MASTER%刺入的阴茎发出呻吟。
		ENDIF
	ENDIF
ENDIF
RETURN 1

