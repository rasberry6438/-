;==================================================
;150,雙乳間抽插
;==================================================
@M_KOJO_MESSAGE_COM_K129_150
CALL M_KOJO_MESSAGE_COM_K129_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K129_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「硬、硬塞进欧派里…！？」
		RETURN 1
	ENDIF
	;基本セット
	;胸3以上
	IF ABL:胸 > 2
		IF RAND:3 == 0
			PRINTFORMW 「欧派被侵犯了%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW （被强迫了也会有感觉……%UNICODE(0x2665) *1%）
		ELSE
			PRINTFORMW 「嗯嗯……这么喜欢被胸部摩擦、真是个了不起的胸部星人啊♪」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「在里面脉动着……能感觉到%CALLNAME:MASTER%先生……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「太、太强硬了……！」
		ELSE
			PRINTFORMW 「嗯嗯……稍微有些痛……」
		ENDIF
	ENDIF
ENDIF
RETURN 1
