					PRINTFORMDW 尽管已经怀孕,%CALLNAME:TARGET%的子宫还是被%CALLNAME:MASTER%的精液完全填满了......
				CASE 1
					CALL HPH_PRINT,@"「很高兴你能射这么多......HPH」","L"
					CALL HPH_PRINT,@"「――嗯,帮我抚摸一下......HPH」","L"
					PRINTFORMDW 惊到了她吗。%CALLNAME:MASTER%温柔地抚摸着%CALLNAME:TARGET%的肚子。
				CASE 2
					CALL HPH_PRINT,@"「嗯HPH 宝宝好像被吓到了......HPH」","L"
					PRINTFORMDL 是不是有点太激烈了。%CALLNAME:MASTER%一边道歉,一边抚摸%CALLNAME:TARGET%的肚子。
					CALL HPH_PRINT,@"「呼......HPH 没关系的,继续吧......HPH」","W"
			ENDSELECT
		;危険日允许无套済
		ELSEIF CFLAG:允许无套 >= 2
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「真厉害......还在射精......HPH」","L"
					CALL HPH_PRINT,@"「这么想让我受孕吗......?HPH」","L"
					PRINTFORMDW %CALLNAME:MASTER%点头后,%CALLNAME:TARGET%的阴道紧紧收缩
				CASE 1
					CALL HPH_PRINT,@"「啊啊......HPH 我的里面,被填满了......HPH」","L"
					CALL HPH_PRINT,@"「做爱让人上瘾呢......HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%一边与%CALLNAME:MASTER%连接在一起,一边慵懒地抚摸着下腹......
				CASE 2
					CALL HPH_PRINT,@"「再深一点,捅进来......HPH」","L"
					CALL HPH_PRINT,@"「好好地......让宝宝成形......HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%紧紧收缩阴道,仿佛要不漏一滴精液般......
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「啊啊......哈......哈......HPH」","L"
					PRINTFORMDL 达到一个高潮的结尾,两人陷入舒适的疲劳之中
					PRINTFORMDW 汗流浃背地凝视着对方,唇舌交缠
				CASE 1
					PRINTFORMDL 尽管全部注入,%CALLNAME:MASTER%的阴茎仍保持坚硬
					CALL HPH_PRINT,@"「啊HPH 鸡鸡还硬着呢......HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%紧紧收缩阴道,摆动腰肢索求下一轮
				CASE 2
					CALL HPH_PRINT,@"「呼......好激烈呀......HPH」","L"
					CALL HPH_PRINT,@"「但是......感觉很好呢......HPH」","L"
					PRINTFORMDW 两人仍深深连接在一起,暂时沉浸在余韵中......
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

