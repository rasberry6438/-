	IF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「不行,已经......♪HPH」","L"
				PRINTFORMDL 为了得到最后一击,身体不由自主地伸出手
				PRINTFORMDW %CALLNAME:MASTER%握住她,在她耳边哄她再忍耐一下
			CASE 1
				CALL HPH_PRINT,@"「要去了......♪HPH 啊,欸......?」","L"
				PRINTFORMDW %CALLNAME:TARGET%眼中含泪,似乎在问“为什么?”
			CASE 2
				CALL HPH_PRINT,@"「请......让我去吧......♪HPH」","L"
				CALL HPH_PRINT,@"「我......已经到极限了......♪HPH」","W"
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「啊......已经......♪HPH」","L"
				CALL HPH_PRINT,@"「差一点就能......♪HPH」","W"
			CASE 1
				CALL HPH_PRINT,@"「呜......不要再逗了......♪HPH」","L"
				CALL HPH_PRINT,@"「再一会儿......再一会儿就......♪HPH」","W"
			CASE 2
				CALL HPH_PRINT,@"「这里......已经要去了,所以......♪HPH」","L"
				CALL HPH_PRINT,@"「一起......一起舒服起来好吗?HPH 好吗......?HPH」","W"
		ENDSELECT
	ENDIF
ENDIF
RETURN 1