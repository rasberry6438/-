@M_KOJO_MESSAGE_COM_K99_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;初回会話
	IF FIRSTTIME(SELECTCOM)
		PRINTFORML 「好久没像这样和地上的人说话了」
		PRINTFORML 「上一次是距今一千五百多年前……他的名字叫水江浦岛子」
		PRINTFORMW 「啊、说浦岛太郎更容易理解吧？」
	;怒り
	ELSEIF CFLAG:ブチギレ > 0  || MARK:反発刻印 >= 3
		PRINTFORML 「……………」
		PRINTFORMDW %CALLNAME:TARGET%用扇子遮住嘴角、目不转睛地注视着%CALLNAME:MASTER%
	;会話不能
	ELSEIF TCVAR:302
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 「………」
			PRINTFORMDW %CALLNAME:TARGET%微笑着、默默倾听着%CALLNAME:MASTER%的话
		;それ以外
		ELSE
			PRINTFORML 「算了、不用硬找话题了」
			PRINTFORMW 「先放松一会儿吧」
		ENDIF
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:10
				CASE 0
					;１５分の１の確率でフェムトファイバーについて語る
					IF !RAND:15
						PRINTFORML 「是不是快忘了、我之前和你提过的“费米纤维”的性质？」
						PRINTFORMW 「那么、就在这里复习一下吧」
						PRINTFORML 
						;選択肢
						CALL ASK_YN("好的","不想听")
						;判定
						SELECTCASE RESULT
							;はい
							CASE 0
								GOTO FEMTOFIBER
							;いいえ
							CASE 1
								PRINTFORMW 「我就知道你会这么说、没得选择哦」
								GOTO FEMTOFIBER
						ENDSELECT
					;通常
					ELSE
						PRINTFORMDL 连着说了一段时间、两人稍微喘了口气
						PRINTFORMDL %CALLNAME:MASTER%轻轻搂住%CALLNAME:TARGET%的肩膀、对她甜言蜜语着
						PRINTFORMDL %CALLNAME:TARGET%羞得面红耳赤、却又高兴地微笑着
						PRINTFORMW 「……呵呵、最喜欢你了、%CALLNAME:MASTER%♪」
					ENDIF
				CASE 1
					PRINTFORML 「总有一天、你也会……」
					PRINTFORML 「……不、没什么」
					PRINTFORMW 「还是来讲一讲你的故事吧？」
				CASE 2
					PRINTFORML 「欸？欸欸欸欸————你在看哪里呢？」
					PRINTFORML 「和人说话的时候要看着对方的眼睛」
					PRINTFORMDW 但是、说这话的同时、%CALLNAME:TARGET%解開了衬衫的扣子、故意拢起双乳、露出乳沟
				CASE 3
					PRINTFORML 「……噗……噗噗」
					PRINTFORML 「啊哈哈、对不起」
					PRINTFORMW 「你的话……哈哈……真的很有意思」
				CASE 4
					PRINTFORML 「那个故事、应该还有下文吧？」
					PRINTFORML 「请说出来吧、我会认真倾听的♪」
					PRINTFORMDW %CALLNAME:TARGET%听着%CALLNAME:MASTER%的话、開心地笑了起来
				CASE 5
					PRINTFORML 「你听说过吗？」
					PRINTFORML 「月人和地上人之间的传闻」
					PRINTFORML 「即使超过了75天、也始终没有消失的人」
					PRINTFORMW 「呼呼……说的到底是谁和谁呢」
				CASE 6
					PRINTFORML 「我在想、有一天、我们可以一起去旅行」
					PRINTFORML 「去了一个谁也不认识我们的地方、在那里……」
					PRINTFORML 「我们可以举办一个小型的婚礼」
					PRINTFORML 「要把依姬叫过来、啊，还有八意大人……」
					PRINTFORMDW 总觉得话题突然加速了……
				CASE 7
					PRINTFORML 「嗯？想听我讲以前的事吗？」
					PRINTFORML 「这样啊……那么……」
					PRINTFORMDW %CALLNAME:MASTER%从%CALLNAME:TARGET%那里听到了%TEXTR("令人怀念的往事/与依姬的回忆/有趣的故事/")%
				CASE 8
					CALL HPH_PRINT,@"「………HPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%一边聆听着%CALLNAME:MASTER%的话语、一边投以热切的视线
					PRINTFORML 「……啊？没关系、请继续吧？」
					PRINTFORMW 「你的话、我一直都在听哦」
				CASE 9
					PRINTFORML 「呐……可以叫我一声『%CALLNAME:TARGET%（とよねぇ）』吗？」
					PRINTFORML 「………啊呀呀♪」
					PRINTFORMW 「心脏……怦怦直跳……♪」
			ENDSELECT
		;炮友
		ELSEIF TALENT:炮友
			SELECTCASE RAND:10
				CASE 0
					PRINTFORML 「嘿，和我在一起开心吗？」
					PRINTFORML 「——啊，不，只是随口问问而已」
					PRINTFORMW 「没什么。真的哦？」
				CASE 1
					PRINTFORML 「本来想成为可靠的姐姐」
					PRINTFORML 「但现实却总是靠着别人。无论是依姬还是你」
					PRINTFORMW 「——为什么会谈起这种话题呢？呵呵，不过想来的话，你还不错」
				CASE 2
					PRINTFORML 「话说回来……你是怎么看依姬的？」
					PRINTFORML 「你看，她那样的性格，对男人一点兴趣都没有」
					PRINTFORML 「你的话，我觉得可以信赖，可以放心交给你」
					PRINTFORMDW 豊姬似乎想要把依姬和自己联系起来……
				CASE 3
					;豊姫が兎たちに向ける優しさは池の鯉に向けるものと同程度、という考察
					PRINTFORML 「如果有兔子袭击你，你可以反击哦」
					PRINTFORML 「反正死了还能找个替代品……嘛，当然是最好不要死」
					PRINTFORMDW 不像对待妹妹那样，豊姬对每只兔子并没有太多的情感……
				CASE 4
					PRINTFORML 「哎呀哎呀，又想要做了？」
					PRINTFORML 「嗯呢……怎么办才好呢……」
					PRINTFORMDW 豊姬做出故意犹豫的样子……
				CASE 5
					PRINTFORML 「你其实是个挺肉食的类型呢」
					PRINTFORML 「呵呵，不知怎么的，感觉和你有亲近感」
					PRINTFORMDW %CALLNAME:MASTER%想要问是什么意思，但是豊姬笑而不答……
				CASE 6
					PRINTFORML 「如果我能继续接受你的堕落，我也能像八意那样吗？」
					PRINTFORML 「如果是这样的话，堕落到底也没什么不好的」
					PRINTFORMDW 这是不是开玩笑都不好说。%CALLNAME:MASTER%稍微有些担心起来了
				CASE 7
					PRINTFORML 「——你对铃瑚怎么看？」
					PRINTFORML 「那个迟钝而木讷的孩子。比之前那个还不好用，而且还会逃跑」
					PRINTFORML 「所以不能放松警惕。对于养宠物来说，她也太没用了」
					PRINTFORMDW 至少相比其他的兔子，她似乎被更加重视。%CALLNAME:MASTER%这样理解着
				CASE 8
					PRINTFORML 「——哎呀，有什么事要向我请教吗？」
					PRINTFORML 「呵呵，就交给姐姐吧♪」
					PRINTFORMDW 正是因为是这种关系，有些事情才能够谈论。%CALLNAME:MASTER%向豊姬请教了烦恼
				CASE 9
					PRINTFORML 「——喜欢性爱吗？」
					PRINTFORML 「非常喜欢。毕竟，通过这个可以与你连接在一起」
					PRINTFORMDW 豊姬毫不避讳地这样说道
			ENDSELECT
		;思慕
		ELSEIF TALENT:思慕
			SELECTCASE RAND:10
				CASE 0
					PRINTFORML 「你和依姬关系好吗？」
					PRINTFORML 「直接和那孩子“嘿”地打招呼就可以了」
					PRINTFORMW 「一開始会很害羞、但很快就会习惯所以没关系的」
				CASE 1
					PRINTFORML 「兔子之间也在谈论着关于你的话题哦」
					PRINTFORML 「欸？是怎么说你的？」
					PRINTFORML 「……………」
					PRINTFORMW 「呃……呃……还需要再了解一下才能知道」
				CASE 2
					PRINTFORML 「我为什么不去工作？」
					PRINTFORML 「啊、在你看来是这样吗？」
					PRINTFORMW 「读书、吃桃子、像这样做这些也是很忙的……」
				CASE 3
					PRINTFORML 「每100克桃子含有40kcal的热量」
					PRINTFORML 「也就是说、热量低、食用不易发胖」
					PRINTFORML 「所以吃多少都没问题」
					PRINTFORML 「然而……依姬她啊」
					PRINTFORML 「我一吃桃子、她就说我吃多了」
					PRINTFORMW 「我说我不会胖也不听……」
				CASE 4
					PRINTFORML 「啊……肚子有点饿了」
					PRINTFORML 「对了、有的桃子差不多熟了、一起去摘吧？」
					PRINTFORMW 「……啊、要对依姬保密哦、不然她就要说教了」
				CASE 5
					PRINTFORML 「你问我是怎么看待你的？」
					PRINTFORML 「嗯……可靠的弟弟……吧？」
					PRINTFORMDW 有个这么费事的姐姐、依姬也很辛苦啊
				CASE 6
					PRINTFORML 「道在近而求之于远」
					PRINTFORML 「走捷径不一定就是快的」
					PRINTFORMW 「啊、这是八意大人的口头禅」
				CASE 7
					PRINTFORML 「往事？让我想想……」
					PRINTFORMW 「那是大概三十六万……呃……一万四千年前」
				CASE 8
					PRINTFORML 「听我说哦、我看到了……」
					PRINTFORML 「依姬在镜子前、穿着自己的衣服、摆着姿势」
					PRINTFORML 「刚好我拿着相机路过、就顺手拍下来了」
					PRINTFORMW 「想看吗？那就下次只有你我两人的时候♪」
				CASE 9
					PRINTFORML 「一时有些迷惘……举个例子」
					PRINTFORML 「如果是%CALLNAME:MASTER%这样的『污秽』、接受下来、也无所谓吧」
					PRINTFORMW 「……这样说的话、妹妹会骂我的吧」
			ENDSELECT
		;それ以外
		ELSE
			$TOYOHIME_KAIWA
			SELECTCASE RAND:10
				CASE 0
					;５分の１の確率でフェムトファイバーについて語る
					IF !RAND:5
						PRINTFORMW 「你知道所谓的“费米纤维”吗？」
						;選択肢
						CALL ASK_YN("好的","不想听")
						;判定
						SELECTCASE RESULT
							;はい
							CASE 0
								PRINTFORML 「费米纤维、简单来说……呃、已经知道了？」
								PRINTFORMW 「是吗……遗憾」
								RETURN 1
							;いいえ
							CASE 1
								GOTO FEMTOFIBER
						ENDSELECT
						$FEMTOFIBER
						PRINTFORMW 「『费』……简单地说、就是『须臾』」
						PRINTFORMW 「说的是生物完全无法认知到的、非常短暂的时刻」
						PRINTFORMW 「而所谓时间……正是由无数的这些无法认知的时刻构成的」
						PRINTFORMW 「因为时间的最小单位『须臾』是无法被认知到的、所以生物才会认为时间是连续的」
						PRINTFORMW 「实际上、时间却是由这些短暂的时刻所组成的」
						PRINTFORMW 「绳子看起来是一整根、但是实际上却是无数细小的丝线编织而成」
						PRINTFORMW 「无法感觉到的细小纤维编制成的绳子、在你看来就是无限连续的物质、对吧」
						PRINTFORMW 「此时此刻、这根毫无杂质的绳子、拥有的是最高的强度」
						PRINTFORMW 「同时更完全不会被脏东西所污秽」
						PRINTFORMW 「用这样的绳子编织出来的更粗的绳、就是永不腐朽的绳索」
						PRINTFORMW 「而它从遥远的古代、就被用来防止不洁之人进入」\n（译注：解说来自东方儚月抄 ～ Silent Sinner in Blue 第19话、原作者直接把%CALLNAME:TARGET%的话原样搬上来了、译者也是搬的中译原文）
						PRINTFORMW 「这就是所谓的『费米纤维』、理解了吗？」
						PRINTFORML 
						EXP:MASTER:学習経験 ++
						TCVAR:99:350 += 1
						;選択肢
						CALL ASK_M("是", 1, "否", TCVAR:99:350 < 3)
						;判定
						SELECTCASE RESULT
							;はい
							CASE 0
								CALL M_KOJO_COLOR_K99
								PRINTFORMW 「真棒♪ 如果你还想听、我随时都可以告诉你哦」
								RESETCOLOR
								TCVAR:99:350 = 0
							;いいえ
							CASE 1
								PRINTFORML 「啊、对地上的人来说有点难吧？」
								PRINTFORMW 「那我再解释一遍」
								TIME += 10
								GOTO FEMTOFIBER
						ENDSELECT
					;それ以外の場合は会話再抽選
					ELSE
						GOTO TOYOHIME_KAIWA
					ENDIF
				CASE 1
					;依姫と面識が無い
					IF !CFLAG:98:面識
						PRINTFORML 「我妹妹……已经见过依姬了吗？」
						PRINTFORML 「那孩子要是偶尔也去海边看看、放松放松就好了」
						PRINTFORMW 「你不也这么认为吗？」
					;面識がある
					ELSE
						PRINTFORML 「呼嗯。真不容易啊……」
						PRINTFORMDW %CALLNAME:TARGET%附和着%CALLNAME:MASTER%的话
					ENDIF
				CASE 2
					;小説版儚月抄ほぼまんま
					PRINTFORML 「一年之中、春天温暖、夏天活泼、秋天丰饶、冬天孤寂」
					PRINTFORML 「蓝色星星下有着美丽的桃树、还有兔子们笑容不断的月之都……」
					PRINTFORMW 「到底是谁说月球表面是荒凉的世界呢？」
				CASE 3
					PRINTFORML 「像你这样、自由地在地上和月球之间穿梭、可不是什么正常的行为」
					PRINTFORML 「这样做的话、就算被杀、也不要有怨言哦……」
					PRINTFORMDL 在%CALLNAME:MASTER%身体僵硬的时候、%CALLNAME:TARGET%的表情突然缓和了下来
					PRINTFORML 「……呵呵、開玩笑的、不需要那么害怕」
					PRINTFORMW 「做个好孩子的话、没有人会伤害你的」
				CASE 4
					PRINTFORML 「你见过八意大人了吗？」
					PRINTFORML 「她是我们姐妹的老师」
					PRINTFORML 「她非常有才华、多亏了她、我们才能……」
					PRINTFORMW 「……哎呀、感觉气氛热烈起来了啊」
				CASE 5
					PRINTFORML 「啊……最近什么事都没有、很闲啊」
					PRINTFORMW 「偶尔也去参加一下兔子们的训练吧」
				CASE 6
					PRINTFORML 「对了、兔子们没有给你惹麻烦吧？」
					PRINTFORML 「她们里面有很多迟钝的家伙、我有点担心」
					PRINTFORMW 「……言过其实？你很温柔啊」
				CASE 7
					PRINTFORML 「啊、对了、如果看到妖精、请告诉我或我妹妹」
					PRINTFORML 「对我们月人而言、那就是一团污秽的聚合体」
					PRINTFORML 「不把它捣碎、会有很多麻烦的」
					PRINTFORMW 「就像是……你们在遇到一群小虫子时一样、会很崩溃吧？」
				CASE 8
					PRINTFORML 「嗯……真是有趣的故事啊」
					PRINTFORMW 「兔子们听到的话、一定也会很高兴的」
				CASE 9
					PRINTFORML 「嗯？对这把扇子有兴趣吗？」
					PRINTFORML 「小心、像这样对着自己扇动、是自杀行为哦」
					PRINTFORML 「哦？你以为我在開玩笑？不害怕的话、我给你扇扇吧」
					PRINTFORMW 「放心、一点儿痛苦都不会感觉到、就能登上极乐净土的」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
