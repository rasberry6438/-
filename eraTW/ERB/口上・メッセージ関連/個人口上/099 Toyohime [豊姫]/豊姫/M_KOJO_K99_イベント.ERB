SELECTCASE RESULT
	CASE 0
		SETBIT CFLAG:[[豊姫]]:约会後イベントフラグ , 1
		CFLAG:99:1000 = 1
		CFLAG:99:処女喪失記念日 = -1
		CFLAG:99:初吻喪失記念日 = -1
		CSTR:99:処女喪失履歴 = 献给了曾经的爱人
		CSTR:99:初吻喪失履歴 = 献给了曾经的爱人
		TALENT:99:無接吻経験 = 0
		TALENT:99:大胃王 = 1
		TALENT:99:舌頭靈活 = 0
		EXP:99:接吻経験 += 20
		EXP:99:Ｖ性交経験 += 10
		SIF TALENT:99:処女 == 1
			TALENT:99:処女 = 0
		PRINTFORMDW 变更完毕
	CASE 1
		PRINTFORMDW 不进行变更
ENDSELECT

