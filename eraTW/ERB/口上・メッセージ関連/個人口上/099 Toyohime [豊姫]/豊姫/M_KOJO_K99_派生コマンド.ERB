;1,シックスナイン
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_SCOM_K99_1
RETURN 0

@M_KOJO_MESSAGE_SCOM_K99_1
CALL M_KOJO_MESSAGE_SCOM_K99_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K99_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「舔......舔着......呼噜......♪HPH」","L"
						CALL HPH_PRINT,@"「呜......呜呜......吧唧......咕噜......♪HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 灵活地用舌头舔弄龟头,看到%CALLNAME:MASTER%还不太满足就会深深地含下阴茎
							PRINTFORMDW 只能任由摆布,%CALLNAME:MASTER%也忘记了侍奉,只顾享受着快感,口水从嘴边流出
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「呜呼......咕......♪ 呜咕......吧唧......♪HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 摇头吞吐阴茎,将口中的先走液吞下,感受着味道连花唇都跟着抽搐
						CALL HPH_PRINT,@"「咕噜......呜......♪HPH 噢......含着......♪HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 就像招待女郎一样,%CALLNAME:TARGET%全力侍奉让%CALLNAME:MASTER%舒服......
					CASE 2
						CALL HPH_PRINT,@"「吧唧......吧唧噗......♪HPH 呜噜......舔......♪HPH」","L"
						CALL HPH_PRINT,@"「一起......呜♪HPH 舒服起来吧......HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 时而激烈,时而温柔地......
							PRINTFORMDW 为了同时达到高潮,两人舔弄着彼此的性器......
						ENDIF
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「呜呜♪HPH 呜......咕......♪HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 将舌头滑入粘稠的蜜穴,缓缓舔弄那湿润的软肉
							PRINTFORMDW 接着用嘴吸吮那里,%CALLNAME:TARGET%的腰部就会微微颤抖
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「那,我开始了哦......HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:TARGET%跨坐在%CALLNAME:MASTER%的脸上,贪婪地吞吐着他的阴茎
							PRINTFORMDW 一定是一副很放荡的表情吧。有点遗憾自己看不到那景色
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「呀啊♪HPH 啊哈,就是这样......♪HPH」","L"
						CALL HPH_PRINT,@"「用舌头,多多品尝......♪HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW %CALLNAME:MASTER%和%CALLNAME:TARGET%贪婪地舔弄彼此的性器,用溢出的爱液润湿彼此的喉咙......
				ENDSELECT
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「呼噜......呼噜噗......♪HPH」","L"
						CALL HPH_PRINT,@"「呜呼......咕......呜呼噜......♪HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:TARGET%闭上眼专心舔弄%CALLNAME:MASTER%的阴茎
							PRINTFORMDW %CALLNAME:MASTER%放松身体,集中精神迎接高潮
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「咕......吧唧......♪HPH 呜......♪HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 热心地吞吐阴茎的%CALLNAME:TARGET%得到了他要射精的预告
							PRINTFORMDW %CALLNAME:TARGET%没有拉开嘴,而是含得更深,将肉棒吞到更里面
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「呼噜,吧唧......要去了吗?HPH」","L"
						CALL HPH_PRINT,@"「没关系,就这样......呜,吧唧♪HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 一边吞吐阴茎,一边微微摇头的%CALLNAME:TARGET%
							PRINTFORMDW %CALLNAME:MASTER%射精时,%CALLNAME:TARGET%停下动作,在口中接住喷出的白浊
						ENDIF
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「看,舌头停下来了吧......?HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 跨坐在%CALLNAME:MASTER%脸上的%CALLNAME:TARGET%,摇动腰部蹭着秘所碾压他的嘴......
						CALL HPH_PRINT,@"「让彼此都舒服起来......好吗HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「呵呵,这里是弱点呢......HPH」","L"
						CALL HPH_PRINT,@"「呜噜......吧唧......♪HPH 舔,舔着......♪HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 描着圈舔弄,集中攻击顶端的%CALLNAME:TARGET%
							PRINTFORMDW %CALLNAME:MASTER%也卖力地舔弄,但主导权完全被%CALLNAME:TARGET%把持
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「很不错哦......HPH 不过,还差的远呢......HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 她像是在说「你看好了」,压低腰阻止了%CALLNAME:MASTER%的侍奉
							PRINTFORMDL 接着%CALLNAME:TARGET%将%CALLNAME:MASTER%的阴茎整根含入......
						ENDIF
						CALL HPH_PRINT,@"「呜......呜......♪HPH 呜噗......呼噜......♪HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 收紧咽喉,缓缓摇动起头来......
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
