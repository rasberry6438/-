								CALL HPH_PRINT,@"「看不见的话……感觉变得敏锐起来……HPH」","L"
								CALL HPH_PRINT,@"「咕……HPH 变得好奇怪……HPH」","W"
						ENDSELECT
					;受虐属性２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「我……还是更喜欢看着你的脸做……」
								SIF !CFLAG:99:1998
									PRINTFORMDW 戴着眼罩的%CALLNAME:TARGET%似乎有点不满
							CASE 1
								PRINTFORML 「……那个、还是拿掉比较好吧？」
								PRINTFORML 「看不到你的脸……有点可怕」
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%害怕的这种姿态更煽动起了%CALLNAME:MASTER%的嗜虐心
							CASE 2
								PRINTFORML 「这样就好了吗、%CALLNAME:MASTER%？」
								PRINTFORML 「……%CALLNAME:MASTER%？」
								PRINTFORML 「说……说话啊、为什么沉默着……？」
								SIF !CFLAG:99:1998
									PRINTFORMDW 沉默了一会儿、%CALLNAME:TARGET%的反应相当撩人
						ENDSELECT
				ENDSELECT
			;それ以外
			ELSE
				;受虐属性で分岐
				SELECTCASE ABL:受虐属性
					;受虐属性２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「摘掉眼罩之后、如果你不在了的话……HPH」","L"
								CALL HPH_PRINT,@"「……呼呼HPH 简直让人更兴奋了不是么……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%淫乱地妄想着、流出的蜜液在腿上滴下、真的放任不管吗……
							CASE 1
								CALL HPH_PRINT,@"「那么、接下来会做什么呢……HPH」","L"
								CALL HPH_PRINT,@"「诶嘿嘿……っHPH 好兴奋啊……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%对接下来的行为充满了期待……
							CASE 2
								CALL HPH_PRINT,@"「嗯……稍等一下……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:MASTER%让%CALLNAME:TARGET%戴上眼罩、%CALLNAME:TARGET%乖乖地照做了
						ENDSELECT
					;受虐属性２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「装上这个就好了？」
								PRINTFORML 「……嗯、黑漆漆的、什么也看不见」
								PRINTFORMW 「喂、你还在那里吗？」
							CASE 1
								PRINTFORML 「呵呵、好黑啊、我想起来小时候和依姬一起探险的日子」
								PRINTFORML 「被关在漆黑的房间里……那孩子哇哇地哭了呢」
								SIF !CFLAG:99:1998
									PRINTFORMDW 回忆也没多久、%CALLNAME:MASTER%又開始了行动
							CASE 2
								PRINTFORML 「一戴上这个就困了……」
								PRINTFORML 「嗯……如果睡着了就叫醒我……」
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%已经困倦了
						ENDSELECT
				ENDSELECT
			ENDIF
		;解除
		ELSE
			;受虐属性で分岐
			SELECTCASE ABL:受虐属性
				;受虐属性２以上
				CASE IS > 1
					CALL HPH_PRINT,@"「喜欢看着你的脸做……HPH」","L"
					CALL HPH_PRINT,@"「但偶尔这样也不错……HPH」","W"
				;受虐属性２未満
				CASEELSE
					PRINTFORML 「唔……好刺眼……」
					SIF !CFLAG:99:1998
						PRINTFORMDW %CALLNAME:TARGET%眨了眨眼睛
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
