	IF FLAG:時間停止
		PRINTFORMDL 
		RETURN 1
	;通常
	ELSE
		;装着
		IF TEQUIP:眼罩
			;恋慕
			IF TALENT:恋慕
				;受虐属性で分岐
				SELECTCASE ABL:受虐属性
					;受虐属性２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「啊……等等、不要说话……HPH」","L"
								CALL HPH_PRINT,@"「不知道要被做什么、这种令人兴奋的感觉……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW 自发戴上眼罩的%CALLNAME:TARGET%兴奋地等待着下一步的行为……
							CASE 1
								CALL HPH_PRINT,@"「呵……什么都看不见了呢……HPH」","L"
								CALL HPH_PRINT,@"「这样就……兴奋起来了呢……HPH」","L"
