					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「我喜欢看到别人被束缚……」
								PRINTFORML 「……而不是自己啊！」
								IF !CFLAG:99:1998
									PRINTFORMDL 有必要让你了解一下被缚者的心情！
									PRINTFORMDW %CALLNAME:MASTER%用绳子狠狠地绑住了%CALLNAME:TARGET%
								ENDIF
							CASE 1
								PRINTFORML 「嗯……绳子陷进去了……」
								IF !CFLAG:99:1998
									PRINTFORMDL 为了不让%CALLNAME:TARGET%动弹、紧紧地束缚住她、让她坐下
									PRINTFORMDW 可能还没习惯被束缚、她的身体绷得很紧
								ENDIF
							CASE 2
								PRINTFORML 「唔……不能再放松一下吗……？」
								PRINTFORML 「会留下痕迹的……」
								SIF !CFLAG:99:1998
									PRINTFORMDW %CALLNAME:TARGET%似乎很担心的样子……
						ENDSELECT
				ENDSELECT
			ENDIF
		;解いた時
		ELSE
			;受虐属性で分岐
			SELECTCASE ABL:受虐属性
				;受虐属性２以上
				CASE IS > 1
					CALL HPH_PRINT,@"「下次把你也绑起来吧……？HPH」","L"
					CALL HPH_PRINT,@"「啊、不用客气哦……HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW %CALLNAME:TARGET%出于纯粹的善意想要束缚%CALLNAME:MASTER%、郑重地拒绝了
				;受虐属性２未満
				CASEELSE
					PRINTFORML 「啊……这个痕迹会消失吗……」
					SIF !CFLAG:99:1998
						PRINTFORMDW %CALLNAME:TARGET%用手摩挲着皮肤上的绳痕
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

