RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMDL 
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;受虐属性で分岐
			SELECTCASE ABL:受虐属性
				;受虐属性２以上
				CASE IS > 1
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「咕呜……HPH 啊啊……HPH 噫呀……HPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL 每次拍屁股的时候、%CALLNAME:TARGET%都会发出jiao'chuan
								PRINTFORMDW 从被虐的喜悦中觉悟的%CALLNAME:TARGET%的身体、认识到疼痛是最舒服的东西……
							ENDIF
						CASE 1
							CALL HPH_PRINT,@"「啊……噶……HPH 再多的……っHPH」","L"
							CALL HPH_PRINT,@"「还……没有满足……っHPH 啊啊……HPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL 被拍打了好几下、肿胀的部分发出灼烧般的疼痛
								PRINTFORMDW 这种痛楚正是%CALLNAME:TARGET%所追求的……
							ENDIF
						CASE 2
							CALL HPH_PRINT,@"「不……不行……HPH 被那样拍打的话……HPH」","L"
							CALL HPH_PRINT,@"「我的屁股……要坏掉了啊啊啊啊————————HPH HPH HPH」","L"
							SIF !CFLAG:99:1998
								PRINTFORMDL 手臂像鞭子一样抽动、使出浑身的力气去打%CALLNAME:TARGET%的臀部
							CALL HPH_PRINT,@"「啊……啊咕……っHPH 叽……呜哈……っHPH」","L"
							SIF !CFLAG:99:1998
								PRINTFORMDW 白皙与红肿的肌肤相对比、就像桃子一样美丽……
					ENDSELECT
				;受虐属性２未満
				CASEELSE
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「噶！…… 好痛！……」
							PRINTFORML 「……没关系……继续……呜！」
							SIF !CFLAG:99:1998
								PRINTFORMDW %CALLNAME:TARGET%眼中含着泪水、接受了%CALLNAME:MASTER%的行为……
						CASE 1
							PRINTFORML 「没关系……不用担心我……」
							PRINTFORML 「咕哇！……就……这样……啊！」
							IF !CFLAG:99:1998
								PRINTFORMDL %CALLNAME:MASTER%继续毫不留情地敲打忍耐着的%CALLNAME:TARGET%的屁股
								PRINTFORMDW 令观者着迷的漂亮桃臀肿得惨不忍睹……
							ENDIF
						CASE 2
							PRINTFORML 「嗯……嗯……哈啊……」
							PRINTFORML 「已经……可以了吗……？」
							SIF !CFLAG:99:1998
								PRINTFORMDW %CALLNAME:MASTER%温柔地抚摸着因疼痛而肿胀的%CALLNAME:TARGET%的臀部……
					ENDSELECT
			ENDSELECT
		;それ以外
		ELSE
			;受虐属性で分岐
			SELECTCASE ABL:受虐属性
				;受虐属性２以上
				CASE IS > 1
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「呼……唔嗯……HPH 再用力一些……っHPH」","L"
							CALL HPH_PRINT,@"「我还要……折磨我……っHPH 咕……HPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL 看到%CALLNAME:TARGET%很是喜欢的样子、答应了她的请求
								PRINTFORMDW %CALLNAME:MASTER%一直拍她的屁股、直到她满意为止……
							ENDIF
						CASE 1
							CALL HPH_PRINT,@"「咕……HPH 啊……HPH 嗯呀……HPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL 每次被%CALLNAME:MASTER%打屁股、%CALLNAME:TARGET%都发出很舒服的呻吟
								PRINTFORMDW %CALLNAME:TARGET%的身体如此顺利地接受疼痛、肯定有着受虐的天赋吧
							ENDIF
						CASE 2
							CALL HPH_PRINT,@"「不行……不要太温柔……っHPH」","L"
							CALL HPH_PRINT,@"「没有疼痛的话……就无法舒服起来……っHPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL 屁股都已经被拍红了、对%CALLNAME:TARGET%而言还不够
								PRINTFORMDW %CALLNAME:MASTER%使出浑身的力气、狠狠地打在了她的屁股上
							ENDIF
					ENDSELECT
				;受虐属性２未満
				CASEELSE
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「呜……啊……嗯……」
							PRINTFORML 「好过分……怎么突然打我……」
							SIF !CFLAG:99:1998
								PRINTFORMDW %CALLNAME:TARGET%的屁股上还清晰地留有%CALLNAME:MASTER%的手印……
						CASE 1
							PRINTFORML 「连八意大人都没打过……哇！」
							IF !CFLAG:99:1998
								PRINTFORMDL %CALLNAME:MASTER%每次拍%CALLNAME:TARGET%屁股的时候、都会响起清脆的打击声
								PRINTFORMDW %CALLNAME:TARGET%紧咬着嘴唇、强忍着疼痛……
							ENDIF
						CASE 2
							PRINTFORML 「好痛……唔哇！ 噶……！ 啊！」
							IF !CFLAG:99:1998
								PRINTFORMDL %CALLNAME:MASTER%为了不让%CALLNAME:TARGET%逃跑、抓住了她的腰
								PRINTFORMDW 狠狠地打了她几下屁股……
							ENDIF
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

