@M_KOJO_MESSAGE_COM_K75_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL K75_POISON_COUNTER(2)
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:75:人形 == 99
			IF TALENT:恋慕 || TALENT:恋人
				IF TALENT:無知
					PRINTFORML 抚摸着梅蒂欣那陶瓷般的白皙肌肤
					PRINTFORML 梅蒂欣带着不可思议的表情注视着%CALLNAME:MASTER%的好意
					PRINTW 
					PRINTFORML 「这个是、为了什么而做的呢？」
					PRINTFORML 「并不讨厌哇。只是、不明白理由而已」
					PRINTW 
					PRINTFORML 告诉了梅蒂欣、这是男女加深关系的行为
					PRINTFORML 梅蒂欣虽然表示了初步的理解、但似乎还有疑问
					PRINTW 
					PRINTFORML 「我是明白、%CALLNAME:MASTER%是很珍视我的」
					PRINTFORML 「我也、多触摸%CALLNAME:MASTER%的肌肤就可以了呢」
					PRINTW 
					PRINTFORML 梅蒂欣也模仿着%CALLNAME:MASTER%的动作从胸口摸索了过来
					PRINTFORML 虽说手法很慢且笨拙、但毫无疑问是在回报着情爱的情感
					PRINTW 
					PRINTFORML 「%CALLNAME:MASTER%、更多地触摸我吧。我也、想要更多地触摸%CALLNAME:MASTER%」
					PRINTFORML 梅蒂欣张開了双手、微笑着…
				ELSE
					PRINTFORML 「你触摸的方法、喜欢哟。十分的温柔」
					PRINTFORML 梅蒂欣的语调很温和
					PRINTW 
					PRINTFORML 「虽然我觉得、这个和真正意义上的舒服不一样…
					PRINTFORML 　你触摸到的部分很温暖、好像有什么东西从我的内心深处溢出来了」
					PRINTW 
					PRINTFORML 「这肯定、这就是对于身为人偶的我来说的『很舒服』对吧」
					PRINTFORML 「还是说、%CALLNAME:MASTER%想要看到不一样的反应呢。如果是这样的话、对不起」
					PRINTW 
					CALL ASK_YN("没必要介意","现在放弃还太早了")
					IF !RESULT
						PRINTFORML 轻轻地抚摸着梅蒂欣垂下的头、吻着她的脸颊
						PRINTFORML 告诉她重要的并不是做出怎样的反应、而是梅蒂欣是怎么感觉的
						PRINTW 
						PRINTFORML 「谢谢你、%CALLNAME:MASTER%」
						PRINTFORML 「『開心』『温暖』『喜欢』『想要被多摸摸』」
						PRINTFORML 「仅仅是被你触摸着、我的心就会如此动摇着」
						PRINTFORML 「这也是、一种sex吧」
						PRINTW 
						PRINTFORML 从幼女体型般的梅蒂欣口中说出「sex」这样生动的话语令人激动不已
						PRINTFORML 即使无法像人类那样做出反应、她也会以自己的方式来接受sex这一行为的样子
						PRINTW 
						PRINTFORML 这样的话、就用两个人的方式来摸索相爱的方法吧
						PRINTFORML 一边这样想着、一边继续精心地爱抚着梅蒂欣…
						PRINTW 
					ELSE
						PRINTFORML 人偶肯定也是存在舒服起来的手段的
						PRINTFORML 而且、这样继续相爱下去的话、也许就会发生改变
						PRINTW 
						PRINTFORML 回应着%CALLNAME:MASTER%的话语、梅蒂欣只是微微一笑
						PRINTW 
					ENDIF
				ENDIF
			ELSE
				IF TALENT:無知
					PRINTFORML 抚摸着梅蒂欣那陶瓷般的白皙肌肤
					PRINTFORML 梅蒂欣带着不可思议的表情注视着%CALLNAME:MASTER%的好意
					PRINTW 
					PRINTFORML 「而且、这到底有什么意义？」
					PRINTFORML 「被人类触摸着、并没有什么很好的感觉」
					PRINTW 
					PRINTFORML 告诉了梅蒂欣、这是男女加深关系的行为
					PRINTFORML 然而梅蒂欣、即使理解了字面意思却依旧无法信服
					PRINTW 
					PRINTFORML 「做这种事情、我和你就能关系变好吗？」
					PRINTFORML 询问她实际被触摸了之后感觉如何
					IF CFLAG:75:信賴度 >= 500
						PRINTFORML 「这样子的、我不明白哇」
						PRINTFORML 梅蒂欣把脸扭向另一边
					ELSE
						PRINTFORML 「总觉得被蛞蝓爬来爬去比这个更好一点呢」
						PRINTFORML 收到了十分严厉的评价…
					ENDIF
					PRINTW 
				ELSE
					PRINTFORML 「人类、也会对人偶有欲情的吗？」
					PRINTFORML 「还是说、只有你会这样呢。呐、变态桑」
					PRINTW 
					PRINTFORML 梅蒂欣理解了这种行为的意义
					PRINTFORML 在此基础上、似乎是在挑衅着这边
					PRINTW 
					PRINTFORML 为了让那样的梅蒂欣可爱地啼叫而执拗地爱抚着
					PRINTFORML 「只是这样的话、是无法让身为人偶的我的身体感到舒服的哇」
					PRINTW 
					PRINTFORML 「还要更多。更多地把你那肮脏、悲惨、一心一意的情欲发泄在我身上」
					PRINTFORML 「人类的欲望、我想要知晓全部哇」
					PRINTW 
				ENDIF
			ENDIF
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;扶她
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:恋人
		;人形の乙女
		IF TALENT:75:人形 == 99
			SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「%CALLNAME:MASTER%的手、好温暖」
			CASE 1
			CASE 2
			CASE 3
				PRINTDATADL 
					DATAFORM 「%CALLNAME:MASTER%的手、好温暖」
					DATAFORM 「表情看起来很舒服呢」
					DATAFORM 「并不讨厌哇。只是觉得很无聊」
					DATAFORM 「我看起来也很開心？　是这样吗？」
					DATAFORM 「只有你一人噢。想让我做这种事情的」
					DATAFORM 「好的好的、老样子的那个呢」
					DATAFORM 「让你開心起来的事情也变得不是那么讨厌了呢」
				ENDDATA
			ENDSELECT
		ELSE
		ENDIF
		RETURN 1
	ELSE
		;人形の乙女
		IF TALENT:75:人形 == 99
		ELSE
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
