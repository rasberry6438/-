	SOURCE:快Ｍ = ABL:PLAYER:舌 * 6 + 20
	;時間停止中は駄目
	IF FLAG:70
	ELSE
		IF !RAND:3 == 0
			CALL AddEXP("愛情経験", TARGET, 1)
		ENDIF
	ENDIF
ENDIF

