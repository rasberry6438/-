;121,貝あわせ
;==================================================
@M_KOJO_MESSAGE_COM_K151_121
CALL M_KOJO_MESSAGE_COM_K151_121_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K151_121_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF FIRSTTIME("YMScissoring")
		SELECTCASE K151_YM_NATURE()
			CASE 0, 1
				PRINTFORMW 「这是什么?」
				PRINTFORMDW 幽幻魔眼在你压在她身上磨豆腐时面无表情地看着你
				PRINTFORMW 「...你在做什么?这不是一种生殖行为」
				PRINTFORMW 「我们不单纯追求快乐.我们需要交配.让我们召集触手...」
				PRINTFORMW 「...噢?这样的行为有助于你分泌蜜汁，以便后来的触手交配和你的受孕?」
				PRINTFORMW 「那么，我们会尝试这一行为」

			CASE 2
				PRINTFORMW 「嗯?你在做什么?」
				PRINTFORMDW 幽幻魔眼在你压在她身上磨豆腐时面无表情地看着你
				PRINTFORMW 「用那种成人道具不是更加轻松?或者我可以召集我的触手」
				PRINTFORMW 「...你真的很想做这个?行吧，随它去吧...」
		ENDSELECT

	ELSE

		IF ABL:151:腰 >= 5
			SELECTCASE K151_YM_NATURE()
				CASE 0, 1
					SELECTCASE RAND:3
						CASE 0
							PRINTFORMDW 幽幻魔眼在吸胸部的同时，手指也没有停下来
							PRINTFORMW 「喜欢这样吗?」
						CASE 1
							PRINTFORMDW 在用她的%FSYN_CN("阴道")%摩擦你的时候，幽幻魔眼的手指游走遍你的全身
							PRINTFORMDW 每次攻击到你的弱点的时候，娇喘声都不经意从嘴中发出
						CASE 2
							PRINTFORMDW 幽幻魔眼正在戏弄你.一开始她尽用她所能的速度来摩擦你的阴道，就在你快要高潮的时候，她却又慢了下来
							PRINTFORMDW 「我们不允许你在这种事情上耗费精力，你应该更加注重于正常的生殖行为」
							PRINTFORMDW ...你发誓你从她脸上看出了笑意
					ENDSELECT

				CASE 2
					SELECTCASE RAND:3
						CASE 0
							PRINTFORMDW 在幽幻魔眼取悦你的时候，你可以感觉到她温热的气息吐在脸上
							PRINTFORMW 「感觉很舒服吗?」
							PRINTFORMDW 在开始正事之前，她四处寻找着你最敏感的地方
						CASE 1
							PRINTFORMDW 幽幻魔眼一边亲吻着你的脖子，一边缓慢地摩擦着你
							CALL HPH_PRINT, @"「唔...你已经湿透了，不是吗?你这个坏孩子HPH」", "W"
						CASE 2
							PRINTFORMDW 当她在摩擦你的时候,幽幻魔眼将手指伸入了你的雌腔中，带出了一点蜜汁
							PRINTFORMDW 她将它送入嘴中，舔了舔手指
							PRINTFORMW 「呣...看来某人兴奋起来了〜」
					ENDSELECT
			ENDSELECT
		ELSEIF ABL:151:腰 >=3

			SELECTCASE K151_YM_NATURE()
				CASE 0, 1
					SELECTCASE IFRAND("0TO1", 1, "2", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 0, "3", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 1)
						CASE 0
							PRINTFORMW 「让我们试试这个姿势」
							PRINTFORMDW 幽幻魔眼挪动着身子，然后继续摩擦着你
						CASE 1
							PRINTFORMDW 幽幻魔眼摩擦着你的时候，笨拙地爱抚着你的身体
						CASE 2
							PRINTFORMW 「这很...啊！舒服...」
							PRINTFORMDW 在摩擦着你的时候，幽幻魔眼看上去有一点紧张
						CASE 3
							PRINTFORMDW 幽幻魔眼的技术弥补了她身躯冰冷的缺点
							PRINTFORMDW 大体上是的
					ENDSELECT
				CASE 2
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「啊...我感觉我找到了！」
							PRINTFORMDW 在摩擦到某处的时候，幽幻魔眼双眼放光
						CASE 1
							PRINTFORMDL 在磨擦你的时候，幽幻魔眼缓慢地爱抚着你
							PRINTFORMW 「感觉哪个更好呢？我的手，还是我的技术？」
						CASE 2
							PRINTFORMDW 当她在摩擦你的时候，幽幻魔眼吮吸着你的欧派
							PRINTFORML 「你哪边最敏感呢...？这边？还是这边？」
					ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE K151_YM_NATURE()
				CASE 0, 1
					SELECTCASE RAND:3
						CASE 0
							PRINTFORMW 「或许我们需要召唤一只触手来扩张你的雌腔...」
						CASE 1
							PRINTFORMDW 幽幻魔眼在试图磨你的时候差点撞上了你的脸
						CASE 2
							PRINTFORMW 「你感觉到什么了吗？至少我们没有」
					ENDSELECT

				CASE 2
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「或许我们应该这么做...？」
							PRINTFORMDW 幽幻魔眼又换了一个姿势想让自己舒服一点

						CASE 1
							PRINTFORMW 「我能直接把你吃了吗？这太无趣了」

						CASE 2
							PRINTFORMW 「我真的觉得我们应该召点触手...现在这样太无趣了」
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

