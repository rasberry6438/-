@M_KOJO_MESSAGE_COM_K151_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;ここに口上書いて下さい
	IF TFLAG:193 > -1
		IF RAND:3
			CALL K151_CONVERSATION_INTERACTION
			SIF RESULT
				RETURN 0
		ENDIF

		IF TALENT:[[ユウゲンマガン]]:恋人
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
				CASE 1
					PRINTFORML 
				CASE 2
					PRINTFORML 
			ENDSELECT
		ELSEIF TALENT:[[ユウゲンマガン]]:セフレ || TALENT:[[ユウゲンマガン]]:愛欲
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
				CASE 1
					PRINTFORML 
				CASE 2
					PRINTFORML 
			ENDSELECT
		ELSEIF TALENT:[[ユウゲンマガン]]:思慕 || TALENT:[[ユウゲンマガン]]:恋慕
			$YM_YEARNING_CONVOS
			SELECTCASE RAND:9
				CASE 0
					PRINTFORMDW 幽幻魔眼皱了皱眉头
					PRINTFORMW 「最近一看到你，这副躯体的血液就冲上了脸」
					PRINTFORMW 「你有什么头绪吗？这种情况十分罕见」
				CASE 1
					PRINTFORMW 「你们...怎么交朋友的？」
					PRINTFORMW 「看起来似乎其他人都有这层关系，我们却没有，还挺令人烦恼的」
					PRINTFORMDW 并非如此。还有你是她的朋友
					PRINTFORMW 「你？什么时候是的？又怎么成为朋友的？」
					PRINTFORMDW 你向她解释道朋友就是可以一起享受一段愉快时间的人
					PRINTFORMW 「啊...不过"享受"是什么意思？」
					PRINTFORMDW ...看来解释还得持续一段时间
					PRINTFORMW 在给幽幻魔眼解释了一大堆东西后，她将手放在胸口
					PRINTFORMW 「所以当我们在你面前时，那种奇怪的感觉是...愉悦？」
					PRINTFORMW 「然后这就意味着你和我们是朋友？」
					PRINTFORMDW 你点了点头，稍微有点不耐烦了
					PRINTFORMW 「那很好」
					PRINTFORMDW 她用她独特而又生涩地微笑着
					PRINTFORMW 「我很高兴...你是我们的朋友」

				CASE 2
					PRINTFORML 「什么是太阳？」
					PRINTFORMW 「是天照大神，还是一团炽热的等离子球体？」
					PRINTFORMW 「我们听到过许多相互矛盾的说法」

				CASE 3
					PRINTFORMW 「你知道命运的红线吗？」
					PRINTFORMW 「每个人都有一个命中注定的伴侣吗？」
					CALL ASK_YN("是的", "并非如此")
					IF !RESULT
						PRINTFORMW 「真有趣」
						PRINTFORMW 「只有你们这种物质存在才有吗？」
						PRINTFORMW 「...我们很好奇！仅此而已！」
						PRINTFORMDW 但她通红的脸表示并非如此
					ELSE
						PRINTFORMW 「这只是一种迷信？真可惜」
					ENDIF

				CASE 4
					SIF !CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 0
						GOTO YM_YEARNING_CONVOS
					PRINTFORMW 「我们理解这可能是一个很怪的问题...」
					PRINTFORMW 「不过...如果你关心的人一直保留着一个秘密而且还在对你说谎」
					PRINTFORMW 「在他坦白之后，你会原谅他吗？」
					CALL ASK_YN("会的", "我想不会")
					IF !RESULT
						PRINTFORMW 「...」
						PRINTFORMW 「你很善良」
						PRINTFORMW 「我认为其他人可能...」
					ELSE
						PRINTFORMW 「...」
						PRINTFORMW 「我知道了...」
						PRINTFORMDW 幽幻魔眼将目光从你身上移开
					ENDIF

				CASE 5
					SIF !TALENT:妊娠
						GOTO YM_YEARNING_CONVOS
					PRINTFORML 「即使我们怀有子嗣，我们也没有理应分开」
					SELECTCASE K151_YM_NATURE()
						CASE 0
							PRINTFORMW 「我们必须一起抚养他...」
							PRINTFORMDW 幽幻魔眼祥和地闭上了眼
						CASE 1
							PRINTFORMDW 幽幻魔眼生涩地微笑着
							PRINTFORMW 「我们还要诞下更多。非常多...」
					ENDSELECT

				CASE 6
					SIF !TALENT:妊娠
						GOTO YM_YEARNING_CONVOS
					PRINTFORML 「如果我们的子嗣看起来很像我的话，不要太惊讶」
					PRINTFORMW 「这正是一种优越的证明」

				CASE 7
					SIF !FLAG:異常気象 == 7
						GOTO YM_YEARNING_CONVOS
					PRINTFORMW 「内衣在空气中到处乱飞」
					PRINTFORMW 「在你们世界这个很常见吗？」
					PRINTFORMW 「只发生在幻想乡？真奇怪...」

				CASE 8
					SIF !BASE:[[ユウゲンマガン]]:体力 < 750
						GOTO YM_YEARNING_CONVOS
					PRINTFORMDW 幽幻魔眼打了个哈欠
					PRINTFORMW 「真令人恼火...这副躯体出现故障了」
					PRINTFORMW 「如果我们仍然拥有力量的话，会破坏它然后再重新制作一个」
					PRINTFORMW 「真可惜，看来现在只能睡觉了」

				CASE 9
					PRINTFORMW 「真奇怪」
					PRINTFORMW 「*在下面*，我们可以通过自己的意志随意地改变世界。处非有一种更加强大的意志阻止我们」
					PRINTFORMW 「但在这个物质世界中，我们却无法做到...」
					PRINTFORMW 「难道说你们的世界本身就拥有自己的意志？」

			ENDSELECT
		ELSE
			SELECTCASE RAND:7
				CASE 0
					PRINTFORMW 「你问我什么是“下面”？」
					PRINTFORMDL 幽幻魔眼笑了，至少你觉得那是一个笑颜 
					PRINTFORMDW 比起吠叫，它更像是...有人告诉她笑该是什么声音，然后她面无表情的发了出来
					PRINTFORMDW 看来她之前从来没见过别人笑
					PRINTFORMW 「你不会理解的，那个地方超出你们的认知。让我们想想有没有什么方法可以用你们的语言描述它...」
					PRINTFORMDW 她停下来开始思考。幽幻魔眼一动不动，你留意了一下，确保不是停止了时间
					PRINTFORMW 「那边有...非常绚丽的颜色，但是会损害人的精神。我们喜欢它，毕竟我们不会遭受干扰」
				CASE 1
					PRINTFORMW 「你喜欢我们的眼睛吗？我们认为它们是最杰出的特征，世上独一无二」
					PRINTFORMW 「虽然你们人类的眼睛与我们不同。你不需要用眼睛去看，就可以直到我们来自哪里」
					PRINTFORMW 「那些眼睛只是我们在你们世界所展现的投影，我们也不知道为什么会是这样」
				CASE 2
					PRINTFORMW 「...你和神琦说过话了吗？」
					PRINTFORMW 「...明白了」
					PRINTFORML 「这个女人是一个欺诈者，不值得相信」
					PRINTFORMW 「不要相信任何她告诉你的事情。它只会把你带到毁灭边缘」
					PRINTFORMDW 幽幻魔眼看上去格外的紧张...
				CASE 3
					PRINTFORMW 「我们对那个叫古明地觉的个体很感兴趣。她的觉之瞳是她的一部分，还是她附属的另一个生物？亦或者是她的投影？」
					PRINTFORMW 「我们很好奇，但我们不希望她读取我们的思想」
					PRINTFORMW 「不...不只是因为我们有不能被世人所知的秘密。我们也担心她的安全」
					PRINTFORMDW 突然慌了一下呢
					PRINTFORMW 「如果她读取了我们的内心的话，我们担心她会发疯的」
				CASE 4
					PRINTFORMW 「萨拉还在为魔界当门卫吗？」
					PRINTFORMDW 幽幻魔眼叹了口气。真是个奇怪的人...
					PRINTFORMW 「为什么这些年她不和那个神社巫女斗争到底？为什么我们一定要做她的工作？」
					PRINTFORMW 「就算我们没有任何充足理由去恨她...」
				CASE 5
					PRINTFORMW 「我们对那位闭上觉之瞳的少女很感兴趣。我们五个共用一个身体，但是她一个人却没有任何的思想」
					PRINTFORMW 「如果她无意识的话...或许我们...可以在她不发疯的条件下和她讨论我们的世界？」
					PRINTFORMW 「如果我们可以和别人分享我们的秘密的话，或许...」
					PRINTFORMDW 幽幻魔眼看上去有点...寂寞？
				CASE 6
					PRINTFORMW 「你说，有人仿制了我的眼睛？有趣...」
					PRINTFORMW 「我们有必要好好感谢她，或许她可以成为我们的邪恶祭司...」
				CASE 7
					PRINTFORMW 「什么是"帕露"？」
					PRINTFORMW 「桥姬在我们没有听她说话的时候，经常发出这样的声音」
					PRINTFORML 「我们觉得那应该不是一个友善的词汇。一句咒骂？」
					PRINTFORMW 「不管那是什么，侮辱我们的话是不可容忍的」
					PRINTFORMDW 幽幻魔眼淡淡地笑了，眼神里充满着冷漠和怒火
					PRINTFORMW 「我们会在合适的时间...好好给她上一课的」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
