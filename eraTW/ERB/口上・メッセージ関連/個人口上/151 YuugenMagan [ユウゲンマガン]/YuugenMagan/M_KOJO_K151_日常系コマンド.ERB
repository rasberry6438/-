;301,お茶を淹れる
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_SUCCESS_COM_K151_301
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K151_301
CALL M_KOJO_MESSAGE_COM_K151_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K151_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FIRSTTIME("MaganTea")
		SELECTCASE K151_YM_NATURE()
			CASE 0, 1
				PRINTFORML 「你说,茶？」
				PRINTFORMDW 幽幻魔眼面无表情地看着杯子
				PRINTFORMW 「它对于你们而言有什么用吗？」
				PRINTFORMDW 你通俗地解释到，她可以把它倒在嘴里然后享受它的味道
				PRINTFORMW 「然后，就可以从中汲取营养...有趣」
				PRINTFORMW 「在我们的世界里，我们不需要这种东西」
				PRINTFORMW 「我们只需要...」
				PRINTFORMDW 幽幻魔眼拿起了茶杯抿了一口
				PRINTFORMW 「...嗯？」
				PRINTFORMDW 她又喝了一口
				PRINTFORMW 「我们无法理解你上面所说的话...但出于礼貌我们决定继续享用它」
				PRINTFORMW 「我们希望和你们这些本土生物建立更好的关系」
			CASE 2
				PRINTFORMW 「嗯？茶？」
				PRINTFORMW 「嘛，我一直喝冰咖啡来着...但我不介意试一下」
				PRINTFORMDW 幽幻魔眼端起茶杯轻轻抿了一口，眼睛亮了起来
				PRINTFORMW 「哇，真好喝！」
		ENDSELECT
	ELSE
		SELECTCASE TFLAG:193
			CASE 1
				SELECTCASE K151_YM_NATURE()
					CASE 0, 1
						IF FIRSTTIME("GoodTea")
							PRINTFORMW 「味道尝上去与众不同.某种方面上更加的...诱人？为什么？」
							PRINTFORMDW 你自豪地解释道，你是泡茶领域大神
							PRINTFORMW 「嗯哼？」
							PRINTFORMDW 幽幻魔眼的声音同往常一样不带有任何感情
							PRINTFORMW 「在你们的世界，任何行为的结果都不单一？它们有...」
							PRINTFORMDW 幽幻魔眼停下来思考了一下
							PRINTFORMW 「副产物，你们大概是这么描述的？即使是同一件事情发生也会产生完全不同的结果？」
							PRINTFORMW 「你们真是神奇的生物」
						ELSE
							PRINTFORMW 「你的茶又不纯净了，我的意思是好的那一方面上」
							IF CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 0
								PRINTFORMDW 幽幻魔眼在对你笑吗？
								PRINTFORMDW 当你一仔细观察她的表情，她的笑容立刻藏了起来，变回她原来面无表情的样子
							ENDIF
						ENDIF
					CASE 2
						PRINTFORMW 「哇哦，尝上去真不错！」
						PRINTFORMDW 幽幻魔眼一口闷了你的茶，目光闪闪地看着你
						PRINTFORMW 「改天教教我怎么样？」
				ENDSELECT
			CASE 0
				IF TALENT:[[ユウゲンマガン]]:恋人
					SELECTCASE K151_YM_NATURE()
						CASE 1
							SELECTCASE RAND:3
								CASE 0
									PRINTFORMW 「这种物质没有被施展过魔法吗？」
									PRINTFORML 「...真没有？那为什么我们摄取它时完全停不下来」
									PRINTFORMW 「仅仅是因为它来自你？」
								CASE 1
									PRINTFORML 「你给我们泡过多少茶了？」
									PRINTFORMW 「应该比宇宙中的星尘还要多了吧」
									PRINTFORMW 「不仅如此...即使这样，我们还没有喝腻」
								CASE 2
									PRINTFORMW 「也许我们应该向我们的信徒索要茶叶作为贡品？」
									PRINTFORMW 「嗯...或许也没必要。没人泡茶和你一样好」
							ENDSELECT
						CASE 2
							SELECTCASE RAND:3
								CASE 0
									PRINTFORML 「哼哼哼~等舞发现你的茶很好喝时会很嫉妒的！」
									PRINTFORMW 「诚然，她魔法天赋比我好。但她可没有一个可以去当泡茶专家的帅哥作为%PRINT_MALE("男", MASTER)%朋友」
									IF TALENT:105:恋人
										PRINTFORMW 「...啊等等，她有了」
										PRINTFORMDW 幽幻魔眼皱了皱眉
									ENDIF
								CASE 1
									PRINTFORML 「我感觉我开始对你的茶上瘾了」
									PRINTFORMW 「我都不记得上一次喝速溶咖啡是什么时候的事了！我之前可是一天能喝四杯哦！就一天！」
								CASE 2
									PRINTFORMW 「你觉得如果我用魔法把茶变成毒药的样子一口气喝下去，大伙儿会不会觉得我很酷！」
									PRINTFORMW 「...不会吗？好吧...」
							ENDSELECT
					ENDSELECT
				ELSEIF TALENT:[[ユウゲンマガン]]:セフレ || TALENT:[[ユウゲンマガン]]:愛欲
					SELECTCASE K151_YM_NATURE()
						CASE 1
							SELECTCASE RAND:3
								CASE 0
									PRINTFORML 
								CASE 1
									PRINTFORML 
								CASE 2
									PRINTFORML 
							ENDSELECT
						CASE 2
							SELECTCASE RAND:3
								CASE 0
									PRINTFORML 
								CASE 1
									PRINTFORML 
								CASE 2
									PRINTFORML 
							ENDSELECT
					ENDSELECT
				ELSEIF TALENT:[[ユウゲンマガン]]:思慕 || TALENT:[[ユウゲンマガン]]:恋慕
					SELECTCASE IFRAND("0TO1", 1, "2", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 0, "3", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 1)
						CASE 0
							PRINTFORML 「虽然我们不知道为什么...但我们确实很中意这个仪式」
							PRINTFORMDW 幽幻魔眼放下茶杯，用奇怪的笑容对着你笑了笑
						CASE 1
							PRINTFORMW 「(真奇怪...这杯茶很暖和，但又不像我们在\@ TALENT:MASTER:性別 == 2 ? 他的 # 她的\@视线里感到的那般温暖...)」
							PRINTFORMDW 当从你手中接过茶杯时，幽幻魔眼喃喃自语道
						CASE 2
							PRINTFORMW 「这个...很好喝」
							PRINTFORMDW 幽幻魔眼对着你笑了一会儿。不过在她发现你也在看着她时，又立刻变回了平常的模样
						CASE 3
							PRINTFORML 「你设计了一个很有意思的仪式」
							PRINTFORMW 「它唤醒了什么样的存在？祂会在我们喝茶之前，还是之后出现？」
					ENDSELECT
				ELSE
					SELECTCASE IFRAND("0TO1", 1, "2", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 0, "3", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 1)
						CASE 0
							PRINTFORML 「啊，所以这个仪式是设计用来摄取营养和促进理解的吗？聪明的想法」
							PRINTFORMDW 没有你想的那么多...
						CASE 1
							PRINTFORMW 「你是怎么知道这个仪式的？一位神明教授给你的？」
							PRINTFORMDW 你...说实话，你也不知道
							PRINTFORMW 「这种...泡茶的艺术？你是这么说的。已经古老到无法追溯到本源了吗？」
							PRINTFORMW 「真有趣...」
						CASE 2
							PRINTFORMW 「可以加更多的糖...但我觉得现在也可以了」
						CASE 3
							PRINTFORML 「当我们享受你的茶传来的温度的时候，实际上我们没有办法通过味蕾去品尝它的味道...所以我们无法真正享用它」
							PRINTFORMDW 幽幻魔眼看上去有点沮丧
							PRINTFORMW 「茶实际喝上去是什么味道的？」
					ENDSELECT
				ENDIF
			CASE -1
				SELECTCASE K151_YM_NATURE()
					CASE 0
						PRINTFORML 「这茶好难喝！」
						PRINTFORMDW 幽幻魔眼把茶放到一边怒气冲冲地看着你
						PRINTFORMW 「你是在提供变质的饮料来羞辱我们吗？！」
					CASE 1
						PRINTFORML 「嗯」
						PRINTFORMDW 幽幻魔眼喝茶的时候面无表情地看着你
						PRINTFORMW 「各种意义上，都不好喝」
					CASE 2
						PRINTFORML 「额」
						PRINTFORMDW 幽幻魔眼放下了茶杯，脸色有点难看
						PRINTFORMW 「我们不知道你放了什么，但这茶...好难喝」
				ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
