@M_KOJO_MESSAGE_COM_K151_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
	ELSE
		SELECTCASE TFLAG:193
			CASE 4
				IF TALENT:[[ユウゲンマガン]]:恋人
				ELSEIF TALENT:[[ユウゲンマガン]]:セフレ || TALENT:[[ユウゲンマガン]]:愛欲
				ELSEIF TALENT:[[ユウゲンマガン]]:思慕 || TALENT:[[ユウゲンマガン]]:恋慕
				ELSE
					PRINTFORMW 「啊！」
					PRINTFORMDW 幽幻魔眼惊呼一声。在你怀疑耳朵的时间里她立刻抓住了你伸向她屁股的手
					PRINTFORMW 「我们没有期望收到这样的回应...」
				ENDIF
			CASE 2,3
				IF TALENT:[[ユウゲンマガン]]:恋人
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 
						CASE 1
							PRINTFORML 
						CASE 2
							PRINTFORML 
					ENDSELECT
				ELSEIF TALENT:[[ユウゲンマガン]]:セフレ || TALENT:[[ユウゲンマガン]]:愛欲
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 
						CASE 1
							PRINTFORML 
						CASE 2
							PRINTFORML 
					ENDSELECT
				ELSEIF TALENT:[[ユウゲンマガン]]:思慕 || TALENT:[[ユウゲンマガン]]:恋慕
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 
						CASE 1
							PRINTFORML 
						CASE 2
							PRINTFORML 
					ENDSELECT
				ELSE
					SELECTCASE RAND:5
						CASE 0
							PRINTFORMW 「你在抚摸这副躯体的屁股？」
							PRINTFORMW 「它有什么不对劲的地方吗」
						CASE 1
							PRINTFORMW 「嗯，没错。根据我们的观察，人类普遍喜欢这副身体的这一部分」
							PRINTFORMW 「不过如果你是想和我们交配的话...我们拒绝」
						CASE 2
							PRINTFORMW 「我们身体这一部分有什么特别的吸引力吗？」
							PRINTFORMW 「几乎每个人类都有这一部分」
							PRINTFORMW 「还是说，你觉得在我们身上的这一部分...更加有吸引力？」
						CASE 3
							PRINTFORMW 「呣。我们创造了这副躯壳的臀部来吸引合格的伴侣...」
							PRINTFORMW 「但是星星尚未闪烁。请停下」
						CASE 4
							PRINTFORML 「你是打算向我们传递什么信号吗？」
							PRINTFORMW 「我们不需要这种东西」
					ENDSELECT
				ENDIF
			CASEELSE
				IF TALENT:[[ユウゲンマガン]]:恋人
				ELSEIF TALENT:[[ユウゲンマガン]]:セフレ || TALENT:[[ユウゲンマガン]]:愛欲
				ELSEIF TALENT:[[ユウゲンマガン]]:思慕 || TALENT:[[ユウゲンマガン]]:恋慕
				ELSE
					PRINTFORMDW 幽幻魔眼死死盯着你看
					PRINTFORMW 「把你的手从这副身体的臀部上拿开，否则...我们将会撕碎它」
				ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
