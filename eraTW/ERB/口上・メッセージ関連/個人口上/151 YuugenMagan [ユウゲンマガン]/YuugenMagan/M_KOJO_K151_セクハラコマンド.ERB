@M_KOJO_MESSAGE_COM_K151_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CHK_DATENOW(CFLAG:LOCAL:デート中)
	ELSEIF CFLAG:睡眠
	ELSE
		IF FIRSTTIME("YMHug")
			SELECTCASE K151_YM_NATURE()
				CASE 0, 1
					PRINTFORMW 「你这是打算吞噬我们？」
					PRINTFORMDW 幽幻魔眼面无表情的看着你
					PRINTFORMW 「你无法吞噬一个伟大的存在。我们建议你不要去尝试，否则你自己也会被吞入」
					PRINTFORMW 「...嗯？一个"拥抱"？你说这是一种好意的象征？」
					PRINTFORMW 「很好，我们会接受它的」
				CASE 2
					PRINTFORMDW 幽幻魔眼钻入了你的怀抱中，抚摸着你的背
					PRINTFORMW 「啊...让我想起了在家的时候」
					PRINTFORMW 「每一次神琦大人来拜访的时候，她总是会给我一个大大的拥抱!每一次都感觉我肋骨快断了~」
					PRINTFORMDW 她开心地笑了笑
					PRINTFORMW 「很高兴你也能这样温柔的拥抱我...」
			ENDSELECT
		ELSEIF TALENT:151:恋人
			SELECTCASE K151_YM_NATURE()
				CASE 1
					SELECTCASE TFLAG:193
						CASE 1
							PRINTFORML 「啊...」
							PRINTFORMDW 幽幻魔眼在你的怀抱中微微发抖
							PRINTFORML 「你的怀抱好温暖...」
							PRINTFORMDW 她所有的七只眼睛都带着欲望的看着你
							CALL HPH_PRINT, @"「现在，负起责任来给我们提供所需的东西HPH」", "W"
						CASE 0
							SELECTCASE RAND:6
								CASE 0
									PRINTFORMW 「如果我们有一颗心脏的话，它也许现在会砰砰乱跳吧...」
								CASE 1
									PRINTFORML 「不冷吗？待在我们身边」
									PRINTFORMDW 你摸了摸幽幻魔眼，她在晚上比想象的还要冷
									PRINTFORMW 「于我们而言，我们不希望现在的时光会结束...就算是被湮灭，我们也已经离不开你了」
								CASE 2
									PRINTFORML 「抱紧我们，亲爱的」
									PRINTFORMW 「让我们靠近一点吧...直到我们合二为一」
								CASE 3
									PRINTFORMW 「我们的躯体不是很冷，对吗？」
								CASE 4
									PRINTFORMDL 幽幻魔眼紧紧抱住了你
									PRINTFORMDW 她拥抱的技巧改善了不少...
								CASE 5
									PRINTFORML 「啊...这种情况下,我们可以很轻易地吞噬你」
									PRINTFORMW 「然后你将会永远的和我们合而为一...」
									PRINTFORMW 「这只是一个玩笑！别那么紧张！」
							ENDSELECT
						CASE -1
							PRINTFORMDW 当你打算走过去拥抱幽幻魔眼时，你撞上了她的一只眼睛
							PRINTFORMW 「啊！你还好吗,亲爱的！」
							PRINTFORMDW 你揉了揉撞到的部分，向她示意自己很好
							PRINTFORMW 「那就好。是我们的失误」
							PRINTFORMW 「我们..太...兴奋了，忘记了我们的真身是...」
					ENDSELECT

				CASE 2
					SELECTCASE TFLAG:193
						CASE 1
							PRINTFORMDL 幽幻魔眼的手在你们拥抱的时候从你的背上滑下，最后停在了你的屁股上
							PRINTFORMDW 她停顿了一下...你也打算把自己的手停在对方屁股上
							PRINTFORMDL 她噗一下笑出声来，轻轻按了按你的屁股
							CALL HPH_PRINT, @"「变态HPH」", "W"
						CASE 0
							SELECTCASE RAND:4
								CASE 0
									PRINTFORMDL 你感觉有啥黏糊糊的玩意儿在拍着你的屁股
									PRINTFORMDW 你将幽幻魔眼轻轻推开转身看去，正好看到一条触手从传送门里消失
									PRINTFORMDL 幽幻魔眼趁机靠了过来，在你耳边低语到
									PRINTFORMW 「嘿嘿....我抓到你啦」
								CASE 1
									PRINTFORMDL 幽幻魔眼倾身投入你的怀抱中，嗅着你的气息
									PRINTFORMW 「你闻上去真不错...」
									PRINTFORMDW 你白了她一眼。她用自己其中的一只眼球模仿着你
									PRINTFORMW 「怎么了?这在我们那边非常常见~」
								CASE 2
									PRINTFORMDW 在你们拥抱的时候，幽幻魔眼轻吻了你的脖颈
								CASE 3
									PRINTFORMDL 在你们拥抱的时候，你可以感受到幽幻魔眼温热的气息吐在你的脖颈上
									PRINTFORMW 「我是如此爱着你，亲爱的...」
							ENDSELECT
						CASE -1
							PRINTFORMDL 在你抱着幽幻魔眼的时候，你的手从她的背部开始慢慢往下
							PRINTFORMDW 往下...接着往下...当它到达她的屁股的时候，你的手突然被顶开了
							PRINTFORMDW 幽幻魔眼的脸唰的一下红了
							PRINTFORMW 「对不起，%CALLNAME:MASTER%.我以为你不会注意到我用魔法把我的屁股“增大”一点...」
					ENDSELECT

			ENDSELECT
		ELSEIF TALENT:151:セフレ || TALENT:151:愛欲
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
				CASE 1
					PRINTFORML 
				CASE 2
					PRINTFORML 
			ENDSELECT
		ELSEIF TALENT:151:思慕 || TALENT:151:恋慕
		SELECTCASE TFLAG:192
				CASE 1
					PRINTFORMDL 幽幻魔眼依偎在你的怀中，亲吻着你的脸颊
					CALL HPH_PRINT, @"「你尝起来非常美味HPH」", "W"
					PRINTFORMW 「啊...我们的错。旧习难改...」

				CASE 0
					SELECTCASE IFRAND("0TO1", 1, "2TO3", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 0, "4", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 1)
						CASE 0
							PRINTFORMDW 幽幻魔眼生疏地回抱了你
							PRINTFORMW 「是这样吗？」

						CASE 1
							PRINTFORMDW 在你拥抱她的时候尴尬地轻拍你的嗯托

						CASE 2
							PRINTFORMDW 当你抱着她的时候，你能感受到幽幻魔眼的心砰砰直跳
							PRINTFORMDW 看她那么慌乱感觉好可爱...

						CASE 3
							PRINTFORMDW 幽幻魔眼回抱了你并把脸搭在你的肩膀上
							PRINTFORMW 从来没见过她这样拥抱...
							PRINTFORMDW 你感觉有什么东西打湿了你的肩膀
						CASE 4
							PRINTFORMDW 在拥抱的时候，你轻抚着幽幻魔眼的后背...既是出于关爱，也是想让她暖和一点
							PRINTFORMDW 目前拥抱着她依然还是冷冰冰的...

					ENDSELECT

				CASE -1
					SELECTCASE K151_YM_NATURE()
						CASE 0
							PRINTFORMDL 拥抱幽幻魔眼时，你刚打算把手稍微往下一点，她就立刻把它拍开
							PRINTFORMW 「在这么做的话，我们会扯下你的双手」

						CASE 1
							PRINTFORMDL 你打算尝试一下，但这个女人身上散发出的不详的气息还是让你感到很难受
							PRINTFORMDW 你快速抱了幽幻魔眼一下，然后在你昏过去之前感觉放开了她
							PRINTFORMDW 直到你冷静下来之后，你才回到她身边...不过这一次离得稍微远了那么一点
					ENDSELECT

			ENDSELECT
		ELSE
			SELECTCASE TFLAG:192
				CASE 1
					SELECTCASE K151_YM_NATURE()
						CASE 0
							PRINTFORMDW 幽幻魔眼无力地抱住了你
						CASE 1
							PRINTFORMDW 幽幻魔眼紧紧抱着你，甚至把你抱了起来，你感觉自己双脚离地了
					ENDSELECT
					PRINTFORMDW ...至少她尝试过了？

				CASE 0
					SELECTCASE IFRAND("0", 1, "1TO2", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 0, "3TO4", CFLAG:[[ユウゲンマガン]]:K151EN_YM_NATURE == 1)
						CASE 0
							PRINTFORMDW 在你抱着她的时候，幽幻魔眼也将双手紧紧环绕着你...
							PRINTFORMDW 你感觉脊椎快断了
							PRINTFORML 「强壮...健康...五脏俱全...」
							PRINTFORMW 「你会成为一个合格的伴侣」
						CASE 1
							PRINTFORMDW 当你拥抱她的时候，幽幻魔眼仅仅是站在那边，和往常一样面无表情

						CASE 2
							PRINTFORMW 「(这个...好熟悉的感觉...)」
							PRINTFORMDW 当你拥抱她的时候，你听到了幽幻魔眼的低语.还是说这只是幻觉吗？

						CASE 3
							PRINTFORMDW 当你拥抱幽幻魔眼的时候，你感觉到其中一只眼睛在...流口水？
							PRINTFORMDW 你打算无视它

						CASE 4
							PRINTFORMDW 拥抱幽幻魔眼给人的感觉就像是拥抱一具尸体，还没有僵化的那种
							PRINTFORMDW 至少你觉得是这样的.毕竟你也没有拥抱过一具尸体
							PRINTFORMDW 你脑海中一直有一股声音让你赶紧跑开，你打算无视它
					ENDSELECT

				CASE -1
					SELECTCASE K151_YM_NATURE()
						CASE 0
							PRINTFORML 「在...在我们吞噬你之前拿开你的手！」
							PRINTFORMDW 你赶紧把手收了回来

						CASE 1
							PRINTFORMDL 太多了.这种来自衣服和皮肤的那种冰冷,不自然的感觉使幽幻魔眼身上的诡异感越来越重...
							PRINTFORMDW 你推开了她，但还是抑制住了逃跑的想法
							PRINTFORMW 「有问题吗？」
							PRINTFORMDW 幽幻魔眼看上去并没有被冒犯到...她只是面无表情的站在那边
					ENDSELECT
			ENDSELECT
		ENDIF
	ENDIF

ENDIF
RETURN 1
