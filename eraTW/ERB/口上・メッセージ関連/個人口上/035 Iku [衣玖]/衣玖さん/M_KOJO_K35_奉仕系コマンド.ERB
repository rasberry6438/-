;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_k35_80
CALL M_KOJO_MESSAGE_COM_k35_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_k35_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「这、这是要用手……\/\/\/」
		PRINTFORMW  衣玖战战兢兢的向你的男根伸出了手
		PRINTFORML  指尖刚碰到龟头就弹跳着的有了反应
		PRINTFORMW 「咿呀！？对、对不起！很疼吗！？」
		PRINTFORML  制止了慌忙道歉的衣玖、教导她要怎么做才能舒服起来
		PRINTFORML 「不、不对吗？原、原来如此……碰到先端就会有这样的反应呢……」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:140
		PRINTFORMW 「嗯……%UNICODE(0x2764)%好热……%UNICODE(0x2764)%」
		PRINTFORML  衣玖带着发情的表情激烈的取悦着男根
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML  你现在快要射精了腰都震颤起来了但是衣玖的手停了下来
			PRINTFORMW 「真是的……不行的哟……%UNICODE(0x2764)%只有你一个人舒服起来了%UNICODE(0x2764)%」
			PRINTFORML  你扭动着腰用男根拍打着衣玖的脸颊催促着衣玖
		CASE 1
			PRINTFORMW  你现在快要射精了腰都震颤起来了衣玖加快了手的速度，用另一只手刺激着龟头
			PRINTFORMW 「感觉如何%UNICODE(0x2764)%看招……快点把精液射出来……%UNICODE(0x2764)%」
			PRINTFORMW  衣玖眼中充满了嗜虐感，她会这样一直到你射精为止吧
		ENDSELECT
	ELSEIF TALENT:恋人
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「又要用手来上下撸动%UNICODE(0x2764)%这样做吗？很好哟%UNICODE(0x2764)%」
			PRINTFORMW  衣玖温柔的将男根收入手中慢慢地刺激着
			PRINTFORML 「呼呼%UNICODE(0x2764)%　什么时候想射精了随时射出来也没问题的%UNICODE(0x2764)%」
			PRINTFORMW 「看招%UNICODE(0x2764)%
			PRINTL 撸动%UNICODE(0x2764)%
			PRINTL 撸动%UNICODE(0x2764)%%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「想要用手来做吗？好的%UNICODE(0x2764)%」
			PRINTFORMW  衣玖清楚你的所点在哪里精确地用手进攻着诱导你射精
			PRINTFORML 「要这样做的话马上就会biu%UNICODE(0x2764)%biu%UNICODE(0x2764)%的射出来对吧？你的腰颤抖着反应出来了哟%UNICODE(0x2764)%」
			PRINTFORMW  你看着眼中充满了爱的衣玖，像如你所愿激烈地射精一样衣玖激烈的用手侍奉着
			PRINTFORMW 「看起来很擅长忍耐呢%UNICODE(0x2764)%即使如此直到你射精为止我的手都不会停下来呢%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ELSEIF TALENT:恋慕
		PRINTFORMW 「是想要用手来做吗？我明白了」
		PRINTFORMW  衣玖看起来很平静的样子，用手抚摸着男根给予着刺激
		PRINTFORMW 「总觉得很不可思议呢……一開始觉得很恐怖现在就是我愿意为你进行侍奉了……%UNICODE(0x2764)%」
		PRINTFORMW  依旧抬着头看着你温柔的促进着射精
		PRINTFORMW 「随时射精都是没问题的呢……%UNICODE(0x2764)%」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「又要用手来做吗？我、我明白了……请放轻松一点吧！」
		PRINTFORMW  是还在害怕着吗，衣玖的侍奉仍然很生硬
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,口交
;==================================================
@M_KOJO_MESSAGE_COM_k35_81
CALL M_KOJO_MESSAGE_COM_k35_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_k35_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「这、这个要……那个……」
		PRINTFORMW  你拿着你的男根放到了衣玖面前
		PRINTFORML 「嗅……嗅……呼啊啊……哈啊……」
		PRINTFORML  因为不习惯的气味吗衣玖眼神有点动摇的看着你的男根
		PRINTFORML 「呼……请、请多指教了！」
		PRINTFORMW  这样说着衣玖战战兢兢的用舌头触碰着男根
		PRINTFORML  对着起了反应你的男根跳了一下
		PRINTFORML 「咿哇！原原来会动的吗！这个……」
		PRINTFORMW  你笑着看着做出了可爱的反应的衣玖并摸着她的头
		PRINTFORML 「真是的……这种程度我还是做得到的！呼……唉柔……」
		PRINTFORML  衣玖先用着舌尖舔舐着直到有点习惯为止
		PRINTFORML 「呀呜……奇怪的味道……」
		PRINTFORMW  为了让衣玖能做到口交的程度你先让衣玖休息一会
		PRINTFORML 「好的……是要口交对吧……唉哈姆……」
		PRINTFORMW  衣玖舔了舔就感觉不行了忍不住了，你在衣玖准备放弃的瞬间动起了腰插入了深处
		PRINTFORML 「呼卟……啾噗晤……唉呜啾姆……%UNICODE(0x2764)%」
		PRINTFORMW  衣玖带着泪眼在第一次的口交中、接受了深喉的侍奉
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORML 「还想要用口做吗%UNICODE(0x2764)%」
		PRINTFORML  衣玖凝视着眼前的突起的陰茎犹豫着用口含住了
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML  但是并不能把全部都含下去只能将龟头含住
			PRINTFORML 「啾姆……%UNICODE(0x2764)%伽噗……呼呼……是不是很舒服%UNICODE(0x2764)%」
			PRINTFORML  衣玖用舌尖刺激着尿道口偶尔会甜蜜的咬着，不停地挑逗着就是不让你绝顶
			PRINTFORML 「欧金金%UNICODE(0x2764)%想去吗%UNICODE(0x2764)%想去吗%UNICODE(0x2764)%金金尖端上出了好多的液体哟？%UNICODE(0x2764)%」
			PRINTFORMW  衣玖舔舐着欧金金，并不想让我射精而是想这样一直玩弄着
			IF TALENT:恋人
				PRINTFORML 「为了让你不想着出轨%UNICODE(0x2764)%我会让你在我的身体里大量的射出来的%UNICODE(0x2764)%%UNICODE(0x2764)%」
			ENDIF
		CASE 1
			PRINTFORML  衣玖一口气将男根连根部也含了进去
			PRINTFORML  嘴唇、舌头、还有喉咙的連携一口气就带给了你极大的愉快
			PRINTFORML 「啾噗%UNICODE(0x2764)%咕噗%UNICODE(0x2764)%嗯噗%UNICODE(0x2764)%」
			PRINTFORML  毫不留情地搾取让你快直接升天般的射出来了
			PRINTFORML 「啾噗哈……%UNICODE(0x2764)%在这里射出来是不行的啊……要射就在膣内射吧%UNICODE(0x2764)%」
			IF TALENT:恋人
				PRINTFORML 「直到有了孩子为止膣内以外射精禁止哟%UNICODE(0x2764)%%UNICODE(0x2764)%」
			ENDIF
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「是要进行口交吗……我明白了……」
		PRINTFORML  衣玖抬着头看着你为你进行着奉仕
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;82,乳交
;==================================================
@M_KOJO_MESSAGE_COM_k35_82
CALL M_KOJO_MESSAGE_COM_k35_82_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_k35_82_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「要、要用胸……夹着这个吗……？」
		PRINTFORML  衣玖怯生生地把你的男根用胸夹住了
		PRINTFORMW 「是……这样吗？感觉舒服吗……？」
		PRINTFORML  你指示这衣玖在动胸部之前要先将口水垂下来润滑
		PRINTFORMW 「流口水吗……我明白了……嗯库啾……唉咯……则样子可以吗？」
		PRINTFORMW  尽管衣玖的动作很笨拙，但是因为有了润滑液每次胸部运动的时候都感受到快感
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:140
		PRINTFORML 「呼呼%UNICODE(0x2764)%在胸部里头一跳一跳的欧金金看起来很高兴呢%UNICODE(0x2764)%%UNICODE(0x2764)%」
		PRINTFORMW 「更多的动起腰biu%UNICODE(0x2764)%biu%UNICODE(0x2764)%的在胸部里面射精吧%UNICODE(0x2764)%」
	ELSEIF ABL:胸 >= 4
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「呼呼%UNICODE(0x2764)%发出了下流的声音呢……就这么想在胸部里射精吗？」
			PRINTFORMW 「请不要再忍耐着了……啾%UNICODE(0x2764)%%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「被夹在谷間的寂寞的亀頭在哭呢%UNICODE(0x2764)%」
			PRINTFORML 「呼呼真可爱呢……%UNICODE(0x2764)%」
			PRINTFORMW 「啾%UNICODE(0x2764)%啊姆%UNICODE(0x2764)%嗯啾姆%UNICODE(0x2764)%」
		ENDSELECT
	ELSEIF ABL:胸 >= 2
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「我正在逐渐的掌握窍门……这样做的话就可以了吧？」
			PRINTFORMW 「呼呼……已经因为先走汁变得黏黏糊糊了哟%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「被夹在胸部里再用嘴来侍奉看着要忍不住了呢%UNICODE(0x2764)%」
			PRINTFORMW 「啊啊啊…变得这么大了，真厉害呢%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「嗯……总觉得很困难啊……」
			PRINTFORMW 「感觉如何？很舒服吗？」
		CASE 1
			PRINTFORMW 「将男根夹住然后用口侍奉真的很困难呢……啊姆……啾姆%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

