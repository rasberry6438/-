;==================================================
;82,乳交
;==================================================
@M_KOJO_長短髪_MESSAGE_COM_K59_82
CALL M_KOJO_長短髪_MESSAGE_COM_K59_82_1
RETURN RESULT

@M_KOJO_長短髪_MESSAGE_COM_K59_82_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「想要胸部夹紧肉棒吗?呼呼っ、真的是、非常喜欢胸」
		PRINTFORM %NICKNAME:59%把、%CALLNAME:MASTER%的
		SIF TALENT:MASTER:形状 <= 2
			PRINTFORM 小小的
		PRINTFORMW 肉棒、用巨大的胸部夹住了
		PRINTFORML
		SIF TALENT:MASTER:形状 <= 2
			PRINTFORMW 「啊啦啊啦、%MASTERNAME:59%的、真是个很容易害羞的孩子♡藏进胸部里面了♡」
		PRINTFORMW 「那么、欧派要夹紧肉棒咯♡」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 
				PRINTFORMW 
			CASE 0
				IF TALENT:MASTER:形状 <= 2 && RAND:2
					PRINTFORML 
					PRINTFORMW 
					PRINTFORMW 
					SIF NOWEX:PLAYER:射精
						PRINTFORMW 
				ELSE
					PRINTFORML 「」
					PRINTFORMW 
					SIF NOWEX:PLAYER:射精
						PRINTFORMW 
				ENDIF
			CASEELSE
				;可愛的おちんちん＆半分の確率
				IF TALENT:MASTER:形状 <= 2 && RAND:2
					PRINTFORML 「%MASTERNAME:59%的肉棒、真是喜欢在%NICKNAME:59%的胸部里玩捉迷藏♡」
					PRINTFORMW 「呼呼っ♪到底在哪里呢ー?不要在藏了快点出来吧ー♡」
					PRINTFORMW %NICKNAME:59%一边这么说着、一边用胸部夹紧%CALLNAME:MASTER%的肉棒
					SIF NOWEX:PLAYER:射精
						PRINTFORMW 「…哦呀?好像出来了?虽然很容易害羞、但也是个容易寂寞的孩子嘛♡」
				ELSE
					PRINTFORML 「嗯っ、被胸部这么夹紧的话、会怎么样呢?」
					PRINTFORMW 「%MASTERNAME:59%、被%NICKNAME:59%的胸部弄得这么舒服、就不用多虑的射出来吧ー♡」
					SIF NOWEX:PLAYER:射精
						PRINTFORMW 「…一跳一跳的、真好懂♡」
				ENDIF
		ENDSELECT
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

