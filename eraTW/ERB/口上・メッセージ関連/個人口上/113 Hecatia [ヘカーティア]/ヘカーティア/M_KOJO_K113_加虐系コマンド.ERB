;100,スパンキング
;==================================================
@M_KOJO_総合スレ７４９_MESSAGE_COM_K113_100
CALL M_KOJO_総合スレ７４９_MESSAGE_COM_K113_100_1
RETURN RESULT

@M_KOJO_総合スレ７４９_MESSAGE_COM_K113_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「啊啊啊HPH HPH HPH」","L"
		PRINTFORML 「……你、你在干什么！发出那种奇怪的声音……！」
		PRINTFORMDL 遭受屁股被拍打的羞辱，赫卡提亚感到愤怒。
		PRINTFORMDW 但从她的反应来看，也许并不那么讨厌呢……
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:TARGET:マゾ == 1 || ABL:TARGET:マゾっ気 >= 2
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「嘿呀HPH HPH别停…啊啊HPH HPH」","L"
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%……当主人的屁股是什么啊啊HPH HPH」","L"
				PRINTFORMDW 每次被%CALLNAME:MASTER%打屁股，赫卡提亚都发出淫荡的娇声…！
			CASE 1
				CALL HPH_PRINT,@"「啊啊HPH HPH好的HPH 啊啊HPH HPH」","L"
				CALL HPH_PRINT,@"「这、这样…！地狱女神，竟然对你啊啊HPH HPH」","L"
				PRINTFORMDW 赫卡提亚被打屁股，露出充满懊恼和喜悦的表情…！
			CASE 2
				CALL HPH_PRINT,@"「啊嘿HPH HPH%CALLNAME:MASTER%啊HPH HPH再、再来啊HPH」","L"
				PRINTFORML 「不…没什么，算了―」
				CALL HPH_PRINT,@"「啊呃HPH HPH啊啊HPH啊啊啊HPH HPH」","L"
				PRINTFORMDW 自己和赫卡提亚，或许已经陷入无法回头的地步了……！
		ENDSELECT
		RETURN 1
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「啊呀HPH HPH啊啊HPH HPH%CALLNAME:MASTER%…！记住这一刻―」","L"
				CALL HPH_PRINT,@"「啊啊HPH HPH等一下…！停停啊啊HPH HPH，越来越厉害了啊HPH」","L"
				PRINTFORMDW 反正等下会受到惩罚的……趁着还可以打，就先打一下吧。
			CASE 1
				CALL HPH_PRINT,@"「哈呀HPH好啊HPH说啊HPH HPH HPH」","L"
				PRINTFORMDW 一连串地打着赫卡提亚的屁股，仿佛在打着某种节奏。
			CASE 2
				CALL HPH_PRINT,@"「啊嘿HPH HPH」","L"
				PRINTFORML 「啊，你啊…！对着谁说这种话―」
				CALL HPH_PRINT,@"「啊啊啊HPH HPH」","L"
				PRINTFORMDW %CALLNAME:MASTER%打了赫卡提亚的屁股，打断了她的话语。
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1

