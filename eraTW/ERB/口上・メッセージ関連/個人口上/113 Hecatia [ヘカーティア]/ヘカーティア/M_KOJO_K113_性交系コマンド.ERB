						PRINTFORMDW 腰部扭动，赫卡提亚低声说道…！
					CASE 2
						CALL HPH_PRINT,@"「呼HPH啊HPH哈HPH肛门会变得不正常呢HPH HPH」","L"
						PRINTFORMDW 毫无羞耻地呻吟，赫卡提亚身体仰仰弓起…！
					CASE 3
						CALL HECA_SEX
				ENDSELECT
				RETURN 1
			ENDIF
		ELSE
			;刚射精
			IF NOWEX:PLAYER:11
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「呜…！哈…HPH啊…HPH阴茎…HPH里面膨胀得好大啊…HPH」","L"
						CALL HPH_PRINT,@"「啊…真的要…要射出来吗！？HPH」","L"
						PRINTFORMDL 赫卡提亚失去往常的冷静，陷入慌乱之中。
						PRINTFORMDW %CALLNAME:MASTER%意识到，即使是女神也只是个普通的女人，带着奇怪的幸福感射精了―
					CASE 1
						CALL HPH_PRINT,@"「卡哈…HPH啊HPH难、难道%CALLNAME:MASTER%…就这样…！？」","L"
						CALL HPH_PRINT,@"「别…别停…啊啊HPH接下来…太过分了啊HPH HPH」","L"
						PRINTFORMDW 抱紧赫卡提亚，猛烈地动着腰部，射精进入肛门深处…！
					CASE 2
						CALL HECA_SEX
				ENDSELECT
				RETURN 1
		;前回射精した
			ELSEIF TCVAR:104
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「呼…HPH呼…HPH」","L"
						PRINTFORML 「……这样很舒服吧？……快拔出来―」
						CALL HPH_PRINT,@"「哎呀！？为、为什么又变得这么大…啊，你，你有多喜欢啊HPH」","L"
						PRINTFORMDW 紧紧抱住赫卡提亚的身体，再次顶入肛门深处…！
					CASE 1
						CALL HPH_PRINT,@"「嗯…哈…啊…HPH…对主人…做这种事情…！」","L"
						CALL HPH_PRINT,@"「之后…会受到惩罚，啊啊啊…HPH」","L"
						PRINTFORMDW "如果要受到惩罚的话"，%CALLNAME:MASTER%再次顶动腰部，抽插着肠道…！
					CASE 2
						CALL HECA_SEX
				ENDSELECT
				RETURN 1
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「嗯呜…！我，明明是女神…！%CALLNAME:MASTER%的主人，却…呜…HPH」","L"
						PRINTFORMDW 在这种姿势下，看不清赫卡提亚的表情，反而被想象刺激而兴奋…！
					CASE 1
						CALL HPH_PRINT,@"「嗯…！啊…HPH嘿嘿…！啊…HPH啊啊…！」","L"
						PRINTFORMDW 不断顶入肛门，赫卡提亚的声音带着一丝艳感…！
					CASE 2
						CALL HECA_SEX
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
