	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;Ａ感覚４以上
	ELSEIF ABL:Ａ感覚 >= 4
		SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「这么喜欢主人的肛门吗？…没办法呢…HPH」","L"
				CALL HPH_PRINT,@"「嗯…呜…咕…！啊…啊啊…HPH」","L"
				PRINTFORMDW 赫卡提亚一边紧抱着%CALLNAME:MASTER%，一边坐下来，让阴茎深深进入肛门…！
			CASE 1
