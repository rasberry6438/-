				CALL HPH_PRINT,@"「哈…！啊，呼…HPH呼呼呼―…HPH HPH」","L"
				PRINTFORMDW 赫卡提亚缓缓呼出气息，坐下来，将阴茎插入自己的肛门…！
		ENDSELECT
		RETURN 1
	ENDIF
