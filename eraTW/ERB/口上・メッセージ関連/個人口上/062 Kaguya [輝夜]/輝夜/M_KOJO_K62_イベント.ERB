@M_KOJO_EVENT_K62_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲惫（多分輝夜の場合死に分岐）
		TCVAR:62:370 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
			IF TALENT:恋人 || (TALENT:迷信 && TALENT:MASTER:追加種族 == 4)
				;恋人輝夜は全部注がれたい（精力判定３００
