					CASEELSE
						PRINTFORMW 「辉夜、那个……想去一个没人打扰的地方……」
					ENDSELECT
			ELSE
				PRINTFORML 「……我说」
					SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「要不去我的房间聊会儿天？」
					CASE 1
						PRINTFORMW 「看你有些累。就来我的房间吧？」
					CASEELSE
						PRINTFORMW 「这里有些吵闹呢。……不来我的房间里待会儿吗？　那里很安静哟」
					ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	CASE 4
		;-------------------------------------------------
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 || (TALENT:迷信 && TALENT:MASTER:追加種族 == 4)
				PRINTFORMW 「……呼呼、那就走吧」
			ELSE
				PRINTFORMW 「……呼呼、那就走吧」
			ENDIF
			RETURN 1
		ENDIF
	CASE 5
		;-------------------------------------------------
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 || (TALENT:迷信 && TALENT:MASTER:追加種族 == 4)
				PRINTFORMW 「……夫君大人真坏。……不、没什么」
			ELSE
				PRINTFORMW 「……呃呃」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
