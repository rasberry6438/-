@M_KOJO_EVENT_K62_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	CASE 0
		;-------------------------------------------------
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 || (TALENT:迷信 && TALENT:MASTER:追加種族 == 4)
				PRINTFORML 「夫君大人、一起出去转转吧？」
					SELECTCASE STR:(5000 + ARG:1)
					CASE "博麗神社"
						PRINTFORMW 「今天想去博丽神社那边……」
					CASE "命蓮寺"
						PRINTFORMW 「我要去命莲寺办点事……」
					CASE "人間之里"
						PRINTFORMW 「想和夫君大人在村子里随便逛逛……」
					CASE "霧之湖～紅魔館"
						PRINTFORMW 「有些想去湖边看看……」
					CASE "迷途竹林"
						PRINTFORMW 「想带夫君大人去看看新的盆栽……」
					CASE "魔法之森"
						PRINTFORMW 「呼呼、要不要去魔法之森里摘点蘑菇啊……」
					CASE "三途之川～冥界"
						PRINTFORMW 「要不去三途川、坐渡船观观光……」
					CASE "妖怪之山 (山麓)"
						PRINTFORMW 「偶尔想去感受山里的空气……」
					CASE "妖怪之山 (山頂)"
						PRINTFORMW 「我想去景色优美的地方……」
					CASE "地底"
						PRINTFORMW 「要不去地底探险吧……」
					CASE "夢之世界～月"
						PRINTFORMW 「应该不会出什么问题、我想偷偷地去趟月之都……」
					ENDSELECT
			ELSE
				PRINTFORML 「闲的话要不陪我出去转转？」
					SELECTCASE STR:(5000 + ARG:1)
					CASE "博麗神社"
						PRINTFORMW 「我今天想去博丽神社」
					CASE "命蓮寺"
						PRINTFORMW 「我去命莲寺有些事」
					CASE "人間之里"
						PRINTFORMW 「想去村庄里吃点甜品呢」
					CASE "霧之湖～紅魔館"
						PRINTFORMW 「想去湖边看看」
					CASE "迷途竹林"
						PRINTFORMW 「想带你去看看新的盆栽……」
					CASE "魔法之森"
						PRINTFORMW 「呼呼、要不要去魔法之森里摘点蘑菇啊……」
					CASE "三途之川～冥界"
						PRINTFORMW 「要不去三途川、坐渡船观观光……」
					CASE "妖怪之山 (山麓)"
						PRINTFORMW 「偶尔想感受一下山里的空气……」
					CASE "妖怪之山 (山頂)"
						PRINTFORMW 「我想带你去个景色优美的地方」
					CASE "地底"
						PRINTFORMW 「要不去地底探险吧……」
					CASE "夢之世界～月"
						PRINTFORMW 「感觉应该不会出什么问题、我想偷偷地去趟月之都……」
					ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	CASE 1
		;-------------------------------------------------
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 || (TALENT:迷信 && TALENT:MASTER:追加種族 == 4)
				PRINTFORMW 「呼呼、那么、就这么定下来了呢」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「呼呼、那么决定了呢」
			ENDIF
			RETURN 1
		ENDIF
	CASE 2
		;-------------------------------------------------
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 || (TALENT:迷信 && TALENT:MASTER:追加種族 == 4)
				PRINTFORML 「……是吗」
				PRINTFORMW 辉夜有些悲伤地垂下了目光。
			ELSE
				PRINTFORML 「……、也不会勉强你就是了」
				PRINTFORMW 辉夜有些不高兴地撅着嘴。
			ENDIF
			RETURN 1
		ENDIF
	CASE 3
		;-------------------------------------------------
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
