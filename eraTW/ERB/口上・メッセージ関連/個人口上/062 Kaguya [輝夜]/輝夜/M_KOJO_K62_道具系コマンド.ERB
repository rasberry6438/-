;==================================================
;50,避孕套装备
;==================================================
@M_KOJO_MESSAGE_COM_K62_50
CALL M_KOJO_MESSAGE_COM_K62_50_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K62_50_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF FLAG:70
	ELSEIF ABL:膣射中毒
					PRINTFORML
			IF TALENT:恋人 || (TALENT:迷信 && TALENT:MASTER:追加種族 == 4)
				;ゴムハメ嫌いな輝夜様。
				SELECTCASE RAND:10
					CASE 9
						PRINTFORML 「夫君大人、辉夜、不想戴套……」
						PRINTFORML
					CASE 8
						PRINTFORML 「夫君大人、要带套吗……？」
						PRINTFORMW
					CASE 7
						PRINTFORML 「为什么要带套……？」
						PRINTFORML
					CASE 6
						PRINTFORML 「……辉夜不想戴套、想和夫君大人直接接触」
						PRINTFORML
					CASE 5
						PRINTFORML 「……想让辉夜欲求不满吗？」
						PRINTFORML
					CASE 4
						PRINTFORML 「夫君大人、辉夜想让您、直接射进子宫里……」
						PRINTFORML
					CASE 3
						PRINTFORML 「辉夜想要夫君大人的精液哦……？」
						PRINTFORML
					CASE 2
						PRINTFORML 「夫君大人、带着套套的话夫君大人会不舒服的吧……？」
						PRINTFORML
					CASE 1
						PRINTFORML 「那是对辉夜的讥讽吗……？」
						PRINTFORML
					CASE 0
						PRINTFORML 「夫君大人不是想和辉夜做、而是想和套套做个爽吧……？」
						PRINTFORML
				ENDSELECT
						PRINTFORML 辉夜目不转睛地盯着男根上的套套。
						PRINTFORMW
			ELSE
				SELECTCASE RAND:8
					CASE 7
						PRINTFORML 「……呼呼」
						PRINTFORML
					CASE 6
						PRINTFORML 「有那么、讨厌我吗。……直接说出来不就行了」
						PRINTFORML
					CASE 5
						PRINTFORML 「……故意气我？」
						PRINTFORML
					CASE 4
						PRINTFORML 「…………也不是不行」
						PRINTFORML
					CASE 3
						PRINTFORML 「唔唔……为何要用那个」
						PRINTFORML
					CASE 2
						PRINTFORML 「呼哼……还是戴了呢」
						PRINTFORML
					CASE 1
						PRINTFORML 「……套套」
						PRINTFORML
					CASE 0
						PRINTFORML 「……要戴吗？」
						PRINTFORML
				ENDSELECT		
						PRINTFORML 辉夜目不转睛地盯着男根上的套套。
						PRINTFORMW
			ENDIF
	ELSEIF TALENT:恋人 || (TALENT:迷信 && TALENT:MASTER:追加種族 == 4)
						PRINTFORML 辉夜有些不满的看着男根上的套套。
						PRINTFORMW
	ENDIF
ENDIF
RETURN 1