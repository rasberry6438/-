;==================================================
;100,打屁股
;==================================================
@M_KOJO_PLUS_MESSAGE_COM_K62_100
CALL TRAIN_MESSAGE
CALL M_KOJO_PLUS_MESSAGE_COM_K62_100_1
RETURN RESULT

@M_KOJO_PLUS_MESSAGE_COM_K62_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:時間停止
		PRINTFORMW 「去死吧！好痛！！」
		PRINTFORMW 「为什么要敲……」
		RETURN 1
	ENDIF
	IF FLAG:時間停止
	ELSE
		IF EXP:62:苦痛快楽経験 >= 50
			PRINTFORMW 「啊啊！哎呀，请，请停下……」
		ELSE
			PRINTFORMW 「好痛！为什么……停下吧%CALLNAME:MASTER%……」
		ENDIF
	ENDIF
ENDIF
RETURN 1

