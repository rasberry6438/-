	ELSEIF DISH_TYPE == "甜点"
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「是在做点心？」
		CASEELSE
			PRINTFORML 「我想要的口味么……没什么特别的」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「我也想试试」
		CASE 2
			IF TALENT:62:思慕 || TALENT:62:恋慕
				PRINTFORML 「我来帮忙吧」
			ELSE
				PRINTFORML 「我来帮忙，之后就一起尝尝吧」
			ENDIF
		CASEELSE
			IF TALENT:62:思慕 || TALENT:62:恋慕
				PRINTFORML 「一起做？」
			ELSE
				PRINTFORML 「加油啊」
			ENDIF
		ENDSELECT
		;曲奇
		IF DISHNAME == "曲奇"
			PRINTW 
			PRINTFORML 「味道不错」
			TCVAR:62:357 = 1
			IF TALENT:62:恋慕 && !RAND:8
				PRINTFORML 「那个……不是%CALLNAME:MASTER%什么的啦……」
				PRINTFORML 辉夜有些着急的抢下你手里的曲奇
				PRINTFORML 似乎和你有点相似
				TCVAR:62:357 = 2
			ENDIF
			PRINTFORML 「还好么？」
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「做饭吗？」
		CASEELSE
			PRINTFORML 「是要做饭吗？」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「我也想试试」
		CASE 2
			IF TALENT:62:思慕 || TALENT:62:恋慕
				PRINTFORML 「我来帮忙」
			ELSE
				PRINTFORML 「辛苦了」
			ENDIF
		CASEELSE
			IF TALENT:62:思慕 || TALENT:62:恋慕
				PRINTFORML 「一起做？」
			ELSE
				PRINTFORML 「加油哦～」
			ENDIF
		ENDSELECT
	ENDIF
	PRINTW 
	;禁断の料理
	IF STRCOUNT(DISH_TASTE, "怪诞/")
		PRINTFORML 「虽然完成了……不过，这是什么啊？」
		PRINTFORML 「真，真的要吃吗……？」
	ELSE
		IF ABL:62:料理技能 >= 3
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「那么～来了」
				PRINTFORML 「好像还不错」
				PRINTFORML 辉夜陶醉似地闻着味道
			CASE 2
				PRINTFORML 「辛苦了，做的还不错」
				PRINTFORML 没有辉夜小姐，是做不了这么好的啦。
				PRINTFORML 「嗯，那就给你尝尝看吧」
				PRINTFORML 微笑着说着，辉夜喂了你一口
				PRINTFORML 真香啊
			CASE 3
				PRINTFORMW 「做好了」
				PRINTW 　　什么
				PRINTFORML 「味道还不错♪」
			CASEELSE
				PRINTFORML 「意外的简单啊」
				PRINTFORML 「我的料理做得好吗？」
			ENDSELECT
		ELSE
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「呼，疲惫不堪……」
				PRINTFORML 「做菜真够呛啊……」
			CASE 2
				PRINTFORML 「问我感觉怎样啊？」
				PRINTFORML 「嗯…辛苦但很有趣」
			CASE 3
				PRINTFORML 「做好了～！这样就可以了吧！」
				PRINTFORML 「累死我了～～」
				PRINTFORML 辉夜不顾形象的坐在椅子上
				PRINTFORML 你把早已放在一旁放凉的水递了上去
				PRINTFORML 「唔，谢谢～」
			CASEELSE
				PRINTFORML 「咦？完了？」
				PRINTFORML 「这么轻松？」
			ENDSELECT
		ENDIF
	WAIT
ENDIF
RETURN 1
