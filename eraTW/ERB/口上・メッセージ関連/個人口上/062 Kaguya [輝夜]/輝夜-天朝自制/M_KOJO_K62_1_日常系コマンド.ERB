						EXP:62:接吻経験 += 1
						EXP:62:Ｍ経験 += 1
						EXP:MASTER:Ｍ経験 += 1
						EXP:MASTER:接吻経験 += 1
					;2/3
					ELSEIF RAND:3
						PRINTL 
						PRINTW 白
						PRINTL 
						PRINTFORMW 「哈哈哈，骗人的~」
					ENDIF
					PRINTFORMW 「嗯~还行♪」
				ENDIF
			ENDIF
		;禁断
		IF STRCOUNT(DISH_TASTE, "怪诞/")
			PRINTFORML 「哦……呜呜……！」
			PRINTFORMW 「总觉得……」
		ELSE
			PRINTFORML 「我…尝尝看吧」
			PRINTW 
			IF STRFIND(DISH_TASTE, "香料") >= 0 && TCVAR:62:料理評価値 >= 70 && !RAND:2
				PRINTFORMW 「嗯~♪辣的刚好啊，这个」
			ELSEIF TCVAR:62:料理評価値 >= 85
				PRINTFORMW 「嗯~~！好吃」
			ELSEIF TCVAR:62:料理評価値 >= 55
				PRINTFORMW 「嘛，算了」
			ELSE
				PRINTFORMW 「…还需要多练练啊」
			ENDIF
		ENDIF
	;一緒に作ってない
		IF DISHNAME == "金吉獣料理"
			PRINTFORMW 「这是什么？」
			PRINTFORMDW 金吉兽料理
			PRINTFORMW 「材料是？」
			PRINTFORMDW 金吉兽
			PRINTFORMW 「是么……」
	ENDIF
	TCVAR:62:356 = 0
	TCVAR:62:357 = 0
ENDIF
RETURN 1

