@M_KOJO_MESSAGE_COM_K34_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:時間停止
			PRINTFORMW 在时间流动停止的情况下，爱抚天子的身体。
			PRINTFORMW 虽然没有反应，但感觉她身体在微微颤抖。
		ELSE
			PRINTFORMW 「所以，哪里啊......哇啊！%UNICODE(0x2764)%」
			PRINTFORMW 天子对每次碰触都很敏感的样子
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 「哈啊....%UNICODE(0x2665) *1%唔嗯嗯.....%UNICODE(0x2665) *1%呀啊——%UNICODE(0x2665) *1%」
			PRINTFORMW 天子的全身都对这极上的快乐作出了反应。子宫里装不下的精液溢了出来，黏在两个人的小腹和下肢上。
			PRINTFORMW 「%CALLNAME:MASTER%%UNICODE(0x2665) *1%　%CALLNAME:MASTER%%UNICODE(0x2665) *1%」
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 「诶…不要啊.....%UNICODE(0x2665) *1%…那里，那里很敏感的…%UNICODE(0x2665) *1%」
			PRINTFORMW %CALLNAME:MASTER%听到了天子的话，開始猛烈的抽插
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;扶她
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「啊啊…」
			PRINTFORMW 「光靠手已经无法忍受了…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯…更多…还要更多…」
		ELSE
			PRINTFORMW 「呃，哈……啊啊啊——！」
			PRINTFORMW 「不行…已经快坏掉了……」
		ENDIF
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「已经.....到极限了…」
			PRINTFORMW 「赶紧抱我吧…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 你突然停止了爱抚，完全无法忍受的天子向你屈服了
			PRINTFORMW 「不要这样啊…再做一点……」
		ELSE
			PRINTFORMW 「啊！那里不行啊…太让人愉悦了……%UNICODE(0x2665) *1%」
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「稍微摸一摸那里…啊.....」
			PRINTFORMW 稍微加强一下进攻，天子的抵抗便完全停止了
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…不要太用力哦…」
		ELSE
			PRINTFORMW 「啊.....再温柔一点……」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
