;301,泡茶
;==================================================
@M_KOJO_MESSAGE_COM_K63_301
CALL M_KOJO_MESSAGE_COM_K63_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(301)
		PRINTFORMW 「诶、这茶是泡给我喝的吗。谢谢」
		SELECTCASE DAY:2
		;夏
		CASE 2
			PRINTFORMW 「炎炎夏日能喝到冰茶真是沁入心扉呢～」
			PRINTFORMW 「不过热茶我也能喝得下去哦？」
		;夏じゃない
		CASEELSE
			PRINTFORMW 「是不是有点烫了？」
		ENDSELECT
			PRINTFORMW 「的确、野生的狼都是猫舌呢」
			PRINTFORMW 「但是觉得作为妖怪的狼人也会一样就大错特错了」
			PRINTFORMW 「不过为什么要叫做猫舌呢…咕咚咕咚……」

	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「不愧是%CALLNAME:MASTER%、动作很流畅呢♪」
			PRINTFORMW 「嗯～美味～♪」
		CASE 1
			PRINTFORMW 「哎呀谢谢♪我就不客气了…」
			PRINTL 呼~呼~嘶…
			PRINTFORMW 「因为人类会使用火所以没有猫舌呢」
			PRINTFORMW 「所以从自然的角度来看人类的舌头反而很奇怪呢」
			PRINTFORMW 「也就是说反之该称为人类舌、我有、%CALLNAME:MASTER%也有」
			PRINTFORMW 「啊嘞？不过我是妖怪所以得叫妖怪舌？」
		CASE 2
			PRINTFORMW 「呀啊～！正好喉咙干的要命呢！谢谢你%CALLNAME:MASTER%♪」
			PRINTFORMW 「因为喝茶不会醉、比起喝酒没有那么多顾虑」
			PRINTFORMW 「但是无论喝什么东西都要有节制哦」
			PRINTFORMW 「茶喝的太多也会变的睡不着的！」
		CASE 3
			PRINTFORMW 「之前就在想了……」
			PRINTFORMW 「这茶是什么时候泡好的」
			PRINTFORMW 「%CALLNAME:MASTER%偶尔也会做些不可思议的事情呢」
		CASE 4
			PRINTFORMW 「是%CALLNAME:MASTER%亲手泡的茶呢、好開心♪」
			PRINTFORMW 「一直以来真是多谢了♪」
		CASE 5
			SELECTCASE DAY:2
			CASE 1
				PRINTFORMW 「哎呀真少见呢、是抹茶呢。谢谢、我就不客气了」
				PRINTFORMW 「呜～、好像有点苦……」
			CASE 2
				PRINTFORMW 「夏天果然要喝麦茶呢」
				PRINTFORMW 「适度补充水分是不可或缺的！」
			CASE 3
				PRINTFORMW 「哎呀今天是红茶呢。谢谢、我就不客气了」
				PRINTFORMW 「唔嗯ー、不但味道不错、香气也很诱人呢」
			CASE 4
				PRINTFORMW 「哇啊～谢谢！」
				PRINTFORMW 「喝起来又暖和又美味呢～！」
			ENDSELECT
		ENDSELECT

	;恋慕じゃない
	ELSE
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「啊啦、动作很流畅呢。我就不客气了」
			PRINTFORMW 「嗯～美味～」
		CASE 1
			PRINTFORMW 「哎呀谢谢。我就不客气了」
			PRINTL 
			PRINTFORMW 「人类以外的動物全部都是猫舌呢」
			PRINTFORMW 「不过变成妖怪后还是不是就不得而知了」
		CASE 2
			PRINTFORMW 「嗯ー！正好喉咙干的要命呢。谢谢」
			PRINTFORMW 「茶与酒的美味是所有種族的共识呢」
		CASE 3
			PRINTFORMW 「之前就在想了……」
			PRINTFORMW 「这茶是什么时候泡好的」
			PRINTFORMW 「难道说是用了妖术？！」
		CASE 4
			PRINTFORMW 「一直以来都麻烦你了呢。非常感謝」
			PRINTFORMW 「呵呵、你的親切好像与茶水一起沁入我的五臓六腑了呢～」
		CASE 5
			SELECTCASE DAY:2
			CASE 1
				PRINTFORMW 「哎呀真少见呢、是抹茶呢。谢谢、我就不客气了」
				PRINTFORMW 「呜～、好像有点苦……」
			CASE 2
				PRINTFORMW 「夏天果然要喝麦茶呢」
				PRINTFORMW 「适度补充水分是不可或缺的！」
			CASE 3
				PRINTFORMW 「哎呀今天是红茶呢。谢谢、我就不客气了」
				PRINTFORMW 「唔嗯ー、不但味道不错、香气也很诱人呢」
			CASE 4
				PRINTFORMW 「哇啊～谢谢！」
				PRINTFORMW 「喝起来又暖和又美味呢～！」
			ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
;==================================================
;302,身体接觸
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K63_302
;成否判定
;TFLAG:192(-2=指令終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
IF TCVAR:発情
	IF TALENT:恋慕
		TFLAG:192 = 1
	ELSE
		TFLAG:192 = -1
	ENDIF
ELSEIF (TALENT:恋慕 || TALENT:思慕) && DAY > K63_EYEBROW && AT_HOME(MASTER)
	TFLAG:192 = 1
ELSE
	RETURN 0
ENDIF
RETURN 1

@M_KOJO_MESSAGE_COM_K63_302
CALL M_KOJO_MESSAGE_COM_K63_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(302)
		PRINTFORMW 「等、等等！不要随便摸啊！」
		PRINTFORMW 「啊、对不起…冷不防的吃了一惊呢……」
		SIF TALENT:思慕
			PRINTFORMW 「嘛…%CALLNAME:MASTER%的话稍微碰一碰也没事……」
		RETURN 1
	ENDIF
	
	;恋慕（お風呂）
	IF TALENT:恋慕 && BATHROOM()
		PRINTFORMW 「要帮我擦背？」
		PRINTFORMW 「谢谢……」
		PRINTFORML 
		PRINTFORMW 「%TEXTR("那个…不要这么一直盯着看呀……/啊哈～♪好～、好痒啊～！嘻嘻～/好的、接下来轮到我了……")%」
		RETURN 1
	ENDIF
	IF TCVAR:発情
		FONTBOLD
		CALL HPH_PRINT,@"「呀嗯゛っっHPH」","W"
		FONTREGULAR
		IF TALENT:恋慕
			CFLAG:积攒度 += 100
			PALAM:TARGET:潤滑 += 500
			BASE:TARGET:情緒 += 500
			PRINTFORMW 「对、对不起……现在被碰到的话该说是有些困扰还是这么说呢……那个、总之不是讨厌你哟……」
			PRINTFORMW （呜呜、被%CALLNAME:MASTER%碰到的时候、身子擅自就为交尾做好准备了…）
			IF TALENT:自己愛 >= 0
				CFLAG:TARGET:同行 = 60
				CFLAG:MASTER:同行 = 60
				PRINTFORMW 「好啦……你也明白的吧？　呐、我们去那边吧？」
				PRINTFORMW %CALLNAME:TARGET%轻轻捏住%CALLNAME:MASTER%的袖子朝某个方向拉了拉
			ENDIF
		ELSEIF TALENT:思慕
			PRINTFORMW 「不、不好意思……我想暂时独处一会、还请不要来碰我……」
		ELSE
			PRINTFORMW 「不要擅自摸过来啊！离远点！」
			CFLAG:ブチギレ = 1
		ENDIF
		CALL COLORMESSAGE(@"欲求不満上升了",C_YELLOW,1)
		CFLAG:积攒度 += 100
		IF PALAM:TARGET:潤滑 < 3000
			CALL COLORMESSAGE(@"潤滑值上升了",C_YELLOW,1)
			PALAM:TARGET:潤滑 += 500
		ENDIF
		RETURN 1
	ENDIF
	IF (TALENT:恋慕 || TALENT:思慕) && DAY > K63_EYEBROW && AT_HOME(MASTER)
		CALL K63_眉カット
		RETURN 1
	ENDIF
	CALL KAGEROU_SKINSHIP
	;恋慕
	IF TALENT:恋慕
		SELECTCASE RESULT
		;手を握る
		CASE 0
			IF !RAND:3
				PRINTFORML 「果然%CALLNAME:MASTER%的手跟我的手是不一样的呢」
				PRINTFORML 「是能够好好制作使用工具的人类的手呢」
			ELSEIF RAND:2
				PRINTFORMW 「啊…等等……」
				PRINTFORMW 「我的手、爪子立起来的时候是很危险的、要小心哦…」
			ELSE
				PRINTFORMW 「呵呵、%CALLNAME:MASTER%的体温隐隐约约的从手上传过来了呢……」
				PRINTFORMW 「这样牵手不是跟恋人一样了吗……」
				PRINTFORMW 「有点、不好意思呢……」
			ENDIF
		;頬を撫でる
		CASE 1
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「啊っ…怎么了吗%CALLNAME:MASTER%……」
					PRINTFORML 「呵呵、也让我来摸摸你的脸……」
				CASE 1
					PRINTFORML 「哇！突然这么摸过来会吓一跳的呀」
					PRINTFORML 「不、不要用力捏哦」
				CASE 2
					PRINTFORML 「嗯……」
					PRINTFORML 影狼很舒服似的眯起眼、轻轻把头靠在了你的手上
			ENDSELECT
		;耳に触る
		CASE 2
			IF RAND:2
				PRINTFORMW 「这也是真正的耳朵哦」
				PRINTFORMW 「就和妖兽们的耳朵是一个道理呢♪」
			ELSE
				PRINTFORMW 「这边的耳朵是可以动的呢、看～」
				PRINTFORMW 「想藏起来的时候就这样、让它趴下……」
			ENDIF
		;顎を擽る
		CASE 3
			IF RAND:2
				PRINTFORML 「啊、这样还挺舒服的……」
				PRINTFORML 「不、不过有点痒痒的诶」
			ELSE
				PRINTFORML 「呼啊…有点痒痒的……」
				PRINTFORML 「啊…还有就是、跟摸狗狗一样有点不好意思……」
			ENDIF
		;髪に触る
		CASE 4
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「虽然平时也有好好打理啦…」
					PRINTFORML 「但被你这么仔细的观察果然还是会紧张啊……」
				CASE 1
					PRINTFORML 「不要那么仔细地闻啦～…」
					PRINTFORML 「很害羞的诶……」
				CASE 2
					PRINTFORML 「摸起来的手感很不错吧？」
					PRINTFORML 「哼哼、关于这点我可是很有自信的哦」
			ENDSELECT
			PRINTFORML 享受了一番影狼美艳的黑发…
		;肩を揉む
		CASE 5
			PRINTFORMW 「啊～好舒服哇～♪」
			IF RAND:2
				PRINTFORMW 「不是挺拿手的吗」
				PRINTFORMW 「是在灵梦那儿练出来的这个技巧吧？」
			ELSE
				PRINTFORMW 「接下来让我帮你揉揉吧」
				PRINTL 
				PRINTFORMW 「客人的肩膀可真是僵硬呢～」
				PRINTFORMW 「怎么样？我的手艺还行吗？」
			ENDIF
		;背を撫でる
		CASE 6
			IF RAND:2
				PRINTFORMW 「呼啊～……！」
				PRINTFORMW 「虽然有些痒痒的、但不知怎的就平静下来了呢」
			ELSE
				PRINTFORMW 「啊啊…总觉得心情好舒畅啊……」
				PRINTFORMW 「特别的安心…都有点想睡了呢……」
			ENDIF
		;脇腹をつつく
		CASE 7
			IF RAND:2
				PRINTFORMW 「啊哈哈！才不是让你突然挠痒痒啦！」
				PRINTFORMW 「%CALLNAME:MASTER%也来感受一下吧」
				PRINTFORMW 「看招～！看招看招～！」
			ELSE
				PRINTFORMW 「有点痒呢！啊哈哈！」
				PRINTFORMW 「呀啊～！够了快住手吧！」
				PRINTFORMW 「很羞人啊别弄了～！」
			ENDIF
		;鼻を擽る
		CASE 8
			IF RAND:2
				PRINTFORMW 「呜啊…痒痒的呢～……」
				PRINTFORMW 「啊嚏～！」
			ELSE
				PRINTFORMW 「呵呵、想干什么呢」
				PRINTFORMW 「%CALLNAME:MASTER%也感受一下吧」
				PRINTFORMW 「怎么样？是不是感到怪怪的呢？」
			ENDIF
		;ぺたぺた
		CASE 9
			PRINTFORMW 「从刚才開始就缠着我想干什么啊」
			IF RAND:2
				PRINTFORMW 「虽然并不是很讨厌、感覚还可以…」
				PRINTFORMW 「但总觉得哪里怪怪的呢…%CALLNAME:MASTER%」
			ELSE
				PRINTFORMW 「这么想摸的话…那么……」
				PRINTFORMW 「去稍微隐蔽一点的地方吧……」
			ENDIF
		CASE 10
			IF FIRSTTIME("肉球")
				PRINTFORMW 「想摸摸肉球？嘛、如果变身的话确实是有啦……」
				PRINTFORML 影狼把右手伸进了左边的袖子里轻轻地晃动了两下
				PRINTFORML 从袖子里抽出的右手已经变成了狼爪毛茸茸的样子
				PRINTFORMW 「是、どうぞ」
				PRINTFORML 真是柔软……想这么说但并非如此、简直像是鞋底一样的粗糙手感
				PRINTFORMW 「跟公主她们当时失望的表情一模一样呢ー……」
			ELSE
				PRINTFORML 「%TEXTR("到底是有什么好玩的啦……/还来？拿你没办法馁……")%」
				PRINTFORML 尽情触摸了影狼变身后的狼爪
				IF !RAND:3
					PRINTFORML 「只有一只手是野兽的样子像是哪里的绘本故事？诶、还有这样的故事吗？」
					PRINTFORML 「啊、故事里是狐狸吧……并不是狼呢……」
				ELSE
					PRINTFORML %TEXTR("虽然与平日里温润如玉的手不一样、但这无疑也是影狼的另一面/是为了稳稳抓牢地面如风般奔驰、充满力量的野兽的前肢啊")%
				ENDIF
			ENDIF
		;人狼
		CASE 100
			CALL ERATOHO_JINROU_GAME
		ENDSELECT
	;恋慕じゃない
	ELSE
		SELECTCASE RESULT
		;手を握る
		CASE 0
			PRINTFORMW 「啊啦、你的手很漂亮呢」
			PRINTFORMW 「毎天都有护理吗？」
		;頬を撫でる
		CASE 1
			PRINTFORMW 「干嘛干嘛干嘛ー！？」
			PRINTFORMW 「真是的、老是冷不防的这样吓人一跳呢！」
		;耳に触る
		CASE 2
			PRINTFORMW 「这也是真正的耳朵哦」
			PRINTFORMW 「原理是什么？哼哼哼、秘密♪」
		;顎を擽る
		CASE 3
			PRINTFORMW 「……这是、把我当狗吗？」
			PRINTFORMW 「要生气了哦？」
		;髪に触る
		CASE 4
			PRINTFORMW 「在对我的头髪作什么呢……」
			PRINTFORMW 「嘛头髪可是女人的生命呢」
			PRINTFORMW 「怎么了嘛、妖怪说这种话很奇怪吗？」
		;肩を揉む
		CASE 5
			PRINTFORMW 「啊ー真不错呢。好舒服」
			PRINTFORMW 「再多帮我揉一会儿吧～」
		;背を撫でる
		CASE 6
			PRINTFORMW 「呀啊啊！干什么啊真是的」
			PRINTFORMW 「背上都发毛了呢」
		;脇腹をつつく
		CASE 7
			PRINTFORMW 「哇哇～！你在干什么啊！」
			PRINTFORMW 「可恶～！可恶～！」
		;鼻を擽る
		CASE 8
			PRINTFORMW 「呜啊……干什……啊嚏～！」
			PRINTFORMW 「真是的ー！搞什么啊ー！」
		;ぺたぺた
		CASE 9
			PRINTFORMW 「从刚才開始就缠着我想干什么啊」
			PRINTFORMW 「既然你住在神社里、妖怪什么的对你来说也没那么稀罕吧」
			PRINTFORMW 「再做这样的事要是被吃掉了可别说我没警告过你哦？」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
