		PRINTDL ｜　不保证收益　　　　　　　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　　　　　　　守矢神社　｜
		PRINTDL └────────────────────────────┘
		PRINTW 
		PRINTFORMW 「山上神社的相关物品吗？」
		RETURN 1
	CASE 1 TO 20, 22 TO 60
		LOCALS = %FISH_NAME(TFLAG:193)%
	CASE 21
		LOCALS = 跳蛋
	CASEELSE
		LOCALS = %ITEMNAME:(TFLAG:193)%
	ENDSELECT
	;影狼が釣った
	IF TFLAG:194 > 1000
		PRINTFORMW 「！」
		PRINTFORMW 「来了！」
		SELECTCASE LOCALS
			CASE "雀鳝", "雷魚", "腔棘鱼", "鲟", "伊富魚", "巨骨舌鱼", "泷太郎"
				PRINTFORMW 「大家伙上钩了！把公主钓上来后第一次有这样的手感！」
				PRINTFORMW 「%CALLNAME:MASTER%也来帮帮忙！」
		ENDSELECT
		PRINTL 
		PRINTFORMDL %CALLNAME:TARGET%把%LOCALS%を釣上来了！
		RESETCOLOR
		;説明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
	;MASTERが釣った
	ELSE
		PRINTFORMW 「%CALLNAME:MASTER%、咬钩了咬钩了！」
		RESETCOLOR
		PRINTFORMDL %LOCALS%上钩了！
		;跳蛋以降を弾いて尺寸と漁獲数の処理
		IF INRANGE(TFLAG:193, 1, 60) && TFLAG:193 != 21
			;尺寸の出力
			LOCAL:1 = FISHING_SIZE(TFLAG:193)
			PRINTFORML 　尺寸：{LOCAL:1}cm
			;ここまでの最高尺寸と比較して大きければ記録
			IF LOCAL:1 > FISHSIZE_RECORD:(TFLAG:193)
				SETCOLOR C_YELLOW
				SIF FISH_EXP:(TFLAG:193)
					PRINTFORML 　是至今为止钓上来的%LOCALS%中最大的！
				RESETCOLOR
				FISHSIZE_RECORD:(TFLAG:193) = LOCAL:1
			ENDIF
			;漁獲数の加算
			FISH_EXP:(TFLAG:193) ++
		ENDIF
		;説明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
		CALL M_KOJO_COLOR_K63
	ENDIF
	SELECTCASE TFLAG:193
		CASE 11 TO 20, 24 TO 60
			IF TIME_PROGRESS(TCVAR:MASTER:空腹時刻) && TIME_PROGRESS(TCVAR:TARGET:空腹時刻)
				PRINTFORMW 「呐、%CALLNAME:MASTER%、你肚子不饿吗？」
				CALL ASK_YN("当场进行一个鱼的吃","还是回头再吃吧")
				IF !RESULT
					CALL PRINT_FACE, [[影狼]], "笑顔", "服", "", ""
					PRINTFORMW 「好耶！」
					PRINTFORMW 把钓上来的%LOCALS%处理了一下吃掉了
					CALL LOST_ITEM("好吃的魚", 1, 1)
					TCVAR:[[影狼]]:空腹時刻 = 1440 * DAY + TIME + 200
					CALL RECOVER_PERMIL([[影狼]], 400, "体力", 0, "因为饮食而产生的回復")
					CALL RECOVER_PERMIL([[影狼]], 400, "気力", 0, "因为饮食而产生的回復")
					TCVAR:MASTER:空腹時刻 = 1440 * DAY + TIME + 200
					CALL RECOVER_PERMIL(MASTER, 400, "体力", 0, "因为饮食而产生的回復")
					CALL RECOVER_PERMIL(MASTER, 400, "気力", 0, "因为饮食而产生的回復")
					CALL RECOVER_PERMIL(MASTER, 200, "精力", 0, "因为饮食而产生的回復")
					TIME += 20
					CFLAG:1004 += 3
				ENDIF
			ENDIF
	ENDSELECT
ENDIF
RETURN 1

