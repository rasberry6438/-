		ENDIF
		RETURN 1
	;木片
	CASE 610
		IF TFLAG:194 > 1000
			PRINTL 
			PRINTFORMDL %CALLNAME:TARGET%钓上来了木片…
			PRINTW
			PRINTFORMW 「……你要吗？这东西」
		ELSE
			PRINTFORMDL 钓上来了木片……
			PRINTW 
		ENDIF
		RETURN 1
	;紙切れ
	CASE 131
