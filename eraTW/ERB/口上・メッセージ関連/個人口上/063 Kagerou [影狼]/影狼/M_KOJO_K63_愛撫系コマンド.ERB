	ELSE
		IF ABL:Ｃ感覚 < 2
			PRINTFORML 「%TEXTR("唔、嗯っ……/嗯っ……好像、有点舒服馁……")%」
		ELSE
			CALL HPH_PRINT,@"「啊啊っHPH　啊っHPH　啊HPH　哈啊っHPH」","L"
			IF RAND:2
				PRINTFORML （好认真地在舔着诶……）
				PRINTFORML （%CALLNAME:MASTER%也有在想让我好好舒服起来啊……）
			ELSE	
				CALL HPH_PRINT,@"（%TEXTR("啊ー好舒服ー……HPH　脑袋都晕乎乎的了/啊…HPH　人家的爱液、被喝下去好多")%……HPH）","W"
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
