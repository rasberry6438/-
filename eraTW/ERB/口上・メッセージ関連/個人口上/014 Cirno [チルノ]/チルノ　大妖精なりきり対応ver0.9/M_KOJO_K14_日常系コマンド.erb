@M_KOJO_MESSAGE_COM_K14_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
	IF FLAG:時間停止
	ELSEIF FLAG:扮演 == [[大妖精]] && !CFLAG:14:1002
	;大妖精に限り初対面口上へ移行
		CALL M_KOJO_ENCOUNTER_K14
	ELSEIF TCVAR:302 && !TALENT:恋慕 && !TALENT:思慕 && CFLAG:14:1002 != 1
		SELECTCASE RAND:3
			CASE 1
				CALL COLORMESSAGE14("　阿、阿嚏！",C_GREEN,1,0)
				PRINTFORMW 「呼…靠人家太近的话会被冻伤的ze」
			CASE 0
				PRINTFORML 「说起来你是谁啊？」
				CALL COLORMESSAGE14("(看起来还没记住名字呀……）",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「…你从刚刚開始就在一直顶撞人家啊？」
				CALL COLORMESSAGE14("　非、非常抱歉！",C_GREEN,2,0)
		ENDSELECT
	ELSEIF CFLAG:318 == 1
		PRINTFORML 「………」
	ELSEIF CFLAG:行動 == 5 && CFLAG:MASTER:現在位置 == 2 || CFLAG:行動 == 5 && CFLAG:MASTER:現在位置 == 202
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「是来买冰淇淋的吗？1根500円呢」
				IF MONEY < 500
					CALL COLORMESSAGE14("　啊呜…钱不够呀…。",C_GREEN,2,0)
				ELSE
					CALL COLORMESSAGE14("　看起来很美味的冰淇淋…。因为有钱所以买一根吧？",C_GREEN,2,0)
				ENDIF
			CASE 0
				PRINTFORML 「现在在工作中啦。忙地要死不能到处乱看啦」
				PRINTFORMW 「受欢迎的人真的很辛苦啊ー」
				CALL COLORMESSAGE14("（看起来非常的忙碌啊…。这里应该帮忙比较好吧？)",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「太阳这个畜生混蛋准备把人家和冰淇淋都给溶化掉…」
				PRINTFORMW 「呜哦ー！怎么可能输啊！」
				CALL COLORMESSAGE14("　加油哇琪露诺酱！",C_GREEN,2,0)
		ENDSELECT
	;ELSEIF TALENT:恋慕 
		;PRINTFORML 
	ELSEIF TALENT:思慕 || CFLAG:14:1002 == 1
		SELECTCASE RAND:8
			CASE 6
				PRINTFORML 「作为凶器冰可是胜于刀的哟！」
				PRINTFORML 「如果冰刀溶化的话证据就会直接消灭呢」
				CALL COLORMESSAGE14("　琪露诺酱。难道说是读了外面世界的侦探漫画吗？",C_GREEN,1,0)
				CALL COLORMESSAGE14("「什么嘛。大酱也有在看呀」",C_AQUA,2,0)
			CASE 5
				CALL COLORMESSAGE14("「大酱是什么妖精呢？」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　唉？我？我是……什么妖精呢。",C_GREEN,1,0)
				CALL COLORMESSAGE14("「像拉尔瓦一样的蝴蝶妖精？」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　但是我不会冬眠哟。…唔ー嗯我到底是。",C_GREEN,1,0)
				PRINTFORMW 「…总觉得要发生什么异常的预感」
				CALL COLORMESSAGE14("　我的真实身份不会引发什么异变哟。",C_GREEN,2,0)
			CASE 4
				PRINTFORMW 「会不会又被晒黑呢ー。最近的太阳实在是太过软弱」
				CALL COLORMESSAGE14("　那个时候的琪露诺酱、十分的强大和帅气哟。",C_GREEN,2,0)
			CASE 3
				PRINTFORMW 「最好要注意下鸦天狗噢。因为会把一些有的没得的事情给记载啦！」
				CALL COLORMESSAGE14("　说的是大蝦蟇桑的报道吗…？",C_GREEN,1,0)
				CALL COLORMESSAGE14("「没错！那个新闻满是谎言。那家伙以大蝦为对手完全可以打好几个来回啊」",C_AQUA,2,0)
			CASE 2
				PRINTFORML 「有没有青蛙呢」
				CALL COLORMESSAGE14("　又要把青蛙冻住吗？因为太可怜了别干了吧…。",C_GREEN,1,0)
				CALL COLORMESSAGE14("「会好好的让她们苏醒所以没问题的」",C_AQUA,2,0)
			CASE 1
				PRINTFORML 「啊ー总觉得好闲啊ー。会不会又发生个异变什么的啊ー」
				CALL COLORMESSAGE14("　和平可是最棒的哟琪露诺酱。",C_GREEN,1,0)
				PRINTFORMW 「对人家来说可以大摇大摆的玩耍异变时光每天都很開心哟」
			CASE 0
				PRINTFORML 「在草原和砂利道的四周冻住躺着很開心哟」
				PRINTFORMW 「表面滑溜溜的在上面滚来滚起…真的是ー最棒啦ー！」
			CASEELSE
				PRINTFORML 「快看、花被冻住了很厉害吧！」
				CALL COLORMESSAGE14("　是要做成干花吗？",C_GREEN,1,0)
				PRINTFORMW 「这样子冻起来就能够做成凶器哇！」
				CALL COLORMESSAGE14("　哇、哇啊…。",C_GREEN,2,0)
		ENDSELECT
	ELSE
		SELECTCASE RAND:6
			CASE 4
				PRINTFORML 「人家只在夏日的大晴天、才会在人間之里的村子里卖冰淇淋」
				PRINTFORMW 「在盛夏的太阳下卖冰淇淋这种绝技只有人家能做到所以能赚大钱呢！」
				CALL COLORMESSAGE14("　我也最喜欢冰淇淋了。要是碰到了的话请让我购买呢。",C_GREEN,2,0)
				SIF DAY:2 == 4
					CALL COLORMESSAGE14("　虽说冬天的冰淇淋看起来也很美味、但是感觉会感冒啊…。",C_GREEN,2,0)
			CASE 3
				PRINTFORML 「神社附近在的三妖精很厉害噢」
				PRINTFORML 「桑尼可以隐藏气息露娜可以让光熄灭、斯塔她…竟然能够感知声音呢！」
				PRINTFORMW 「虽然很不甘心、但是在捉迷藏里没人可以强于她们呢」
				CALL COLORMESSAGE14("（啊咧…总觉得有哪里不对？)",C_GREEN,2,0)
			CASE 2
				IF DAY:2 == 4
					PRINTFORMW 「冬天真无聊啊。就算什么都不做周围也会冻结上」
					CALL COLORMESSAGE14("　昆虫和夏天妖精的女孩子们也都在冬眠没办法一起玩耍、冬天稍微有点寂寞呢",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「那ー么今天要冻些什么来玩呢」
					CALL COLORMESSAGE14("　请、请不要把我冻起来啊…。",C_GREEN,2,0)
				ENDIF
			CASE 1
				PRINTFORML 「说起来你的名字是什么来着？」
				CALL COLORMESSAGE14("　是问我的名字吗？我是…",C_GREEN,1,0)
				PRINTFORMW 「因为记起来很麻烦就叫「你」就好了吧」
				CALL COLORMESSAGE14("　怎么这样…。",C_GREEN,2,0)
			CASE 0
				PRINTFORML 「总觉得是个很老实规矩的家伙啊ー。是忘记吃饭了吗？」
				CALL COLORMESSAGE14("　有、有好好的吃饭哟",C_GREEN,1,0)
				IF TALENT:MASTER:胸圍 > -1
					CALL COLORMESSAGE14("「…如果没吃也不能长这么大啊」",C_AQUA,1,0)
					CALL COLORMESSAGE14("　那、那个…是在看着哪里呢？",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「真的？不是只在光吃草吧？」
				ENDIF
			CASEELSE
				PRINTFORML 「你啊知道捉迷藏吗？」
				CALL COLORMESSAGE14("　是由鬼来寻找藏起来的孩子的游戏…对吧？我经常和太阳花田的孩子们一起玩呢。",C_GREEN,1,0)
				PRINTFORML 「在湖之妖精之间也是大流行啊」
				PRINTFORML 「而且总是人家一个人的胜利呢。唉嘿！」
				PRINTFORML 「对啦！下次要玩的时候也把你叫上吧」
				CALL COLORMESSAGE14("　我能够找到琪露诺桑吗…。我会努力的呢。",C_GREEN,2,0)
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
