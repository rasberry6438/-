;===========================================================
;冰之妖精的节日问候！但是傻子哪里认识那么多节日呢。
;「什么嘛、这种程度咱还是知道的呀！」
;===========================================================
@CIRNO_MEET_FESTIVAL
SELECTCASE FESTIVAL()
	CASE "元旦"
		IF TALENT:恋慕
			PRINTFORML	「新年快乐！%CALLNAME:MASTER%、今年也要玩得开心哦！」
			PRINTFORMDW	在听清祝福之前、琪露诺就已经飞着扑到了怀里、笑着打起了滚。
		ELSEIF TALENT:思慕 || !TALENT:幼稚
			PRINTFORML	「新年好哦！」
			PRINTFORMDW	琪露诺眼珠子滴溜溜地转着、不知道在想什么呢。
		ELSE
			PRINTFORML	「过年好——」
			PRINTFORMDW	说着敷衍祝福的妖精、似乎只在盯着钱包的样子……
		ENDIF
	CASE "七夕"
		IF TALENT:恋慕
			PRINTFORML	「今天是七夕哦、%CALLNAME:MASTER%有许愿吗？」
			PRINTFORMDL	琪露诺一边把一张纸筏藏到身后、一边笑眯眯地问候着。
			PRINTFORMW	「想看看咱的？……才不给哦。」
		ELSE
			PRINTFORML	「七夕快乐！听说这个日子的星星会很好看呢。」
			PRINTFORMDW	琪露诺看起来对节日的含义不太感兴趣、重点还是玩吧。
		ENDIF
	CASE "萬聖節"
		PRINTFORML	「不给糖就捣蛋！」
		LOCAL = 1
		SIF CFLAG:現在位置 == CFLAG:5:現在位置
			LOCAL ++
		SIF CFLAG:現在位置 == CFLAG:6:現在位置
			LOCAL ++
		SIF CFLAG:現在位置 == CFLAG:7:現在位置
			LOCAL ++
		SIF CFLAG:現在位置 == CFLAG:13:現在位置
			LOCAL ++
		SIF CFLAG:現在位置 == CFLAG:18:現在位置
			LOCAL ++
		SIF CFLAG:現在位置 == CFLAG:19:現在位置
			LOCAL ++
		SIF CFLAG:現在位置 == CFLAG:118:現在位置
			LOCAL ++
		IF LOCAL >= 3
			PRINTFORMD	以琪露诺为首的妖精们
		ELSE
			PRINTFORMD	琪露诺
			SIF CFLAG:現在位置 == CFLAG:5:現在位置
				PRINTFORMD	和桑尼
			SIF CFLAG:現在位置 == CFLAG:6:現在位置
				PRINTFORMD	和斯塔
			SIF CFLAG:現在位置 == CFLAG:7:現在位置
				PRINTFORMD	和露娜
			SIF CFLAG:現在位置 == CFLAG:13:現在位置
				PRINTFORMD	和大妖精
			SIF CFLAG:現在位置 == CFLAG:18:現在位置
				PRINTFORMD	和莉莉白
			SIF CFLAG:現在位置 == CFLAG:19:現在位置
				PRINTFORMD	和莉莉黑
			SIF CFLAG:現在位置 == CFLAG:118:現在位置
				PRINTFORMD	和拉瓦尔
		ENDIF
		PRINTFORMDL	突然窜了出来、大声叫喊着、吓了%CALLNAME:MASTER%一跳。
		PRINTFORMDW	说起来、今天是万圣节来着……
		PRINTFORMDL	[1] 被恶作剧
		SIF MONEY >= LOCAL*500 || ITEM:216 >= LOCAL	;钱或糖
			PRINTFORMDL	[2] 给糖
		INPUT
		SELECTCASE RESULT
			CASE 1
				CALL RECOVER(MASTER,-LOCAL*100,"体力")
				CALL RECOVER(MASTER,-LOCAL*100,"気力")
				PRINTFORMDW	被\@ LOCAL <= 1? 妖精 # 妖精们\@狠狠地捉弄了……
			CASE 2
				IF ITEM:216 >= LOCAL
					CALL LOST_ITEM("糖",LOCAL,2)
					PRINTFORMDW	给了糖果、\@ LOCAL <= 1? 妖精 # 妖精们\@就兴高采烈地离开了……
				ELSE
					MONEY -= LOCAL*500
					PRINTFORMDW	花钱买了糖、\@ LOCAL <= 1? 妖精 # 妖精们\@就兴高采烈地离开了……
				ENDIF
		ENDSELECT
	CASE "情人節"
		SELECTCASE CFLAG:2
			CASE 1000 TO 3000
				PRINTFORML	「什么、要巧克力？」
				PRINTFORMW	「唔……那这个给你吧、本来还想咱自己吃的——」
			CASE 3001 TO 5000
				PRINTFORML	「是要送巧克力的节日的吧？」
				PRINTFORMW	「不过咱不是很懂、就只是随便买了一点啦。」
			CASE IS > 5000
				IF EXP:愛情経験 < 100
					PRINTFORML	「情人节的话、是那个吧？要给喜欢的人送巧克力来着？」
					PRINTFORMW	「平时一起玩得还挺开心的啦、这个给你哦！」
				ELSE
					PRINTFORML	「呀、%CALLNAME:MASTER%、那个……」
					PRINTFORMDL	琪露诺不安地递出了一个纸盒、看起来是仔细封装起来的巧克力。
					IF ABL:料理技能 <= 1
						PRINTFORMDL 「虽然因为咱不太懂、所以是让\@ TALENT:13:恋慕? 大酱 # 文文\@教咱做的啦……」
						PRINTFORMDW 「总之就是、那个、是情人节的礼物啦！%CALLNAME:MASTER%、能收下吗？」
					ELSE
						PRINTFORMDL 「情人节的话、是要给喜欢的人送巧克力对吧？虽然咱不太懂、不过也试着做了一点……」
						PRINTFORMDW 「喜欢的人的话、嗯……嘿嘿、%CALLNAME:MASTER%、会收下的吧？」
					ENDIF
				ENDIF
		ENDSELECT
	CASE "平安夜"
		IF TALENT:恋慕
			PRINTFORML	「呀、是%CALLNAME:MASTER%啊……」
			PRINTFORMDL	琪露诺似乎刚好想到什么点子、微笑着凑了过来。
			PRINTFORML	「不是恶作剧哦？只是你看嘛、这个日子好像很多人类都会两个两个黏在一起的样子……」
			PRINTFORMW	「……咱能不能、也跟%CALLNAME:MASTER%一起待上一整天呢？」
		ELSE
			PRINTFORML	「唔、明天才是圣诞节吧？不过今天就看到、有很多人类两个两个地黏在一起哦。」
			PRINTFORMDW	琪露诺似乎因情侣们在平安夜集群行动的行为困惑了。
		ENDIF
	CASE "聖誕節"
		IF TALENT:恋慕
			PRINTFORML	「圣诞快乐哦！%CALLNAME:MASTER%！」
			PRINTFORMDL	琪露诺笑眯眯地走了过来、头上空荡荡的、可是顶上的翅膀却套着圣诞帽。
			PRINTFORMW	「嗯？那个没所谓的吧？戴在头上会挡住蝴蝶结的啦……」
		ELSE
			PRINTFORML	「到圣诞节了呢！虽然不知道具体是什么节日不过看招！」
			PRINTFORMDL	话音未落、琪露诺的问候跟着一颗雪球飞来了。
			PRINTFORMDW	还好、砸在身上碎掉了、看起来不是弹幕……
		ENDIF
	CASE "大晦日"
		IF TALENT:恋慕
			PRINTFORML	「%CALLNAME:MASTER%、要过年了呢！」
			PRINTFORMDL	在年末年始的时刻、琪露诺也有点兴奋的样子。
			PRINTFORML	「毕竟在这个时候、到处都会有好吃的吧！」
			PRINTFORMDW	重点果然还是在于吃吗、年节的时候。
		ELSE
			PRINTFORML	「要过年了呢！应该能有好吃的吧！」
			PRINTFORMDW	琪露诺对年节最关注的部分、果然还是食物啊。
		ENDIF
ENDSELECT

;==================================================
;冰之妖精的赴宴提醒！
;虽然是这样说，但傻子很有可能压根没被邀请……
;是否参加会在一天开始时就已经决定。
;==================================================
@CIRNO_MEET_PARTY
IF CFLAG:現在位置 == TFLAG:22	;正在会场
	IF TIME < FLAG:33	;等待开始
		IF CFLAG:20 == 2
			IF TALENT:恋慕
				PRINTFORML 「对了、%CALLNAME:MASTER%、咱今天要在这里开宴会哦！」
			ELSE
				PRINTFORML 「对了、今天还要准备开宴会的事呢！」
			ENDIF
		ELSE
			IF TALENT:恋慕
				PRINTFORML 「话说、今天这里会有宴会的吧……咱也是听说的哦。」
			ELSE
				PRINTFORML 「话说、不是说有宴会的嘛？怎么还没开始呀？」
			ENDIF
		ENDIF
	ELSEIF CFLAG:20	;参加宴会
		IF TALENT:恋慕
			IF BASE:酒気 > (MAXBASE:酒気 / 10) * 5
				PRINTFORML 「唔……是%CALLNAME:MASTER%啊、来得也太晚了吧？」
			ELSE
				PRINTFORML 「啊、%CALLNAME:MASTER%、是也来参加宴会的嘛？」
			ENDIF
		ELSE
			IF BASE:酒気 > (MAXBASE:酒気 / 10) * 5
				PRINTFORML 「唔……你也是、要喝酒的？」
			ELSE
				PRINTFORML 「你也是来参加宴会的嘛？」
			ENDIF
		ENDIF
	ELSE	;不参加但刚好在会场？
		IF TALENT:恋慕
			PRINTFORML 「%CALLNAME:MASTER%是来参加宴会的吗？」
			PRINTFORML 「不过、咱没有被邀请呢……」
		ELSE
			PRINTFORML 「你也是来参加宴会的嘛？」
		ENDIF
	ENDIF
ELSE	;不在会场，因此自然不可能是参加宴会的途中
	IF TIME < FLAG:33	;尚未前往
		IF CFLAG:20 == 2
			IF TALENT:恋慕
				PRINTFORML 「对了、%CALLNAME:MASTER%、咱今天要开宴会哦！」
				PRINTFORML 「位置的话、是在%宴会場表示()%啦。」
			ELSE
				PRINTFORML 「对了、今天还要准备开宴会的事呢！」
			ENDIF
		ELSE
			IF TALENT:恋慕
				PRINTFORML 「话说、听说今天在%宴会場表示()%有宴会要开哦。」
			ELSE
				PRINTFORML 「话说、今天在%宴会場表示()%、似乎会开宴会呢。」
			ENDIF
		ENDIF
	ELSEIF CFLAG:20	;宴会前后
		IF TALENT:恋慕
			PRINTFORML 「话说、听说今天在%宴会場表示()%有宴会哦。」
		ELSE
			IF CFLAG:20 >= 3
				PRINTFORML 「话说、今天在%宴会場表示()%的宴会、你没有参加嘛？」
			ELSE
				PRINTFORML 「话说、今天在%宴会場表示()%、似乎会开宴会呢。」
			ENDIF
		ENDIF
	ELSE	;单纯的未参加
		IF TALENT:恋慕
			PRINTFORML 「话说、今天在%宴会場表示()%有宴会哦。」
			PRINTFORML 「嘛、虽然没有邀请咱去呢。」
		ELSE
			PRINTFORML 「话说、今天在%宴会場表示()%、似乎会开宴会呢。」
		ENDIF
	ENDIF
ENDIF
IF CFLAG:20 && CFLAG:現在位置 == TFLAG:22 && CFLAG:行動 == 5	;宴会中
	SELECTCASE ALCOHOL_FACE(14)
		CASE "不省人事"
			PRINTFORMDW	琪露诺晕乎乎的、也不知道是不是在对着这边说话。
		CASE "酔酒"
			PRINTFORMDW	琪露诺大概是已经喝了会酒、看起来有点晃悠悠的。
		CASE "微醺"
			PRINTFORMDW	琪露诺似乎才刚开始喝酒、还很有兴致的样子。
		CASE "清醒"
			IF CFLAG:20 == 4	;怀孕了不喝酒
				PRINTFORMDW	琪露诺似乎没怎么喝、只是吃着宴会上的点心。
			ELSE	;刚开始或未开始
				PRINTFORMDW	琪露诺也还没开始喝酒、只在先吃着点心的样子。
			ENDIF
	ENDSELECT
ELSEIF CFLAG:20	;宴会前后
	IF TIME < FLAG:33
		PRINTFORMDW	琪露诺似乎一直想着今天的宴会、很期待的样子。
	ELSE
		PRINTFORMDW	琪露诺大概是因为参加过宴会、看起来还在兴奋的样子。
	ENDIF
ELSE
	PRINTFORMDW	琪露诺不怎么关心的样子、大概只是随便提一下。
ENDIF
IF TALENT:恋慕 || CFLAG:20 == 2
	SELECTCASE CFLAG:20
		CASE 0
			IF TIME < FLAG:33	;对应等待开始或尚未前往
				IF GET_MAPID(FLAG:32) == 3
					PRINTFORMW	「哼、咱也不打算参加就是了……」
				ELSEIF GET_MAPID(FLAG:32) == 0
					PRINTFORMW	「怎么会没邀请咱呢、真是的！」
				ELSE
					PRINTFORMW	「毕竟也没有邀请咱嘛、就只能是听说了呀……」
				ENDIF
			ELSE	;对应不参加但刚好在会场或单纯的未参加
				PRINTFORMW	「%CALLNAME:MASTER%也不用在意咱啦、去喝酒就好了……」
			ENDIF
		CASE 1	;对应参加宴会
			IF CFLAG:20 && CFLAG:現在位置 == TFLAG:22 && CFLAG:行動 == 5	;宴会中
				IF CIRNO_FIND_AROUND(3)	;检测是否有其他恋慕在场
					PRINTFORML	「应该不是来找咱的吧？咱也是能发现的啦。」
					PRINTFORMW	「毕竟、有盯着%CALLNAME:MASTER%看得很入迷的家伙啊……」
				ELSE
					PRINTFORML	「是来找咱？还是只为了蹭酒喝呢……」
					PRINTFORMW	「啊、酒碗的话在那边哦。要咱给%CALLNAME:MASTER%加点冰块嘛？」
				ENDIF
			ELSE	;宴会前
				PRINTFORMW 「到时候、%CALLNAME:MASTER%要参加吗？咱也想一起去啦。」
			ENDIF
		CASE 2	;主办方，虽然傻子原本没有
			IF TALENT:恋慕
				IF CFLAG:現在位置 == TFLAG:22 && CFLAG:行動 == 5	;宴会中
					IF CIRNO_FIND_AROUND(3)	;检测是否有其他恋慕在场
						PRINTFORML	「应该不是来找咱的吧？咱也是能发现的啦。」
						PRINTFORMW	「毕竟、有盯着%CALLNAME:MASTER%看得很入迷的家伙啊……」
					ELSE
						PRINTFORML	「是来找咱？还是只为了蹭酒喝呢……」
						PRINTFORMW	「啊、酒碗的话在那边哦。要咱给%CALLNAME:MASTER%加点冰块嘛？」
					ENDIF
				ELSE	;宴会前
					PRINTFORMW 「{FLAG:33 / 60}点{(FLAG:33 % 60)/10}{(FLAG:33 % 60)%10}的时候、就要开始了%CALLNAME:MASTER%可一定要来哦？」
				ENDIF
			ELSE
				IF CFLAG:現在位置 == TFLAG:22 && CFLAG:行動 == 5	;宴会中
					PRINTFORML	「唔、不过、咱有邀请过你吗？不太记得了……」
					PRINTFORMW	「……算啦！既然来了的话、就一起玩吧！」
				ELSE	;宴会前
					PRINTFORMW 「就是咱要开的宴会啦、怎么、你要来吗？」
				ENDIF
			ENDIF
		CASE 3	;喝爽了
			IF CFLAG:20 && CFLAG:現在位置 == TFLAG:22 && CFLAG:行動 == 5	;宴会中
				PRINTFORML	「唔、怎么%CALLNAME:MASTER%……有三个啊……」
				PRINTFORMDW	在宴会中喝了个爽的琪露诺摇晃着头、差点把脸栽到酒碗里去了。
			ELSE	;宴会后
				PRINTFORMW 「咱是已经去过了啦……不过%CALLNAME:MASTER%要去的话、咱跟着也可以哦？」
			ENDIF
		CASE 4	;不能喝酒或妊娠
			IF CFLAG:20 && CFLAG:現在位置 == TFLAG:22 && CFLAG:行動 == 5	;宴会中
				IF CFLAG:無自覚妊娠	;检测是否自觉妊娠
					PRINTFORML	「唔、总感觉不是很喝得下去……」
					PRINTFORMW	「……呕……呃……咱这是怎么了呀……」
				ELSE
					PRINTFORML	「不过咱现在好像不太能喝得了酒……会吐出来诶。」
					PRINTFORMW	「%CALLNAME:MASTER%就、多喝一点也没关系啦。要咱给加点冰块嘛？」
				ENDIF
			ELSE	;宴会前
				PRINTFORML 「要去参加吗？虽然咱现在是不太敢喝酒……」
				PRINTFORMW 「不过如果是跟%CALLNAME:MASTER%一起去的话、没酒喝也没关系啦。」
			ENDIF
	ENDSELECT
ENDIF

;==================================================
;冰之妖精的提及！
;是偶尔会谈到在场其他人的情况。有认识的，也有不认识的。
;尽量让傻子多交点朋友吧。
;==================================================
@CIRNO_TALK_ABOUT_OTHERS
#DIM 谈及对象
谈及对象 = CIRNO_FIND_AROUND()
SIF FIRSTTIME(@"谈及{谈及对象}")
	CFLAG:1010 ++
SELECTCASE 谈及对象
	CASE 1
		IF FLAG:73 == 谈及对象 || TALENT:1:恋慕
			IF FIRSTTIME("第一次谈到灵梦")
				PRINTFORML	「呜哇、怎么带着巫女过来了啦。」
				PRINTFORML	「有揍过咱啦、不过关系也没有很差。」
				PRINTFORMDW	说到底、妖精的人际关系基本上跟恶作剧和弹幕战是分不开的。
			ELSE
				PRINTFORML	「说起来、你跟灵梦关系很好的吧？」
				PRINTFORMDW	小妖精哼哼地笑着、似乎在想什么恶作剧的点子。
			ENDIF
		ELSE
			IF FIRSTTIME("第一次谈到灵梦")
				PRINTFORML	「那边那个颜色喜庆的家伙、是神社里的巫女呢。」
				PRINTFORML	「有揍过咱啦、不过关系也没有很差。」
				PRINTFORMDW 说到底、妖精的人际关系基本上跟恶作剧和弹幕战是分不开的。
			ELSE
				PRINTFORML	「红白色的家伙也在啊、什么时候会办宴会呢——」
				PRINTFORMDW	琪露诺跟在场的灵梦也聊了几句。
			ENDIF
		ENDIF
		CFLAG:1012 |= 1p1
	CASE 2
		IF FIRSTTIME("第一次谈到留琴")
			PRINTFORML	「以前不记得神社有女仆的来着、湖边的大房子里倒是有一个。」
			PRINTFORMDW	琪露诺跑过去问了名字之后、跟对方乖乖地问了好。
		ELSE
			PRINTFORML	「那个女仆、之前好像说是机器人来的……？」
			PRINTFORMDW	跟琪露诺谈到了在场的留琴。
		ENDIF
	CASE 3
		IF FIRSTTIME("第一次谈到喀娜")
			PRINTFORML	「那个穿洋装的家伙、是有在吓唬人的哦。难道是妖精的同伴吗？」
			PRINTFORMDW	琪露诺看起来对喀娜并不熟悉。
		ELSE
			PRINTFORML	「那是叫骚灵吗？可是……」
			PRINTFORMDW	跟琪露诺谈到了在场的喀娜。
		ENDIF
	CASE 4
		IF FIRSTTIME("第一次谈到魅魔")
			PRINTFORML	「那个戴披风的家伙、是有在恶作剧的哦。难道是妖精的同伴吗？」
			PRINTFORMDW	琪露诺看起来对魅魔并不熟悉。
		ELSE
			PRINTFORML	「那是叫恶灵吗？可是……」
			PRINTFORMDW	跟琪露诺谈到了在场的魅魔。
		ENDIF
	CASE 5
		IF FLAG:73 == 谈及对象 || TALENT:5:恋慕
			IF FIRSTTIME("第一次谈到桑尼")
				PRINTFORML	「啊、也有在跟桑尼一起玩嘛——」
				PRINTFORML	「咱也没有很在意啦、哼——」
				PRINTFORMDW	琪露诺分明就生着闷气的样子、撇开了头。
			ELSE
				PRINTFORML	「该不会其实、早就跟桑尼在一起了、只是用能力躲起来的吧？」
				PRINTFORMDW	小妖精撇着嘴、猜疑着这边的关系。
			ENDIF
		ELSE
			IF FIRSTTIME("第一次谈到桑尼")
				PRINTFORML	「桑尼也在呢、其他两个一定也在附近吧。」
				PRINTFORML	「嗯？就是认识而已啦、之前跟大酱玩捉迷藏的时候……」
				PRINTFORMDW	跟琪露诺聊起了她怎么认识的三月精。
			ELSE
				PRINTFORML	「啊、桑尼也在这里！」
				PRINTFORMDW	两只妖精撞在一起、打闹了好一会。
			ENDIF
		ENDIF
		CFLAG:1012 |= 1p2
	CASE 6
		IF FLAG:73 == 谈及对象 || TALENT:6:恋慕
			IF FIRSTTIME("第一次谈到露娜")
