;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K143_80
CALL M_KOJO_MESSAGE_COM_K143_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;初めての阴茎
		IF CFLAG:1003 == 0
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
				PRINTFORML 「！！！」
				PRINTFORML 「哇、這是、什麼啊…」
				PRINTFORML 「這比我的手臂還粗耶？」
				IF BASE:MASTER:勃起 >= 1000
					PRINTFORML 「血管都浮現出來了…這個看起來很有嚼勁啊」
					PRINTFORML 「桀桀桀、開玩笑啦、開玩笑的。應該只需要用手套弄就好了吧？」
				ELSE
					PRINTFORML 「明明還沒變硬、但就有這樣的大小嗎…」
					PRINTFORML 「這個、應該還會變更大吧？　我一定要看看」
				ENDIF
			CASE 1,2
				PRINTFORML 「哦哦、這個真的很可愛呢」
				PRINTFORML 「不用感到羞恥啊、這對我的身體來說、是剛剛好的啊」
				PRINTFORML 「好了、先來好好摸摸它吧」
			CASE 5
				PRINTFORML 「桀桀桀、一根長的十分兇惡的雞雞啊」
				PRINTFORML 「這就是你使女人哭泣的方式嗎。而現在、你也想讓我這樣哭泣嗎？」
				IF TALENT:MASTER:非童貞 == 0
					PRINTFORML 「什麼…從來沒有這樣的經驗…？」
					PRINTFORML 「……」
					PRINTFORML 「嗯…那個…對不起啊」
				ELSE
					PRINTFORML 「真期待啊。不過、先來點前戲吧」
					PRINTFORML 「……」
					PRINTFORML 「但這個、好像不太好握緊啊...」
				ENDIF
			CASE 4
				PRINTFORML 「…嘿、聽著」
				PRINTFORML 「這個太耀眼了、怎麼辦啊？」
				PRINTFORML 「都被分心了、我都沒有性慾了啊...」
			CASEELSE
				PRINTFORML 「嗯...這就是你的雞雞嗎」
				PRINTFORML 「看起來像香腸一樣美味啊...不知道能不能吃啊？」
				PRINTFORML 「桀桀桀、開玩笑、開玩笑的」
				PRINTL 
			ENDSELECT
		;阴茎2回目以降
		ELSE
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
				PRINTFORML 「像往常一樣、還是很大一根啊…」
				IF TALENT:130:恋人
					PRINTFORML 「你用這個粗大的東西、讓吉弔高潮的嗎？」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「這種大小恰好是驪駒喜愛的樣子」
				ENDIF
				IF BASE:MASTER:勃起 >= 1000
					PRINTFORML 「血管都浮現出來了…這個看起來很有嚼勁啊」
					PRINTFORML 「桀桀桀、開玩笑的啦、別當真。來、我幫你打一槍、老老實實躺好吧」
				ELSE
					PRINTFORML 「竟然還能再變的更大…真是驚人啊」
					PRINTFORML 「來吧、快點勃起起來讓我看看」
				ENDIF
				PRINTL 
			CASE 1,2
				PRINTFORML 「桀桀桀、你的東西還是一如既往地長得很可愛啊」
				IF TALENT:130:恋人
					PRINTFORML 「吉弔是喜歡這種類型的人嗎？」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「這麼小、居然還能滿足驪駒？　真的很厲害」
				ENDIF
				PRINTFORML 「好了好了、那麼硬度和持久力是怎麼樣的呢…？」
				PRINTL 
			CASE 5
				PRINTFORML 「桀桀桀、一如既往的很兇惡啊」
				IF TALENT:130:恋人
					PRINTFORML 「竟然還對這樣的東西產生興奮、吉弔的口味是好還是壞呢？」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「驪駒是不是認為、普通的東西已經無法滿足她了嗎？」
				ENDIF
				PRINTL 
			CASE 4
				PRINTFORML 「…還是一如既往地耀眼呢」
				PRINTFORML 「太難做了、難以忍受」
				PRINTL 
			CASEELSE
				PRINTFORML 「桀桀桀、外表看起來還是很美味啊」
				PRINTFORML 「只吃一點點也不行嘛？　只吃前端也行啊」
				PRINTL 
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		IF K143C_鬼傑組 == 1
		ELSEIF K143C_勁牙組 == 1
		ELSEIF K143C_剛欲同盟 == 1
		ELSE
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,口交
;==================================================
@M_KOJO_MESSAGE_COM_K143_81
CALL M_KOJO_MESSAGE_COM_K143_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;初めてのペニス
		IF CFLAG:1003 == 0
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
				PRINTFORML 「!!!」
				PRINTFORML 「诶,这是什么...」
				PRINTFORML 「比我的胳膊粗了吧?这个」
				IF BASE:MASTER:勃起 >= 1000
			        PRINTFORML 「血管都凸起来了...感觉会很有食感」
			        PRINTFORML 「嘿嘿,开玩笑的,开玩笑。我就该吸吮这个对吧?」
				ELSE
			        PRINTFORML 「没硬起来就已经这么大了...」
			        PRINTFORML 「硬起来会变得更大吧?我好想亲眼见识一下」
				ENDIF
			CASE 1,2
				PRINTFORML 「哦哦,真是可爱呢」
				PRINTFORML 「来来,不用害羞。对我的身体来说刚刚好」
				PRINTFORML 「来,用我的嘴好好吸吮它吧」 
			CASE 5
			    PRINTFORML 「嘿嘿,摆出了相当凶残的表情呢」
			    PRINTFORML 「就是用这个让女人们痛哭流涕的吗?还想对我也这么做吧?」
			    IF TALENT:MASTER:非童貞 == 0
			        PRINTFORML 「什么...一次也没有这种经验吗...?」
			        PRINTFORML 「......」
			        PRINTFORML 「抱歉...我用嘴帮你做,所以不要哭了」
			    ELSE
			        PRINTFORML 「很期待呢。不过先从前戏和调情开始吧」
			        PRINTFORML 「......」
			        PRINTFORML 「然而这个,感觉有点不好吸吮...」
			    ENDIF
			CASE 4
			    PRINTFORML 「...喂」
			    PRINTFORML 「这么刺眼能做点什么吗?」
 			    PRINTFORML 「分心了,兴致全无...」
			CASEELSE
			    PRINTFORML 「哦...这就是你的阴茎吗」
			    PRINTFORML 「看起来像香肠,感觉会很美味...可以吃吗?」
			    PRINTFORML 「嘿嘿,开玩笑的,开玩笑」
			    PRINTL
			ENDSELECT
		;ペニス2回目以降
		ELSE
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
			    PRINTFORML 「和以前一样,大小真夸张...」
			    IF TALENT:130:恋人
			        PRINTFORML 「是用这个粗壮的东西满足着吉弔吗?」
				ELSEIF TALENT:133:恋人
			        PRINTFORML 「看起来很符合骊驹的喜好呢」
			    ENDIF
			    IF BASE:MASTER:勃起 >= 1000
			        PRINTFORML 「血管都凸起来了...感觉会很有食感」
			        PRINTFORML 「嘿嘿,开玩笑的,开玩笑。来,我给你口交,你别动」
			    ELSE
			        PRINTFORML 「说这还能变得更大...真是令人震惊」
			        PRINTFORML 「来来,快让它勃起吧」
			    ENDIF
			    PRINTL
			CASE 1,2
			    PRINTFORML 「嘿嘿,外表和以前一样可爱呢」
			    IF TALENT:130:恋人
			        PRINTFORML 「吉弔喜欢这种类型吗?」
			    ELSEIF TALENT:133:恋人
			        PRINTFORML 「虽然这么小,但也能满足骊驹吗?真厉害」
			    ENDIF
			    PRINTFORML 「那硬度和持久力怎么样呢...?」
			    PRINTL 
			CASE 5
			    PRINTFORML 「嘿嘿,和以前一样凶残的表情呢」
			    IF TALENT:130:恋人
			        PRINTFORML 「被这样的粗物插入,不知道吉弔的品味是好是坏」
			    ELSEIF TALENT:133:恋人
			        PRINTFORML 「骊驹没法被普通的东西满足吗?」
			    ENDIF
			    PRINTL
			CASE 4
			    PRINTFORML 「...一如既往的刺眼啊」
			    PRINTFORML 「好难做,做不了」
			    PRINTL
			CASEELSE
			    PRINTFORML 「嘿嘿,看起来和以前一样美味」
			    PRINTFORML 「就吃一点头部可以吗?没关系的,就吃一点头部」
			    PRINTL 
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

