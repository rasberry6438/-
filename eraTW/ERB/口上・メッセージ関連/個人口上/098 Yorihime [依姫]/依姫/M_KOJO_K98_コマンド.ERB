RETURN RESULT

@M_KOJO_MESSAGE_COM_K98_180_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		GOTO YORIHIME_LUBRICANT
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;発情顔絵を表示（オプションでオンにしていないと表示されない）
		IF !TEQUIP:0 && !TEQUIP:1
			CALL PRINT_FACE, [[依姫]], "発情", "裸", "", ""
		ELSE
			CALL PRINT_FACE, [[依姫]], "発情", "服", "", ""
		ENDIF
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「嗯嗯……嗯……HPH」","L"
					CALL HPH_PRINT,@"（好厉害……触摸的方式,好色情……HPH）","L"
					PRINTFORMDL 将大量倒出的润滑油涂抹在%CALLNAME:TARGET%的身体上
					PRINTFORMDW 感受到四处游走的手的触感,%CALLNAME:TARGET%轻轻地喘着气……
				CASE 1
					CALL HPH_PRINT,@"「连那种地方都要……呜HPH 不涂也……嗯HPH」","L"
					PRINTFORMDL 你将润滑油涂抹到腋下和臀缝里
					PRINTFORMDW %CALLNAME:TARGET%可能是因为害羞,脸颊泛红……
				CASE 2
					CALL HPH_PRINT,@"「还、还要做吗……？HPH」","L"
					PRINTFORMDL 在手心倒上大量润滑油,涂抹在%CALLNAME:TARGET%的身体上
					PRINTFORMDW 当然重要的地方要仔细涂。你把%CALLNAME:TARGET%的身体涂得满是润滑油
			ENDSELECT
		;それ以外
		ELSE
			$YORIHIME_LUBRICANT
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL 你把润滑油的瓶子递给%CALLNAME:TARGET%,说也想让她涂在自己身上
					PRINTFORML 「我知道了……」
					PRINTFORMDW %CALLNAME:TARGET%在手心倒上润滑油,像涂抹一样用手抚摸你的肌肤……
				CASE 1
					PRINTFORML 「嗯……」
					PRINTFORMDL 倒上润滑油时,%CALLNAME:TARGET%发出了小小的声音
					PRINTFORMDW 虽然放在怀里温着,但可能还是有点冷
				CASE 2
					PRINTFORMDL 用整整一瓶润滑油把%CALLNAME:TARGET%的身体弄得到处都是
					PRINTFORML 「……考虑到之后的事情头都大了呢」
					PRINTFORMDW 这可是不能说的约定啊
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

