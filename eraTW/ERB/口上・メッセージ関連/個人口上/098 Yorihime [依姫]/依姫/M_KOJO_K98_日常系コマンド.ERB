@M_KOJO_MESSAGE_COM_K98_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;会話不能
	IF TCVAR:302
		;顔絵を表示（オプションでオンにしていないと表示されない）
		;お風呂
		IF BATHCHECK(CFLAG:MASTER:現在位置) && IN_HOME(CFLAG:MASTER:現在位置)
			CALL PRINT_FACE, [[依姫]], "通常", "裸", "", ""
		;それ以外
		ELSE
			CALL PRINT_FACE, [[依姫]], "通常", "服", "", ""
		ENDIF
		;親密５以上
		IF ABL:親密 > 4
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「那个……」
					PRINTFORMDL %CALLNAME:TARGET%试图找个话题
					PRINTFORMDW 但最后还是变得尴尬起来……
				CASE 1
					PRINTFORML 「——我喜欢吃的东西?」
					PRINTFORML 「啊……我很喜欢吃桃子……」
					PRINTFORMDW 试图缓解尴尬的气氛,结果却更加尴尬了……
				CASE 2
					PRINTFORML 「有点……口渴了呢」
					PRINTFORMDL 是啊。你附和着%CALLNAME:TARGET%
					PRINTFORMDW ……不知为何,%CALLNAME:TARGET%叹了口气……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「话说完了吗？」
					PRINTFORMDW 试着聊了很多在地上的体验,但依姫似乎提不起兴趣……
				CASE 1
					PRINTFORML 「…………」
					PRINTFORMDW %CALLNAME:TARGET%完全不理会你说话,一直在摆弄手里的古书型终端……
				CASE 2
					PRINTFORML 「——尴尬？只有你一个人觉得尴尬吧?」
					PRINTFORMDL 那依姫应该也没有很开心……
					PRINTFORMDW 她大概根本就无所谓……
			ENDSELECT
		ENDIF
