IF LOCAL
	IF CFLAG:睡眠
		PRINTFORML 
		RETURN 1
	;通常
	ELSE
		;依姫と一緒に作った料理を食べる
		IF DISH_ASSI == [[依姫]]
			;判定
			SELECTCASE TFLAG:193
				;大成功
				CASE 1
					PRINTFORML 「――谢谢款待」
					PRINTFORML 「洗碗我来处理。你负责擦桌子」
					PRINTFORMDL 接过%CALLNAME:TARGET%递来的湿布
					PRINTFORMDW 不把一切都推给别人，果然%CALLNAME:TARGET%很可靠
				;成功
				CASE 0
					PRINTFORMDL 你一边动筷子，一边说着『好吃』，表达出对菜品的评价
					PRINTFORML 「嘴里还有食物的时候不要说话。这样很没礼貌哦」
					PRINTFORMDL %CALLNAME:TARGET%默默地把菜品送入口中
					PRINTFORMDW 总觉得，她的口吻像妈妈一样……
				;失败
				CASE -1
					PRINTFORML 「……不好吃」
