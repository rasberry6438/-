;ハードな調教コマンド
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;140,イラマチオ
;==================================================
;==================================================
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K98_140
RETURN 0

@M_KOJO_MESSAGE_COM_K98_140
CALL M_KOJO_MESSAGE_COM_K98_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K98_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;奉仕精神が１０以上
	IF ABL:奉仕精神 > 9
		;このコマンドであなたが射精
		IF NOWEX:MASTER:射精
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL 射精的瞬间，你将腰部插入%CALLNAME:TARGET%，好像要让她怀孕一样
					CALL HPH_PRINT,@"「咦呃呃HPH 嗯嗯……咯……HPH」","L"
					PRINTFORMDW 或许是喝不完这么多，%CALLNAME:TARGET%突然呛到，精液从她的鼻子飞溅而出……
				CASE 1
					CALL HPH_PRINT,@"「嗯呃HPH 嗯呃HPH 嗯啊HPH 嗯扑HPH」","L"
					PRINTFORMDL 紧紧抓住%CALLNAME:TARGET%的头，猛烈地抽插着腰部，迎接射精的到来
					PRINTFORMDW 泪水和鼻涕，以及为奉献而感到愉悦，%CALLNAME:TARGET%的表情完全崩溃了……
				CASE 2
					PRINTFORMDL 在你多次猛烈抽插后，你将腰部插入深处，直到阴茎完全插入内部
					CALL HPH_PRINT,@"「噢呵呃HPH 咕HPH 嗯嗯、噗HPH」","L"
					PRINTFORMDW 如果流出来就会受到惩罚，你说道，%CALLNAME:TARGET%流下眼泪点头表示理解……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「嗯吞、嗯HPH 嗯扑HPH 呼HPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%动着脸，深深吞噬着被粗暴插入的阴茎……
					CALL HPH_PRINT,@"（如果能让他……感到愉悦的话……咯HPH）","W"
				CASE 1
					CALL HPH_PRINT,@"「咯HPH 嗯——咯……呼——咯……HPH 吞噜……HPH」","L"
					PRINTFORMDL %CALLNAME:TARGET%重新吞咽被粗暴插入的阴茎，
					PRINTFORMDW 嘴巴变窄，缓慢地摇动着脸部前后……
				CASE 2
					PRINTFORMDL 就像玩具一样，你侵犯了%CALLNAME:TARGET%的喉咙，然后拔出阴茎，让%CALLNAME:TARGET%张开嘴巴
					CALL HPH_PRINT,@"「啊……哈……啊……HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%口水淌下，渴望着，喉咙轻微抽搐着……
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;このコマンドであなたが射精
		IF NOWEX:MASTER:射精
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「——嗯咕呼呼呃！？？」
					PRINTFORMDL 你将阴茎插入%CALLNAME:TARGET%的喉咙深处，舒服地射精了
					PRINTFORMDW 大量精液穿过%CALLNAME:TARGET%的食道，流入胃中……
				CASE 1
					PRINTFORML 「噗————」
					PRINTFORMDL 粗暴地插入阴茎，无声射精
					PRINTFORMDW 你将欲望的污秽一滴不剩地倾注进%CALLNAME:TARGET%的胃中……
				CASE 2
					PRINTFORMDL 砰地一声，将阴茎推入喉咙深处，抽动几次后射精
					PRINTFORML 「————」
					PRINTFORMDW %CALLNAME:TARGET%的脸被泪水和鼻涕弄得一团糟，瞳孔翻白……
			ENDSELECT
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「咯——！？ 嗯、嗯呃！」
					PRINTFORMDL 紧握住%CALLNAME:TARGET%的头，不停地用腰部冲击侵犯着喉咙
					PRINTFORMDW %CALLNAME:TARGET%看起来在痛苦中拍打着你的腰部……
				CASE 1
					PRINTFORML 「——咕、诶……喔诶……诶……咯」
					PRINTFORMDL 一下下有力地冲击，慢慢地，慢慢地抽出
					PRINTFORMDW 在喉咙收缩的感触中，你沉醉其中，呼吸急促地漏出声音
				CASE 2
					PRINTFORML （呼吸、停……止了……）
					PRINTFORMDL 在%CALLNAME:TARGET%即将失去意识时，你慢慢地将阴茎抽出
					PRINTFORMDW %CALLNAME:TARGET%一边用手捂住喉咙，一边剧烈地吞咽着……
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

