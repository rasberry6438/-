			ELSEIF TALENT:恋慕
				PRINTFORMW 「看来你很受欢迎啊，%CALLNAME:MASTER%」
			ELSE
				PRINTFORMW 「呵呵，你碍事了」
			ENDIF
			RETURN 1
		ENDIF
		;その他
		PRINTFORML  
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORML 
		RETURN 1
	ENDIF
