			RETURN 1
		ELSE
			CALL HPH_PRINT,@"「嗯嗯HPH嗯啊HPH」","L"
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「嗯HPH再更多一点HPH」","L"
			CALL HPH_PRINT,@"「更多地戏弄我HPH」","L"
		ELSEIF RAND:2 == 0
