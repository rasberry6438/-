@M_KOJO_POWERED_BY_DARKMAN_MESSAGE_COM_K140_80
CALL M_KOJO_POWERED_BY_DARKMAN_MESSAGE_COM_K140_80_1
RETURN RESULT

@M_KOJO_POWERED_BY_DARKMAN_MESSAGE_COM_K140_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
CALL M_KOJO_POWERED_BY_DARKMAN_COLOR_K140
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「竟然想要被大天狗手淫，真是贪得无厌HPH」","L"
		RETURN 1
	ENDIF
	;基本设定
	;时姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「被大天狗的手弄得高潮，真是奢侈HPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「天狗也不常见到这么大的呢HPH」","L"
		ELSE
			CALL HPH_PRINT,@"「已经快要射了吗？好吧，射出来吧HPH」","L"
		ENDIF
	;技巧4以上
	ELSEIF ABL:技巧 > 3
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「稍微熟练了一点吧HPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯HPH有点臭……HPH」","L"
		ELSE
			CALL HPH_PRINT,@"「呵呵，做得不错吧HPH」","L"
		ENDIF
	;其他情况
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「这样做舒服吗？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「竟然让大天狗手淫，真是」
		ELSE
			PRINTFORMW 「这样的怎么样？」
		ENDIF
	ENDIF
ENDIF
RETURN 1

