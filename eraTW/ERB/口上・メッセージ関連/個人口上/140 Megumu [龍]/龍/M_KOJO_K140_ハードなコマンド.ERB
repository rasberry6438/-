;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
CALL M_KOJO_POWERED_BY_DARKMAN_COLOR_K140
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「んんん～～～！」
			CALL HPH_PRINT,@"「嗯啊……已经不行了，不要再……HPH」","L"
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「ん～～～HPH」","L"
			PRINTFORMW 龙敏锐地感受到了%CALLNAME:MASTER%的射精，并巧妙地用喉咙来促进。
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯HPH嗯啊HPH」","L"
			PRINTFORMW 龙敏锐地感受到了%CALLNAME:MASTER%的射精，并巧妙地用喉咙来促进。
		ELSE
			CALL HPH_PRINT,@"「嗯嗯HPH嗯嗯嗯HPH」","L"
			PRINTFORMW 龙敏锐地感受到了%CALLNAME:MASTER%的射精，并巧妙地用喉咙来促进。
		ENDIF
	;其他情况
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯～！」
			PRINTFORMW 龙因为被%CALLNAME:MASTER%插入阴茎而呻吟着。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯嗯～～～！！！」
			PRINTFORMW 龙因为被%CALLNAME:MASTER%插入阴茎而呻吟着。
		ELSE
			PRINTFORMW 「嗯～！！！」
			PRINTFORMW 龙因为被%CALLNAME:MASTER%插入阴茎而呻吟着。
		ENDIF
	ENDIF
ENDIF
RETURN 1

