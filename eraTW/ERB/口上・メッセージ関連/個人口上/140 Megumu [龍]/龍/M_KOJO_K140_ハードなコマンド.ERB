;==================================================
;150,胸の谷間を犯す
;==================================================
@M_KOJO_POWERED_BY_DARKMAN_MESSAGE_COM_K140_150
CALL M_KOJO_POWERED_BY_DARKMAN_MESSAGE_COM_K140_150_1
RETURN RESULT

@M_KOJO_POWERED_BY_DARKMAN_MESSAGE_COM_K140_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
CALL M_KOJO_POWERED_BY_DARKMAN_COLOR_K140
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			CALL HPH_PRINT,@"「嗯……有点强硬呢HPH」","L"
		RETURN 1
	ENDIF
	;基本セット
	;胸3以上
	IF ABL:胸 > 2
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「没有必要这么强硬，我的胸部已经很好了HPH」","L"
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯嗯……这么喜欢胸部吗？」
		ELSE
			CALL HPH_PRINT,@"「嗯HPH可能我喜欢被强硬……HPH」","L"
		ENDIF
	;其他情况
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「就算不这么强硬，胸部也不会逃跑的……HPH」","L"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯HPH没关系，来侵犯我的胸部吧HPH」","L"
		ELSE
			CALL HPH_PRINT,@"「就算不这样，大天狗的胸部也是属于你的HPH」","L"
		ENDIF
	ENDIF
ENDIF
RETURN 1
