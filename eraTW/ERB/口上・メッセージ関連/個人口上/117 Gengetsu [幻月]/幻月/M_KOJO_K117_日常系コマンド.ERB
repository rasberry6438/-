;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;失敗、だと思う
	IF TFLAG:194
		SELECTCASE TFLAG:194
			;人目があるのは稍微……
			CASE 1
				IF TALENT:恋慕
					PRINTFORML
					PRINTFORML 「要是有人来了你打算怎么办啊……」
					PRINTFORML 「如果你想做的话、我是会回应你的啦……」
					PRINTFORMW 好像不行呢。
				ELSE
					PRINTFORML
					PRINTFORML 「你是说被谁看见也可以吗？」
					PRINTFORMW 「你这变态」
				ENDIF
			;外でするのは……
			;邀请去阴暗处の時って呼ばれるのかな
			CASE 2
				IF TALENT:恋慕
					PRINTFORML 「唉、想在这里做吗……？」
					PRINTFORML 「这个有点……」
					PRINTFORMW 似乎对在外面做感到很羞耻。
				ELSE
					PRINTFORML 「想在外面做什么的……」
					PRINTFORMW 「你这变态」
				ENDIF
			;多人数はやだ
			;恋慕以外用とわけたいかも
			CASE 3
				PRINTFORML
				PRINTFORML 「呐、%CALLNAME:MASTER%」
				PRINTFORMW 「只有我一个无法满足吗？」
			;そんな気分じゃないよ
			CASE 4
				IF TALENT:恋慕
					PRINTFORML
					PRINTFORML 「唉、现在？」
					PRINTFORMW 「那个、现在还没有那个心情呢……」
				ELSE
					PRINTFORML
					PRINTFORML 「呐%CALLNAME:MASTER%」
					PRINTFORMW 「不能注意下气氛之类的吗？」
				ENDIF
		ENDSELECT
	;初回
	ELSEIF TCVAR:初次拥抱
		SELECTCASE TCVAR:初次拥抱
			;成功
			;なんか弱々しい気がする
			CASE 1
				PRINTFORML
				PRINTFORML 「嗯……」
				PRINTFORML 「是想要抱住我呢……」
				PRINTFORMW 试图推倒幻月时、她带着紧张的神色把身体托付给了你。
				PRINTFORML 「那个、因为是第一次……请温柔一点、呐」
				PRINTFORMW %CALLNAME:MASTER%为了尽可能减少幻月的紧张感在脸上轻柔的吻了一下。
				PRINTFORMW 「那么就……请多指教了呢」

			;接吻もしてないのに
			CASE 2
				PRINTFORML
				PRINTFORML 「诶？　现在、就要」
				PRINTFORMW 幻月对%CALLNAME:MASTER%的举动感到惊讶。
				PRINTFORMW 「不是说不想要，但是……那个，我们还没接吻过吧？」
				PRINTFORMW 幻月看起来很不满的样子，%CALLNAME:MASTER%松开了手。
				PRINTFORMW 「等一下、我可不希望在这里停下来！」

			;媚薬OR発情には勝てなかった
			;幻月には被逆推たい
			CASE 3
				PRINTFORML
				PRINTFORML 「啊……」
				PRINTFORMW %CALLNAME:MASTER%试图推倒幻月时她用充满情欲的眼睛看着这边。
				PRINTFORML 「呐、我的身体好疼痛呀……」
				PRINTFORMW 「%CALLNAME:MASTER%能够……让这份疼痛镇定下来吗？」

			;刻印のせいで……
			CASE 4
				PRINTFORML
				PRINTFORML 幻月被推倒，她用手抓住%CALLNAME:MASTER%，瞪着他。
				PRINTFORML 「%CALLNAME:MASTER%、你打算干什么？」
				PRINTFORML 你知道是什么吧？　我就这么回答你。
				PRINTFORML 「别说这种蠢话！　放开我！」
				PRINTFORMW 幻月挣扎着用翅膀扑腾着，但是她的全力抵抗也被%CALLNAME:MASTER%的双臂轻易压制住。
				PRINTFORML 一个恶魔竟然无法挣脱这点束缚吗？
				PRINTFORMW 库、她的表情扭曲了。但是，似乎并没有多大的抵抗。
				PRINTFORML 她的身体似乎放弃了抵抗，试图接受%CALLNAME:MASTER%。
				PRINTFORML 「%CALLNAME:MASTER%、做好觉悟了吗……！」
				PRINTFORMW %CALLNAME:MASTER%舔了舔嘴唇，把手放在了她的衣服上。

			;幻月には関係ないですね
			CASE 5

			;まんざらでもない
			;これって失敗？ →失敗らしいです
			CASE 6
				PRINTFORML
				PRINTFORML 「想要和我做吗？」
				PRINTFORML 「呼呼……”还”不行哟」
				PRINTFORMW 幻月浮现出含蓄的微笑。

			;ダメ			
