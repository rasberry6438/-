@M_KOJO_UPDATE_K87
;口上独自のUPDATE用
;ENCOUNTER専用に独自分岐フラグを仕込んだ場合等にどうぞ
;UPDATE判定でCFLAGを使用場合、角色番号の指定が必要になります
IF FIRSTTIME("U87-1",0,87) && CFLAG:87:面識
	PRINTFORML 改变了称呼的情况下的救済措置
	PRINTFORML 给没有改变称呼的玩家添麻烦了、实在抱歉
	MASTERNAME:87 = 
	PRINTFORMW 「那么、请先告诉我你的名字」
	PRINTL 请输入名称(请直接输入或按钮选择)
	IF NAME:MASTER != "你"
		LOCALS = %NAME:MASTER%
	ELSE
		LOCALS = %CALLNAME:MASTER%
	ENDIF
	PRINTBUTTON @"[名字就叫(%LOCALS%)]", @"%LOCALS%"
	PRINTL 　
	;名前を入力するループ
	$INPUT_LOOP_ONAMAE
	INPUTS
	;/見事な響子口上の丸パクだと感心するがどこもおかしくはない
	; 行頭行尾の空白文字削除
	LOCALS = %RESULTS%
	REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
	;10文字以上は受け付けない
	IF STRLENS(LOCALS) > 10
		CLEARLINE 1
		REUSELASTLINE 「…有点太长了呢。您有统称或者昵称吗？」
		GOTO INPUT_LOOP_ONAMAE
	;空白は受け付けない
	ELSEIF RESULTS == ""
		PRINTL 请输入名称
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		; 同姓同名
		IF FLAG:扮演 != 87 && STRCOUNT(LOCALS, "\(寅丸\)\*\\s\*星")
			CLEARLINE 1
			REUSELASTLINE 「…你在開玩笑吧」W
			GOTO INPUT_LOOP_ONAMAE
		; 白蓮が主人ではなく、白蓮というフレーズが含まれている。もしくは
		; 響子が主人ではなく、幽谷もしくは響子というフレーズが含まれている
		ELSEIF (FLAG:扮演 != 55 && STRCOUNT(LOCALS, "聖\*\\s\*白蓮")) || (FLAG:扮演 != 88 && STRCOUNT(LOCALS, "\(幽谷\)\*\\s\*響子\$"))
			CLEARLINE 1
			REUSELASTLINE 「…是那个对吧」
			GOTO INPUT_LOOP_ONAMAE
		; 命蓮寺勢はじき
		; うんそうだね多多良勝五郎とか教える人もいるかもしれないもんね
		ELSEIF (FLAG:扮演 != 39 && LOCALS == "納茲琳") || (FLAG:扮演 != 40 && STRCOUNT(LOCALS, "\(多多良\)\*\\s\*小傘")) || (FLAG:扮演 != 85 && STRCOUNT(LOCALS, "\(雲居\)\*\\s\*一輪")) || (FLAG:扮演 != 86 && STRCOUNT(LOCALS, "\(\(村紗\)*\\s\*水蜜\|村紗\)"))
			CLEARLINE 1
			REUSELASTLINE 「…因为很容易和我们家的那个混淆、如果可以的话请换一个」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF (FLAG:扮演 != 41 && STRCOUNT(LOCALS, "\(封獣\)\*\\s\*鵺")) || (FLAG:扮演 != 69 && STRCOUNT(LOCALS, "\(二ツ岩\)\*\\s\*猯藏"))
			CLEARLINE 1
			REUSELASTLINE 「唉…难道说是恶作剧吗？」
			GOTO INPUT_LOOP_ONAMAE
		; 道教勢力はじき
		ELSEIF (FLAG:扮演 != 89 && STRCOUNT(LOCALS, "\(宮古\)\*\\s\*芳香")) || (FLAG:扮演 != 90 && STRCOUNT(LOCALS, "\^霍\*\\s\*青娥")) || (FLAG:扮演 != 91 && STRCOUNT(LOCALS, "\(蘇我\)\*\\s\*屠自古")) || (FLAG:扮演 != 92 && STRCOUNT(LOCALS, "\(物部\)\*\\s\*布都")) || (FLAG:扮演 != 56 && STRCOUNT(LOCALS, "\(\(豊聡耳\)\*\\s\*神子\|聖徳太子\)"))
			CLEARLINE 1
			REUSELASTLINE 
			GOTO INPUT_LOOP_ONAMAE
		; 博麗霊夢というフレーズが含まれてる。もしくは
		; 東風谷早苗というフレーズが含まれている
		ELSEIF STRCOUNT(LOCALS, "\(博麗\)\*\\s\*霊夢") || STRCOUNT(LOCALS, "\(東風谷\)\*\\s\*早苗")
			PRINTFORML 「你看起来并没有她那样放纵…这样真的可以么？」
		; 霧雨魔理沙というフレーズが含まれてる。
		ELSEIF STRCOUNT(LOCALS, "\(霧雨\)\*\\s\*魔理沙")
			PRINTFORML 「一听上去就像马上放出很强的直线激光似得！…这样真的可以吗？」
		; 直接入力ならいけると思ったか？弾くぜ！
		ELSEIF FIRSTTIME("U87-1") &&  ABL:親密 < 4 && STRCOUNT(LOCALS, "\(\(\(哥哥\|尼\)\|\(姐姐\|内\)\)\(桑\|酱\)\)\$")
			CLEARLINE 1
			REUSELASTLINE 「这实在有点…」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF FIRSTTIME("U87-1") &&  ABL:親密 < 7 && STRCOUNT(LOCALS, "\(\(\(哥哥\|尼\)\|\(姐姐\|内\)\)\(样\|大人\)\)\$")
			CLEARLINE 1
			REUSELASTLINE 「这实在有点…」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF FIRSTTIME("U87-1") &&  (ABL:従順 < 7 || ABL:奉仕精神 < 7) && STRCOUNT(LOCALS, "\(\(御\|ご\)\(主人\|Master\)\(様\|大人\)\)$")
			CLEARLINE 1
			REUSELASTLINE 「这实在有点…」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF FIRSTTIME("U87-1") &&  (!TALENT:恋人 || !TALENT:恋慕) && STRCOUNT(LOCALS, "\(\(旦那\|相公\)\(样\大人\)\)\$")
			CLEARLINE 1
			REUSELASTLINE 「这实在有点…」
			GOTO INPUT_LOOP_ONAMAE
		ELSE
			PRINTFORML 「%RESULTS%…是么」
		ENDIF
		PRINTL [0] 是
		PRINTL [1] 否
		;入力した名前を確定するループ
		$INPUT_LOOP_KAKUTEI
		INPUT
		IF RESULT == 0
			;你の名前を%MASTERNAME:87%に格納
			MASTERNAME:87 = %RESULTS%
			PRINTFORML 
		ELSEIF RESULT == 1
			PRINTL 请输入名称
			GOTO INPUT_LOOP_ONAMAE
		ELSE
			PRINTL 请输入名称
			GOTO INPUT_LOOP_ONAMAE
		ENDIF
	ENDIF
ENDIF
SIF TALENT:87:自尊心 == -1
	TALENT:87:自尊心 = 1
IF TALENT:87:舌頭靈活 && ABL:87:舌 < 6
	TALENT:87:舌頭靈活 = 0
	TALENT:87:猫舌 = 1
ENDIF
SIF ABL:87:舌 == 6 && TALENT:87:舌頭靈活 == 0
	TALENT:87:舌頭靈活 = 1
RETURN 1

