@M_KOJO_MESSAGE_COM_K16_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:無知
			PRINTFORMW 「这是什么啊……？触摸这个就可以了吗？？」
			PRINTFORMW  蕾米莉亚摸到了粗糙的东西
			PRINTFORML 「这样的奖励就可以了吗、真是个奇怪的家伙啊……」
		ELSE
			PRINTFORML 「嗯……嘿嘿……一跳一跳的呢」
			PRINTFORMW 「真是的……竟然想让我碰这么粗糙的东西……真是个无礼的人啊……%UNICODE(0x2764)%」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:無知
		PRINTFORML 「又用手可以吗？」
		IF TALENT:恋慕
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「我知道了%UNICODE(0x2764)%」
				PRINTFORMW 「好了！快点把白色的东西射出来」
			CASE 1
				PRINTFORML 「还没好吗？快射出来！」
			ENDSELECT
		ELSE
			PRINTFORML 「这样真的好吗?」
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「好像要握着呢……这个」
			CASE 1
				PRINTFORMW 「没办法啊……。这样可以吗？」
			ENDSELECT
		ENDIF
	ELSE
		PRINTFORML 「你想要我用手吗？」
		IF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「可以哟……」
				PRINTFORMW 「也可以好好看到%CALLNAME:MASTER%的反应呢%UNICODE(0x2764)%」
			CASE 1
				PRINTFORML 「嘿嘿……竟然想让我摸这么粗糙的东西……%UNICODE(0x2764)%」
				PRINTFORMW 「好了%UNICODE(0x2764)%快点射精吧%UNICODE(0x2764)%」
			CASE 2
				PRINTFORML 「让我碰这么凶恶的东西……%UNICODE(0x2764)%」
				SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「这是想侵犯我吧……那就再硬一点吧……%UNICODE(0x2764)%」
				CASE 1
					PRINTFORML 「光是玩弄前端就这么胡闹……」
					PRINTFORMW 「和平时不同、真是个粗暴的孩子……%UNICODE(0x2764)%」
				ENDSELECT
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「嘿嘿……我的手够大的呢……%UNICODE(0x2764)%」
			CASE 1
				PRINTFORML 「光靠手就满足了吗……？」
			CASE 2
				PRINTFORML 「好了……积存的东西放出来了吗？%UNICODE(0x2764)%」
			ENDSELECT
		RETURN 1
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

