@UNIQUE_FA_49(CHARA, ARGS,時間係数)
#DIM CHARA
#DIM CHAIN
#DIM 現在地
#DIM メッセージ表示
#DIM 時間係数
現在地 = CFLAG:CHARA:現在位置
メッセージ表示 = 現在地 == CFLAG:MASTER:現在位置 ? 1 # 0

UNIQUE_FA_NAME:CHARA = 被妄想困扰着

SELECTCASE ARGS
	CASE "発生"
		SIF CHARA != 49 || CFLAG:49:998 != 1
			RETURN 0
		SIF TCVAR:CHARA:350 == 1
			RETURN 0
		IF メッセージ表示
			PRINTFORML 似乎有些不太正常，覚脸颊泛着微妙的红色，呼吸也显得急促
			PRINTFORMW 覚将下唇咬的发白不断忍耐的样子，不由得令人担心……
			CFLAG:49:1536 = 1
			CFLAG:49:1515 ++
			RETURN 60 + RAND:30
		ENDIF
	CASE "行動中"
		;行動中の処理、地の文もここで
		IF メッセージ表示
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 覚合拢的腿根不断摩擦，在看不见的地方不断扭曲伸展着脚踝
			CASE 1
				PRINTFORMW 覚神情恍惚的发着呆，双臂不自覚的抱住身体
			CASE 2
				PRINTFORMW 覚努力抑制着喉咙中娇滴滴的呻吟声
			ENDSELECT
		ENDIF
	CASE "完了"
		;行動時間を使い切ったときの処理、地の文もここで
		SIF メッセージ表示
			PRINTFORMW 覚长长舒了口气，瘫倒在椅子上
	CASE "中断"
		PRINTFORMW 感受到从身后传来的幽怨目光
		;中断したときの処理、地の文もここで
	CASE "交手"
		;自由行動に付き合ったとき用メッセージ
		PRINTFORML %CALLNAME:MASTER%将手伸向样子不太对劲的覚
		PRINTFORML 覚痉挛的同时努力抑制的短促呻吟漏了出来
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 裙子紧夹在腿间的部分沾到了些许水渍……
		CASE 1
			PRINTFORMW 面对%CALLNAME:MASTER%佯装镇静掩饰着慌乱……
		CASE 2
			PRINTFORMW 从覚的方向飘来了湿暖的香气……
		ENDSELECT
		SELECTCASE RAND:5
		CASE 0
			PALAM:49:快Ｖ += 10000
		CASE 1
			PALAM:49:快Ａ += 10000
		CASE 2
			PALAM:49:快Ｃ += 10000
		CASE 3
			PALAM:49:快Ｂ += 10000
		CASE 4
			PALAM:49:快Ｍ += 10000
		ENDSELECT
		CALL ORGASM_ADD(49)
	CASE "ソース"
		TCVAR:49:350 = 1
ENDSELECT
