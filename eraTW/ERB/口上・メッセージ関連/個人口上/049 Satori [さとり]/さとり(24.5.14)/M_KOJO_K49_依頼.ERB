				CASE "一般依頼名"
				CASE "給花澆水"
				CASE "誰的遺失物"
			ENDSELECT
			RETURN
		ENDIF
		SELECTCASE STR_DATA_IRAI(IRAI_ID, "依頼名", CLIENT_ID)
			CASE "固有依頼名"
		ENDSELECT
ENDSELECT

