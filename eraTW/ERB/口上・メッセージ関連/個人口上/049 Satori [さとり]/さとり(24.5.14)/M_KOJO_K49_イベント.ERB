	;--------------------------------------------------
	;借用一下这里啦～
	;——————————————————————————————————————————————————
	;上周目陥落状態
	; SIF CFLAG:49:面識 != 0
		; 上周目陥落状態 = 陥落状態()
	;——————————————————————————————————————————————————
	;HATE()上限每日决定
	IF 陥落状態() == 2
		IF GETBIT(CFLAG:49:1491,0) || GETBIT(CFLAG:49:1491,1)
			CFLAG:49:1101 = 5+RAND:10
		ELSE
			CFLAG:49:1101 = 10+RAND:10
		ENDIF
	ENDIF
