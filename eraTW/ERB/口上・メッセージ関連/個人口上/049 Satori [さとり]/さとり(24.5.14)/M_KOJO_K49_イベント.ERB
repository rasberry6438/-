	;HATE()次每日清零
	CFLAG:49:1102 = 0 
	;——————————————————————————————————————————————————
	;1019一日一次的重置
	; SIF CFLAG:49:1019 == 3
		; CFLAG:49:1019 = 2
	;--------------------------
	;忘记咯～
	IF CFLAG:49:1104 == 1
		CFLAG:49:面識 = 0
		CALL EVENT_49_1103_RESET
	ENDIF
