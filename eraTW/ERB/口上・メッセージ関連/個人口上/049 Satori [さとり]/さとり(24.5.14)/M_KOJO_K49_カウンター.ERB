;==================================================
@M_KOJO_MESSAGE_COUNTER_K49_1
IF GETBIT (CFLAG:1490,0) && !TALENT:恋慕 && !TALENT:恋人
	SELECTCASE RAND:4
	CASE 3
		PRINTFORMDW  觉带着莫名的神情望着你
	CASE 2
		PRINTFORMDW  觉的眼神中透露出一丝厌恶的神情
		CFLAG:49:好感度 -= 1
	CASE 1
		PRINTFORMDW  觉有些冷漠地望着你
	CASE 0
		PRINTFORMDW  觉有些微妙地与你拉开了距离
	ENDSELECT
	RETURN 0
ELSEIF SELECTCOM == 312 && GET_TARGETNUM() == 1
	CALL UFUFUTO49(1)
ELSE
	CALL EVENT_COUNTER_MESSAGE
	FOR LOCAL,0,3
		IF PALAM:LOCAL == 9999
			LOCAL:1 = 2
		ELSEIF PALAM:LOCAL >= 5000
			LOCAL:1 = 1
		ENDIF
	NEXT
	IF LOCAL:1 == 2
		PRINTFORMDW  双瞳润湿的觉，轻轻吐露出微热的气息
	ELSEIF LOCAL:1 == 1
		PRINTFORMDW  觉嘟起嘴，好像要说些什么的样子
	ENDIF
	CALL M_KOJO_MESSAGE_COUNTER_K49_1_1
ENDIF
RETURN RESULT
@M_KOJO_MESSAGE_COUNTER_K49_1_1
IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
	CALL DRUNKER49(300)
ELSEIF GROUPMATCH(SELECTCOM,310,311,313,314,315,316,330) && GET_TARGETNUM() == 1
	CALL UFUFUTO49(1)
ELSEIF 陥落状態() >= 4
	IF 陥落状態() == 5
		PRINTFORML 「%CALLNAME:MASTER%、%CALLNAME:MASTER%、喜欢喜欢最喜欢了ー」
		PRINTFORML 「『看起来很开心的样子』？不对哦、 我就是很开心一」
		PRINTFORMW 「『离得好近』？嗯哼哼、 偶然也想欣赏一下%CALLNAME:MASTER%的尊顔呢一」
	ELSE
		PRINTFORML 「哼哼、我喜欢%CALLNAME:MASTER%哦」
		PRINTFORML 「『靠得好近』？哼哼、偶尔这样其实也不错吧？」
		PRINTFORMW 「还是说你想让我黏在你身边比较好呢？
	ENDIF
ELSEIF 陥落状態() == 3
	PRINTFORML 「『真是微妙的距离啊』？　哼哼、 谁知道呢」
	PRINTFORMW 「%CALLNAME:MASTER%这么想的话那就是这样吧」
ELSEIF 陥落状態() == 1
