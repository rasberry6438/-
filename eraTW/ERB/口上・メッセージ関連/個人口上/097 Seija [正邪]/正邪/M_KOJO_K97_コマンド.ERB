@M_KOJO_MESSAGE_COM_K97_189_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯…怎么了？不合适吗？」
		RETURN 1
	ENDIF
	;基本セット
	;開始
	IF TEQUIP:新妻PLAY
		;恋慕
		IF TALENT:恋慕
			IF RAND:3 == 0
				PRINTFORMW 「什么啊？」
				PRINTFORMW 「…你喜欢这种是吗？我会做的」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「啊？」
				PRINTFORMW 「围裙不合身吗？」
				RETURN 1
			ELSE
				PRINTFORMW 「啊…你…」
				PRINTFORMW 「…想干什么呢？」
				RETURN 1
			ENDIF
		;屈服3
		ELSEIF MARK:不埒刻印 == 3
			IF RAND:3 == 0
				PRINTFORMW 「…虽然没关系……」
				PRINTFORMW 「…不合适吧、我这样的…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「…怎么了？」
				PRINTFORMW 「…身上只穿着一件围裙…」
				RETURN 1
			ELSE
				PRINTFORMW 「…啊？」
				PRINTFORMW 「…看到裸体围裙会兴奋吗？」
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「…啊…」
				PRINTFORMW 「…我不是说不要盯着看吗」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「…啊…什么啊？」
				PRINTFORMW 「…哼、就算是被你说很适合什么的也…」
				RETURN 1
			ELSE
				PRINTFORMW 「…好小子」
				PRINTFORMW 「果然还是不喜欢啊…」
				RETURN 1
			ENDIF
		ENDIF
	;終了
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「…嗯」
			PRINTFORMW 「…脱下来吗…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…啊…」
			PRINTFORMW 「…就这样也没关系的…」
			RETURN 1
		ELSE
			PRINTFORMW 「啊、脱掉围裙吗？」
			PRINTFORMW 「…稍微有点遗憾呢…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
