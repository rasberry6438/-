;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 在停止的时间中用正邪的手握住了肉棒。
			ELSE
				PRINTFORMW 「…啊？」
				PRINTFORMW 「…知道了、好吧、我来给你做」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 在停止的时间中用正邪的手握住了肉棒。
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「哦？已经快要出来了吧」
			PRINTFORMW 这么说着、正邪開始更加激烈地动起来了。
			PRINTFORMW 「你看你看、都这个样子了！」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊、终于要出来了」
			PRINTFORMW 这么说着、正邪開始更加激烈地动起来了。
			PRINTFORMW 「欧拉！把精液都倒出来吧！」
		ELSE
			PRINTFORMW 「嗯？都溢出来了呢」
			PRINTFORMW 这么一说、正邪更加兴奋起来了。
			PRINTFORMW 「嗯…你喜欢这个样子吗」
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「嗯…瞧、」
			PRINTFORMW 「快点出来吧」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「激烈一点比较好吗？」
			PRINTFORMW 「…变得越来越硬了」
		ELSE
			PRINTFORMW 「哎呀？要用手来吗？」
			PRINTFORMW 「别胡闹了、在说什么呢」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「嗯哼、想要被我这样惩罚吗？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯…什么时候看都觉得恶心啊」
			PRINTFORMW 「好啦、不要乱动」
		ELSE
			PRINTFORMW 「多给你来一点吧」
			PRINTFORMW 「…这和自慰没什么区别吧」
		ENDIF
	;それ以外
	ELSE
