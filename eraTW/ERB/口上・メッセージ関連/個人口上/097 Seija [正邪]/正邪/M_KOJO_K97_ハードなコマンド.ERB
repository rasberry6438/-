;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K97_140
CALL M_KOJO_MESSAGE_COM_K97_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K97_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 在停止的时间中、開始侵犯正邪的口腔深处。
				RETURN 1
			ELSE
				PRINTFORMW 「…嗯呜…咕…」
				PRINTFORMW 「嗯…啾噜…」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 在停止的时间中、開始侵犯正邪的口腔深处。
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「嗯！？…啊…」
			PRINTFORMW 「…嗯…吸噜…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…！？…咕噜…」
			PRINTFORMW 「…嗯…嗯呐…！」
		ELSE
			PRINTFORMW 「…嗯…！？」
			PRINTFORMW 「嗯…唔…！」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「阿嘎！？」
			PRINTFORMW 「…嗯呜…咕…！」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯啊…！」
			PRINTFORMW 「嗯……！」
		ELSE
			PRINTFORMW 「…！？」
			PRINTFORMW 「…呜噜…」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「咯啊…！？」
			PRINTFORMW 「…噫噜…啊…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…哼唔！？」
			PRINTFORMW 「…嗯…啊…」
		ELSE
			PRINTFORMW 「…哼啊！？」
		ENDIF
	ENDIF
ENDIF
RETURN 1

