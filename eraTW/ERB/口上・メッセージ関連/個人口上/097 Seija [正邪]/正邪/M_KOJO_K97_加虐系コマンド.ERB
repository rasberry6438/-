;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K97_100
CALL M_KOJO_MESSAGE_COM_K97_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K97_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;時姦中
			IF FLAG:70
				PRINTFORMW 在静止的时间中打着正邪的屁股。
				RETURN 1
			ELSE
				PRINTFORMW 「…嗯！？」
				PRINTFORMW 「啊…嗯！？喂！？」
				RETURN 1
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 在静止的时间中打起来正邪的屁股。
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「嗯…啊呜…！？」
			PRINTFORMW 「…屁股好疼…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊…嗯…！？」
			PRINTFORMW 「…你啊…」
			RETURN 1
		ELSE
			PRINTFORMW 「好！？…啊！？」
			PRINTFORMW 「嗯…已经……」
			RETURN 1
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「啊！？」
			PRINTFORMW 「…啊…真是的…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊！？…嗯…」
			PRINTFORMW 「…这个…给我住手…」
			RETURN 1
		ELSE
			PRINTFORMW 「…嗯！？…啊！？」
			PRINTFORMW 「好痛…！」
			RETURN 1
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯…啊！？」
			PRINTFORMW 「…真是的…你啊…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊！？…别打了…」
			PRINTFORMW 「…真的很痛啊…」
			RETURN 1
		ELSE
			PRINTFORMW 「…哼啊！？」
			PRINTFORMW 「好痛啊…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

