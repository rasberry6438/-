	    PRINTFORMW 美宵慌忙开始洗%CALLNAME:MASTER%的内裤。
	ENDIF
ENDIF
RETURN 1


;==================================================
;一日の结束时(AFTERTRAニーの前)、认识がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K134_31(ARG,ARG:1)
;台词主人公为TARGET
;ARGは会ってない日数
;何日以上会ってなかったら愚痴を言うとかできます
;CFLAG:自慰暴露で分岐入れてもよさそう
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
CALL M_KOJO_COLOR_K134
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;今日会っていない
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG
		IF TALENT:恋人 || TALENT:恋慕
			CALL EVENTEND_TSUBUYAKI
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「明天能见到你吗……%CALLNAME:MASTER%桑HPH」","W"
				PRINTFORML 美宵自言自语道。
			ELSEIF RAND:2 == 0
				PRINTFORML （%CALLNAME:MASTER%桑在做什么呢）
				PRINTFORML 美宵自言自语道。
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%桑……我好寂寞啊……」
				PRINTFORML 美宵自言自语道。
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会っている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		RETURN 1
	ENDIF
	IF CFLAG:134:1002 == 1
		CFLAG:134:1002 = 0
	ENDIF
	IF CFLAG:134:1003 == 1
		CFLAG:134:1003 = 0
	ENDIF
ENDIF
RETURN 1

