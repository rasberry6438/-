;1,六九式
;==================================================
@M_KOJO_MESSAGE_SCOM_K134_1
CALL M_KOJO_MESSAGE_SCOM_K134_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K134_1_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K134
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		CALL HPH_PRINT,@"「让我们来比比看谁先高潮吧HPH」","W"
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「嗯？嗯嗯嗯～HPH」","W"
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
				CALL HPH_PRINT,@"（再过一会儿就会有很多美味的新鲜精液出来了呢……HPH）","W"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「嗯HPH嗯啾HPH」","W"
				CALL HPH_PRINT,@"「很多无法忍耐的我慢汁流出来了呢、请在美宵的嘴里射精吧HPH」","W"
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
			ELSE
				CALL HPH_PRINT,@"「嗯嗯HPH肉棒越来越硬了呢HPH」","W"
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
				CALL HPH_PRINT,@"（好想快点喝浓厚的精液啊HPH）","W"
			ENDIF
		ELSE
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「嗯嗯HPH」","W"
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
				CALL HPH_PRINT,@"「肉棒越来越硬了、看来快要射精了呢HPH」","W"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「嗯嗯HPH嗯啾HPH」","W"
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
			ELSE
				CALL HPH_PRINT,@"「嗯嗯HPH已经快要射精了吗HPH」","W"
				PRINTFORMW 美宵感觉到%CALLNAME:MASTER%即将要射精、用舌头巧妙地催促着。
			ENDIF
		ENDIF
	;舌3以上
	ELSEIF ABL:舌 > 2
		;精液中毒4以上
		IF ABL:精液中毒 > 4
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「嗯嗯HPH嗯嗯嗯HPH」","W"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「嗯嗯HPHHPHHPH」","W"
				CALL HPH_PRINT,@"「肉棒又大又硬的、好难含住HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「啾HPH哈呼HPH」","W"
			ENDIF
		ELSE
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「嗯～HPH」","W"
				CALL HPH_PRINT,@"「美宵的嘴舒服吗HPH我会让您更舒服的哦HPH」","W"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「嗯嗯HPH请放心交给美宵吧HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「嗯嗯啾HPH我会让肉棒更加舒服的HPH」","W"
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「嗯HPH啾啾HPH」","W"
			CALL HPH_PRINT,@"「肉棒一抖一抖的、好可爱啊HPH」","W"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯啾HPH嗯嗯HPH」","W"
		ELSE
			CALL HPH_PRINT,@"「嗯嗯……HPH」","W"
			PRINTFORMW 美宵有些不自然地含住肉棒。
		ENDIF
	ENDIF
ENDIF
;未记入
;CALLFORM M_KOJO_MESSAGE_COM_K134_{SELECTCOM}_1
RETURN 1

