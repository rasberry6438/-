;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K134
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「美宵会让肉棒感觉很舒服的哦HPH」","W"
