			PRINTFORMW 「以这种方式来做就可以了吗？」
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「您总是一个人做吗？那么今天让美宵来为您提供手交服务吧HPH」","W"
		ELSE
			CALL HPH_PRINT,@"「现在要用手来让您舒服起来哦HPH」","W"
		ENDIF
	ENDIF
