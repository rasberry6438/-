	ELSEIF ABL:技巧 > 3
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「好意外啊、我好像很擅长用手做呢HPH」","W"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯嗯HPH哎呀HPH您喜欢被这样做吗？」","W"
