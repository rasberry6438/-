;==================================================
;150,双乳间抽插
;==================================================
@M_KOJO_MESSAGE_COM_K134_150
CALL M_KOJO_MESSAGE_COM_K134_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_150_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K134
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		CALL HPH_PRINT,@"「呜啊啊、就这样突然插进来是不对的哦HPH」","W"
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「请在乳穴里满满地射出来吧HPH」","W"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「乳交的时候会很兴奋吗HPH肉棒越来越大了呢HPH」","W"
		ELSE
			CALL HPH_PRINT,@"「哎呀！太粗暴了HPH」","W"
		ENDIF
	;胸3以上
	ELSEIF ABL:胸 > 2
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「请用美宵的胸部舒服起来吧HPH」","W"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯HPH用胸来做舒服吗HPH」","W"
		ELSE
			CALL HPH_PRINT,@"「请多多射出来吧HPH」","W"
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「嗯嗯HPH感觉很舒服呢HPH」","W"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「请多多侵犯胸部吧HPH」","W"
		ELSE
			CALL HPH_PRINT,@"「请不要对胸部太粗暴了啊HPH」","W"
		ENDIF
	ENDIF
ENDIF
RETURN 1
