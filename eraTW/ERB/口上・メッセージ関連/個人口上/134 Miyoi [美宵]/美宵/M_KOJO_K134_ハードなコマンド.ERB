;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K134_140
CALL M_KOJO_MESSAGE_COM_K134_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_140_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K134
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯！哈啊啊～～～！」
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「呵呵HPH」","W"
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「啊HPH嗯HPH」","W"
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ELSE
			CALL HPH_PRINT,@"「嗯嗯HPH呀HPH」","W"
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯嗯～！」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯啊啊～～～！！！」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ELSE
			PRINTFORMW 「啊啊！！！」
			PRINTFORMW 美宵因为被%CALLNAME:MASTER%的肉棒深深插入而呻吟着。
		ENDIF
	ENDIF
ENDIF
RETURN 1

