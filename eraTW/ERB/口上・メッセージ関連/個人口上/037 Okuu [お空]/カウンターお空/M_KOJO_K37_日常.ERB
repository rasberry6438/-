	;Counter接吻
	IF TCVAR:20 == 30
		IF RAND:2 == 0
			PRINTFORML 「感觉怎么样？我会给你做更多的！」
			PRINTFORMW 「嗯♪」
		ELSE
			PRINTFORML 「哈…♪」
			PRINTFORMW 「舌头放进来了？还没？」
		ENDIF
	;Counterディープ接吻
	ELSEIF TCVAR:20 == 60
		IF RAND:3 == 0 
			PRINTFORML 「嗯…嗯…啾啾…嗯…♪」
			PRINTFORMW 「噗哈哈…嗯…呼呼♪」
		ELSEIF RAND:2 == 0
			PRINTFORML 「啾♪哈哈！」
			PRINTFORMW 「嗯…舔舔！啾…♪」
		ELSE
			PRINTFORML 阿空把舌头伸进了你的嘴里
			PRINTFORMW 「再吻一会哦♪嗯…♪」
		ENDIF
	;Counter騎乗位
	ELSEIF TCVAR:20 == 85
		IF RAND:3 == 0
			PRINTFORMW 「哈！好舒服♪」
			PRINTFORMW 「也要更加享受%CALLNAME:MASTER%哦！」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「变成习惯了呢♪这个进入的瞬间♪」
			PRINTFORMW 「看吧看吧，腰要动了哦？」
		ELSE
			PRINTFORML 「什么也不做」
			PRINTFORMW 「已经可以随你喜欢的做了呢♪」
		ENDIF
	;これ大堂ドね
	ELSEIF TCVAR:20 == 86
		IF RAND:6 == 0
			PRINTFORMW 「已经不行了呢♪」
			PRINTFORMW 「在出来之前要变得開心哦♪」
		ELSEIF RAND:5 == 0
			PRINTFORMW 「心情如何？」
			PRINTFORMW 「出来了吗？」
			PRINTFORMW 「不能再忍耐一下吗？」
			PRINTFORMW 「那么…射出来吧♪」
		ELSEIF RAND:4 == 0
			PRINTFORML 「非常紧张…。」
			PRINTFORMW 「已经出来了呢…♪」
			PRINTFORMW 「还能做对吧♪」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「诶？不抵抗吗？」
			PRINTFORMW 「嗯♪那么就请多多的射出来呢♪好多好多的♪」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯…已经出来了呢…」
			PRINTFORMW 「嗯♪我会紧紧抱住你的♪所以快点啦♪」
		ELSE
			PRINTFORML 「无论你说什么」
			PRINTFORMW 「我都听不见…♪」
			PRINTFORMW 「在被榨出来之前…♪」
		ENDIF
	;腰
	ELSEIF TCVAR:20 == 88
		IF RAND:3 == 0
			PRINTFORMW 「不怎么想动腰对吗？」
			PRINTFORMW 「那么阿空就会更加激烈的动哦♪」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯♪嘿嘿~」
			PRINTFORMW 「怎么样？这么动的话！很舒服对吧！」
		ELSE
			PRINTFORML 「如果不动的话是不行的……」
			PRINTFORMW 「按照你喜欢的频率动腰吧♪」
			PRINTFORMW 「嗯♪哈！哈…♪」
		ENDIF
		;MASTERの気力が0
		IF BASE:MASTER:気力 <= 0
			PRINTFORML 「哈…哈…」
			PRINTFORMW 「还可以做更多事情吧？」
			PRINTFORMW 「如果回答可以的话就可以！」
		ENDIF
	ENDIF
ENDIF
RETURN 1

