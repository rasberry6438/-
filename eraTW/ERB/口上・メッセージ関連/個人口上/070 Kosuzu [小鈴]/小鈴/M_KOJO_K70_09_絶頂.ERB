;膣内
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K70_UFUFU()
;-------------------------------------------------
IF LOCAL
	IF !FLAG:時間停止 && M_KOJO_K70_UFUFU() && CFLAG:[[小鈴]]:允许无套
		PRINTDATAL
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 能感受到热热的一大股涌来……❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% %CALLNAME:MASTER%先生的精液真温暖呢~❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% %CALLNAME:MASTER%先生的那么多精液,我很幸福~❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 出了这么多呢……❤ 好高兴…❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 啊……❤ %M_KOJO_K70_AUTO("接吻:前","…❤")%」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 太厉害了……❤ 心脏砰砰跳呢…❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% %CALLNAME:MASTER%先生的精液,流了好多呢……❤」 
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 啊……填满了我的里面……❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 好厉害…❤ 热热的一大波涌出来了…❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 内射……感觉很舒服呢……❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 啊……❤ 还在继续流出来……❤ 呵呵,射太多了哦❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 出了那么多呢……❤ 我明白的……❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 呵呵,想射多少就射多少吧……❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% %CALLNAME:MASTER%先生……❤ 满足了吗?」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 太好了……❤ 内射,感觉很舒服……❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 充满了幸福……❤ 在我的里面……❤」
            DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 啊,流了那么多出来……❤ 请把你的精液全都给我吧……❤」
			DATAFORM 「%M_KOJO_K70_AUTO("喘ぎ：冷静", "……❤")% 喂喂~射了这么多……❤ 请把你的精液全都给我吧……❤」
        ENDDATA
        WAIT
        IF !TALENT:70:妊娠 && CFLAG:[[小鈴]]:允许无套 == 2
            IF CFLAG:70:生理周期 == 7
                SELECTCASE RAND:3
                CASE 1
                    PRINTFORMW 「啊…❤ 绝对会怀孕的……❤」
                CASE 2
                    PRINTFORMW 「呵呵…❤ 这样就可以怀宝宝了呢……❤」
                CASEELSE
                    PRINTFORMW 「啊~…❤ 一定已经受孕了吧❤」
                ENDSELECT
            ELSE
                SELECTCASE RAND:3
                CASE 1
                    PRINTFORMW 「嘻嘻…❤ 好像很容易怀孕的样子呢……❤」
                CASE 2
                    PRINTFORMW 「嗯…❤ 光靠这点还不够呢……还要更多……❤」
                CASEELSE
                    IF CFLAG:70:兒童人数
                        PRINTFORMW 「呵呵…❤ 总觉得快要该要第{CFLAG:70:兒童人数 + 1}个宝宝了呢…♪」
                    ELSE
                        PRINTFORMW 「呵呵…❤ 想快点怀上宝宝呢…♪」
                    ENDIF
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

