;9,雙重口交
;==================================================
@M_KOJO_MESSAGE_SCOM_K70_9
CALL M_KOJO_MESSAGE_SCOM_K70_9_1
RETURN RESULT

;一人目口上
@M_KOJO_MESSAGE_SCOM_K70_9_1(ARG)
#DIM CHARA
IF !ARG
	CHARA = MASTER_POSE(4,1,1)
ELSE
	CHARA = ARG
ENDIF
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K70_UFUFU()
;-------------------------------------------------
IF LOCAL
	IF CHARA == [[阿求]]
		SELECTCASE M_KOJO_K70_BALANCE([[小鈴]], [[阿求]])
		CASE "独占", "信頼"
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「阿求，一起来让%CALLNAME:MASTER%先生舒服起来吧❤」
			CASEELSE
				PRINTFORMW 「呵呵❤ 和阿求一起做❤」
			ENDSELECT
		CASE "悲恋", "揶揄"
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「那个，那个阿求！ 来比一比谁先让%CALLNAME:MASTER%先生舒服起来吧…！」
			CASEELSE
				PRINTFORMW 「嗯嗯……只有阿求太狡猾了……！即使是对我来说……！」
			ENDSELECT
		CASEELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「我会比阿求更能让你舒服起来的！」
			CASEELSE
				PRINTFORMW 「我不会输给阿求的…っ！  %M_KOJO_K70_AUTO("おくち：前", "❤")%」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「嗯嗯……%CALLNAME:CHARA%先生总是很狡猾……！即使对我来说……！」
		CASEELSE
			PRINTFORMW 「嗯ー！ %CALLNAME:CHARA%先生，也请给我吧……っ！」
		ENDSELECT
	ENDIF
	;ここから[フェラチオ]に転送
	CALL M_KOJO_MESSAGE_COM_K70_81_1
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K70_{SELECTCOM}_1
RETURN 1

