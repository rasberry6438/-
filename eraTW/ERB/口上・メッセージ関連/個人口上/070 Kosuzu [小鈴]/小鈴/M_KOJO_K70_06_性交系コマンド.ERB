	PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：挿入", "❤ ", 3)%%M_KOJO_K70_AUTO("喘ぎ：急", "❤")% おっ❤」
	IF CFLAG:[[小鈴]]:允许无套 == 2 && !TALENT:[[小鈴]]:妊娠
		CALL M_KOJO_K70_NAKADASHISET("後背位_子作り")
	ELSE
		CALL M_KOJO_K70_NAKADASHISET("後背位")
    ENDIF
;騎乗位
