;==================================================
;スイカ割り
;スイカ割りの口上はムシバトル同様、PRINTを使うのではなく、
;台詞一行のみをRESULTSに入れてください
;カギ括弧は自動で付きます
;"衝突","結果"のみ、口上色が自動で付きます
;"衝突","結果"以外は半角26文字（全角13文字）までの文字数制限があります。はみ出ないように気をつけてください
;数パターンあると雰囲気が出ます（%TEXTR( / )%を使うと良い）
;==================================================
@M_KOJO_MESSAGE_COM_K70_SUIKA(ARGS, ARG)
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL:1 = 1
;-------------------------------------------------
IF LOCAL:1
	SELECTCASE ARGS
	CASE "衝突"
		;-------------------------------------------------
		;場外に出てあなたがキャラと衝突した際の台詞
		;文字数制限はありません,汎用台詞もありません
		;-------------------------------------------------
		RESULTS = 碰！那是我！
	CASE "そこだ！"
		RESULTS = %TEXTR("那里！/现在！/现在！")%
	CASE "近い"
		RESULTS = %TEXTR("离得很近！/正在靠近！/就快了！")%
	CASE "遠い"
		RESULTS = %TEXTR("还很远/离得很远呢～/还有距离")%
	CASE "かなり遠い"
		RESULTS = %TEXTR("一直很远哦/还很远呢")%
	CASE "前"
		RESULTS = %TEXTR("前方/一直直走/那边")%
	CASE "少し前"
		RESULTS = %TEXTR("再往前一点！/稍微前一些！/再走一步吧？")%
	CASE "後ろ"
		RESULTS = %TEXTR("反方向哦！/不是那边哦！/朝相反方向！")%
	CASE "少し後ろ"
		RESULTS = %TEXTR("就在后面！/正在转身哦/稍微后一些！")%
	CASE "右"
		RESULTS = %TEXTR("右边/就在右边哦/再往右一些")%
	CASE "少し右"
		RESULTS = %TEXTR("稍微往右/稍微右边")%
	CASE "左"
		RESULTS = %TEXTR("左边/就在左边哦/再往左一些")%
	CASE "少し左"
		RESULTS = %TEXTR("稍微往左/稍微左边")%
	CASE "結果"
		;-------------------------------------------------
		;終了後の一言
		;文字数制限はありません,汎用台詞もありません
		;TFLAG:193,
		;   -1=失敗
		;    0=成功
		;    1=大成功
		;    2=木刀で大成功（ボーナス点）
		;ARG
		;    0=正直者
		;    1=ウソつき
		;-------------------------------------------------
		IF ARG
			SELECTCASE TFLAG:193
			CASE -1
				RESULTS = "啊哈哈，真可惜～♪"
			CASE 0
				RESULTS = "哦，做得好啊！"
			CASE 1
				RESULTS = "太厉害了！真不错！"
			CASE 2
				RESULTS = "哇，直接分成两半！"
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
			CASE -1
				RESULTS = "嗯～，有点遗憾......"
			CASE 0
				RESULTS = "哇，做到了！"
			CASE 1
				RESULTS = "太厉害了！真不错！"
			CASE 2
				RESULTS = "哇！直接分成两半！"
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF

