;分岐用のゲストキャラ判定
@M_KOJO_K70_GUEST(ARGS)
#DIM CHARA
;ARGS=呼び出したコマンド
;RESUTS=ゲストキャラの名前
;RESULT=分岐用の値
;RESULT:1=キャラ番号
;RESULTS:1=ゲストキャラの呼び方
;会話,茶以外は一日一回を無視する（現在は日常愛撫のみ）
VARSET RESULT
VARSET RESULTS
;阿求(80)は最優先,RETURNは1で固定
IF CFLAG:[[阿求]]:現在位置 == CFLAG:[[小鈴]]:現在位置 && M_KOJO_K70_GUEST_ABLE([[阿求]])
	IF !GROUPMATCH(ARGS, "会話", "茶") || !TCVAR:[[小鈴]]:(M_KOJO_K70_GUEST_TCVAR([[阿求]]))
		RESULTS = 阿求
		RESULTS:1 = 阿求
		RESULT:1 = [[阿求]]
		RETURN 1
	ENDIF
ENDIF
FOR LOCAL, 1, 10
	;霊夢(1)、魔理沙(11)、紫(26)、文(29)、猯藏(69)
	CHARA = TARGET:LOCAL
	IF GROUPMATCH(CHARA, [[霊夢]], [[魔理沙]], [[紫]], [[文]], [[猯藏]]) && M_KOJO_K70_GUEST_ABLE(CHARA)
		IF !GROUPMATCH(ARGS, "会話", "茶") || !TCVAR:[[小鈴]]:(M_KOJO_K70_GUEST_TCVAR(CHARA))
			RESULTS = %CALLNAME:CHARA%
			RESULTS:1 = %CALLNAME:CHARA%さん
			RESULT:1 = CHARA
			;台詞を先に考えてたらよくわからん分岐になってしまった
			;恋慕の小鈴と同行中（デート含む）
			IF CFLAG:[[小鈴]]:同行 && TALENT:[[小鈴]]:恋慕
				RETURN 4
			;鈴奈庵でゲストと同行中
			ELSEIF CFLAG:CHARA:同行 && M_KOJO_K70_SUZUNAAN(CFLAG:[[小鈴]]:現在位置) == 1
				RETURN 3
			;ゲストとデート中
			ELSEIF CFLAG:CHARA:同行 && FLAG:约会的对象 == CHARA
				RETURN 2
			;恋慕のゲストと同行中
			ELSEIF CFLAG:CHARA:同行 && TALENT:CHARA:恋慕
				RETURN 2
			;小鈴が恋慕
			ELSEIF TALENT:[[小鈴]]:恋慕
				RETURN 4
			ELSE
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
NEXT

