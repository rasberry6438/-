;オナバレ
;==================================================
@M_KOJO_EVENT_K70_26(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	CASE 0
		;-------------------------------------------------
		;見られた瞬間
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF FIRSTTIME("オナバレ")
				PRINTFORMW 「啊……%CALLNAME:MASTER%さん……你、你从什么时候在那里……」
				PRINTFORMW 「我、我不要啊ーーーー!!\/\/\/ 嫁不出去啦ーーーー!!\/\/\/」
				TCVAR:70:350 += 1
				TALENT:70:心情 = -1
				TCVAR:70:356 = 1
			ELSEIF TALENT:70:恋慕 && GETBIT(CFLAG:70:既成事實, 1)
				;20日以上会ってない
				IF DAY - CFLAG:70:1002 >= 20
					PRINTFORML 「啊……%CALLNAME:MASTER%さん……!」
					PRINTFORMW 「这个是……那个,因为……」
					PRINTFORMW 「因为~最近都见不到,很寂寞的说~……」
					PRINTFORMW 「都是%CALLNAME:MASTER%さん的错~……呜呜……」
					TCVAR:70:356 = 4
				ELSE
					PRINTFORML 「啊……%CALLNAME:MASTER%さん……!」
					PRINTFORMW 「这个是……那个,读了一本那种内容的书……稍微有点……」
					PRINTFORMW 「一想到%CALLNAME:MASTER%さん就忍不住了……\/\/\/」
					TCVAR:70:356 = 3
				ENDIF
			ELSE
				PRINTFORML 「啊……%CALLNAME:MASTER%さん……!」  
				PRINTFORMW 「你、你为什么在这里啊ーー!!\/\/\/ 笨蛋ーーー!!\/\/\/」
				TCVAR:70:356 = 2
			ENDIF
			RETURN 1
		ENDIF
	CASE 1
		;-------------------------------------------------
		;何もしない
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			;初回、恋慕なしor合意まだ
			IF TCVAR:70:356 == 1 || TCVAR:70:356 == 2
				PRINTFORMW 「请出去ーーーー!!\/\/\/」
			;恋慕+合意  
			ELSEIF TCVAR:70:356 == 3
				PRINTFORMW 「啊!等一下…啊……」
			;久しぶり
			ELSEIF TCVAR:70:356 == 4
				PRINTFORMW 「啊!不要这样……呜……」
			ENDIF
			RETURN 1
		ENDIF
	CASE 2
		;-------------------------------------------------
		;そのまま続行
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			;初回,恋慕なしor合意まだ
			IF TCVAR:70:356 == 1 || TCVAR:70:356 == 2
				PRINTFORMW 「不要看哦~~\/\/\/」
			;恋慕+合意,久しぶ里 
			ELSEIF TCVAR:70:356 == 3 || TCVAR:70:356 == 4
				;自慰口上を表示
				IF EXP:70:調教自慰経験 >= 30
					SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「啊……❤ 嗯❤ 感觉好舒服…❤」
						PRINTFORMW 「再看着我吧……❤ 看,一想到%CALLNAME:MASTER%さん我就会……❤」
					CASE 2 
						PRINTFORMW 「哈~❤ 被%CALLNAME:MASTER%さん看着自慰❤ 变成了习惯❤」
						PRINTFORMW 「呀……❤ 没办法停下来呢……%M_KOJO_K70_AUTO("喘ぎ：小", "❤")%」
					CASEELSE
						PRINTFORMW 「%CALLNAME:MASTER%さん……❤ %M_KOJO_K70_AUTO("喘ぎ：小", "❤")% %M_KOJO_K70_AUTO("喘ぎ：小", "❤")%」
						PRINTFORMW 「来、摸摸我吧……❤ 呜……」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMW 「%CALLNAME:MASTER%さん,我好寂寞……❤ %M_KOJO_K70_AUTO("喘ぎ：小", "❤")%」  
						PRINTFORMW 「因为这样的话……❤ 只有我一个人……嗯❤」
					CASEELSE
						PRINTFORMW 「平、平时都是……一边想着%CALLNAME:MASTER%さん……❤」
						PRINTFORMW 「%M_KOJO_K70_AUTO("喘ぎ：小", "❤ ", 1)%哈啊……!」
					ENDSELECT
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
	CASE 3
		;-------------------------------------------------
		;追い出し
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			PRINTFORMW 「%CALLNAME:MASTER%さん笨蛋啊ーーーっっ！！\/\/\/」
			TCVAR:70:350 = 3
			TALENT:70:心情 = -1
			BASE:70:憤怒 += MAXBASE:70:憤怒
			CALL M_KOJO_K70_YAMETEKUDASAI(70, 3, 1)
			RETURN 1
		ENDIF
	CASE 4
		;-------------------------------------------------
		;やっちゃう
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = M_KOJO_K70_UFUFU()
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL
			PRINTFORMW 「嗯……❤ 那样的话……❤ 但是……❤」
			PRINTFORMW 「啊嗯❤ 嘿嘿❤ 一直在等待您❤❤」
			PRINTFORMW 「请对我温柔一点❤ 多多疼爱我吧……❤」
			RETURN 1
		ENDIF
	CASE 5
		;-------------------------------------------------
		;事後
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = M_KOJO_K70_UFUFU()
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL
			PRINTFORML 「哈啊哈啊……舒服……❤」
			PRINTFORMW 「但、但是敲门之类的……还是要做一下的呢……」
			RETURN 1
		ENDIF
	CASE 6
		;-------------------------------------------------
		;事後お泊り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = M_KOJO_K70_UFUFU()
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL
			PRINTFORMW 「啊,要留下过夜吗?呵呵,当然可以哦♪」
			PRINTFORMW 「正好有想让您看看的书」  
			PRINTFORMW 「如果可以的话,一起然后――之后就是――」
			PRINTFORMW 「……❤」
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF

