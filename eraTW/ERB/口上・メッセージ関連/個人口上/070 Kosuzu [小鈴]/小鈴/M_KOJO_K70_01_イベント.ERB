;==================================================
;相手からデートのお誘い
;==================================================
@M_KOJO_EVENT_K70_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	CASE 0
		;-------------------------------------------------
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE STR:(5000 + ARG:1)
			CASE "博麗神社"
				PRINTFORML 「那个,要一起去博丽神社吗?」
				PRINTFORMW 「上次借给灵梦的书,想知道她看了没有啊♪」
			CASE "命蓮寺"
				PRINTFORML 「那个,要一起去命莲寺吗?」
				PRINTFORMW 「呵呵,我虽然不练,但可以念经哦♪」
			CASE "人間之里"
				PRINTFORML 「那个,要不要一起去人间之里?」  
				PRINTFORMW 「那个……想和你约会……❤」
			CASE "霧之湖～紅魔館"
				PRINTFORML 「那个,要不要一起去雾之湖散步?」
				PRINTFORMW 「偶尔也想悠闲地散个步啊♪」
			CASE "迷途竹林"
				PRINTFORML 「那个,要不要一起去迷途竹林?」
				PRINTFORMW 「那边的客人老是迷路,很难碰到呢~」
			CASE "魔法之森"
				PRINTFORML 「那个,要不要一起去魔法之森?」 
				PRINTFORMW 「上次借给魔理沙的书,想知道她看了没有啊♪」
			CASE "三途之川～冥界"
				PRINTFORML 「那个,要不要一起去中有之道吗?」
				PRINTFORMW 「不、不是去死掉啦!是有客人在那里等我的!」
			CASE "妖怪之山 (山麓)"
				PRINTFORML 「那个,要不要一起去山麓?」
				PRINTFORMW 「河童们借了些感兴趣的书走了,我很担心呢」
			CASE "妖怪之山 (山頂)"
				PRINTFORML 「那个,要不要一起去山顶的神社?」  
				PRINTFORMW 「那边的神社经常借书,去取回来很麻烦呢」
			CASE "地底"
				PRINTFORML 「那个,我想去一次地底!」
				PRINTFORMW 「有一点可怕,不过有%CALLNAME:MASTER%さん陪我的话……」
			CASE "夢之世界～月"
				PRINTFORML 「那个,我想去月球旅行!」
				PRINTFORMW 「会、会很浪漫吧……!」
			ENDSELECT
			RETURN 1
		ENDIF
	CASE 1
		;-------------------------------------------------
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORMW 「太好了!没问题,我已经准备好啦♪」
			TCVAR:70:354 = 1
			TALENT:70:心情 = 1
			;日記を書く
			CALL M_KOJO_DIARYSETTING_K70(39)
			RETURN 1
		ENDIF
	CASE 2
		;-------------------------------------------------
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「是吗……啊,别介意」
			PRINTFORMW 「那就下次再约吧」
			RETURN 1
		ENDIF
	CASE 3
		;-------------------------------------------------
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「那个,今天要来我家吗……?」
			IF FIRSTTIME("招待", 1, 70) 
				;什么情况可以不用店番
				PRINTFORMW 「今天父母都出去了,所以……那个……」
			ELSE
				SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「有想让%CALLNAME:MASTER%看看我家的猫会后空翻……」
				CASE 2
					PRINTFORMW 「有想向您请教的事情……」
				CASEELSE
					PRINTFORMW 「今天父母都出去了……」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	CASE 4
		;-------------------------------------------------
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF FIRSTTIME("招待", 0, 70)
				PRINTFORML 「真的吗!?太好了!」
				PRINTFORMW  (把房间收拾干净真是正确的选择……)
			ELSE
				PRINTFORML 「哇─♪」
				PRINTFORMW 「那我们马上出发吧!」
			ENDIF
			TALENT:70:心情 = 1
			TCVAR:70:354 = 1
			RETURN 1
		ENDIF
	CASE 5
		;-------------------------------------------------
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML 「这样啊…… 啊,请别介意」
            PRINTFORMW 「下次有机会务必……!」
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

