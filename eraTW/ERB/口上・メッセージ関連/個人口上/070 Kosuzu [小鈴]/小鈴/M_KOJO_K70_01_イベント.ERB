;==================================================
;一日の終了時(AFTERTRAニーの前)、面識がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K70_31(ARG,ARG:1)
#DIM 前回のブーム
#DIM 今回のブーム
;口上主はTARGET
;ARGは会ってない日数
;何日以上会ってなかったら愚痴を言うとかできます
;CFLAG:オナバレで分岐入れてもよさそう
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;※口上はないけどフラグの管理だけする
	;マイブームが無い場合、確率で発生
	IF (!CFLAG:[[小鈴]]:1080 || CFLAG:[[小鈴]]:1080 > 10)
		IF !RAND:24
			前回のブーム = CFLAG:[[小鈴]]:1080 % 10
			CFLAG:[[小鈴]]:1080 = 0
			WHILE !CFLAG:[[小鈴]]:1080
				;種類数を取得
				CALLF M_KOJO_K70_KOSUZUBOOM()
				今回のブーム = RAND:RESULT + 1
				SIF 今回のブーム != 前回のブーム
					CFLAG:[[小鈴]]:1080 = 今回のブーム
			WEND
			;残り日数は40～80日
			CFLAG:[[小鈴]]:1081 = 40 + RAND:41
			CFLAG:[[小鈴]]:1082 = 0
			[IF_DEBUG]
				PRINTFORML ※小鈴口上デバッグ用
				PRINTFORML ！マイブーム発生
			[ENDIF]
		ENDIF
	;残り日数が無くなったらマイブーム終了,
	ELSEIF CFLAG:[[小鈴]]:1081 <= 0
		CFLAG:[[小鈴]]:1080 += 10
		CFLAG:[[小鈴]]:1081 = 0
		CFLAG:[[小鈴]]:1082 = 0
	ELSE
		;マイブームカウントダウン
		CFLAG:[[小鈴]]:1081 --
	ENDIF
	[IF_DEBUG]
		CALL EVENTEND_TSUBUYAKI
		PRINTFORML ※小鈴口上デバッグ用
		PRINTFORML マイブーム：%M_KOJO_K70_KOSUZUBOOM()%（あと {CFLAG:[[小鈴]]:1081}日）
		PRINTFORML 数値：{CFLAG:[[小鈴]]:1080}, 回数：{CFLAG:[[小鈴]]:1082}
	[ENDIF]
;	;-------------------------------------------------
;	;小分岐
;	;-------------------------------------------------
;	;今日会っていない
;	;記入チェック（=0, 非表示、1, 表示）
;	LOCAL:1 = 0
;	;-------------------------------------------------
;	IF LOCAL:1 && ARG
;		RETURN 1
;	ENDIF
;	;-------------------------------------------------
;	;今日会っている
;	;記入チェック（=0, 非表示、1, 表示）
;	LOCAL:1 = 0
;	;-------------------------------------------------
;	IF LOCAL:1 && ARG == 0
;		RETURN 1
;	ENDIF
ENDIF
RETURN 1

