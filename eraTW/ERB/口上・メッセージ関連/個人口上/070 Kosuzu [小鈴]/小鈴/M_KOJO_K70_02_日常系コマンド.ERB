@M_KOJO_MESSAGE_COM_K70_603_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K70
	;酔ってる
	IF ALCOHOL_FACE(70) == "酔酒" || ALCOHOL_FACE(70) == "不省人事"
		CALL M_KOJO_K70_DRUNK("手を繋ぐ")
	;発情中
	ELSEIF TCVAR:[[小鈴]]:発情
		CALL M_KOJO_K70_HATSUJO("手を繋ぐ")
	ELSEIF TALENT:[[小鈴]]:恋人
		SELECTCASE RAND:8
		CASE 1
			PRINTFORMW 「我可不会放开这只手的♪」
		CASE 2
			PRINTFORMW 「紧紧地……❤」
		CASE 3
			PRINTFORMW 「感受到与%CALLNAME:MASTER%先生的羁绊❤」
		CASE 4
			PRINTFORMW 「请带我走吧♪」
		CASE 5
			PRINTFORMW 「真正的恋人牵手呢…❤」
		CASE 6
			PRINTFORMW 「有点害羞呢…♪」
		CASE 7
			PRINTFORMW 「更加贴近些吧❤」
		CASEELSE
			PRINTFORMW 「这、这可是恋人牵手的样子呢…❤」
		ENDSELECT
	ELSEIF TALENT:[[小鈴]]:恋慕
		SELECTCASE RAND:8
		CASE 1
			PRINTFORMW 「请不要放开我哦♪」
		CASE 2
			PRINTFORMW 「紧紧地……❤」
		CASE 3
			PRINTFORMW 「%CALLNAME:MASTER%先生的手…❤」
		CASE 4
			PRINTFORMW 「感觉有点像在约会呢…❤」
		CASE 5
			PRINTFORMW 「啊，梦寐以求的恋人牵手…❤」
		CASE 6
			PRINTFORMW 「手有点湿……有点害羞……」
		CASE 7
			PRINTFORMW 「能再靠近些吗…❤」
		CASEELSE
			PRINTFORMW 「这、这感觉像是恋人呢…❤」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 1
			PRINTFORMW 「这、这样可以吗…？」
		CASE 2
			PRINTFORMW 「紧紧地……❤」
		CASE 3
			PRINTFORMW 「这、这就是约会吗……❤」
		CASEELSE
			PRINTFORMW 「好、好的！」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

