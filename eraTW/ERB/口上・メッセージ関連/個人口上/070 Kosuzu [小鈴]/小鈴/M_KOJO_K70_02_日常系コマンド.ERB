;332,お酒をふるまう
;TFLAG:193(0=下戸飲み成功 -1=下戸飲み失敗 -2=下戸飲み大失敗 -5=下戸飲みあきらめ)
;TFLAG:194(51=どぶろく 52=清酒 53=鬼殺し)
;ALCOHOL_TASTE(TFLAG:194)(15以上=気に入る 9以下=口に合わない)
;==================================================
@M_KOJO_SUCCESS_COM_K70_332
;お酒を飲む前の酒気
TCVAR:[[小鈴]]:382 = BASE:[[小鈴]]:酒気

;小鈴専用の変換ALCOHOL_FACE
@M_KOJO_K70_ALCOHOL_FACE(ARG)
#FUNCTIONS
SELECTCASE ARG
CASE IS <= 2
	RETURNF "清醒"
CASE IS <= MAXBASE:[[小鈴]]:酒気 / 10 * 5
	RETURNF "微醺"
CASE IS <= MAXBASE:[[小鈴]]:酒気 / 10 * 8
	RETURNF "酔酒"
CASEELSE
	RETURNF "趴窝"
ENDSELECT

@M_KOJO_MESSAGE_COM_K70_332
CALL M_KOJO_MESSAGE_COM_K70_332_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K70_332_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K70
	;同梱CSVを前提としています
	;[酒に弱い]ので どぶろく=10, 清酒=8, 鬼殺し=5
	IF M_KOJO_K70_ALCOHOL_FACE(TCVAR:[[小鈴]]:382) == "趴窝"
		PRINTFORMW 「きゅ～～～…………」
	ELSEIF M_KOJO_K70_ALCOHOL_FACE(TCVAR:[[小鈴]]:382) == "酔酒"
		PRINTFORMW 「う～～ん、うんうんうんうん……」
	;発情＞ほろ酔い,ここは段階があるので関数使わない
	ELSEIF TCVAR:[[小鈴]]:発情
		IF TALENT:[[小鈴]]:恋慕
			PRINTFORML 「给我喝酒，您打算做什么呢…❤」
			PRINTFORMW 「嗯嘿嘿❤ 谢谢您❤」
		ELSE
			PRINTFORML 「是酒吗？」
			PRINTFORMW 「感激不尽，就麻烦您了❤」
		ENDIF
	ELSEIF M_KOJO_K70_ALCOHOL_FACE(TCVAR:[[小鈴]]:382) == "微醺"
		;仕事中
		IF CFLAG:70:行動 == 5
			PRINTFORML 「诶！您还在喝酒吗～？」
			PRINTFORMW 「呃，嗯，没办法呢～～」
		ELSE
			PRINTFORML 「嘻嘻，谢谢♪」
			PRINTFORMW 「呐！要保重身体哦！」
		ENDIF
	ELSE
		;仕事中
		IF CFLAG:70:行動 == 5
			PRINTFORML 「诶，是酒吗？我还在工作中呢……」
			PRINTFORMW 「那、那就只喝一点点……」
		ELSE
			PRINTFORML 「哎呀！是酒吗？」
			PRINTFORMW 「非常感谢～！」
		ENDIF
	ENDIF
	PRINTL 
	;ここも酔いどれ＞発情＞ほろ酔い
	IF TCVAR:[[小鈴]]:発情 && ALCOHOL_FACE(70) != "不省人事" && ALCOHOL_FACE(70) != "酔酒"
		PRINTFORML 「嘻嘻❤ 喝醉啦～❤」
		IF TALENT:[[小鈴]]:恋慕
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「让我们一直保持愉快的心情，做些愉快的事情吧…❤」
			CASE 2
				PRINTFORMW 「所以……%CALLNAME:MASTER%先生想要的，尽管去做吧…❤」
			CASEELSE
				PRINTFORMW 「%CALLNAME:MASTER%先生❤ 温柔地照顾我吧❤」
			ENDSELECT
		ELSE
			WAIT
		ENDIF
	;鬼殺し
	ELSEIF ALCOHOL_TASTE(TFLAG:194) <= 5
		IF ALCOHOL_FACE(70) == "不省人事"
			PRINTFORMW 「んくんく、んくんく……～～ッ！！＠＃！＄％！？＊＊＊＊ッ！！」
		ELSEIF ALCOHOL_FACE(70) == "酔酒"
			PRINTFORML 「嗯嗯嗯……嗯嗯～～～～呜姆！！」
			PRINTFORMW 「再～～～～～来一次～～～！！！」
		ELSE
			PRINTFORML 「嗯嗯……啊呀！这、这是鬼杀酒吗？」
			PRINTFORMW 「对我来说有点太猛了……」
		ENDIF
	;どぶろく,甘酒
	ELSEIF ALCOHOL_TASTE(TFLAG:194) >= 10
		IF ALCOHOL_FACE(70) == "不省人事"
			PRINTFORMW 「嗯嗯嗯，嗯嗯嗯……王の御前へ～～！ 御免あさい～～……」
		ELSEIF ALCOHOL_FACE(70) == "酔酒"
			PRINTFORMW 「嗯嗯嗯……嗯嗯～～！ 这种感觉真是太美妙了～」
		ELSE
			PRINTFORMW 「嗯嗯……嗯～♪ 我喜欢这种略微温和的感觉～♪」
		ENDIF
	;清酒
	ELSE
		IF ALCOHOL_FACE(70) == "不省人事"
			PRINTFORMW 「嗯嗯嗯，嗯嗯嗯……啊嗚啊～♪ 好舒服～♪ 心情真是棒极了～～～♪♪」
		ELSEIF ALCOHOL_FACE(70) == "酔酒"
			PRINTFORMW 「嗯嗯嗯……很有趣，很美味呢～，这料理真是赛高呐～♪」
		ELSE
			PRINTFORML 「嗯嗯……嗯～！ 这个真是美味呢……！」
			PRINTFORMW 「不过对我来说可能有点辣……」
		ENDIF
	ENDIF
ENDIF
RETURN 1
