RETURN RESULT

@M_KOJO_MESSAGE_COM_K70_604_1
#DIM POINT
#DIM デート会話, 120
SIF !TCVAR:70:352
	VARSET デート会話
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;酔ってるor発情中だと表示しない
	SIF ALCOHOL_FACE(70) == "酔酒" || ALCOHOL_FACE(70) == "不省人事" || TCVAR:[[小鈴]]:発情
		RETURN 1
	;試しにデート会話をこっちに持ってきた
	IF ABL:[[小鈴]]:親密 >= 7 && !デート会話:(CFLAG:[[小鈴]]:現在位置 / 10)
		PRINTL 
		POINT = CFLAG:70:現在位置 / 10
		;こんなにこだわるつもりはなかったのに、某美人妻がたく先生喋るのに釣られてつい
		;新たに地点ができたら修正します（多分）
		SELECTCASE STR:(POINT + 6000)
		;▼博麗神社
		CASE "桜花道的鳥居"
			;春
			IF GET_MONTH() == "春之月"
				PRINTFORML 「樱花，真的很美呢～！」
				PRINTFORMW 「要不要一起去赏花呢？」
			;春以外
			ELSE
				PRINTFORML 「好像在春天经常在这里赏花呢」
				PRINTFORMW 「一般都在晚上，所以我不能常来……」
			ENDIF
		CASE "神社境内"
			PRINTFORML 「果然这里让人感到宁静呢～」
			PRINTFORMW 「妖怪们也来这里玩，感觉有点理解了」
		CASE "神社本堂"
			PRINTFORML 「既然来了，就来参拜一下吧」
			PRINTFORMW 「许愿的内容……是个秘密♪」
		CASE "神社走廊"
			PRINTFORML 「不过话说回来，这里有很多人呢」
			PRINTFORMW 「看来灵梦真的很受欢迎呢」
		CASE "妖精的大樹"
			PRINTFORML 「咦，这里是妖精的巢穴吗？」
			PRINTFORMW 「平时只是看起来是普通的树木……好奇怪……」
		CASE "夢幻遺跡"
			PRINTFORML 「哇，原来在神社附近有这样的遗迹啊」
			PRINTFORMW 「感觉形状好奇妙……真的是遗迹吗？」
		;▼命蓮寺
		CASE "地蔵小路"
			PRINTFORML 「偶尔也会来到这附近呢」
			PRINTFORMW 「……主要是有些烦心事的时候，或者来献供」
		CASE "倒垂的柳樹下"
			PRINTFORML 「为什么柳树看起来那么诡异呢……」
			PRINTFORMW 「白天还好，但晚上可不敢来呢……」
		CASE "命蓮寺境内"
			PRINTFORML 「这里妖怪也住了不少呢……」
			PRINTFORMW 「嗯，妖怪却也能做一些善事，这样真的没问题吗？」
		CASE "命蓮寺方丈"
			PRINTFORML 「好像有些人从村子里来这个寺庙修行呢」
			PRINTFORMW 「看到有人回来，果然还是很严格吧？」
		CASE "墓地"
			PRINTFORML 「呀～，为什么会有墓地啊……」
			PRINTFORMW 「赶快去别的地方吧～！」
		CASE "神霊廟広場"
			PRINTFORML 「好华丽的门啊～」
			PRINTFORMW 「仙人的形象不是应该更温和吗，难道不是这样吗？」
		CASE "神霊廟道場"
			PRINTFORML 「知道的，这个道场是仙界的一部分」
			PRINTFORMW 「成为仙人呢～，%CALLNAME:MASTER% 先生有这个想法吗？」
		;▼人里
		CASE "広場"
			PRINTFORML 「%CALLNAME:MASTER% 先生！ 今天我们去那边吧！」
			PRINTFORMW 「嘿嘿，我们决定下次约会就去那里♪」
		CASE "商家町"
			;这里的“ウチ”指的是鈴奈庵
			PRINTFORML 「要来我这里吗？ 还是来购物？」
			PRINTFORMW 「呵呵，感觉像是在约会一样，很有趣呢♪」
		CASE "呑屋小道"
			PRINTFORML 「%CALLNAME:MASTER% 先生，这个时间开始喝酒吗？」
			PRINTFORMW 「喝太多的话可能就回不去了哦～」
		CASE "寺子屋"
			PRINTFORML 「我以前也在这里上学呢」
			PRINTFORMW 「啊，好怀念！这个涂鸦还在呢」
		CASE "大衆浴場"
			IF DAY * 1440 + TIME - EX:MASTER:上次的入浴時间 < 360
				PRINTFORML 「呼～！感觉清爽了！」
				PRINTFORMW 「暖洋洋的哦♪」
			ELSE
				PRINTFORML 「我偶尔也来这个澡堂呢」
				PRINTFORMW 「确实，大浴池很舒服呢～！」
			ENDIF
		CASE "情人用宿屋"
			;入った時点で合意取得するのね…,うふふ前か後かは絶頂したかで判断するので事前に絶頂させてるとちょっと齟齬
			IF EX:70:Ｃ絶頂 + EX:70:Ｖ絶頂 + EX:70:Ａ絶頂 + EX:70:Ｂ絶頂 + EX:70:Ｍ絶頂
				IF TALENT:70:処女
					PRINTFORMW 「……❤」
				ELSE
					PRINTFORMW 「……❤」
				ENDIF
			ELSE
				IF TALENT:70:処女
					PRINTFORMW 「那个……嗯，请温柔一点哦……❤」
				ELSE
					PRINTFORMW 「じー……❤」
				ENDIF
			ENDIF
		CASE "長屋街"
			PRINTFORML 「这边主要是居住区域」
			PRINTFORMW 「要不要去阿求的家玩玩？」
			;虽然说着要去，但试图去的时候会被拒绝。女孩子真是难懂啊
		;▼霧の湖
		CASE "霧之湖"
			IF GET_MONTH() == "夏之月"
				PRINTFORML 「呼，真想游个泳啊」
				PRINTFORMW 「但是这个湖很危险，不能进去游泳的」
			ELSE
				PRINTFORML 「这个湖里会不会有什么大家伙呢？」
				PRINTFORMW 「大鲶鱼啦，人鱼啦，白色大蛇啦，各种目击传闻都有」
			ENDIF
		CASE "冰造的小雪屋"
			PRINTFORML 「为什么这里会有雪屋……」
			IF GET_MONTH() == "夏之月"
				PRINTFORMW 「好凉快……」
			ELSE
				PRINTFORMW 「呜呜……好冷……」
			ENDIF
		CASE "廃洋館"
			PRINTFORML 「哇，废弃的地方肯定有什么东西！」
			PRINTFORMW 「吓！ 看！ 刚才那是什么……」
		CASE "正門"
			PRINTFORML 「哇，这就是吸血鬼的宅邸啊」
			PRINTFORMW 「不、不可以进去哦……！」
		CASE "広間"
			PRINTFORML 「进、进吸血鬼的宅邸真的没问题吗……」
			PRINTFORMW 「不过真的好宽敞…… 感觉比从外面看要大得多……」
		CASE "食堂"
			PRINTFORML 「在这么大的地方用餐吗……」
			PRINTFORMW 「我觉得有点不安呢～」
		CASE "大図書館"
			PRINTFORML 「哇！ 太棒了！ 这就是那个红魔馆图书馆！」
			PRINTFORMW 「好羡慕好羡慕，我想在这里待一整天……」
		CASE "地下室"
			PRINTFORML 「%CALLNAME:MASTER% 先生！ 我们真的可以进去吗！？」
			PRINTFORMW 「好可怕哦～！ 我们还是回去吧～！」
		;▼迷いの竹林
		CASE "竹林入口"
			PRINTFORML 「这里往前没有导游是无法前进的」
			PRINTFORMW 「所以，%CALLNAME:MASTER% 先生，请您带路！」
		CASE "迷失的小道", "斜角的竹林"
			PRINTFORML 「嗯？这地方感觉刚刚好像来过……%CALLNAME:MASTER% 先生，没事吧……？」
			PRINTFORMW 「不会迷路出不去吧……？」
		CASE "兎的洞穴"
			PRINTFORML 「要在这样的洞穴里做什么～？」
			PRINTFORMW 「这、好可怕啊！黑暗的！」
		CASE "永遠亭"
			PRINTFORML 「%CALLNAME:MASTER% 先生，是来看诊吗？」
			PRINTFORMW 「虽然看起来没什么不适的样子」
		CASE "無名之丘"
			PRINTFORML 「这地方虽然氛围宁静，但好像是个危险的地方……」
			PRINTFORMW 「这么一想，这景色也有点诡异呢……」
		CASE "太陽花田"
			IF GET_MONTH() == "夏之月"
				PRINTFORMW 「哇！一片向日葵田！好壮观啊！」
			ELSE
				PRINTFORML 「这全是向日葵吗～」
				PRINTFORMW 「夏天一定会变成美丽的景色吧」
			ENDIF
		CASE "永遠亭的内院"
			PRINTFORML 「真厉害，地方这么豪华呢」
