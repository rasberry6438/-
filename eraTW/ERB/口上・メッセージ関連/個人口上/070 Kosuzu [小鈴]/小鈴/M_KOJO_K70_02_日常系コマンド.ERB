					ENDIF
				ELSE
					PRINTFORMW 「不要啦…！会、会有点痛…！」
				ENDIF
			ENDSELECT
		ENDIF
	ELSE
		;簡易同室人チェック,確率
		IF !RAND:3 && TARGET:2
			;ゲストキャラ判定
			CALL M_KOJO_K70_GUEST("日常愛撫")
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「等等……%RESULTS:1%在看呢……」
			CASE 2
				PRINTFORMW 「在%RESULTS:1%面前你在做什么啊！」
			CASEELSE
				PRINTFORMW 「我要告诉%RESULTS:1%哦～……？」
			ENDSELECT
		;野外,確率
		ELSEIF !RAND:3 && OUTROOF(CFLAG:[[小鈴]]:現在位置)
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「那个……在外面哦……」
			CASE 2
				PRINTFORMW 「我、我要报警了哦…！」
			CASEELSE
				PRINTFORMW 「请在这种地方停止……！」
			ENDSELECT
		ELSE
			SELECTCASE RAND:8
			CASE 1
				PRINTFORMW 「诶……变态！」
			CASE 2
				PRINTFORMW 「别、别这样啦……」
			CASE 3
				PRINTFORMW 「嗯～！ 变态！」
			CASE 4
				PRINTFORMW 「诶……色色的！」
			CASE 5
				PRINTFORMW 「别、别做这种事啦！」
			CASE 6
				PRINTFORMW 「那、那里是不行的…！」
			CASE 7
				IF ARGS == "胸愛撫"
					PRINTFORMW 「嗯呢！男人就是喜欢胸部啊……！」
				ELSE
					PRINTFORMW 「呜呜……好羞耻……」
				ENDIF
			CASEELSE
				IF ARGS == "胸愛撫"
					IF TALENT:[[小鈴]]:胸圍 > 0
						PRINTFORMW 「这只是碍事而已……」
					ELSEIF TALENT:[[小鈴]]:胸圍 < 0
						PRINTFORMW 「唔～…！对不起，我胸小啦……！」
					ELSE
						PRINTFORMW 「男人真是喜欢胸部啊……」
					ENDIF
				ELSE
					PRINTFORMW 「唔唔……好害羞……」
				ENDIF
			ENDSELECT
		ENDIF
	ENDIF
