@M_KOJO_K70_日常愛撫(ARGS)
SELECTCASE ARGS
CASE "怒る"
	;理論上できなくないけど、計算式が関数ではなく判定フラグも無いのでやめておく
CASE "不機嫌"
	IF TALENT:[[小鈴]]:恋慕
		PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：急", "…！")%」
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「对不起…现在不是心情的时候……」
		CASE 2
			PRINTFORMW 「对不起…请停下来……」
		CASEELSE
			PRINTFORMW 「请，请停下来…！」
		ENDSELECT
		BASE:70:憤怒 += MAXBASE:70:憤怒 / 4
	ELSE
		PRINTFORMW 「请停下来！」
		BASE:70:憤怒 += MAXBASE:70:憤怒 / 3
	ENDIF
CASE "絶頂"
	PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：急", "❤")%」
	SELECTCASE RAND:8
	CASE 1
		PRINTFORMW 「不行啊…！ 现在被触摸的话……啊❤」
	CASE 2
		PRINTFORMW 「不要啊…！ 不行了……啊❤」
	CASE 3
		PRINTFORMW 「等、等一下……！ 别……啊❤」
	CASE 4
		PRINTFORMW 「那、那里不行……啊❤」
	CASE 5
		PRINTFORMW 「嗯、不行了……不行啊……啊❤」
	CASE 6
		PRINTFORMW 「不要啊…！ 嗯…啊❤」
	CASE 7
		PRINTFORMW 「等等…！ 不好了啊……啊❤」
	CASEELSE
		PRINTFORMW 「啊…那以上就……啊❤」
	ENDSELECT
CASE "睡眠"
	SELECTCASE RAND:5
	CASE 1
		PRINTFORMW 「嗯…！ 好痒啊……」
	CASE 2
		PRINTFORMW 「嗯嗯…！ 好舒服～……」
	CASE 3
		PRINTFORMW 「嗯嗯～…！ 好软……」
	CASE 4
		PRINTFORMW 「嗯…！ 是什么啊～……」
	CASEELSE
		PRINTFORMW 「嗯～…！ 好柔软啊～……」
	ENDSELECT
CASE "射精"
	SELECTCASE RAND:4
	CASE 1
		PRINTFORMW 「要射了吗❤ 尽情射吧❤ 多射点❤ 给我❤」
	CASE 2
		PRINTFORMW 「好的❤ 来吧❤ 射给我吧❤ %M_KOJO_K70_AUTO("喘ぎ：挿入", "❤ ")%」
	CASE 3
		PRINTFORMW 「请吧❤ 射在我身上吧❤ %M_KOJO_K70_AUTO("喘ぎ：挿入", "❤ ")%」
	CASEELSE
		PRINTFORMW 「来吧❤ %M_KOJO_K70_AUTO("喘ぎ：挿入", "❤ ")% 就这样来吧❤ 射给我～❤」
	ENDSELECT
;胸,クリ,膣
CASEELSE
	IF TALENT:[[小鈴]]:恋慕 && CFLAG:[[小鈴]]:积攒度 >= 750
		PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：急", "…❤")%」
	ELSE
		PRINTFORML 「%M_KOJO_K70_AUTO("喘ぎ：急", "…！")%」
	ENDIF
	IF TALENT:[[小鈴]]:恋慕
		;簡易同室人チェック
		IF TARGET:2
			;ゲストキャラ判定
			CALL M_KOJO_K70_GUEST("日常愛撫")
			IF RESULT
				SELECTCASE RAND:4
				CASE 1
					PRINTFORMW 「等、等一下！ %RESULTS:1%在那里呢！」
				CASE 2
					PRINTFORMW 「明明有%RESULTS:1%在这里……不行啊…！」
				CASE 3
					PRINTFORMW 「真是的…！ %RESULTS:1%在这之前啊…！」
				CASE 4
					PRINTFORMW 「等等…！ %RESULTS:1%看着这边呢…！」
				CASEELSE
					PRINTFORMW 「被%RESULTS:1%发现了会怎么办啊…！」
				ENDSELECT
			ELSE
				SELECTCASE RAND:4
				CASE 1
					PRINTFORMW 「等、等一下！ 这里有人啊！」
				CASE 2
					PRINTFORMW 「嗯……可能被听到了吧……」
				CASE 3
					PRINTFORMW 「在人前碰触别人，真、真是大胆啊…！」
				CASEELSE
					PRINTFORMW 「有人看着呢啊…！」
				ENDSELECT
			ENDIF
		;野外,確率
		ELSEIF !RAND:3 && OUTROOF(CFLAG:[[小鈴]]:現在位置)
			SELECTCASE RAND:4
			CASE 1
				PRINTFORMW 「等、等一下！ 这里是外面啊…！」
			CASE 2
				PRINTFORMW 「真是的…！ 被人看到怎么办啊…！」
			CASE 3
				PRINTFORMW 「这、这种事最好还是在房间里……」
			CASEELSE
				PRINTFORMW 「在这种地方的话……会、会被抓住的哦～…」
			ENDSELECT
		ELSE
			SELECTCASE RAND:11
			CASE 1
				PRINTFORMW 「%CALLNAME:MASTER%先生真是个坏蛋呢…❤」
			CASE 2
				PRINTFORMW 「嗯、好舒服啊…❤」
			CASE 3
				PRINTFORMW 「嗯呐……太淘气了！」
			CASE 4
				PRINTFORMW 「嗯、嗯嗯～，好痒痒～！」
			CASE 5
				PRINTFORMW 「嗯呢～，真是不公平啊～！」
			CASE 6
				PRINTFORMW 「真的好淘气啊～！」
			CASE 7
				PRINTFORMW 「那个地方……稍微…❤」
			CASE 8
				PRINTFORMW 「嗯嗯～……❤」
			CASE 9
				PRINTFORMW 「不、不行啊～❤」
			CASE 10
				IF ARGS == "胸愛撫"
					PRINTFORMW 「%CALLNAME:MASTER%先生，喜欢胸部吗～？」
				ELSE
					PRINTFORMW 「等等！不要把手指伸过来啊！！嗯呢！」
				ENDIF
			CASEELSE
				IF ARGS == "胸愛撫"
					IF TALENT:[[小鈴]]:胸圍 > 0
						PRINTFORMW 「这样摸的话会变大的哦～❤」
					ELSEIF TALENT:[[小鈴]]:胸圍 < 0
						PRINTFORMW 「嗯嗯嗯…！小小的对不起……」
					ELSE
