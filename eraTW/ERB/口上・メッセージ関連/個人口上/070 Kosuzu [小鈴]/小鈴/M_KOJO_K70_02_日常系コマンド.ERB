;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K70
	IF CFLAG:70:睡眠
		PRINTFORMW 「嗯ぅ……」
	;酔ってる
	ELSEIF ALCOHOL_FACE(70) == "酔酒" || ALCOHOL_FACE(70) == "不省人事"
		CALL M_KOJO_K70_DRUNK("休憩")
	;発情
	ELSEIF TCVAR:[[小鈴]]:発情
		CALL M_KOJO_K70_HATSUJO("休憩")
