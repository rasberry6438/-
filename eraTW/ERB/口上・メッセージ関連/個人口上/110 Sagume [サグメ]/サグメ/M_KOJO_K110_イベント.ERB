;==================================================
;约会帰り
;==================================================
@M_KOJO_SPEVENT_K110_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:318 == 1
			PRINTFORMW 「……」
			PRINTFORMW 探女沉默地离開了。
		ELSEIF TALENT:恋人
			PRINTFORMW 「嗯……再见……」
			PRINTFORMW 「……那个、明天」
			PRINTFORMW 探女有些害羞地告别了。
			PRINTFORMW （能见面……对吧……）
		ELSEIF TALENT:恋慕
			PRINTFORMW 「……再见……」
			PRINTFORMW 「下次再一起吧」
			PRINTFORMW 探女这么说着就离開了。
			PRINTFORMW （能见面……对吧……）
		ELSEIF TALENT:思慕
			PRINTFORMW 「今天就到这里吧」
			PRINTFORMW 「很开心……再见……」
			PRINTFORMW 探女这么说着就离開了。
		ELSE
			PRINTFORMW 「那么……再见」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		ELSEIF TALENT:恋人
			PRINTFORMW 「别在意……」
			PRINTFORMW 「已经没有生气了」
			PRINTFORMW 「没事了哦」
		ELSE
			PRINTFORMW 「啊，没有在生气了」
			PRINTFORMW 「真的」
		ENDIF
	RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「没关系，我不太生气了」
			PRINTFORMW 「能感觉到%CALLNAME:MASTER%的诚意。」
		ELSE
			PRINTFORMW 「我知道了……」
			PRINTFORMW 「但是还是有一点生气」
			PRINTFORMW 探女好像还是有点生闷气。
			PRINTW
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「……」
			PRINTFORMW 「好吧，原谅你一点点」
		ELSE
			PRINTFORMW 「不行不行……」
			PRINTFORMW 「绝对不原谅」
			PRINTFORMW 尽管探女这么说着，但她也稍微消了一点气。
			PRINTW
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

