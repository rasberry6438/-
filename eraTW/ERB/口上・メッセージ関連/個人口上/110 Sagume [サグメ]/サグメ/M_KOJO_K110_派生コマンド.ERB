	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
			;時姦中
			IF FLAG:70
				PRINTFORMW 在冻结的时间中侵犯了探女的胸部，把阴茎伸进了口腔。
			ELSE
				PRINTFORMW 「……嗯……嘶溜……」
				PRINTFORMW 「唔姆……？」
				PRINTFORMW 探女用胸摩擦阴茎，吮吸着前端。
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 在冻结的时间中侵犯了探女的胸部，把阴茎伸进了口腔。
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「……怎么样？……舒服吗？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……嗯……虽然很大……」
		ELSE
			PRINTFORMW 「……会让你舒服的……」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「啊……嗯嗯……%UNICODE(0x2665) *1%」
			PRINTFORMW 「……胸部……好下流……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯啊……唔唔……？」
			PRINTFORMW 「嗯啊……呲溜……嗯嗯」
		ELSE
			PRINTFORMW 「……嗯……这个……」
			PRINTFORMW 「……呲溜……姆啾……」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「……嗯……这个怎么样？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……怎样？」
		ELSE
			PRINTFORMW 「……」
			PRINTFORMW 探女没说话，用胸部把阴茎完全的盖住
		ENDIF
	ENDIF
ENDIF
