			ENDIF
			
			;夜自慰後遭遇　思慕↑
			IF CFLAG:K17C_ID:K17C_CF_自慰フラグ
				SELECTCASE K17_GRT(TARGET)
					CASE IS >= K17C_恋慕
						PRINTFORML 「……%K17_GT()%……%K17_BLUSH()%」
