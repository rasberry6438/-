		SIF K17_GRT() >= K17C_思慕 && (!(CFLAG:MASTER:既成事實 & 合意_恋人) || CFLAG:TARGET:既成事實 & 合意_恋人)
			CFLAG:TARGET:情事目撃 = ARG
		
		
	ENDIF
	;-------------------------------------------------
	;MASTERに誘われて参加
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG:1 == 4
		;PRINTFORML
		;RETURN 1
	;ENDIF
	;其他
