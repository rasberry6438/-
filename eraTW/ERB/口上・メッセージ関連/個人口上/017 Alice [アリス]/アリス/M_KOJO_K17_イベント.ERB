		IF K17_CHECK_RANDOM_EVENT(TARGET) && !TCVAR:K17C_ID:K17C_TC_お風呂イベント
			;れっつとらぶる
			TRYCALL K17_お風呂イベント(TARGET)
			SIF RESULT
				PRINTFORMDL 只说了那么多、就慌慌张张地出去了
		ELSE
			PRINTFORMDL 出去时看见了若隐若现的%K17_RS(HIP(TARGET)+"/"+OPPAI_DESCRIPTION(TARGET,1)+"/"+THIGHS(TARGET))%
			TCVAR:K17C_ID:K17C_TC_お風呂イベント = 1
		ENDIF
		PRINTW
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;@風呂逐出条件==0 && 把柄 && HETEROSEX==1
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
