			ELSE
				PRINTFORML 「喔、打扰到你了吧」
			ENDIF
		ENDIF
		
		IF K17_GET_FRIENDS(FLAG:约会的对象) && K17_RT_FG(TARGET) == 0
			;愛麗絲にフラグが立ってない時　约会的对象が愛麗絲の知り合い
			SELECTCASE FLAG:约会的对象
				CASE K17C_魔理沙ID,K17C_霊夢ID,K17C_咲夜ID
