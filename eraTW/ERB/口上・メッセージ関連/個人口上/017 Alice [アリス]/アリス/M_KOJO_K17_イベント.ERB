			;れっつとらぶる
			TRYCALL K17_お風呂イベント(TARGET)
			SIF RESULT
				PRINTFORMDL 只说了这些就被撵走了
		ELSE
			PRINTFORMDL 被赶出去时、看见了若隐若现的%K17_RS(HIP(TARGET)+"/"+OPPAI_DESCRIPTION(TARGET,1)+"/"+THIGHS(TARGET))%
			TCVAR:K17C_ID:K17C_TC_お風呂イベント = 1
		ENDIF
		PRINTW
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
