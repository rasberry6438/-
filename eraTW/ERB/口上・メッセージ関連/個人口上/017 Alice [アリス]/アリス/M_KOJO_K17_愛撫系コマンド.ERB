@M_KOJO_MESSAGE_COM_K17_9_1
#DIM iType = 0
#DIMS cText
VARSET cText
;-------------------------------------------------
;感覚部位を選定
;-------------------------------------------------
IF TEQUIP:振動棒
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位V)
ELSEIF TEQUIP:肛用振動棒
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位A)
ELSEIF TEQUIP:淋浴
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位C)
ELSE
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),RAND:2?K17C_部位C#K17C_部位B)
ENDIF

;-------------------------------------------------
;9,自慰
;性癖あり
;-------------------------------------------------
IF K17_RT_FG() & K17C_RTF_性癖 && !(CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い) && GET_TARGETNUM() == 1
	;性癖あり　ねだり系　二人きり
	;妄想発表
	;一旦タイプ切り替え
	iType = CFLAG:K17C_CF_喘ぎタイプ
	cText = %K17_H妄想()%
	CALL K17_SET_AUTO_PVOICE(,,K17_喘ぎタイプ_H妄想(RESULT:0,RESULT:1))
	
	PRINTFORMDL %CALLNAME:MASTER%希望%CALLNAME:TARGET%能像平时一样%K17_RS("自慰/自我发电/秘密研究")%
	
	IF FIRSTTIME(@"{SELECTCOM}口上")
		;初回
		PRINTFORML 「嗯！？　唔……老是抓着人家的弱点……」
		PRINTFORMDL 虽然有些害羞%CALLNAME:TARGET%还是给与了回应
		PRINTW
		PRINTFORML 「听我说、我说呐、……要做是没关系啦……但是一个人…H,的话……有些寂寞……」
		PRINTFORML 「所以、在这之后、一定要好好疼爱我才行哦」
		PRINTFORMDL %CALLNAME:MASTER%温柔的抚摸着坐在自己大腿上的%CALLNAME:TARGET%
	ELSE
		;二回目以降
		PRINTFORML 「又要……这样做么？　mo……%K17_RS("H/变态/色鬼")%」
		PRINTFORMDL %K17_RS("虽然有些害羞/虽然嘴上那么说")%、但%CALLNAME:TARGET%还是做出了回应
		PRINTFORML 「在这之后、如果%K17_CNAME()%%K17_RS("不好好地疼爱我的话/不让我舒服起来的话")%、%K17_RS("我可不放过你哦/我可不会原谅你的/可不行哦")%%K17_HEART()%」
		PRINTFORMDL %CALLNAME:MASTER%温柔的%K17_RS("摸着/抱着")%%K17_RS("跨坐在身上/坐在大腿上的")%%CALLNAME:TARGET%
	ENDIF
	
	PRINTW
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTFORMDL 正在"熱心研究"的%CALLNAME:TARGET%闭上了眼睛沉浸在妄想的自慰中……
	PRINTW
	PRINTFORMDL %CALLNAME:MASTER%在%CALLNAME:TARGET%的耳边悄悄询问到底做了怎样的妄想
	PRINTFORML 「%cText%%K17_RS("妄想/想像")%、%K17_RS("做了H的事/H了")%%K17_BLUSH(1)%」
	PRINTW
	PRINTFORMDL %CALLNAME:TARGET%满脸通红、但似乎还是有些兴奋的样子坦白了
	PRINTFORMDL 让她将妄想的内容曝光出来似乎也成为了了一种play……
	CFLAG:K17C_CF_喘ぎタイプ = iType
	RETURN 1
	
;-------------------------------------------------
;9,自慰
;汎用
;-------------------------------------------------
ELSE
	;性癖無or襲い系
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	IF !(CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い)
		;ねだり系
		PRINTFORMDL 贪图着快感的%CALLNAME:TARGET%、手法似乎已经相当熟练了……
	ELSE
		;襲い系
		PRINTFORMDL 虽然很讨厌、但%CALLNAME:TARGET%还是按照学来的那样去做了、而且感觉手法已经相当娴熟了……
	ENDIF
	RETURN 1
ENDIF



