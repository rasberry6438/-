@M_KOJO_MESSAGE_COM_K17_312_1
#DIM iRT	= 0
#DIM iType	= 0
#DIM iKiss = 0
#DIM iRand =0
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:時間停止

	IF CFLAG:TARGET:睡眠
		;;PRINTFORML %CALLNAME:PLAYER%は沉浸在夢鄉中%CALLNAME:TARGET%の口を、自分の口で堵住…
		IF ABL:親密 >= 5
			;PRINTFORM 一分開嘴唇%CALLNAME:TARGET%就露出淡淡的微笑…
			PRINTFORML 「%K17_SHUFFLE_寝言(K17_RS("唔/诶嘿～/"+K17_HEART()+"Zzz"))%」
		ELSEIF ABL:親密 >= 3
			;PRINTFORM %CALLNAME:TARGET%は少し苦しそうに紧锁眉头…
			PRINTFORML 「%K17_SHUFFLE_寝言(K17_RS("唔/唔～/呼喵"))%」
		ELSE
			;PRINTFORM %CALLNAME:TARGET%は神情扭曲と無意識に顔をそむけた…
			PRINTFORML 「%K17_SHUFFLE_寝言(K17_RS("库/唔唔/喵"))%」
		ENDIF
		RETURN 1
	ENDIF
	
	IF TFLAG:193 == 99
		;PRINTFORM %CALLNAME:PLAYER%が接吻をしようとすると%CALLNAME:TARGET%は厌恶地拉開了距离…
		PRINTFORML 「啧……你刚才想干什么？别再这样了！」
		RETURN 1
	ENDIF

	;-----------------
	;フラグ初回チェック
	;-----------------
	IF !CFLAG:K17C_ID:K17C_CF_FKマ斯塔
		CALL K17_FIRSTKISS(TARGET)
		RETURN 1
	ENDIF
	
	;-----------------
	;各種初期化
	;-----------------
	iRT = K17_GRT_EX()
	iKiss = 0
	iRand = 0
	
	;-----------------
	;被逆推中　タイプ一時変更
	;-----------------
	IF CFLAG:诶嘿嘿 == 2
		iType = CFLAG:K17C_ID:K17C_CF_喘ぎタイプ
		CALL K17_SET_AUTO_PVOICE(iRT, K17C_部位M_接吻,K17C_受け_おねだり系)
		PRINTFORML 「%K17_AUTO_PVOICE()%」
		CFLAG:K17C_ID:K17C_CF_喘ぎタイプ = iType
		RETURN 1
	ENDIF
	
	;-----------------
	;通常接吻
	;-----------------
	;この後憤怒
	IF K17_CHECK_ANGER_COM(TARGET)
		PRINTFORML 「唔……%K17_RS("都说了不要了/都说了不行了/都说了讨厌了")%」
		PRINTFORMDL %CALLNAME:TARGET%用两手阻止了%CALLNAME:MASTER%
		RETURN 1
	ENDIF
	
	;驚きパターン
	IF ABL:欲望 < 5 && ABL:親密 < 5
		CALL K17_SET_AUTO_PVOICE(iRT, K17C_部位M_接吻,K17C_受け_强行系)
		PRINTFORML 「%K17_AP_NUM(1,1)%」
		PRINTFORML 「唔……这样的……是……不可以的……%K17_BLUSH()%」
		PRINTFORMDL %CALLNAME:TARGET%背过脸将%CALLNAME:MASTER%推了回去
		RETURN 1
	ENDIF
	
	
	
	;ちゅっちゅするほう
	IF (PREVCOM == 305 || CFLAG:TARGET:陪睡中) && RESULT:1 <= 300
		iRand = K17_MAX_LIMIT_RAND(iRT,K17C_恋慕告白,K17C_恋人)
		SELECTCASE iRand
			CASE IS >= K17C_恋人
				PRINTFORML 「唔～～%K17_RS("啾/啾呋/嗯啾")%%K17_HEART()%」
				PRINTFORML 「%K17_RS("哎嘿嘿/诶嘿/呼妞")%……呐、再来一次吧？呐？%K17_HEART()%」
				PRINTFORML 「%K17_RS("啾/啾呋/嗯啾")%%K17_HEART()%　%K17_RS("啾/啾呋/嗯啾")%%K17_HEART()%」
			CASE IS >= K17C_恋慕告白
				PRINTFORML 「%K17_RS("唔～啾/哈姆/唔啾")%%K17_HEART()%　……%K17_CNAME()%的嘴唇、为什么会那么%K17_RS("美味呢/柔软呢/让人迷恋呢")%……」
				PRINTFORML 「无论多少次都感觉不够……%K17_RS("唔～啾/哈姆/唔啾")%%K17_HEART()%」
			CASE IS >= K17C_思慕
				PRINTFORML 「唔～～%K17_RS("啾/啾呋/嗯啾")%」
				PRINTFORML 「%K17_RS("哎嘿嘿/诶嘿/呼妞")%……%K17_BLUSH()%」
			CASEELSE
				PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,iKiss,2,1)%」
				PRINTFORMDL %CALLNAME:TARGET%满脸通红、有些害羞的移開了视线
				RETURN 1
		ENDSELECT
		PRINTFORMDL %CALLNAME:TARGET%一副很喜悦的样子
		RETURN 1
	ENDIF
	
	;ディープ判定
	IF ABL:欲望 >= 5
		iKiss = K17_ABL_RT_EX(iRT,K17C_部位M_接吻)
	ELSE
		iKiss = -1
	ENDIF
	
	;関係査定EX
	;ぶちゅぶちゅするやつ
	;K17_チュパ音(タイプ,感覚値,回数,終端
	;反感固定ソース　300
	SELECTCASE BASE:情緒
		CASE IS >= 650
			SELECTCASE iRT
				CASE IS >= K17C_恋人
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%哈啊哈啊%K17_HEART()%还想要―%K17_チュパ音(K17C_受け_おねだり系,iKiss,3,1)%」
					PRINTFORMDL %CALLNAME:TARGET%兴奋的抬起头、不断地索求着
				CASE IS >= K17C_恋慕告白
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2)%再来……做吧―%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%不停渴求着快点继续
				CASE IS >= K17C_思慕接吻
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%的眼睛湿润得看上去就像是快要融化了似的
				CASEELSE
					PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%满脸通红、有些害羞的移開了视线
			ENDSELECT
		CASE IS >= 450
			SELECTCASE iRT
				CASE IS >= K17C_恋人
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2)%再多一点……好吗？　%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%不停渴求着快点继续
				CASE IS >= K17C_恋慕告白
					PRINTFORML 「%K17_チュパ音(K17C_受け_おねだり系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%的眼睛湿润得看上去就像是快要融化了似的
				CASE IS >= K17C_思慕接吻
					PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,iKiss,2,1)%」
					PRINTFORMDL %CALLNAME:TARGET%满脸通红、有些害羞的移開了视线
				CASEELSE
					PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,iKiss,1,1)%」
					PRINTFORMDL %CALLNAME:TARGET%满脸通红、有些害羞的移開了视线
			ENDSELECT
			
		;情緒足りない
		CASEELSE
				PRINTFORML 「%K17_チュパ音(K17C_受け_强行系,-1,1,1)%」
				PRINTFORML 「真是的、%K17_RS("突然这样是不行的啦/不要突然这样啦/不要突然就这样啊")%……%K17_BLUSH()%」
				PRINTFORMDL %CALLNAME:TARGET%有些生气的转过脸去
				RETURN 1
	ENDSELECT
	
	;屋内で二人きり追加　実値逢引以上 被逆推中以外
	IF INROOM(CFLAG:現在位置) && GET_TARGETNUM()==1 && K17_GRT() >= K17C_思慕逢引 && CFLAG:诶嘿嘿 != 2
		;グローバル保存
		CALL K17_SET_AUTO_PVOICE(iRT, K17C_部位M_接吻,K17C_受け_おねだり系)
		PRINTW
		PRINTFORML 「……唔……哈啊……%K17_AP_DEC()%还要……%K17_RS("做么/再做么/来么/再来么")%？」
		PRINTFORML 「%K17_AP_NUM(1)%%K17_RS("等/等等/呀昂")%……%K17_AP_NUM(3)%喜欢%K17_HEART()%」
		PRINTFORMDL %CALLNAME:MASTER%将%CALLNAME:TARGET%推到、不断地热吻着
		PRINTFORML 「%K17_AUTO_PVOICE()%」
		IF RAND:2
			PRINTFORMDL 只剩下两人的%K17_RS("接吻声/吐息声")%在房间中回响着
		ELSE
			PRINTFORMDL 两人的嘴唇一离開、%K17_RS("就立马拉出了一条银色的丝线/就立刻架起了一座银色的桥梁")%
		ENDIF
	ENDIF
	
ENDIF
RETURN 1
