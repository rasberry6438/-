					PRINTFORMDL 从掀起来的裙摆下、%cPANTS%隐约可见
				ENDIF
			ELSE
				PRINTFORMDL %THIGHS(TARGET)%隐约可见
			ENDIF
			PRINTFORM 「呀啊啊啊、%K17_RS("为什么会这样/为什么/讨厌")%……%K17_RS("差劲/真糟")%……」
			TRYCALL K17_人形台詞(iDoll,1,K17_RS("幸运色狼/呀呀ー/看到了")+"！")
		ENDIF
		PRINTW
		RETURN 1
	ENDIF
	
	
	;--------------------
	;思慕か好感度5000↑の
	;被逆推中
	;--------------------
	IF CFLAG:诶嘿嘿 == 2
		PRINTFORML 「%K17_RS("呀啊/呀嗯/喵呜")%%K17_HEART()%　就那么想看我的%cALICE%么？」
		PRINTFORMDL %CALLNAME:TARGET%这么说着一边晃着%K17_RS("腰/屁股")%一边抗议着
		PRINTFORMDL %K17_GET_CLOTHNAME_BOTTOM(TARGET)%下的%cPANTS%隐约可见
		PRINTW
		RETURN 1
	ENDIF
	
	;--------------------
	;思慕か好感度5000↑
	;MOBあり無で反応分け
	;ありは露出癖5　無は露出癖3で見せつけ
	;SIF PALAM:欲情 > PALAMLV:4　で期待
	;--------------------
	;--------------------
	;許可初回限定
	;--------------------
	IF FIRSTTIME(@"{SELECTCOM}口上")
		;人形チェック
		iDoll=K17_GET_DOLL(0)
		IF iDoll ==  K17C_人形数
			;両方平気なら上海優先
			iDoll = K17C_上海
		ENDIF
		PRINTFORM 「喂！你在干什么呢……H%K17_HEART()%」
		TRYCALL K17_人形台詞(iDoll,1,"色狼")
		PRINTFORM 「那么想看胖次的话、就去看%K17C_人形名:iDoll%的吧」
		TRYCALL K17_人形台詞(iDoll,1,"呀ー")
		PRINTFORMDL 说完便掀起了%K17C_人形名:iDoll%的裙子露出了灯笼裤、%CALLNAME:TARGET%让人偶做出了很害羞的演技
		IF CFLAG:K17C_ID:K17C_CF_上海イベント<=0
			PRINTFORMDL 真的很擅长操纵呢、人偶就像活着一样
		ELSE
			PRINTFORMDL 一想到上海的事、就感觉有点不太好……
		ENDIF
		PRINTW
		PRINTFORML 「再说了、掀女孩子裙子什么的这种事、也太幼稚了、别再做了」
		PRINTFORMDL 在%CALLNAME:TARGET%絮絮叨叨说教时、吹来了一阵风、%CALLNAME:TARGET%的%K17_GET_CLOTHNAME_BOTTOM(TARGET)%被掀了起来
		PRINTFORM 「呀啊啊啊、讨厌……看、看见了……？」
		TRYCALL K17_人形台詞(iDoll,1,"被看见了！")
		IF CFLAG:没穿内裤
			PRINTFORMDL 从掀起的裙摆下可以隐约看见屁股……
		ELSE
			PRINTFORMDL 从掀起的裙摆下可以隐约看见胖次……
		ENDIF
		PRINTFORML 「给……给我……忘掉%K17_BLUSH()%」
		PRINTFORMDL 还想好好仔细看看……
		PRINTW
		PRINTFORML 「唔……就那么……想看么……？」
		PRINTFORMDL 总感觉有种快要妥协的感觉了、%CALLNAME:MASTER%将%CALLNAME:TARGET%壁咚在耳边拜托着
		PRINTFORM （呀啊ー呀啊ー……为什么……这么拼命啊……）
		TRYCALL K17_人形台詞(iDoll,1,"你是笨蛋么")
		PRINTFORML 「等等、我知道了、我知道啦……别再这么引人瞩目了……」
		PRINTFORMDL 看来%CALLNAME:TARGET%比起自己的胖次更在意周围的人
		
	;--------------------
	;二回目以降ここから
	;--------------------
	ELSE
		PRINTFORM 「%K17_RS("昂嗯/呀啊/喂！")%……又想要看了么？　%K17_RS("H/変態/色狼")%」
		TRYCALL K17_人形台詞(iDoll,1,K17_RS("痴汉/色狼/变态/H"))
		PRINTFORML 「不是说过了这种地方不行了么……」
		PRINTFORMDL %CALLNAME:TARGET%这么说着警戒的看着四周
	ENDIF
	
	;他角色、路人チェック
	IF iMOB || DATE_HITOGOMI(CFLAG:PLAYER:現在位置) || WITH_MOB()
		PRINTFORML 「露出来是%K17_RS("不行的/不可以的/禁止的")%……呐？拜托了……看是可以啦、但只能是%K17_CNAME()%一个、呐」
	ELSE
		PRINTFORML 「要看的话只能是%K17_CNAME()%一个……%K17_RS("悄悄的/秘密的")%……呐？」
	ENDIF
	PRINTFORMDL %CALLNAME:TARGET%在%CALLNAME:MASTER%耳边低声细语、牵着手带去了%cShade%
	
	;路人描写先にやっちゃう
	CALL K17_DATE_HITOGOMI_MESSAGE
	PRINTW
	
	;見せつけ　既定値超えてたら65％
	IF iOpen && K17_PERCENT(65)
		IF EQUIP:下半身内衣２
			;今日の胖次あり
			PRINTFORML 「……好的、今天的%PANTSNAME(EQUIP:下半身内衣２, TARGET)%……%K17_RS("请仔细看清楚吧/请欣赏吧/要悄悄的看哦")%、呐%K17_BLUSH()%」
		ELSE
			;其他
			PRINTFORML 「……好的、%CALLNAME:TARGET%的%cALICE%……%K17_RS("请仔细看清楚吧/请欣赏吧/要悄悄的看哦")%、呐%K17_BLUSH()%」
		ENDIF
		PRINTFORMDL %CALLNAME:TARGET%自己将%K17_GET_SKIRT(TARGET)%慢慢掀了起来、在%CALLNAME:MASTER%的面前将%cPANTS%展示了出来……
		PRINTFORML 「就这么喜欢这种事……%K17_RS("H/変態/色狼")%……」
		PRINTFORML 「%K17_RS("呀昂/啊嗯/喵呜")%%K17_HEART()%　真是的、也太兴奋过头了吧、鼻息都吹到我了……」
	;恥ずかしがり
	ELSE
		PRINTFORML 「很羞耻的啊……还是%K17_CNAME()%自己来掀%K17_GET_SKIRT(TARGET)%吧……？」
		PRINTFORMDL 这么说完、%CALLNAME:MASTER%刚打算将%K17_GET_SKIRT(TARGET)%掀起来就被按住了手
		PRINTFORML 「掀、掀过头了啊……只能……一点点」
		PRINTFORMDL 无可奈何的%CALLNAME:MASTER%只能将脸钻入的%CALLNAME:TARGET%的%K17_GET_SKIRT(TARGET)%、%K17_RS("鉴赏/欣赏/窥视")%%cPANTS%
		PRINTFORML 「%K17_RS("呀啊～呜/呀啊啊～呜/笨蛋～呜")%、真%K17_RS("H/変態/是个色狼")%」
	ENDIF
	
	;神綺様優先 バレるのか心配するのは友人だけでいいかな
	IF iMOB
		RESULT = K17_GET_FRIENDS_TARGET()
		SIF RESULT
			PRINTFORML （要被发现了……要被%K17_CNAME(RESULT)%看到了……）
	ENDIF
	
	IF PALAM:欲情 > PALAMLV:4 && !iMOB
		PRINTW
		PRINTFORML 「呐……只是看看……么？」
		PRINTFORMDL %CALLNAME:TARGET%那湿润的眼眸、像是抱有什么期待的样子瞄着%CALLNAME:MASTER%…
	ENDIF
	PRINTW
ENDIF
RETURN 1

