@M_KOJO_MESSAGE_COM_K17_351_1
#DIM iFriends = 0
#DIM iMOB = 0
#DIMS cMove
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:時間停止
	PRINTL
	;烂醉
	IF TCVAR:(TARGET:LOCAL):烂醉
		PRINTFORML 「唉～……你要带我去哪里～唉嘿嘿……」
		PRINTW
		RETURN 1
	ENDIF
	
	;友達優先で同行している人を尋找
	iFriends = 0
	iMOB = 0
	;MASTERに同行がついてる場合　誰かを尋找
	IF CFLAG:MASTER:同行
		;友達を見る
		FOR i,0,K17C_友人人数
			IF CFLAG:(K17C_友人ID:i):同行
				iFriends = K17C_友人ID:i
				BREAK
			ENDIF
		NEXT
		;普通に見る
		IF !iFriends
			FOR i,1,CHARANUM
				IF (CFLAG:(TARGET:i):同行) && TARGET:i != K17C_ID
					iMOB = TARGET:i
					BREAK
				ENDIF
			NEXT
		ENDIF
	ENDIF
	
	;二人の移動方式を選択
	VARSET cMove
	IF ( !FIRSTTIME(@"311约会移動",1) && K17_LUCKY_SUKEBE(30) ) && (!iFriends && !iMOB)
		;腕抱き経験済み
		cMove = %CALLNAME:TARGET%自然地抱住了%CALLNAME:MASTER%的手臂、将%K17_RS("胸部/欧派")%%K17_RS("挤了过来/压了上来")%就这样跟着过来了
	ELSEIF ( !FIRSTTIME(603,1) || !FIRSTTIME(@"身体接觸0",1) ) && (!iFriends && !iMOB)
		;牽手経験済み
		IF K17_LUCKY_SUKEBE(40)
			cMove = %CALLNAME:TARGET%有点%K17_RS("脸红/很害羞的样子")%、握着%CALLNAME:MASTER%的手跟着过来了
		ELSE
			cMove = %CALLNAME:MASTER%握住了%CALLNAME:TARGET%的手、她有些%K17_RS("脸红/很害羞的样子")%回握了
		ENDIF
	ELSEIF iFriends || iMOB
		;他に同行がいる
		cMove = %CALLNAME:TARGET%向%CALLNAME:(iFriends!=0 ? iFriends#iMOB)%轻轻的%K17_RS("打招呼/问候")%便跟着%CALLNAME:MASTER%了
	ELSE
		;まだやってない
		cMove = %CALLNAME:MASTER%将%CALLNAME:TARGET%带出去了
	ENDIF
	
	;反応と描写
	SELECTCASE K17_GRT_EX(TARGET)
		CASE IS >= K17C_恋慕
			PRINTFORML 「那么、%K17_RS("你要带我去哪呢/你要带我去哪/去哪儿呢")%？」
			PRINTFORMDL %cMove%
		CASE IS >= K17C_思慕逢引
			PRINTFORML 「那么、%K17_RS("你要带我去哪呢/你要带我去哪/去哪儿呢")%？」
			PRINTFORMDL %cMove%
			
		CASE IS >= K17C_思慕
			PRINTFORML 「那么、%K17_RS("你要带我去哪呢/去哪儿呢")%？」
			PRINTFORMDL %cMove%
		CASEELSE
			PRINTFORML 「……那个……不要去奇怪的地方哦？」
			PRINTFORMDL %CALLNAME:TARGET%似乎有所警戒
	ENDSELECT
	
	;他同行者への反応
	IF iFriends
		PRINTFORML （……%K17_CNAME(iFriends)%也在一起、%K17_RS("啊/呐")%……%K17_RS("到底要做什么呢/你打算干什么")%）
	ELSEIF iMOB
		PRINTFORML （……其他的孩子也在一起……%K17_RS("么/、要做什么呢/呐")%）
	ELSE
		;同行者が他にいない時は诶嘿嘿合意基準
		SELECTCASE K17_GRT()
			CASE IS >= K17C_思慕诶嘿嘿
				PRINTFORML （……%K17_RS("不去找个安静的地方么/去找个二人独处的地方吧/到没什么人的地方去吧")%……？）
			CASE IS >= K17C_思慕逢引
				PRINTFORML （……不会要%K17_RS("带我去/拐我去")%什么奇怪的地方吧……？）
		ENDSELECT
		;同行者が愛麗絲だけで他の人がその場にいる場合
		SIF GET_TARGETNUM() > 1
			PRINTFORMDL 只有自己被带走、%CALLNAME:TARGET%看起来有点害羞
	ENDIF
	
ENDIF
PRINTW
RETURN 1
