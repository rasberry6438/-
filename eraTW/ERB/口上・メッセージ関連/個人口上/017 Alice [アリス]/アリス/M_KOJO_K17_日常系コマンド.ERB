		TRYCALL K17_人形台詞(-1,1,K17_RS("打扫ー/開始打扫/要打扫了ー/打扫时间到ー"))
	ENDIF
	PRINTFORMDL 总感觉%CALLNAME:TARGET%干劲十足……
	
