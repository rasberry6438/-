;*********************************************************
;会話	：人偶劇感想
;会話不能でも優先的に選ばれる
;仕事終わり13時くらいか？からの人偶劇を1度でも見ているときの会話
;*********************************************************
@M_KOJO_MESSAGE_COM_K17_300_1_会話_人偶劇感想
#DIM DYNAMIC iFlagNum = 0
#DIMS DYNAMIC cEX=""
;何個見たか数える
FOR i,0,4
	SIF GETBIT(TCVAR:K17C_ID:K17C_TC_人偶劇,i)
		iFlagNum ++
NEXT

PRINTFORM 「……啊啊、看了人偶剧吗……总觉得有点不好意思呐」
TRYCALL K17_人形台詞(-1,1,"ハズカシー")
PRINTW

;随意好感度うｐ
;読んだ数*10
TFLAG:好感度管理 += 10*iFlagNum

;見た数で判定
IF iFlagNum < 4
	;全部見られなかった
	PRINTFORML 「那个、感觉怎样呢……？」
	PRINTFORMDL 被%CALLNAME:TARGET%这样问道、没有好好看完%CALLNAME:MASTER%只能适当回答了
	PRINTFORML 「呼ー唔……下次要来全部看完哦」
ELSE
	SIF K17_GRT() >= K17C_思慕
		cEX = 也想拯救魔女
	CALL K17_CHOICE("「那个、感觉怎样呢……？」","很普通","很不错",cEX)
	SELECTCASE RESULT
		CASE 0
			;普通
			PRINTFORML 「呼ー嗯、很坦率的感想呢」
			PRINTFORML 「没有什么怪怪的感觉、真是太好了、谢谢你」
		CASE 1
			;よかった
			PRINTFORML 「唔ー嗯、真的……？」
			PRINTFORML 「意外的也有孩子气一面呢。呼呼」
		CASE 2
			;魔女も助けた
			PRINTFORML 「咦…………」
			PRINTFORML 「还真是意外的感想、稍微有点……吓到了、呢……」
			PRINTFORMDL %CALLNAME:TARGET%不知为何脸变得通红、低下了头
			TFLAG:好感度BONUS += 100
			CFLAG:K17C_ID:K17C_CF_上海好感度 = MIN(CFLAG:K17C_ID:K17C_CF_上海好感度 + 100,10000)
	ENDSELECT
ENDIF
RETURN 1

