@M_KOJO_MESSAGE_COM_K17_313_1
;-------------------------------------------------
;313,胸愛撫
;時間停止、Counter诶嘿嘿、睡眠以外
;各種初期化
;-------------------------------------------------
#DIM iRT		;EXでとって
#DIMS cAction,2	;感度による行動
#DIMS cReAction	;感度による反応
#DIM iBust		;胸の状態　服上からなら0 はだけ内衣上なら1 生2
#DIM iFriends	;知り合いがいるか見る
#DIM iMOB		;他人がいるか見る
#DIM iExFlag	;追加条件フラグ
#DIM iAnger		;この指令選択で怒るかどうか

iRT = K17_GRT_EX()
iAnger = K17_CHECK_ANGER_COM(TARGET)
;DEBUGPRINTFORML 【M_KOJO_MESSAGE_COM_K17_313_1 関係性値({iRT})】

;-----------------
;喘ぎタイプ選択
;-----------------
CALL K17_SET_AUTO_PVOICE(iRT,K17C_部位B)
VARSET cAction
cReAction =
iBust = 0
iFriends = 0
iMOB = 0
iExFlag = 0

;他人もしくは路人がいるか見る　ついでに知り合いかどうかもみる
IF DATE_HITOGOMI(CFLAG:PLAYER:現在位置) || GET_TARGETNUM() > 1
	iMOB = 1
	iFriends = K17_GET_FRIENDS_TARGET(TARGET)
ENDIF

;追加条件式
;Ｂ感覚4以上＆思慕逢引以上
SIF ABL:Ｂ感覚 >= 4 && iRT >= K17C_思慕逢引 && BASE:情緒 >= 500
	iExFlag = 1
;4未満なら　愛でカバー
SIF iRT >= K17C_恋慕 && BASE:情緒 >= 500
	iExFlag = 2
;憤怒付くようなら强行に
SIF iAnger && iRT >= K17C_思慕逢引
	iExFlag = 2

;Counter&脱ぎ条件
IF TCVAR:Counter行動 == 27 && iExFlag && !iMOB && !FIRSTTIME(@"{SELECTCOM}口上",1)
	;通常Counter優先
	IF TALENT:TARGET:胸圍 >= 1
		cAction += @"%CALLNAME:PLAYER%将手伸入了%CALLNAME:TARGET%裸露在外的%OPPAI_DESCRIPTION(TARGET,1)%的谷间,"
	ELSE
		cAction += @"%CALLNAME:PLAYER%将手伸入了%CALLNAME:TARGET%裸露在外的%OPPAI_DESCRIPTION(TARGET,1)%的拥挤谷间"
	ENDIF
;Counterあり優先
ELSEIF TCVAR:Counter行動 == 27
	;谷間見せ　胸尺寸見る
	IF TALENT:TARGET:胸圍 >= 1
		cAction += @"%CALLNAME:PLAYER%将手伸入了%CALLNAME:TARGET%的谷间,"
	ELSE
		cAction += @"%CALLNAME:PLAYER%将手伸入了%CALLNAME:TARGET%拥挤的谷间、"
	ENDIF
;以下Counter無　脱ぎ条件
ELSEIF iExFlag && !iMOB && !FIRSTTIME(@"{SELECTCOM}口上",1)
	cAction = %CALLNAME:PLAYER%将%CALLNAME:TARGET%的裸在外的%OPPAI_DESCRIPTION(TARGET,1)%
	
;はだけあり
ELSEIF !iMOB && !iFriends
	;TEQUIP:4 = 1
	cAction = %CALLNAME:PLAYER%
	;服装チェック
	IF EQUIP:上身衣服１ || EQUIP:上身衣服２ || EQUIP:連衣裙 || EQUIP:和服
		;上身衣服の検索
		IF EQUIP:和服
			cAction += @"%K17_RS(@"剥下了%K17_GET_CLOTHNAME_TOP(TARGET)%/将手伸入%K17_GET_CLOTHNAME_TOP(TARGET)%的领口")%、"
		ELSEIF EQUIP:連衣裙 == 1
			cAction += @"%K17_RS(@"解開了%K17_GET_CLOTHNAME_TOP(TARGET)%的扣子/将手伸入了%K17_GET_CLOTHNAME_TOP(TARGET)%的缝隙")%、"
		ELSE
			cAction += @"剥下了%K17_GET_CLOTHNAME_TOP(TARGET)%、"
		ENDIF
	ENDIF
	IF EQUIP:TARGET:上半身内衣１ || EQUIP:TARGET:上半身内衣２ || EQUIP:TARGET:緊身内衣 || EQUIP:TARGET:連体内衣
		cAction += @"隔着内衣的%OPPAI_DESCRIPTION(TARGET,1)%"
		iBust = 1
	ELSEIF TEQUIP:上半身着衣状況 == 0
		;着てない
		cAction = %CALLNAME:PLAYER%将%CALLNAME:TARGET%裸露在外的%OPPAI_DESCRIPTION(TARGET,1)%
		iBust = 2
	ELSE
		;外套あり　内衣無
		SIF !(EQUIP:TARGET:上半身内衣１ || EQUIP:TARGET:上半身内衣２ || EQUIP:TARGET:緊身内衣 || EQUIP:TARGET:連体内衣)
			cAction += "因里面什么也没穿而"
		cAction += @"露出来的%OPPAI_DESCRIPTION(TARGET,1)%"
		iBust = 2
	ENDIF
;はだけ無
ELSEIF iMOB || iFriends
	cAction = %CALLNAME:PLAYER%从%CALLNAME:TARGET%的
	;服装チェック
	IF EQUIP:上身衣服１ || EQUIP:上身衣服２ || EQUIP:連衣裙
		;上身衣服の検索
		FOR i,GETNUM(EQUIP,"和服"),GETNUM(EQUIP,"上半身１"),-1
			IF EQUIP:TARGET:i 
				cAction += @"%CLOTHNAME(i,EQUIP:TARGET:i)%上方"
				BREAK
			ENDIF
		NEXT
	ENDIF
	iBust = 0
ENDIF

;感度別アクション
IF ABL:Ｂ感覚 <= 1
	cAction += "抚摸似地"
ELSEIF ABL:Ｂ感覚 <= 3
	cAction += "温柔地"
ELSE
	cAction += "细心地"
ENDIF
cAction += "揉捏起来………"


;感度別リアクション
IF ABL:Ｂ感覚 <= 1
	cReAction = %CALLNAME:TARGET%好像很痒似的………
ELSEIF ABL:Ｂ感覚 <= 4
	cReAction = %CALLNAME:TARGET%因为愛撫開始呼出火熱的气息………
ELSE
	cReAction = %CALLNAME:TARGET%は露出了迷离的高潮脸任凭摆布………
ENDIF
	

;-------------------------------------------------
;313,胸愛撫
;地の文開始
;-------------------------------------------------
PRINTFORMDL %CALLNAME:MASTER%向着%CALLNAME:TARGET%的欧派伸出了手……

;-------------------------------------------------
;313,胸愛撫
;記入チェック（=0, 非表示、1, 表示）
;失敗　憤怒が付く
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ( iAnger || CFLAG:ブチギレ ) && !iExFlag
	PRINTFORMDL %CALLNAME:TARGET%快速了捂住了胸部、挪開了%CALLNAME:MASTER%的手
	PRINTW
	PRINTFORM 「讨厌……真是够了……」
	TRYCALL K17_人形台詞(-1,1,K17_RS("痴汉/少女之敌/变态"))
	RETURN 1
ENDIF

;-------------------------------------------------
;313,胸愛撫
;記入チェック（=0, 非表示、1, 表示）
;失敗　憤怒が付く がEX条件満たしてる時　強引にレイプっぽく
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ( iAnger || CFLAG:ブチギレ ) && iExFlag
	PRINTFORMDL %CALLNAME:TARGET%快速了捂住了胸部、挡住了%CALLNAME:MASTER%的手
	PRINTW
	;喘ぎのタイプを変える
	CALL K17_SET_AUTO_PVOICE(iRT,K17C_部位B,K17C_受け_强行系)
	cAction:1 = %CALLNAME:MASTER%对一脸讨厌的%CALLNAME:TARGET%兴奋了起来
	;服装チェック
	;上身衣服
	SIF TEQUIP:上半身着衣状況
		cAction:1 += @"%K17_GET_CLOTHNAME_TOP(TARGET)%强行直接摸了上去、"
	;内衣検索
	IF K17_GET_CLOTHNAME_TOP_UNDER(TARGET) != ""
		;内衣
		cAction:1 += K17_GET_CLOTHNAME_TOP_UNDER(TARGET)
		;外套脱いだか見る
		IF K17_GET_CLOTHNAME_TOP(TARGET) != ""
			cAction:1 += "也"
		ELSE
			cAction:1 += ""
		ENDIF
		cAction:1 += "被剥了下来、\n"
	ENDIF
	cAction:1 += @"继续揉着%K17_RS("露出来的/裸露的")%%OPPAI_DESCRIPTION(TARGET,1)%"
	
	;通常処理
	
	PRINTFORML 「%K17_AP_NUM(2)%……等、等、%K17_RS("太过头了/太激烈了")%、%K17_RS("不行！/咿呀/不—要！")%%K17_AP_NUM(2,1)%」
	PRINTFORMDL %cAction:1%
	PRINTFORML 「%K17_CALL_P(K17C_部位B)%、都说了%K17_RS("不要了/不可以了/不要揉了")%……%K17_AP_NUM(3,1)%」
	SIF K17_ABL_RT_EX(iRT,K17C_部位B)
		PRINTFORMDL %CALLNAME:TARGET%虽然很讨厌但还是有感觉了……
	RETURN 1
ENDIF

;-------------------------------------------------
;313,胸愛撫
;記入チェック（=0, 非表示、1, 表示）
;初回成功
LOCAL = 1
;-------------------------------------------------
IF LOCAL && FIRSTTIME(@"{SELECTCOM}口上")
	;タイプ変え
	CALL K17_SET_AUTO_PVOICE(iRT,K17C_部位B,K17C_受け_合意襲い系)
	PRINTFORMDL %CALLNAME:TARGET%慢慢反应过来、将手重叠放在自己胸前的%CALLNAME:MASTER%手上
	PRINTW
	PRINTFORM 「等……等等……那里……想摸么……？」
	TRYCALL K17_人形台詞(-1,1,K17_RS("痴汉/H/变态"))
	PRINTFORMDL 不由自主的就伸出了手、你很讨厌这样么
	PRINTFORMDL %CALLNAME:TARGET%没有回答、手上的力度慢慢弱了下来、满脸通红的闭上了眼
	PRINTFORMDL %cAction%
	
	;通常処理
	PRINTW
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTFORMDL %cReAction%
	;胸の状態分岐
	SELECTCASE iBust
		CASE 0
			PRINTFORML （（这样……和内衣擦到了……感觉有点%K17_RS("痛/难受/痒痒的")%……）
		CASE 1
			PRINTFORML （这样……和内衣擦到了……感觉有点%K17_RS("痛/难受/痒痒的")%……）
		CASE 2
			PRINTFORML （这样……直接就……笨蛋、H!%K17_HEART()%……）
	ENDSELECT
	;人がいるか見る
	IF iFriends
		PRINTFORML （声音……出来了……要被%K17_CNAME(iFriends)%%K17_RS("发现了/听到了")%……）
	ELSEIF iMOB
		PRINTFORML （声音……出来了……要被别人%K17_RS("发现了/听到了")%……）
	ENDIF
	RETURN 1
ENDIF

;-------------------------------------------------
;313,胸愛撫
;記入チェック（=0, 非表示、1, 表示）
;2回目以降 感度4以上 思慕から or 恋慕
;上半身脱ぎ
LOCAL = 1
;-------------------------------------------------
IF LOCAL && iExFlag && !iMOB

	;--------------
	;服をフレーバーで脱がせる
	;--------------
	cAction:1 = 这么说着的%CALLNAME:TARGET%、在%CALLNAME:MASTER%面前
	;服装チェック
	IF EQUIP:上身衣服１ || EQUIP:上身衣服２ || EQUIP:連衣裙|| EQUIP:和服
		IF EQUIP:連衣裙 == 1
			;便装の連衣裙　愛麗絲のワンピは前留め前開きなはず
			cAction:1 += @"解開了%K17_GET_CLOTHNAME_TOP(TARGET)%的前扣、"
		ELSE
			cAction:1 += @"露出了%K17_GET_CLOTHNAME_TOP(TARGET)%下的肌肤、"
		ENDIF
	ENDIF
	IF SHOW_上半身内衣(TARGET) != ""
		IF EQUIP:上身衣服１ || EQUIP:上身衣服２ || EQUIP:連衣裙|| EQUIP:和服
			cAction:1 += "脱下了"
		ELSE
			cAction:1 += "松開了"
		ENDIF
		cAction:1 += SHOW_上半身内衣(TARGET)
	ENDIF
	cAction:1 += @"、\n%K17_RS("曝露出/裸露出了")%%OPPAI_DESCRIPTION(TARGET,1)%"
	
	;Counter見る
	IF TCVAR:Counter行動 == 27 && TALENT:TARGET:胸圍 >= 1
		cAction:1 += "、将乳峰展示了出来"
	ELSEIF TCVAR:Counter行動 == 27
		cAction:1 += "、强行挤出乳沟展示了出来"
	ELSE
		cAction:1 += "、有点害臊地展示了出来"
	ENDIF
	cAction:1 += "\n"
	;乳頭描写
	cAction:1 += NIPPLE_DESCRIPTION(TARGET,1)+"被"
	;披肩チェック
	IF EQUIP:TARGET:外搭==1
		cAction:1 += "用披肩遮着"
	ELSE
		cAction:1 += "用手捂着"
	ENDIF
	cAction:1 += "、能若隐若现地看见"

	;-----------------
	;喘ぎタイプ選択　屋内のみおねだり
	;-----------------
	IF INROOM(CFLAG:現在位置)
		CALL K17_SET_AUTO_PVOICE(iRT,K17C_部位B,K17C_受け_おねだり系)
	ELSE
		CALL K17_SET_AUTO_PVOICE(iRT,K17C_部位B,K17C_受け_お手伝い系)
	ENDIF
	;-----------------
	;表示開始
	;-----------------
	PRINTFORMDL %CALLNAME:TARGET%%K17_RS("一脸陶醉/满脸红晕/喘着香气")%、将手放在了在自己胸前的%CALLNAME:MASTER%手上
	PRINTW
	;脱ぎ初回
	;内衣つけてるか見る
	IF FIRSTTIME(@"{SELECTCOM}口上-2",1) && (EQUIP:TARGET:上半身内衣１ || EQUIP:TARGET:上半身内衣２ || EQUIP:TARGET:緊身内衣 || EQUIP:TARGET:連体内衣)
		PRINTFORML 「那个……稍微等下……之前就想说了……那个……」
		PRINTFORMDL 果然还是很讨厌这样吧
		PRINTFORML 「不是那样的……那个、从胸罩上面的话、稍微有点痛吧……」
		PRINTFORML 「所以呢……」
		PRINTW
		PRINTFORML （没关系的……只是这种程度而已……毕竟真的会弄疼的……嗯）
		PRINTFORMDL %cAction:1%
		PRINTFORML （真的……脱、脱掉了……）
		PRINTW
		CALLF FIRSTTIME(@"{SELECTCOM}口上-2")
	ELSEIF K17_GET_CLOTHNAME_TOP(TARGET,1) != ""
		PRINTFORML 「啊……等等……呐」
		PRINTFORMDL %cAction:1%
		PRINTW
	ENDIF

	PRINTFORML 「说不定会被别人看到的……所以……快点吧、呐？」
	PRINTFORMDL %cAction%
	PRINTFORML 「%K17_AP_NUM(2)%……%K17_CNAME()%、很喜欢%K17_CALL_P(K17C_部位B)%呢……%K17_AP_NUM(3,1)%」
	PRINTFORMDL %cReAction%
	PRINTFORML （果然……直接摸的那一边也……%K17_RS("更会喜欢吧/更舒服吧/更有感觉吧")%）
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	
ENDIF

;-------------------------------------------------
;313,胸愛撫
;記入チェック（=0, 非表示、1, 表示）
;2回目以降 感度3までor思慕未満or誰かいるなどの汎用
