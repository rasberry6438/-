IF LOCAL && (TFLAG:193 == -1 || ABL:料理技能 < 2)
	
	IF FIRSTTIME(@"{SELECTCOM}口上-失敗")
		;初回
		PRINTFORMDL %CALLNAME:TARGET%的料理怎么形容呢……说的难听点就是烧老了、说的好听点就是味道太淡了、对%CALLNAME:MASTER%来说有些欠缺
		PRINTFORML 「难道说……不合你的口味么？」
		PRINTFORML 「啊、没关系、把感想坦率的告诉我吧？」
		PRINTFORMDL %CALLNAME:MASTER%将真实的感想传达给了%CALLNAME:TARGET%
		PRINTW
		PRINTFORML 「真对不起、因为长时间只做过自己能吃的东西……从来没有考虑过别人的事」
		PRINTFORML 「看来还需要更多的练习呢……我会加油的」
	ELSE
		;二回目以降
		PRINTFORML 「唔ー嗯、%K17_RS("还差得远呢/好像还需要更多的练习呢/下次我会更加努力的")%……」
	ENDIF
	RETURN 1
ENDIF

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
;615,吃便當
;成功 料理技能2以上
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	PRINTFORML 「……怎样？好吃吗？呼呼呼、%K17_CNAME()%%K17_RS("吃相、好可爱呐/吃了我亲手做的料理、感觉真好啊/、不用吃得那么着急、慢慢吃也可以哦")%」
	iRT = K17_GRT_EX(TARGET)
	
	IF EXP:接吻経験 && iRT >= K17C_恋慕告白 && K17_PERCENT(50) && !FIRSTTIME(@"{SELECTCOM}口上-成功",1)
		;二回目以降 かつ50%
		PRINTW
		PRINTFORML 「啊啦、嘴边还残留着便当呢……――啾%K17_HEART()%」
		PRINTFORMDL %CALLNAME:TARGET%突然靠了过来夺去了%CALLNAME:MASTER%的嘴唇
		PRINTFORMDL 一分開嘴唇%CALLNAME:TARGET%便吐了吐舌头、十分腼腆的看着你
		PRINTW
		PRINTFORMDL 真的有东西粘在嘴边么……？
		PRINTFORML 「真的啦……啊……――嗯啾%K17_HEART()%　还……有哦、嗯～啾%K17_HEART()%　呸咯%K17_HEART()%」
		;経験値
		CFLAG:K17C_ID:K17C_CF_FK愛麗絲 = MIN(CFLAG:K17C_ID:K17C_CF_FK愛麗絲+1,10000)
		EXP:接吻経験 += 1
		EXP:Ｍ経験 += 1
		EXP:MASTER:Ｍ経験 += 1
		EXP:MASTER:接吻経験 += 1
		
	ELSEIF iRT >= K17C_恋慕
		PRINTW
		IF FIRSTTIME(@"{SELECTCOM}口上-成功")
			;初回
			PRINTFORML 「对了、我有件想试一试的事……那个、就是……」
			PRINTFORMDL 满脸通红的%CALLNAME:TARGET%抬眸眼望着%CALLNAME:MASTER%的同时、伸出了夹着便当小菜的筷子
			PRINTFORML 「很羞人的啦、赶紧把嘴巴打開」
			PRINTFORMDL %CALLNAME:TARGET%虽然很害羞、但依然满怀期待地注视着%CALLNAME:MASTER%张開了嘴
			PRINTW
		ENDIF
		PRINTFORML 「嗯%K17_HEART()%　啊～%K17_HEART(2)%」
		PRINTFORML 「呼呼、%K17_RS(@"总感觉有种投喂的感觉、很開心呐/%K17_CNAME()%、像小孩子一样、很可爱呢/虽然有些害羞、但实际做起来意外的感觉不错呢")%」
	ENDIF
ENDIF


RETURN 1
