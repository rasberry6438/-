	IF FLAG:時間停止
		PRINTFORMW %CALLNAME:MASTER%在停止的时间中决定将%CALLNAME:TARGET%当作性处理的工具使用...
		RETURN 1
	ELSEIF TCVAR:烂醉
		PRINTFORML 「这是什么......？......呕......」
		RETURN 1
	ENDIF
	
	;失敗
	IF TFLAG:194 != 0
		IF TFLAG:194 == 100
			CALL M_KOJO_早苗追加_COLOR_K31
			PRINTFORML 「其实你喜欢被攻击吧？我知道的%UNICODE(0x2665) *1%」
			PRINTFORML 「呵呵%UNICODE(0x2665) *1% 如果你期待着那样的表情，我......%UNICODE(0x2665) *1%」
			PRINTL
			RESETCOLOR
			RETURN 1
		ENDIF
		PRINTFORML 「喂！？离开我！」
		SELECTCASE TFLAG:194
			CASE 1 ;有人看着
				PRINTFORMW 「有人看着的地方不行啦」
			CASE 2 ;不喜欢在户外
				PRINTFORMW 「这里是户外啊......」
			CASE 3 ;不喜欢人多
				PRINTFORMW 「人多的话...那个，有点...」
			CASE 4 ;气氛不对
				PRINTFORMW 「......气氛之类的，我们考虑一下吧」
		ENDSELECT
		RETURN 1
	ELSEIF TCVAR:初次拥抱 >= 6 && TCVAR:初次拥抱 <= 8
		SELECTCASE TCVAR:初次拥抱
			CASE 6 ;似乎也不是那么讨厌
				PRINTFORMW 「那个……那个……请再给我一点时间……」
			CASE 7 ;不行
				PRINTFORML 「我觉得那样做不太好」
				PRINTFORMW 「请放开我」
			CASE 8 ;绝对不行
				PRINTFORMW 「你在想些什么，拜托你收敛点」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;初回成功
	IF TCVAR:初次拥抱 >= 1 && TCVAR:初次拥抱 <= 5
		SELECTCASE TCVAR:初次拥抱
			CASE 1 ;普通的共识达成
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORML 「啊……终于来了呢」
					PRINTFORMW 「我一直在等着你呢」
				ELSEIF TALENT:思慕
					PRINTFORML 「...啊...」
					PRINTFORMW 「请，请多关照...？」
				ELSE 
					PRINTFORML 「嗯...稍微...」
					PRINTFORMDW 只是口头上没有太大的抵抗...
				ENDIF
			CASE 2 ;先亲个嘴也行啊
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORML 「啊......终于来了呢」
					PRINTFORMW 「虽然有点跳跃了...」
				ELSEIF TALENT:思慕
					PRINTFORML 「...啊...额...要先来这边吗...？」
					PRINTFORMW 「嘛，也行啦...」
				ELSE 
					PRINTFORML 「嗯...稍微...」
					PRINTFORMDW 只是口头上没有太大的抵抗...
				ENDIF
			CASE 3 ;对于媚药或发情，我无法抵挡...
				PRINTFORML （有些奇怪...该怎么办呢）
				PRINTFORMW 「嗯...稍微...啊...」
			CASE 4 ;真是懊悔...如果没有刻印的话，就不会这么心跳不已了
				PRINTFORML （那个...为什么呢）
				PRINTFORML （身体自己反应起来...）
				PRINTFORMW 「为什么...会变成这样...」
