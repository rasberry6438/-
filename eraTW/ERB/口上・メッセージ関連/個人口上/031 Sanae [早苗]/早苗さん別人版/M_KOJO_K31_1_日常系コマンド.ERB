@M_KOJO_早苗追加_MESSAGE_COM_K31_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;ここに口上書いて下さい

	;デバッグ用
	;PRINTFORML 「現在ストーリー_第{CFLAG:31:1001}章_第{CFLAG:31:1002}節」
	;CALL K31_特殊会話フラグの確認と出力("デバッグ")
	;PRINTFORML 「現在ストーリー_第{CFLAG:31:1001}章_第{CFLAG:31:1002}節」	
	
	;=======================
	;憤怒、失敗、烂醉分岐を先に書いておく、RETURNさせる
	;=======================
	;憤怒
	IF CFLAG:ブチギレ
		PRINTFORML 「…………」
		PRINTFORMDW %CALLNAME:TARGET%似乎很生气...
		RETURN 1
	;失败
	ELSEIF TFLAG:193 == -1
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「...怎么了？」
			PRINTFORMW 「对不起，我走神了」
		ELSE
			PRINTFORML 「...............啊」
			PRINTFORMW 「话题用完了...」
		ENDIF
		RETURN 1
	;烂醉
	ELSEIF TCVAR:烂醉
		PRINTFORMDL %CALLNAME:TARGET%喝得太多，看起来不舒服...
		PRINTFORMW 「呜...感觉好糟糕...」
		RETURN 1
	ENDIF
	
	;===================================================================
	;ストーリーイベント発生判定、詳細はユーザー関数.ERBを参照
	;===================================================================
	CALL K31_特殊会話フラグの確認と出力("会話")
	SIF RESULT
		RETURN 1
	
	;==========================================================================================
	;1/3で日期会話発生、確率ちょい高めに、通常会話は発生させない（RETURN消して発生させることもできる）
	;==========================================================================================
	IF !RAND(2)
		SELECTCASE DAY:2
			CASE 1
			;元日
				IF DAY:3 == 1
					PRINTFORML 「新年快乐」
					PRINTFORMW 「今年也请多多关照」
					RETURN 1
				ENDIF
			;白色情人节
				IF DAY:3 == 14
					PRINTFORML 「今天是白色情人节」
					PRINTFORMW 「虽然没有什么特别的意义」
					RETURN 1
				ENDIF
			;例大祭
				IF DAY:3 == 25
					PRINTFORML 「今天是例大祭」
					PRINTFORMW 「希望能借此机会增加信徒」
					RETURN 1
				ENDIF
			CASE 2
			;七夕
				IF DAY:3 == 7
					PRINTFORML 「今天是七夕」
					PRINTFORMW 「你打算在短笺上写些什么？」
					RETURN 1
				ENDIF
			CASE 3
			;赏月
				IF NAME_MOON(DAY:3) == "十五夜 -中秋之名月- "
					PRINTFORML 「今天是十五夜」
					PRINTFORMW 「要一起赏月吗？」
					RETURN 1
				ENDIF
			;风神「二百十日」
				IF DAY:3 == 1
					PRINTFORML 「今天是立春后的第210天」
					PRINTFORMW 「据说是天气容易变坏的日子」
					RETURN 1
				ENDIF
			;万圣节
				IF DAY:3 == 30
					PRINTFORML 「今天是万圣节」
					PRINTFORMW 「糖果还是恶作剧，你更喜欢哪个？」
					RETURN 1
				ENDIF
			CASE 4
			;情人节
				IF DAY:3 == 14
					PRINTFORML 「今天是情人节」
					PRINTFORMW 「既然这样，要吃巧克力吗？」
					RETURN 1
				ENDIF
			;节分
				IF DAY:3 == 3
					PRINTFORML 「今天是节分」
					PRINTFORMW 「鬼外面，福里面，就是这个意思吧」
					RETURN 1
				ENDIF
			;圣诞节
				IF DAY:3 == 25
					PRINTFORML 「今天是圣诞节」
					PRINTFORMW 「想吃蛋糕吗？」
					RETURN 1
				ENDIF
			;大年夜
				IF DAY:3 == 31
					PRINTFORML 「今天是大年夜」
					PRINTFORMW 「一年的结束」
					RETURN 1
				ENDIF
		ENDSELECT
	ENDIF
	;=======================
	;ここから通常会話
	;=======================
	IF TALENT:恋人 || TALENT:恋慕
		LOCAL = 10
	ELSEIF TALENT:思慕
		LOCAL = 7
	ELSE
		LOCAL = 5
	ENDIF
	
	SELECTCASE RAND(LOCAL)
		CASE 0
			PRINTFORML 「怎么飞起来的呢？」
			PRINTFORML 「嗯...就当作是奇迹的力量吧」
		CASE 1
			PRINTFORML 「幻想乡因为宴会多，喝酒厉害的人也很多，但其实我不太擅长喝酒」
			PRINTFORML 「有时候会羡慕那些不容易醉倒的人」
			PRINTFORML 「顺便问一下，对于喝酒很多的人，%CALLNAME:MASTER%先生会用哪个词来形容呢？」
			PRINTFORML 「是不懂酒的人？还是酒桶？还是酒虫？听说还有左党和ワク这样的词呢」
		CASE 2
			PRINTFORML 「想看看真人大小的巨型机器人」
			PRINTFORML 「诶？外面的世界有吗？真想快点进入幻想乡啊...」
		CASE 3
			PRINTFORML 「希望你能用能力控制天气？如果规模太大的话，咏唱之类的就麻烦了...」
			PRINTFORML 「嗯，如果有机会的话我会考虑的」
		CASE 4
			PRINTFORML 「信仰对于人类来说非常重要」
			PRINTFORML 「如果没有信仰，神的力量会减弱，也很难给人们带来恩惠」
	;以下是思慕以上的对话
		CASE 5
			PRINTFORML 「刚搬过来的时候也有些担心，但这里的生活也不错呢」
			PRINTFORML 「还有有趣的对话伙伴呢，对吧？」
		CASE 6
			PRINTFORML 「%CALLNAME:MASTER%先生也是，我认识的人都像妖怪一样...」
			PRINTFORML 「我会不会也变成那样呢...」
			PRINTFORMDL %CALLNAME:MASTER%勉强忍住了「你已经变成了吧？」的话...
		CASE 7
			PRINTFORML 「%CALLNAME:MASTER%先生的对话技巧真是丰富啊」
			PRINTFORML 「能不能告诉我秘诀？对于集聚信仰来说，口才确实很重要」
	;以下为恋慕以上的内容
		CASE 8
			PRINTFORML 「呵呵..........和%CALLNAME:MASTER%先生的对话很开心，我喜欢」
			PRINTFORML 「当然，也喜欢%CALLNAME:MASTER%先生......」
		CASE 9
			PRINTFORML 「最近，我会因为某件事而忘记时间」
			PRINTFORML 「特别是和%CALLNAME:MASTER%先生在一起的时候」
	;为了预防
		CASEELSE
			PRINTFORML 「****在对话流程中到达了意料之外的分支点****」
	ENDSELECT	
	

	PRINTFORMW
ENDIF
RETURN 1
