@M_KOJO_MESSAGE_COM_K31_412_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;学生時代の遠い思い出…問題の出し合いっこ
	IF FIRSTTIME(412)
		PRINTFORMW 「这么说来，%CALLNAME:MASTER%在学习什么呢？你会读写算盘吧？」
		PRINTFORMW 「试图解决实际问题吗？也没什么，不过…」
		PRINTFORMW 「……好怀念啊，这是考试前经常和朋友做的一道题。」
		PRINTFORMW 「…当然，你也可以自己出题吧？」
		RETURN 1
	ENDIF
	;出題者:你(文系出題)
	IF RAND(2)
		;SELECTCASE不使用のはイベント踏んでるかの条件をつけられるようにするため
		;出題「世界史」
		IF !RAND(10)
			PRINTFORMW 1805年12月2日发生的战争是第三次对法大同盟崩溃的原因
			PRINTFORMW 
			;正解
			IF ABL:TARGET:43 * 15 > RAND(100)
				;絶対言葉の響きで覚えてたゾ
				PRINTFORMW 「啊！奥斯特里茨三皇之战！」
				PRINTFORMW 「哼哼~可别小瞧了我！」
			;不正解
			ELSE
				PRINTFORMW 「嗯，相当难…」
				PRINTFORMW 「我对世界史不太了解...」
			ENDIF
			RETURN 1
		ENDIF
		;出題「世界史」
		IF !RAND(9)
			PRINTFORMW 两个被称为红色拿破仑的人
			PRINTFORMW 
			;正解
			IF ABL:TARGET:43 * 15 > RAND(100)
				;こいつ…やりよる
				PRINTFORMW 「米哈伊尔·尼古拉维奇·图哈切夫斯基和武元甲！」
				PRINTFORMW 「后者还活着呢！」
				PRINTFORMW 「诶！？去世了吗！！？」
			;不正解
			ELSE
				;ロボ好き早苗さん
				PRINTFORMW 「是什么？那个？」
				PRINTFORMW 「我只知道红色彗星…」
			ENDIF
			RETURN 1
		ENDIF
		;出題「文学」(関係性が「恋人」であることが条件)
		IF !RAND(8) && CFLAG:31:3106 != -1
			PRINTFORMW 『ドグラ・マグラ』的作者夢野久作有三个名为「○○地獄」的作品
			PRINTFORMW 
			;正解
			IF ABL:TARGET:43 * 10 > RAND(100)
				PRINTFORMW 「瓶詰地獄、少女地獄、脑髓地狱！」
				;少女地獄より
				;ペッペッ……。
				PRINTFORMW 「我的身体永远属于你♪ 」
				PRINTFORMW …真不吉利
			;不正解
			ELSE
				PRINTFORMW 「…我只知道『ドグラ・マグラ』」
				PRINTFORMW 「那个妹妹给我留下了奇怪的印象…」
			ENDIF
			RETURN 1
		ENDIF
		;出題「酒虫」你はちゃんと勉強してんすねぇ
		IF !RAND(7)
			PRINTFORMW 如何用“酒虫”酿酒
			PRINTFORMW 
			;正解
			IF ABL:TARGET:43 * 10 > RAND(100)
				PRINTFORMW 「啊，原来是酒虫啊」
				PRINTFORMW 「那就先让他吸点水…啊，水的种类也很重要哦！」
			;不正解
			ELSE
				;むしろこっちの答えのほうがすごくなぁい？
				PRINTFORMW 「你是指芥川龙之介吗，请不要卖弄你的学问…」
				PRINTFORMW 「那么，我可以尝试将她放入水中并搅拌混合...」
				PRINTFORMW 「啊，原来是做酒的那个酒虫吗！？」
			ENDIF
			RETURN 1
		ENDIF
		;汎用会話
		IF 1
			PRINTFORMW 「啊，这里回答错了」
			PRINTFORMW …的确如早苗所说
			PRINTFORMW 「呼呼~我赢了一局~」
			RETURN 1
		ENDIF
	;出題者:早苗(理系問題多し)
	ELSE
		;SELECTCASE不使用のはイベント踏んでるかの条件をつけられるようにするため
		;出題「数学」
		IF !RAND(10)
			;結構有名な京大のアレ
			PRINTFORMW 「这个怎么样：‘tan1°是无理数吗’」
			PRINTFORMW 
			;正解
			IF ABL:MASTER:43 * 20 > RAND(100)
				PRINTFORMW 从背理法翻倍，最后用减法做到30°
				PRINTFORMW 「唉，太出名了吧…」
			;不正解
			ELSE
				PRINTFORMW 是無理数
				PRINTFORMW 「不是这样的吧…」
			ENDIF
			RETURN 1
		ENDIF
		;SELECTCASE不使用のはイベント踏んでるかの条件をつけられるようにするため
		;出題「生物」
		IF !RAND(9)
			PRINTFORMW 「请回答香烟、紫外线、放射线分别对DNA产生怎样的负面影响」
			PRINTFORMW 
			;正解
			IF ABL:MASTER:43 * 10 > RAND(100)
				PRINTFORMW 香烟会引发甲基化，紫外线会引发二量体形成，紫外线会造成DSB
				PRINTFORMW 「嗯，是正确答案…」
			;不正解
			ELSE
				PRINTFORMW 不知道，全都对DNA不好
				PRINTFORMW 「这三种不一样啊!」
			ENDIF
			RETURN 1
		ENDIF
		;SELECTCASE不使用のはイベント踏んでるかの条件をつけられるようにするため
		;出題「数学？」
		IF !RAND(8)
			PRINTFORMW 「让我们稍微扭转一下问题，代数(algebra)的语源数学家的名字是?」
			PRINTFORMW 
			;正解
			IF ABL:MASTER:43 * 15 > RAND(100)
				PRINTFORMW 阿布·阿布杜拉·穆罕默德·伊本·穆萨·胡瓦利兹米
				PRINTFORMW 「……说出全名？意料之外的情况啊…」
			;不正解
			ELSE
				;アル＝フワーリズミー曰く→アルゴリズム
				PRINTFORMW …我现在才知道这个词的语源是数学家。
				PRINTFORMW 「顺便说一下，算法也是他的名字」
			ENDIF
			RETURN 1
		ENDIF
		;SELECTCASE不使用のはイベント踏んでるかの条件をつけられるようにするため
		;例えばこんなものができるよという例
		;出題「你」(関係性が「恋人」であることが条件)
		IF !RAND(7) && CFLAG:31:3106 != -1
			PRINTFORMW 「……这个世界上，我最爱的人是谁?」
			PRINTFORMW 
			;正解
			IF ABL:MASTER:43 * 50 > RAND(100)
				PRINTFORMW 「回答…正确」
				PRINTFORMW 「我们在一起吧?」
			;不正解
			ELSE
				PRINTFORMW 「不对！！」
				PRINTFORMW 「下次再听的时候，请好好地用语言表达出来吧?」
			ENDIF
			RETURN 1
		ENDIF
		;汎用会話
		IF 1
			PRINTFORMW 「啊，这里回答错了」
			PRINTFORMW 
			PRINTFORMW …的确如早苗所说
			PRINTFORMW 「呼呼~我赢了一局~」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
