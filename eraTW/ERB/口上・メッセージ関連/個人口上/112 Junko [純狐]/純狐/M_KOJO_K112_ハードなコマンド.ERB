;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_POWERED_BY_OVERSEAS_MESSAGE_COM_K112_140
CALL M_KOJO_POWERED_BY_OVERSEAS_MESSAGE_COM_K112_140_1
RETURN RESULT

@M_KOJO_POWERED_BY_OVERSEAS_MESSAGE_COM_K112_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
			PRINTFORMW 「嗯…那是什么意思…」
			PRINTFORMW 「哦…噢…！」
		ENDIF
	RETURN 1
	ENDIF
	;基本セット
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF TALENT:恋人
			PRINTFORMW 「噢%UNICODE(0x2665) *1%…噢%UNICODE(0x2665) *1%…噢%UNICODE(0x2665) *1%…」
			PRINTFORMW （快一点%UNICODE(0x2665) *1%…出来%UNICODE(0x2665) *1%…）
		ELSEIF TALENT:恋慕
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%…咕%UNICODE(0x2665) *1%…嗯%UNICODE(0x2665) *1%…」
			PRINTFORMW （哦%UNICODE(0x2665) *1%…变大了%UNICODE(0x2665) *1%…要出来了%UNICODE(0x2665) *1%）
		ELSEIF TALENT:思慕
			PRINTFORMW 「噢%UNICODE(0x2665) *1%…噢%UNICODE(0x2665) *1%…咕%UNICODE(0x2665) *1%…」
			PRINTFORMW （难道…就这样出来了吗？）
		ENDIF
	ELSE
		IF TALENT:恋人
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%…噢%UNICODE(0x2665) *1%…咕%UNICODE(0x2665) *1%…」
			PRINTFORMW （哦%UNICODE(0x2665) *1%…不可以%UNICODE(0x2665) *1%…好舒服%UNICODE(0x2665) *1%…）
		ELSEIF TALENT:恋慕
			PRINTFORMW 「咕%UNICODE(0x2665) *1%…嗯%UNICODE(0x2665) *1%…噢%UNICODE(0x2665) *1%…」
			PRINTFORMW （这样%UNICODE(0x2665) *1%…像是道具一样…%UNICODE(0x2665) *1%）
		ELSEIF TALENT:思慕
			PRINTFORMW 「噢%UNICODE(0x2665) *1%…噢%UNICODE(0x2665) *1%…咕%UNICODE(0x2665) *1%…」
			PRINTFORMW （哎…好难受…）
		ENDIF
	ENDIF
ENDIF
RETURN 1

