				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「嘛，倒也无所谓」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「啊……反正无所谓」
					ELSE
						PRINTFORMW 「真是的，想拿来干什么啊」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
