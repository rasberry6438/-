;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K119_80
CALL M_KOJO_MESSAGE_COM_K119_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K119_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF FLAG:70
		PRINTFORMW %CALLNAME:MASTER%将肉棒放在合欢乃静止的手上反复摩擦、直到他满足为止……。
		RETURN 1
	ENDIF
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「什么呀、用手摩擦肉棒就可以了吗？」
		PRINTFORML 「我虽然不太清楚、不过没问题。真是奇怪的爱好啊。」
		PRINTFORMW 合欢乃满不在乎地用手来回摩擦着%CALLNAME:MASTER%的肉棒……。
		RETURN 1
	ENDIF
	IF NOWEX:PLAYER:射精
		PRINTFORML 「怎么了、哆嗦得这么厉害。是在害怕我吗？」
		PRINTFORMW 合欢乃好像误会了什么……。
	ENDIF
	PRINTFORML 「就好像是在擦山芋一样。」
	PRINTFORMW %CALLNAME:MASTER%想象了一下被合欢乃用卸铁摩擦肉棒的场景……。
ENDIF
RETURN 1

;==================================================
;81,口交
;==================================================
@M_KOJO_MESSAGE_COM_K119_81
CALL M_KOJO_MESSAGE_COM_K119_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K119_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「要含着倒没什么、但我的牙齿嘎吱嘎吱硬邦邦的很危险哦。」
		PRINTFORMW 合欢乃那锐利的犬齿从肉棒上掠过……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORML 把肉棒插进了无法说话的合欢乃的嘴里……。
		PRINTFORMW 得小心不要被牙齿刺到才行……。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「嗯……要是不小心咬到了的话就对不起了。」
		PRINTFORMW 合欢乃有些困难地继续为%CALLNAME:MASTER%口交……。
		RETURN 1
	ENDIF
	IF NOWEX:PLAYER:射精
		PRINTFORML 「唔、唔嗯…………。……这是山药汁吗？」
		PRINTFORMW 将滚烫的精液射在了合欢乃的嘴里……。
	ENDIF
ENDIF
RETURN 1

