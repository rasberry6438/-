@尿道玩法经验结算
IF 前回指令 == "尿道正常位" || 前回指令 == "尿道屈居位" || 前回指令 == "尿道后背位"
	;-------------------------------------------------
	DOWNBASE:PLAYER:体力 += 40
	DOWNBASE:PLAYER:気力 += 40

	;-------------------------------------------------
	;ソース追加処理
	;-------------------------------------------------
	;消費体力、気力
	IF FLAG:70 == 1
		BASE:MASTER:TSP -= 100
		SOURCE:PLAYER:快Ｃ = 400
		SOURCE:快Ｖ = 400
		SOURCE:苦痛 = 250

		;処女だった場合は、苦痛のソースと反発のソースを追加する
		;尿道开发程度是0
		IF CFLAG:36:1015 == 0
			SOURCE:苦痛 += 3000
		ENDIF

	ELSE

		DOWNBASE:体力 += 50
		DOWNBASE:気力 += 60

		SOURCE:PLAYER:快Ｃ = 400
		;SOURCE:快Ｖ = 300
		SOURCE:情愛 = 150
		SOURCE:苦痛 = 500
		SOURCE:露出 = 50
		SOURCE:反感 = 300
		
		;正常位補正
		;PLAYERがTARGETの膣に挿入している
		IF BEDROOM(CFLAG:PLAYER:現在位置) && TEQUIP:50 == PLAYER
			TCVAR:PLAYER:4 += EXP:愛情経験 * 5
			;SOURCE:快Ｖ += EXP:愛情経験
			SIF SOURCE:快Ｖ >= 700
				SOURCE:快Ｖ = 700
			SIF TCVAR:PLAYER:4 >= 3000
				TCVAR:PLAYER:4 = 3000
		ENDIF

		;上のほうで計算した汚れデータ
		SOURCE:不潔 = Y*10 + 60
		CALL 特殊ソース処理
	ENDIF
	;-------------------------------------------------
	;経験の処理
	;-------------------------------------------------
	EXP:PLAYER:挿入経験 ++
	;EXP:Ｖ経験 ++
	CFLAG:36:1015 ++
	;同性愛　レズなら戻り値1ゲイなら戻り値2
	IF FLAG:70 == 1 || CFLAG:恶作剧
		IF HETEROSEX(TARGET,PLAYER)
			EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
		ENDIF
		IF CFLAG:PLAYER:恶作剧 == 2
			EXP:PLAYER:睡眠姦経験 += 1
			EXP:TARGET:睡眠姦経験 += 1
		ENDIF
		SIF !CFLAG:恶作剧
		EXP:PLAYER:時姦経験 += 1
	ELSE
		IF HETEROSEX(TARGET,PLAYER)
			EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 4
			EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
		ENDIF
	;	EXP:Ｖ性交経験 ++
		IF !ASSIPLAY
			EXP:愛情経験 += TALENT:恋慕 * 2
		ENDIF
	ENDIF

ELSEIF 前回指令 == "尿道指插入"

	;尿道指插入
	;-------------------------------------------------
	;消費体力、気力
	IF FLAG:70 == 1
		BASE:MASTER:TSP -= 40
		IF CFLAG:36:1015 == 0
			SOURCE:苦痛 = 10
		ELSE
			SOURCE:苦痛 = 5
		ENDIF

		IF HETEROSEX(TARGET,PLAYER)
			EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
		ENDIF
		;EXP:Ｖ経験 ++
		CFLAG:36:1015 ++
		EXP:PLAYER:手淫経験 ++
	ELSE

		DOWNBASE:体力 += 30
		DOWNBASE:気力 += 80

		IF CFLAG:36:1015 == 0
			SOURCE:苦痛 = 10
			SOURCE:露出 = 30
			SOURCE:逸脱 = 30
			SOURCE:反感 = 30
		ELSE
			SOURCE:苦痛 = 15
			SOURCE:露出 = 10
			SOURCE:逸脱 = 20
			SOURCE:反感 = 20
		ENDIF

		;-------------------------------------------------
		;特殊ソース処理
		;-------------------------------------------------
		;圧抑、抵抗持ちには、抑鬱ソース追加
		IF TALENT:自己愛 < 0 || TALENT:抵抗
			SOURCE:鬱屈 += 20
			;処女
			SIF CFLAG:36:1015 == 0
				SOURCE:鬱屈 += 10
		ENDIF

		;-------------------------------------------------
		;経験の処理
		;-------------------------------------------------
		;同性愛　レズなら戻り値1ゲイなら戻り値2
		IF HETEROSEX(TARGET,PLAYER)
			EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 1
			EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
		ENDIF
		;EXP:Ｖ経験 ++
		CFLAG:36:1015 ++
		EXP:PLAYER:手淫経験 ++
	ENDIF

ELSEIF 前回指令 == "尿道正常位立位"
	
ELSEIF 前回指令 == "尿道对面座位" ||  前回指令 == "尿道对面立位"
	

ENDIF
SIF CFLAG:36:1016 >= TALENT:MASTER:形状
	CFLAG:36:1016 = TALENT:MASTER:形状 + 1
RETURN 1

@尿道插入判断
IF CFLAG:36:1016 >= TALENT:MASTER:形状
	SETBIT CFLAG:36:1014,2
	RETURN 1
ELSE
	CALL 尿道指插入
	CFLAG:36:1016 ++
	RETURN 0
ENDIF

@尿道每日结算
;尿道精液残留计算和尿道扩张度
;------ ALTER 殘留精液
;先流失 20% 總精子量 在扣掉 20% 子宮存量
IF CFLAG:36:1017 > 0
	DEBUGPRINTFORML 阿燐尿道内精子{CFLAG:36:1017}
	SELECTCASE TALENT:36:体型
	CASE -5
		LOCAL:1 = 300
	CASE -4 TO -2
		LOCAL:1 = 600
	CASE -1
		LOCAL:1 = 1000
	CASE 0
		LOCAL:1 = 1200
	CASE IS > 0
		LOCAL:1 = 1500
	ENDSELECT
	CFLAG:36:1017 -= ((LOCAL:1)*2/10)
	SIF CFLAG:36:1017 < 0
		CFLAG:36:1017 = 0
	PRINTFORML 阿燐的尿道和膀胱内残留下{CFLAG:36:1017}的精子
ELSE
	CFLAG:36:1017 = 0
	;尿道内精子
ENDIF
;------ END 殘留精液
CFLAG:36:1016 = CFLAG:36:1018 - 1
;尿道扩张程度 = 尿道中毒等级 - 1
	CFLAG:36:1018 = CFLAG:36:1015 / 10
	;尿道中毒等级 = 尿道经验/10
前回指令 = ""
RETURN 1