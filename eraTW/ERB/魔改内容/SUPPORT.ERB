#FUNCTIONS
VARSET RESULTS
IF !RAND:4
	SELECTCASE TIME:5
		CASE 0, 1
			IF TALENT:ARG:妖怪 == 3 || EQUIP:ARG:飾品 == 6 
				RESULTS = 正高举着伞遮阳
			ELSE
				SELECTCASE RAND:3
					CASE 0
						RESULTS = 寻找阴凉地方避暑
					CASE 1
						RESULTS = 半眯着眼并用手遮挡阳光
					CASE 2
						RESULTS = 用着小手为自己扇风
				ENDSELECT
			ENDIF
		CASE 4
			IF (TALENT:ARG:幼稚 || TALENT:ARG:幼児／幼児退行) && !RAND:3
				RESULTS = 用力踩下地上的水坑而导致积水四溅
