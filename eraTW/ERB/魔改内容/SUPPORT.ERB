		ENDIF
	ELSE
		HTML_PRINT @"<p align='center'>%HTML_COLORBAR(UNLOCK_PROGRESSION, 42, 42, UNICODE(0x2588), UNICODE(0x2588), 0xFF0000, 0x202020)%</p>"
	ENDIF
