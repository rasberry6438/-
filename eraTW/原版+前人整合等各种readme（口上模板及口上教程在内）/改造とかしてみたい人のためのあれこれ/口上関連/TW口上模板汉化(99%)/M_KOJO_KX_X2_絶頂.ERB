;-------------------------------------------------
;不同绝顶程度的差分
IF LOCAL
	;-------------------------------------------------
	;五重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:五重絶頂
		PRINTFORMW 
