			;Ｖ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ絶頂
		;超強絶頂Ｃ
		IF NOWEX:Ｃ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｃ
		ELSEIF NOWEX:Ｃ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ絶頂
		;超強絶頂Ｖ
		IF NOWEX:Ｖ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｖ
		ELSEIF NOWEX:Ｖ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ａ絶頂
		;超強絶頂Ａ
		IF NOWEX:Ａ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ａ
		ELSEIF NOWEX:Ａ絶頂 == 2
			PRINTFORMW 
		;絶頂Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ絶頂
