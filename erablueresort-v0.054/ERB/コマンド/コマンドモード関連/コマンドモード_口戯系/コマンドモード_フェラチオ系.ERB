@MODETYPE_フェラチオ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

詳細文文字列受け渡し変数 = 
IF 処理モード == "写真用文字列_プレイヤー"
	;人妻の場合
	SIF TALENT:ターゲット番号:恋人持ち == 2
		詳細文文字列受け渡し変数 = 背叛丈夫
	;恋人あり
	SIF TALENT:ターゲット番号:恋人持ち == 1
		詳細文文字列受け渡し変数 = 背叛恋人
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 += @"%CALLNAME:ターゲット番号%的%TEXTR("阴茎/阳具/肉棒")%%TEXTR("进行口交/用舌头游走/舔吮侍奉/恭敬献吻/用嘴唇侍奉/用嘴唇爱抚/用舌头爱抚/进行阴茎亲吻侍奉")%"
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	SELECTCASE RAND:3
	CASE 0
		詳細文文字列受け渡し変数 = %TEXTR("肉棒/阴茎")%正被%CALLNAME:プレイヤー番号%%TEXTR("口交着/舔吮着/用唇舌爱抚着/接受口唇爱抚")%
	CASE 1
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的%TEXTR("樱唇/香吻")%正为%TEXTR("肉棒/阴茎/阴茎")%献上侍奉
	CASE 2
		詳細文文字列受け渡し変数 = %TEXTR("肉棒/阴茎")%正接受%CALLNAME:プレイヤー番号%献上的%TEXTR("口唇侍奉/深喉侍奉/吮吸侍奉")%
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_クリトリス
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_口
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_届かない
	CASE 部位_胸
		;プレイヤーの胸には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_膣
		;プレイヤーの膣には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_アナル
		;プレイヤーの尻には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
ENDSELECT


@MODETYPE_パイズリフェラ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード
詳細文文字列受け渡し変数 = 
IF 処理モード == "写真用文字列_プレイヤー"
	;人妻の場合
	SIF TALENT:ターゲット番号:恋人持ち == 2
		詳細文文字列受け渡し変数 = 背叛丈夫
	;恋人あり
	SIF TALENT:ターゲット番号:恋人持ち == 1
		詳細文文字列受け渡し変数 = 背叛恋人
	
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 += @"%CALLNAME:ターゲット番号%的%TEXTR("阴茎/阳具/肉棒")%"
	
		IF TALENT:ターゲット番号:バストサイズ == 3
		;魔乳
			詳細文文字列受け渡し変数 += @"被沉甸甸的魔乳夹住"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 2
		;爆乳
			詳細文文字列受け渡し変数 += @"被丰盈饱满的爆乳夹住"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 1
		;巨乳
			詳細文文字列受け渡し変数 += @"被发育丰满的巨乳夹住"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 0
		;普乳
			詳細文文字列受け渡し変数 += @"被柔软的%TEXTR("胸部/美乳")%夹住"
		ELSEIF TALENT:ターゲット番号:バストサイズ == -1
		;貧乳
			詳細文文字列受け渡し変数 += @"被挺翘有弹性的%TEXTR("贫乳/小乳")%%TEXTR("款待/刺激")%"
		ELSEIF TALENT:ターゲット番号:バストサイズ == -2
		;絶壁
			詳細文文字列受け渡し変数 += @"被小巧的微乳%TEXTR("款待/刺激")%"
		ENDIF
	詳細文文字列受け渡し変数 += @"，龟头%TEXTR("被吮吸爱抚着/被含入口中/被舌尖爱抚着/被亲吻")%"
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	SELECTCASE RAND:3
	CASE 0
		詳細文文字列受け渡し変数 = %TEXTR("チンポ/ペニス")%を%CALLNAME:プレイヤー番号%にパイズリされている
	CASE 1
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%的%TEXTR("胸部/乳房")%和%TEXTR("嘴唇/樱唇")%正爱抚着%TEXTR("阴茎/阳具/肉棒")%
	CASE 2
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%%TEXTR("胸/おっぱい")%と%TEXTR("唇/おくち")%に%TEXTR("チンポ/おちんぽ/ペニス")%奉仕を受けている
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;キスは両者口のみ専有
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_クリトリス
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_指
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_強占有
	CASE 部位_口
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_届かない
	CASE 部位_胸
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
	CASE 部位_膣
		;プレイヤーの膣には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_アナル
		;プレイヤーの尻には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
ENDSELECT


@MODETYPE_イラマチオ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
