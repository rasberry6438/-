@系統リスト_乳首吸い系(モード名)
#FUNCTION
#DIMS モード名
IF GROUPMATCH(モード名, "乳首吸い", "授乳手コキ")
	モード確認結果 = 1
	RETURNF 1
ENDIF



@MODE_持続快楽_乳首吸い(実行キャラ番号, 対象キャラ番号)
#DIM 実行キャラ番号
#DIM 対象キャラ番号

RCVAR:対象キャラ番号:乳首開発フラグ = 2
CALL SOURCE_CALC_快Ｂ(対象キャラ番号, 実行キャラ番号, 150)
CALL SOURCE_CALC_接触(対象キャラ番号, 実行キャラ番号, 80)
CALL SOURCE_CALC_接触(実行キャラ番号, 対象キャラ番号, 80)
EXP:実行キャラ番号:口淫経験 ++

RETURN 1



@MODETYPE_乳首吸い(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = 正在%TEXTR("吮吸/用舌尖拨弄/舔舐")%着%CALLNAME:TARGET%的乳头
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = 被%CALLNAME:PLAYER%%TEXTR("含住乳尖/用舌苔磨蹭/深深吮吸/以舌尖挑逗")%乳头
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;挿入で使用するのはペニスと膣で1を返す
SELECTCASE 部位番号
	CASE 部位_口
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_届かない
	CASE 部位_胸
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
ENDSELECT



@MODE_絶頂相手_乳首吸い(絶頂部位, 処理モード, 判定結果)
#FUNCTION
#DIM 絶頂部位
#DIMS 処理モード
#DIM REF 判定結果

IF 処理モード == "ターゲット"
	IF 絶頂部位 == 部位_Ｂ
		判定結果 = 絶頂相手_モード相手
		RETURNF 絶頂相手_モード相手
	ENDIF
ENDIF


