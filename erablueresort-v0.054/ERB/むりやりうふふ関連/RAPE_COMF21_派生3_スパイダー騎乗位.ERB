
@RAPE_COMNAME_S21_3
#FUNCTIONS
TSTR:コマンド名受渡 = 蜘蛛式骑乘位



;===================================================
;コマンドタイプ
;===================================================
@RAPE_COMTYPE_S_21_3
#FUNCTION
TFLAG:コマンドタイプ受渡 = RAPE_COMTYPE("Ｖ系_被挿入中")

@RAPE_COM_TOOLTIP_S21_3
LOCALS = <br>■蜘蛛式骑乘位<br>
LOCALS += "压在对方身上用力摆动臀部，姿势类似于蜘蛛捕食猎物。<br>这比简单地摆动臀部能产生更大的快感。"
LOCALS += "<br>强奸指令类型：Ｖ系or被插入中<br>获取来源：快Ｃ・暴露・接触"
LOCALS += "<br>专属经验值：Ｃ性交经验_Ｖ插入(TARGET)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@RAPE_COM_S21_3
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
CALL SOURCE_CALC_快Ｖ(PLAYER, TARGET, 800)
CALL SOURCE_CALC_露出(TARGET, PLAYER, 50)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)
CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 800)


RCVAR:PLAYER:嗜虐コマンドフラグ = 1
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｃ性交経験_Ｖ挿入 ++
EXP:PLAYER:Ｖ性交経験 ++
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:同性愛経験 += 4
	EXP:PLAYER:同性愛経験 += 4
ENDIF

EXP:愛情経験 += TALENT:恋慕 * 2

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@RAPE_COM_ABLE_S21_3
;腸内を締める実行判定
;一括管理
SIF GLOBAL_RAPE_COMABLE_S("21_3")
	RETURN RESULT
;挿入系モードじゃない時はアウト
SIF MODE_確認_完全一致("Ｖ挿入系", TARGET, PLAYER) < 0
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@RAPE_MESSAGE_COM_S21_3

PRINTFORML %CALLNAME:PLAYER%如捕食者般笼罩着%CALLNAME:TARGET%，%TEXTR("激烈撞击腰肢/上下摆动臀部")%凌辱着%TEXTR("阴茎/鸡巴")%…


