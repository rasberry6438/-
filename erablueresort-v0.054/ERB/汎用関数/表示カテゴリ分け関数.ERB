@素質カテゴリ(TALENT番号)
#FUNCTIONS
#DIM TALENT番号

;主に素質指定雇用やCSV作成モードで使う表示順制御

IF INRANGE(TALENT番号, 0, 9) || INRANGE(TALENT番号, 71, 76) || INRANGE(TALENT番号, 132, 134) || INRANGE(TALENT番号, 150, 219) || TALENT番号 == 228 || TALENT番号 == 236
	RETURNF "非表示"
ELSEIF TALENT番号 == 235
	RETURNF "優先表示"
ELSEIF INRANGE(TALENT番号, 10, 34) || INRANGE(TALENT番号, 60, 85) || TALENT番号 == 141 || TALENT番号 == 220 || INRANGE(TALENT番号, 224, 225) || INRANGE(TALENT番号, 229, 232) || TALENT番号 == 240
	RETURNF "性格"
ELSEIF INRANGE(TALENT番号, 50, 57) || INRANGE(TALENT番号, 86, 130) || INRANGE(TALENT番号, 144, 149) || INRANGE(TALENT番号, 221, 223)
	RETURNF "肉体"
ELSEIF GROUPMATCH(TALENT番号, 95, 135, 136, 138, 139, 242, 243, 247)
	;表示しない
	RETURNF ""
ENDIF

RETURNF "その他"
