@SOURCE_ABLUP(ARG)

SIF CFLAG:ARG:滞在期間 < 0
	SKIPDISP 1
	
IF ABL:ARG:指テク < 5
	LOCAL = EXP:ARG:手淫経験
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:器用な指 * 2 + TALENT:ARG:習得速度
	LOCAL:2 = ABL:ARG:指テク
	WHILE LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:指テク + 3) / 2 && EXP_UP(24,ARG)
		ABL:ARG:指テク ++
		SIF ABL:ARG:指テク >= 5
			BREAK
	WEND
	IF ABL:ARG:指テク - LOCAL:2 > 0
		PRINTFORMW %CALLNAME:ARG%的手指变得灵巧了
		IF ABL:ARG:指テク >= 5
			PRINTFORMW %CALLNAME:ARG%获得了[手指灵巧]
			TALENT:ARG:器用な指 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:舌技 < 5
	LOCAL = EXP:ARG:口淫経験
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:舌使い * 2 + TALENT:ARG:習得速度
	LOCAL:2 = ABL:ARG:舌技
	WHILE LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:舌技 + 3) && EXP_UP(25,ARG)
		ABL:ARG:舌技 ++
		SIF ABL:ARG:舌技 >= 5
			BREAK
	WEND
	IF ABL:ARG:舌技 - LOCAL:2 > 0
		PRINTFORMW %CALLNAME:ARG%的舌头变得灵活了
		IF ABL:ARG:舌技 >= 5
			PRINTFORMW %CALLNAME:ARG%获得了[舌头灵巧]
			TALENT:ARG:舌使い = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:腰使い < 5
	LOCAL = MAX(EXP:ARG:Ｖ性交経験 +  EXP:ARG:Ａ性交経験, EXP:ARG:Ｃ性交経験_Ｖ挿入 + EXP:ARG:Ｃ性交経験_Ａ挿入)
	LOCAL:1 = 10 + ABL:ARG:技巧 / 2 + TALENT:ARG:習得速度
	LOCAL:2 = ABL:ARG:腰使い
	WHILE LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:腰使い + 3)
		ABL:ARG:腰使い ++
		SIF ABL:ARG:腰使い >= 5
			BREAK
	WEND
	IF ABL:ARG:腰使い - LOCAL:2 > 0
		PRINTFORMW %CALLNAME:ARG%的腰技愈发精进
	ENDIF
ENDIF

