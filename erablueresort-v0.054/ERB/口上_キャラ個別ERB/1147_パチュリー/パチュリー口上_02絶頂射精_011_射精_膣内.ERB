;--------------------------------------------------

;--------------------------------------------------
;被射精：オーナー：膣内
;--------------------------------------------------
;ちょっと長くなりすぎたため分割いたす（これ書いてる時点で膣内射精だけで83KB）
;--------------------------------------------------
@KOJO_ORGASM_被射精_1147_OWNER_膣内(対象キャラ)
#DIM 対象キャラ
#DIM 相手キャラ番号
#DIM 絶頂部位数
#DIM その部位の絶頂強度
#DIM その部位の感度
#DIM 射精された快楽で絶頂したかフラグ
#DIM 現在精液量
#DIM 受精率
#DIM 受胎人数
#DIM 子供人数総計
#DIM オーナーとの子供人数
#DIM ハート強度
#DIM 繰り返し回数
#DIM LCOUNT
#DIM LCOUNT2
#DIMS LTEXTS

	;--------------------------------------------------
	;条件用のアレコレを拾ったりなんだり
	;--------------------------------------------------
	;射精してきた相手……は前提としてオーナーだけ
	;（ここに来る時点で弾いている）
	相手キャラ番号 = RFLAG:口上用射精キャラ番号
	;現在パッチェさんが絶頂しているかのフラグ
	絶頂部位数 = (NOWEX:Ｃ絶頂 != 0) + (NOWEX:Ｖ絶頂 != 0) + (NOWEX:Ａ絶頂 != 0) + (NOWEX:Ｂ絶頂 != 0) + (NOWEX:Ｓ絶頂 != 0)
	;特にその部位が絶頂しているか
	その部位の絶頂強度 = NOWEX:対象キャラ:Ｖ絶頂
	;感度
	その部位の感度 = ABL:対象キャラ:Ｖ感覚
	;とくに、射精した快楽で絶頂したかのフラグ
	射精された快楽で絶頂したかフラグ = RCVAR:対象キャラ:被射精絶頂の強度
	;現在そこにある精液量（子宮内に溜まっている精液量）
	現在精液量 = TCVAR:対象キャラ:膣内精液量
	;受精率関連
	受精率 = TCVAR:対象キャラ:妊娠率補正
	;孕んでいる人数
	受胎人数 = CFLAG:対象キャラ:今回受胎人数

	;「オーナーとの」子供人数のカウント
	CALL FIND_CHILDREN(CFLAG:対象キャラ:人物番号)
	子供人数総計 = RESULT
	オーナーとの子供人数 = 0
	FOR LOCAL, 1, (1 + 子供人数総計)
		SIF GET_PERSON_父親(RESULT:LOCAL) == CFLAG:MASTER:人物番号
			オーナーとの子供人数++
	NEXT

	;主に地の文で使うためのスケベワードをセット
	;（複数回入れる必要はあんまりないのでここでまとめてシュート）
	CALL パッチェさん_汎用エロワードセット(対象キャラ)

	;汎用喘ぎ声用の標準的なハートマーク個数とか繰り返し回数とかをセット
	ハート強度 = 1
	繰り返し回数 = 1


	;--------------------------------------------------
	;パッチェさん_たくさん差分くん
	;--------------------------------------------------
	;思想として「色んなのが出てほしい」「それらはなるべく等確率で出てほしい」
	;それを満たすような工夫をする
	;１、配列に条件に合う候補を突っ込み
	;２、その候補からランダムで選ぶ
	;ザックリ言うとこう
	;--------------------------------------------------
	;１、"セリフ"、"地の文"どちらか
	;２、入れる行数
	;３、本文
	;４、候補を入れ始めるホントの最初のみ、最後に１を入れる
	;/
	;システムの都合上
	;・空行は作らない（全角スペースなりを入れる）こと
	;・0～9までの10行に収めること
	;--------------------------------------------------
	IF 0
		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"本文候補１の１行目")
		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 1, "本文候補１の２行目")
		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"本文候補２の１行目")
		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"本文候補３の１行目")
		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"本文候補４の１行目")
		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 1, "本文候補４の２行目")
		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 1, "本文候補４の３行目……２行目以後は『０以外』ならいいんだけど……")
		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 4, "本文候補４の４行目……まあ行数にしておくことにするヨ")
		;こうすることで下記のように候補が入り、ランダムで選出される
		;/
		;1.1：本文候補１の１行目
		;1.2：本文候補１の２行目
		;/
		;2.1：本文候補２の１行目
		;/
		;3.1：本文候補３の１行目
		;/
		;4.1：本文候補４の１行目
		;4.2：本文候補４の２行目
		;4.3：本文候補４の３行目……２行目以後は『０以外』ならいいんだけど……
		;4.4：本文候補４の４行目……まあ行数にしておくことにするヨ
	ENDIF


;	;--------------------------------------------------
;	;確定文言
;	;--------------------------------------------------
;	;セリフと地の文を確定でセットにしたい特別な場合は先に判断
;	;--------------------------------------------------
;	IF 0
;	;例えば：オーナーが初めての膣内射精
;	ELSEIF 判定条件式
;		CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「初めて女に膣内射精したって？　それ、_一人称_に言う？　……ン、まあ……おめでとう……でいいのかしら」")
;		CALL パッチェさん_たくさん差分くん_ARRAY("地の文", 0, @"パッチェさんはテキトーにあなたを祝った……")
;		;そしてラストに飛ぶ
;		GOTO END_KOJO_ORGASM_被射精_1147_OWNER_膣内
;	ELSEIF 初体験日取得("Ｖ挿入初体験_男側", MASTER, ぱっちぇさん) > 0
;		;パッチェさんにオーナーが童貞を捧げたか見る
;	ENDIF

	;--------------------------------------------------
	;汎用セリフ文言
	;--------------------------------------------------
	;とりあえずどのシチュでもセリフだけは汎用で挿れる
	;あとでRANDするので最低限２個ほど候補に入れておく
	;--------------------------------------------------
	IF 1
		;どっかしら絶頂している
		IF 絶頂部位数
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "オホ", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "アヘ", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "困惑イキ", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
		;絶頂していない。冷静である
		ELSE
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "非絶頂", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "非絶頂", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
		ENDIF

		;首を絞められている場合
		IF MODE_存在判定_ターゲット側("首絞め", 対象キャラ)
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "クビシメロマンチスト", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
		ENDIF

		;絶頂している＋精液出されすぎたりマジイキしたりメチャクチャイかされてたりしたら汎用マジイキ
{ 
		IF 絶頂部位数 && 
			(
				RCVAR:相手キャラ番号:射精量 >= 500 || 
				現在精液量 >= 600 || 
				その部位の絶頂強度 >= 4 || 
				((TCVAR:対象キャラ:うふふ中Ｃ絶頂累積 + TCVAR:対象キャラ:うふふ中Ｖ絶頂累積 + TCVAR:対象キャラ:うふふ中Ａ絶頂累積 + TCVAR:対象キャラ:うふふ中Ｂ絶頂累積 + TCVAR:対象キャラ:うふふ中Ｓ絶頂累積) >= 250) || 
				(NOWEX:対象キャラ:Ｖ絶頂 && 絶頂部位数 >= 4)
			)
}
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "マジイキ", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "オホ", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "アヘ", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
			CALL パッチェさん_汎用喘ぎ声(対象キャラ, "絶息", ハート強度, 繰り返し回数)
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, "「" + パチュリー_汎用ランダムテキスト用変数 + "」")
		ENDIF
	ENDIF



	;--------------------------------------------------
	;シチュエーション分岐
	;--------------------------------------------------
	IF 0
	;--------------------------------------------------
	;シチュエーション：うふふ中
	;--------------------------------------------------
	ELSEIF STRFIND(NOW_SITUATION(対象キャラ), "うふふ中") > -1
		;--------------------------------------------------
		;前半部分：セリフ部
		;--------------------------------------------------
		;主にセリフ部分
		;--------------------------------------------------

		;絶頂してて・陥落してるしてない系
		;とくにV絶頂している
		IF その部位の絶頂強度
			;結婚済み
			;（下の恋慕も出るように独立させておく）
			IF パッチェさん_陥落段階(対象キャラ, "結婚")
				;妊娠を自覚してる場合
				IF CFLAG:対象キャラ:妊娠発覚
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「気、気が早すぎっ……まだ、おなかのなか、赤ちゃん、いるぅ……_H_")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 1, "　そんなに、精液詰め込まれても……だめぇ……_H_」")
				;妊娠してない・妊娠を自覚はしてない
				ELSE
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「うーっ、すき_H_　あああ妊娠する、妊娠妊娠ッ……」")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「出すならっ、しっかり……奥に押し付けてッ……_H_")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 1, "　……きっちり、孕ませてぇっ……_H_」")
					;さらにあっぶねぇ日の場合
					SIF RISKY_DAY(対象キャラ) == 2
						CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「危険日で……卵子の準備もできてるから……孕ませて……_H_」")
				ENDIF
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「んはぁあ……ッ_H_　け、けっこんして……けっこん……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「な、膣出し、つよすぎる……_H_　はひぅ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「おっおおッ……_H_　射精っ……つよすぎ……たのもし……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「んぉ……およめさんまんこイぐっ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「してっ、_二人称_のものにしてっ_H_　膣出ししてっ_H_」")
			ENDIF

			;恋慕
			IF パッチェさん_陥落段階(対象キャラ, "恋慕")
				;妊娠を自覚してない場合のみ追加
				IF !CFLAG:対象キャラ:妊娠発覚
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「んぉ……合意の卵子レイプ効ッく……_H_」")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「うお゛っ……は、孕まされちゃうぅ……_H_」")
				ENDIF
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「こ、これ……好き……_H_　あぁあ、おなか、とける……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「うううっ、おちんぽ硬すぎるっ……硬すぎる……モノにする気がつよしゅぎる……_H_」")
				;結婚はしてないパターン
				IF !パッチェさん_陥落段階(対象キャラ, "結婚")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「な、ナマで出して、結婚しないと許さないんだからッ……_H_」")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「そんな、に、子宮、口説かないでぇ……けっこんする気になっちゃぅ……_H_」")
				ELSE
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「そんな、に、子宮、口説かないでぇ……好きになっちゃぅ……_H_」")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「そんな、に、子宮、口説かないでぇ……惚れ直しなちゃぅ……_H_」")
				ENDIF
			ENDIF

			;セフレのみ、または未陥落
			IF 1
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「チンポ、強すぎるんだから、加減しなさいよ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「お――おおッ、膣出しでイく、膣出しでイっくッ_H_」")
			ENDIF
		ENDIF

		;どっかしら絶頂している
		IF 絶頂部位数
			;結婚はしてないが、恋慕
			SIF パッチェさん_陥落段階(対象キャラ, "恋慕")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「出すならっ、しっかり……奥に押し付けてッ……_H_」")
			CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「おっ、おちんぽ、深すぎッ……イき死ぬぅ……_H_」")
		;絶頂してない
		ELSE
			;陥落してて、絶頂していない
			IF パッチェさん_陥落段階(対象キャラ, "性的陥落")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「おッ……出しすぎッ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「調子に乗って、射精しすぎッ……そ、そんな、詰めないでッ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「射精なんかで、イくわけないでしょッ……_H_」")
			;陥落しておらず、絶頂していない
			ELSE
				;妊娠可能イベントをこなした場合
				IF イベント完了フラグ確認("パッチェさんの疑問")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「うわ、また膣内に……このままじゃ妊娠するわね……」")
				ELSE
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「うわ、また膣内に……魔法かけてなきゃ妊娠してたわね……」")
				ENDIF
			ENDIF
		ENDIF

		;妊娠を自覚しており……
		IF CFLAG:対象キャラ:妊娠発覚
			;絶頂している
			IF 絶頂部位数
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「も、もう、{受胎人数}人抱えてるのっ……出されても追加されないからっ、あぁあ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「お゛っ、おぉお……ッ_H_　射精ブッ濃いッ……_H_　赤ちゃん、おぼれちゃうっ_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「ゆるぢでぇっ_H_　そんなに、子宮脅さなくても、産むからぁっ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「お゛ッ、も、もうデキてるッ、産むからッ、ゆるッぢっ_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「妊娠してるからって、そんなに、ひ_H_　な、膣出し、するんじゃないわよッ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「妊娠してるからって、膣内に射精し放題というわけではないのよっ……")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 1, "　こんなに出されたら、おなかの子が、精液飲むの大好きになっちゃうからぁ……ッ_H_」")
			;絶頂してない
			ELSE
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「妊娠してるからって、そんなに膣内射精するんじゃないわよッ……」")
				;さらに恋慕でない
				SIF !パッチェさん_陥落段階(対象キャラ, "恋慕")
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「妊娠させられたからって、_一人称_が負けたってことじゃないから……！」")
				;さらに３人以上産まされている
				SIF オーナーとの子供人数 > 2
					CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「はあ……この子、兄弟姉妹がたくさんなのは、寂しくなくていいのかしら……」")
			ENDIF
		;妊娠自覚がなく……
		ELSE
			;３人以上産まされている、かつ、結婚はしてない（結婚してたら赤ちゃん産み産みに同意する）
			IF !CFLAG:対象キャラ:妊娠発覚 && オーナーとの子供人数 > 2 && !パッチェさん_陥落段階(対象キャラ, "結婚")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「ちょ、ちょっと赤ちゃん産んであげたからって、調子に乗るなぁっ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「また仕込む気っ、そんな、もうおなか使わせてあげるつもりはっ……_H_」")
			ENDIF
			;多胎以上（これは陥落見なくてもいいだろ、多胎になるまで産ませてるし……）
			;畜生胎から多胎にちょっと条件緩めておく
			IF TALENT:対象キャラ:受胎体質 >= 2
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「出してっ……産ませて……_H_　いっぱい、産むからッ……_H_」")
				CALL パッチェさん_たくさん差分くん_ARRAY("セリフ", 0, @"「気持ちよ、っ、すぎ……_H_　孕まされるのだめぇえ……_H_」")
