			ELSEIF TALENT:対象キャラ:バストサイズ == -1
				LTEXTS += @"/%パッチェさん_GET_CUPSIZE(対象キャラ)%カップの%TEXTR("ごく薄い胸/僅かな膨らみ")%を震わせながら"
			ELSEIF TALENT:対象キャラ:バストサイズ == 0
				LTEXTS += @"/%パッチェさん_GET_CUPSIZE(対象キャラ)%カップの%TEXTR("胸/乳房")%を揺らしながら"
			ELSEIF TALENT:対象キャラ:バストサイズ == 1
				LTEXTS += @"/%パッチェさん_GET_CUPSIZE(対象キャラ)%カップの%TEXTR("巨乳/乳房")%を揺らしながら"
			ELSEIF TALENT:対象キャラ:バストサイズ == 2
