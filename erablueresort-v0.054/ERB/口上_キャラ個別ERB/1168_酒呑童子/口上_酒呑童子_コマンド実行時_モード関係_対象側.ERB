					CASE 1
						KSTR:(K++) = 「好宝宝，乖宝宝～快快睡吧～……呵呵」
				ENDSELECT
			CASEELSE
				;まだ快楽に慣れていない
				SELECTCASE RAND:2
					CASE 0
						KSTR:(K++) = 「摇啊摇，睡宝宝～快快睡，好宝宝～」
					CASE 1
						KSTR:(K++) = 「好宝宝，乖宝宝～快快睡吧～……呵呵」
				ENDSELECT
		ENDSELECT
	ELSEIF TALENT:対象キャラ:セフレ
		KSTR:(K++) = 
	ELSE
		KSTR:(K++) = 
	ENDIF
ENDIF

