;COM0_愛撫
;--------------------------------------------------
@KOJO_COMPLAYER_2_0(対象キャラ)
#DIM 対象キャラ
;取得快楽：Ｃ、Ｂ
;ランダム弱点コマンドの候補
;オートコマンド許可
CALL 口上変数初期化

IF TARGET == MASTER
	;相手があなたの時
	IF STRFIND(コマンド初回フラグ_PLAYER:対象キャラ:0, @"-0-") < 0
		;初回
		KSTR:(K++) = 
	ELSE
		IF TALENT:対象キャラ:恋慕
			KSTR:(K++) = 「啾……_H_　欸嘿嘿～_H_　盖章成功，吻痕_H_」
			WSTR:(K++) = 「可以吧！你可是属于我的_H_」
			KSTR:(K++) = 　
			KSTR:(K++) = 「……嗯呼呼～_H_　我的身体上，也可以留哦_H_」
		ELSEIF TALENT:対象キャラ:セフレ
			SELECTCASE RAND:2
			CASE 0
				KSTR:(K++) = 「嗯，啾_H_　呼呼，这样的时候，你也会露出可爱的表情呢_H_」
			CASE 1
				WSTR:(K++) = 「嗯呼呼～_H_　你明明很喜欢这样嘛_H_　都露出痴态了哦～？_H_」
				KSTR:(K++) = 
				KSTR:(K++) = 「……『变熟练了』什么的，你觉得是谁的错啊～！」
			ENDSELECT
		ELSE
			KSTR:(K++) = 「那个……这样？这样吗？……舒服吗？」
		ENDIF
	ENDIF
ENDIF

CALL 口上変数表示(対象キャラ, 0)



