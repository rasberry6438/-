@PREGNANCY_CHECK(ARG)
#DIM 種親精液量
#DIM 妊娠確率
#DIM 種親
#DIM 種親妊娠補正
#DIM 命中強度1
#DIM 命中強度2
#DIM 危険度
#DIM 異種フラグ

IF GETBIT(CFLAG:ARG:妊娠個別設定フラグ, 0)
	SIF GETBIT(CFLAG:ARG:妊娠個別設定フラグ, 1)
		RETURN 0
ELSE
	SIF OPTION変数:妊娠切り替え
		RETURN 0
ENDIF

;種親の確定
;条件を満たさない種も妊娠対象選びには参加する(完全な異種でも受精はするらしいので)
命中強度1 = 0
命中強度2 = 0
種親 = -1
種親精液量 = 0
FOR LOCAL,0,19
	SIF 性交相手:ARG:LOCAL == -1
		CONTINUE
	SIF 相手ごとの射精量:ARG:LOCAL <= 0
		CONTINUE

	RESULT = 0
	TRYCALLFORM 個別妊娠処理_種親制御_{NO:ARG}(ARG, 種親)
	IF RESULT == 1
		;この種親で妊娠させる
		種親 = 性交相手:ARG:LOCAL
		種親精液量 = 相手ごとの射精量:ARG:LOCAL
		種親妊娠補正 = 相手ごとの妊娠率補正:ARG:LOCAL
		BREAK
	ELSEIF RESULT == -1
		;この種親では絶対妊娠させない
		命中強度2 = -1
	ELSE
		;通常処理
		命中強度2 = 1 + RAND:(相手ごとの射精量:ARG:LOCAL)
	ENDIF

	IF 命中強度2 > 命中強度1
		命中強度1 = 命中強度2
		種親 = 性交相手:ARG:LOCAL
		種親精液量 = 相手ごとの射精量:ARG:LOCAL
	ENDIF
NEXT
SIF 種親精液量 <= 0
	RETURN 0

IF GETBIT(CFLAG:ARG:妊娠個別設定フラグ, 0)
	IF GETBIT(CFLAG:ARG:妊娠個別設定フラグ, 2)
		SIF ARG != MASTER && 種親 != MASTER
			RETURN 0
	ENDIF
ELSE
	IF OPTION変数:他人妊娠
		SIF ARG != MASTER && 種親 != MASTER
			RETURN 0
	ENDIF
ENDIF

異種フラグ = 0
;妊娠可能な種かどうかの判定
IF 種親 < 0 || TALENT:ARG:種族 != TALENT:種親:種族 || (TALENT:ARG:種族 == 種族_その他 && CSTR:ARG:特殊種族 != CSTR:種親:特殊種族)
	;同じ種族でない場合は条件がある
	IF ARG == MASTER
		;あなたが母親の場合 種親が恋慕・セフレでなく、あなたが特異胎質を持たない場合は不可
		IF !あなた特殊能力:特異胎質 && (種親 < 0 || !陥落チェック(種親))
			異種フラグ = 1
		ELSE
			異種フラグ = 2
		ENDIF
	ELSEIF 干渉強化変数:ARG:干渉種類 != 0
		;era様が母体に干渉している場合は種側のコンディションに関わらず可
		異種フラグ = 2
	ELSEIF 種親 == MASTER
		;あなたが種親の場合 母親が恋慕・セフレでなければ不可
		IF !陥落チェック(ARG)
			異種フラグ = 1
		ELSE
			異種フラグ = 2
		ENDIF
	ELSEIF 種親 < 0
		;種親が不明の場合 原則不可
		異種フラグ = 1
	ELSE
		;キャラ同士の場合 両者に恋慕・セフレの関係性が無い場合は不可
		IF DT_SELECT("関係性データベース", @"(対象キャラ１ = {CFLAG:ARG:人物番号} Or 対象キャラ２ = {CFLAG:ARG:人物番号}) And (対象キャラ１ = {CFLAG:種親:人物番号} Or 対象キャラ２ = {CFLAG:種親:人物番号}) And (関係性種別 = '恋慕' Or 関係性種別 = 'セフレ')") == 0
			異種フラグ = 1
		ELSE
			異種フラグ = 2
		ENDIF
	ENDIF
ENDIF

;超危険日,20回膣内射精で6割、+好条件で確定くらい
妊娠確率 = MIN(20, 種親精液量 / 60 + 10)
SIF CFLAG:ARG:発情期フラグ < 0
	妊娠確率 += 5
SIF TALENT:ARG:恋慕
	妊娠確率 += 5
SIF CSTR:ARG:着せ替え服 == "兔女郎服"
	妊娠確率 += 3
SIF CSTR:ARG:着せ替え服 == "逆兔女郎服"
	妊娠確率 -= 3

妊娠確率 += TCVAR:ARG:妊娠率補正 + 種親妊娠補正

危険度 = RISKY_DAY(ARG) + TCVAR:ARG:排卵誘発薬
SIF TCVAR:ARG:排卵誘発薬 && 危険度 <= 0
	危険度 = 1
SELECTCASE 危険度
	CASE 3
		TIMES 妊娠確率, 7.5
	CASE 2
		TIMES 妊娠確率, 4.0
	CASE 1
		TIMES 妊娠確率, 2.0
	CASE 0
		TIMES 妊娠確率, 0.5
	CASE -1
		TIMES 妊娠確率, 0.1
ENDSELECT

LOCAL:0 = RAND:100

LOCALS = \@ INRANGE(種親, 0, CHARANUM - 1)? %CALLNAME:種親%# 得体のしれぬ者\@
CSTR:ARG:妊娠報告_記録文字列 += @"<font color='#%カラーパレット_HTML("クリーム色")%'>%CALLNAME:ARG%的子宫里，"
IF TCVAR:ARG:膣内精液量 >= 600
	CSTR:ARG:妊娠報告_記録文字列 += @"已被%LOCALS%注入的精液填满……</font><br>"
ELSEIF TCVAR:ARG:膣内精液量 >= 300
	CSTR:ARG:妊娠報告_記録文字列 += @"正被%LOCALS%的精液肆意蹂躏……</font><br>"
ELSE
	CSTR:ARG:妊娠報告_記録文字列 += @"%LOCALS%的精子正拼命游向卵子……</font><br>"
ENDIF

IF TCVAR:ARG:避妊薬
	TCVAR:ARG:膣内精液量 = 0
	RETURN -2
ENDIF
IF TALENT:ARG:妊娠
	TCVAR:ARG:膣内精液量 = 0
	RETURN -3
ENDIF
IF TALENT:ARG:妊娠不可
	TCVAR:ARG:膣内精液量 = 0
	RETURN -1
ENDIF

RESULT = 0
TRYCALLFORM 個別妊娠処理_妊娠率制御_{NO:ARG}(ARG, 種親)
IF RESULT == 1
	;絶対に妊娠させる
	妊娠確率 = 100
ELSEIF RESULT == -1
	;絶対に妊娠させない
	妊娠確率 = -1
ELSE
	;通常処理
	IF 異種フラグ == 1
		;異種の時は妊娠させない
		CSTR:ARG:妊娠報告_記録文字列 += "但不同种族之间似乎无法成功受孕。<br>"
		TCVAR:ARG:膣内精液量 = 0
		RETURN -1
	ELSEIF 異種フラグ == 2 && LOCAL:0 <= 妊娠確率 && !TALENT:ARG:妊娠
		CSTR:ARG:妊娠報告_記録文字列 += "受到星晶兽艾拉影响的母体，正试图接受异种的精子……！<br>"
	ENDIF
ENDIF

;妊娠した
IF LOCAL:0 <= 妊娠確率 && !TALENT:ARG:妊娠
	CSTR:ARG:妊娠報告_記録文字列 += "_受精アニメ_"
	CFLAG:ARG:子の父親 = 種親
	SIF TCVAR:ARG:膣内精液量 >= 450
		CSTR:ARG:妊娠報告_記録文字列 += "被注入了大量精液"
	CSTR:ARG:妊娠報告_記録文字列 += @"%CALLNAME:ARG%的\@RISKY_DAY(ARG)?危险期的#\@\@TALENT:ARG:種族 == 種族_ハーヴィン || TALENT:ARG:体格 == -2?娇小的#\@子宫正传来阵阵酥麻的甜痛…<br>"
	;MASTERの子を妊娠した時だけ追加で表示
	IF 種親 == MASTER
		IF TALENT:ARG:恋慕 || CFLAG:ARG:恋慕レベル >= 関係閾値:4
			CSTR:ARG:妊娠報告_記録文字列 += @"%CALLNAME:ARG%的卵子似乎欣然接受了%CALLNAME:MASTER%的精子…<br>"
		ELSEIF INRANGE(CFLAG:ARG:恋慕レベル,関係閾値:2,関係閾値:4)
			CSTR:ARG:妊娠報告_記録文字列 += @"%CALLNAME:ARG%的卵子无法抗拒对%CALLNAME:MASTER%的朦胧憧憬，接受了受精…<br>"
		ELSEIF CFLAG:ARG:発情期フラグ < 0
			CSTR:ARG:妊娠報告_記録文字列 += @"处于发情期的%CALLNAME:ARG%的卵子擅自做好了受精准备，轻易接受了%CALLNAME:MASTER%的精子…<br>"
		ELSEIF 知識素質:ARG:性知識 == -2
			CSTR:ARG:妊娠報告_記録文字列 += @"%CALLNAME:ARG%纯洁的卵子在不明就里的情况下就被受精了…<br>"
		ELSE
			CSTR:ARG:妊娠報告_記録文字列 += @"%CALLNAME:ARG%的卵子被%CALLNAME:MASTER%来势汹汹的精子们裹挟，被迫接受了\@ 異種妊娠(ARG) ? 跨种族 # \@受精…<br>"
		ENDIF
		CFLAG:ARG:MASTERの子供妊娠回数 ++
	ENDIF
	CSTR:ARG:妊娠報告_記録文字列 += "_受精アニメ_"
	CSTR:ARG:妊娠報告_記録文字列 += @"<font color='#%カラーパレット_HTML("ピンク")%'>%CALLNAME:ARG%似乎怀上了"
	IF 種親 >= 0
		CSTR:ARG:妊娠報告_記録文字列 += @"%CALLNAME:種親%的孩子"
		LOCALS = 
		IF 種親 == MASTER
			;あなたが父親
			SIF CFLAG:ARG:MASTERの子供妊娠回数 > 1
				CSTR:ARG:妊娠報告_記録文字列 += "再次"
		ENDIF
		CALL 関係性テキスト取得(ARG, 種親)
		CALL 履歴データベース登録(CFLAG:種親:人物番号, @"<font color='#%カラーパレット_HTML("薄ピンク")%'>%RESULTS:0%%CALLNAME:ARG%使其怀上了孩子</font><img src='えっちハート'>","うふふ実績")
		CALL 履歴データベース登録(CFLAG:ARG:人物番号, @"<font color='#%カラーパレット_HTML("薄ピンク")%'>%RESULTS:1%%CALLNAME:種親%与之结合而受孕</font><img src='えっちハート'>","うふふ実績")
	ELSE
		CSTR:ARG:妊娠報告_記録文字列 += "来历不明对象的孩子"
		CALL 履歴データベース登録(CFLAG:ARG:人物番号, @"<font color='#%カラーパレット_HTML("薄ピンク")%'>怀上了父亲不明的孩子</font><img src='えっちハート'>","うふふ実績")
	ENDIF
	CSTR:ARG:妊娠報告_記録文字列 += "。</font><br>"
	CSTR:ARG:妊娠報告_記録文字列 += "<br>"
	TALENT:ARG:妊娠 = 1
	EXP:ARG:妊娠経験 ++
	SIF 種親 >= 0
		EXP:種親:孕ませ経験 ++
	CFLAG:ARG:産後経過日 = 0
	;今回受胎人数の決定
	CFLAG:ARG:今回受胎人数 = 1
	SELECTCASE TALENT:ARG:受胎体質
		CASE 1
			;双子胎
			SIF RAND:2
				CFLAG:ARG:今回受胎人数 = 2
			SIF RAND:16 == 0
				CFLAG:ARG:今回受胎人数 = 3
		CASE 2
			;多産
			CFLAG:ARG:今回受胎人数 = 5 - SQRT(RAND:16)
		CASE 3
			;畜生胎
			CFLAG:ARG:今回受胎人数 = 12 - SQRT(RAND:49)
		CASEELSE
			SIF RAND:80 == 0
				CFLAG:ARG:今回受胎人数 = 2
			SIF RAND:6400 == 0
				CFLAG:ARG:今回受胎人数 = 3
	ENDSELECT
	TCVAR:ARG:膣内精液量 = 0
	RETURN 1
ELSE
	CSTR:ARG:妊娠報告_記録文字列 += "<br>"
	TCVAR:ARG:膣内精液量 = 0
	RETURN -1
ENDIF

@関係性テキスト取得(母親, 種親)
;<実行結果>
; RESULTS:0 = 種親から見た母親との関係
; RESULTS:1 = 母親から見た種親との関係
#DIM 母親
#DIM 種親
#DIMS DYNAMIC 関係性1F
#DIMS DYNAMIC 関係性1M
#DIMS DYNAMIC 関係性2
#DIMS DYNAMIC 関係性F
#DIMS DYNAMIC 関係性M
#DIM PERSON_ID

;種親と母親の関係1(血縁関係)
IF GET_PERSON_父親(CFLAG:母親:人物番号) == CFLAG:種親:人物番号
	;種親が父/母親が子供
	関係性1F = 子供
	関係性1M = 父親
ELSEIF GET_PERSON_母親(CFLAG:母親:人物番号) == CFLAG:種親:人物番号
	;種親が母/母親が子供
	関係性1F = 子供
	関係性1M = 母親
ELSEIF GET_PERSON_父親(CFLAG:種親:人物番号) == CFLAG:母親:人物番号
	;母親が父/種親が子供
	関係性1F = 父親
	関係性1M = 子供
ELSEIF GET_PERSON_母親(CFLAG:種親:人物番号) == CFLAG:母親:人物番号
	;母親が母/種親が子供
	関係性1F = 母親
	関係性1M = 子供
ENDIF
; 子供の性別による表記の変更
; ・発情による性転換を考慮して現在の性別ではなく「元性別」で判定する
; ・不明のときは「子供」のまま
; ・ふたなりは…とりあえず「娘」にしておこう
IF 関係性1F == "子供"
	;母親が子供
	SELECTCASE CFLAG:母親:元性別保存
		CASE 2
			関係性1F = 息子
		CASE 1, 3
			関係性1F = 娘
	ENDSELECT
ENDIF
IF 関係性1M == "子供"
	;種親が子供
	SELECTCASE CFLAG:種親:元性別保存
		CASE 2
			関係性1M = 息子
		CASE 1, 3
			関係性1M = 娘
	ENDSELECT
ENDIF

;種親と母親の関係2(交際状態)
IF 種親 == MASTER
	;種親(あなた)と母親
	IF TALENT:母親:恋慕
		関係性2 = 恋人
	ELSEIF TALENT:母親:セフレ
		関係性2 = セフレ
	ELSEIF TALENT:母親:浮気
		関係性2 = 浮気相手
	ELSEIF 初体験日:母親:初対面 == DAY
		関係性2 = 初対面
	ENDIF
ELSEIF 母親 == MASTER
	;種親と母親(あなた)
	IF TALENT:種親:恋慕
		関係性2 = 恋人
	ELSEIF TALENT:種親:セフレ
		関係性2 = セフレ
	ELSEIF TALENT:種親:浮気
		関係性2 = 浮気相手
	ELSEIF 初体験日:種親:初対面 == DAY
		関係性2 = 初対面
	ENDIF
ELSE
	;あなたは無関係
	IF DT_SELECT("関係性データベース", @"(対象キャラ１ = {CFLAG:母親:人物番号} Or 対象キャラ２ = {CFLAG:母親:人物番号}) And (対象キャラ１ = {CFLAG:種親:人物番号} Or 対象キャラ２ = {CFLAG:種親:人物番号}) And 関係性種別 = '恋慕'")
		関係性2 = 恋人
	ELSEIF DT_SELECT("関係性データベース", @"(対象キャラ１ = {CFLAG:母親:人物番号} Or 対象キャラ２ = {CFLAG:母親:人物番号}) And (対象キャラ１ = {CFLAG:種親:人物番号} Or 対象キャラ２ = {CFLAG:種親:人物番号}) And 関係性種別 = 'セフレ'")
		関係性2 = セフレ
	ENDIF
ENDIF

;表示する関係性の決定
;・特に関係性がない場合は「知り合い」
IF 0
;併記するのは長いのでやめておく
;ELSEIF 関係性1F != "" && 関係性2 != ""
;	関係性F = %関係性1F%で%関係性2%の
ELSEIF 関係性2 != ""
	関係性F = %関係性2%の
ELSEIF 関係性1F != ""
	関係性F = %関係性1F%の
ELSE
	関係性F = 知り合いの
ENDIF
IF 0
;併記するのは長いのでやめておく
;ELSEIF 関係性1M != "" && 関係性2 != ""
;	関係性M = %関係性1M%で%関係性2%の
ELSEIF 関係性2 != ""
	関係性M = %関係性2%の
ELSEIF 関係性1M != ""
	関係性M = %関係性1M%の
ELSE
	関係性M = 知り合いの
ENDIF
RESULTS:0 '= 関係性F
RESULTS:1 '= 関係性M

;出産 とりあえずの実装
@BIRTHING(MOTHER)
#DIM MOTHER
#DIM FATHER
#DIM DOMINANT
#DIM 子供性別候補, 2
#DIM 子供性別
#DIM 子供種族
#DIM 子供人数

IF CFLAG:MOTHER:妊娠経過日 < 妊娠期間
	PRINTW 已经完成分娩。
	RETURN 0
ENDIF

FATHER = CFLAG:MOTHER:子の父親
;見た目をよく引き継ぐ側の親を1/2ずつで選択(ただしあなたは優先度低)
IF FATHER < 0 || FATHER == MASTER || (MOTHER != MASTER && RAND:2 == 0)
	DOMINANT = MOTHER
ELSE
	DOMINANT = FATHER
ENDIF

;子供の性別・種族の決定
IF (FATHER == MASTER || MOTHER == MASTER) && あなた特殊能力:潜性の血
	;潜性のあなたの場合、常にもう片方の親と同じ性別・種族
	子供性別 = TALENT:DOMINANT:性別
	子供種族 = TALENT:DOMINANT:種族
ELSE
	;性別 両親の性別のどちらか（父親不明の場合は男性扱いとする）
	子供性別候補:0 = TALENT:MOTHER:性別
	IF FATHER < 0
		子供性別候補:1 = 2
	ELSE
		子供性別候補:1 = TALENT:FATHER:性別
	ENDIF
	子供性別 = 子供性別候補:(RAND:2)

	;種族 親の種族が一致していればその種族、それ以外はその他
	IF FATHER < 0 || TALENT:MOTHER:種族 != TALENT:FATHER:種族 || (TALENT:MOTHER:種族 == 種族_その他 && CSTR:MOTHER:特殊種族 == CSTR:FATHER:特殊種族)
		子供種族 = 6
	ELSE
		子供種族 = TALENT:MOTHER:種族
	ENDIF
ENDIF
;男性が許可されてないならふたなり化
SIF GETBIT(OPTION変数:性別招待制御, 1) && 子供性別 == 2
	子供性別 = 3
;ふたなりが許可されてないなら女化
SIF (素質切り替えOPTION記憶:ふたなり_女性 || GETBIT(OPTION変数:性別招待制御, 2)) && 子供性別 == 3
	子供性別 = 1
;その他が許可されてないなら女化
SIF GETBIT(OPTION変数:性別招待制御, 3) && 子供性別 == 0
	子供性別 = 1
;女性が許可されてないなら男化orふたなり化
IF GETBIT(OPTION変数:性別招待制御, 0) && 子供性別 == 1
	IF 素質切り替えOPTION記憶:ふたなり_女性 == 0 && GETBIT(OPTION変数:性別招待制御, 2) == 0
		子供性別 = 3
	ELSEIF GETBIT(OPTION変数:性別招待制御, 1) == 0
		子供性別 = 2
	ENDIF
ENDIF

;受胎人数判定を妊娠した時に移す対応
SIF CFLAG:MOTHER:今回受胎人数 == 0
	CFLAG:MOTHER:今回受胎人数 = 1
子供人数 = CFLAG:MOTHER:今回受胎人数

DRAWLINE
IF CFLAG:MOTHER:滞在期間 == -1 && MOTHER != MASTER
	SETCOLOR カラーパレット("ピンク")
	PRINTFORML %CALLNAME:MOTHER%开始出现临产征兆
	RESETCOLOR

	;子供の姿の描写
