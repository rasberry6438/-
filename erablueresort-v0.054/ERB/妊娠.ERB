@CHILD_NAMING(MOTHER, FATHER, 子供性別, 子供順番)
#DIM MOTHER
#DIM FATHER
#DIM PARENT, 2
#DIM 子供性別
#DIM 子供順番
#DIMS 子供名前
#DIM 命名者
#DIM 命名者種族
#DIM 案出し回数
#DIM 子供人数
#DIM 子供名前表示
#DIM L_CNT

IF FATHER < 0 || FATHER == MASTER || MOTHER == FATHER
	PARENT:0 = MOTHER
	PARENT:1 = -1
ELSEIF MOTHER == MASTER
	PARENT:0 = FATHER
	PARENT:1 = -1
ELSE
	PARENT:0 = MOTHER
	PARENT:1 = FATHER
ENDIF
DRAWLINE
PRINTFORML ・%CALLNAME:MOTHER%的子女命名列表
CALL FIND_CHILDREN(CFLAG:MOTHER:人物番号)
子供人数 = RESULT:0
子供名前表示 = 0
IF 子供人数 > 0
	FOR L_CNT, 1, 子供人数 + 1
		子供名前 '= GET_PERSON_呼び名(RESULT:L_CNT)
		IF 子供名前 != ""
			PRINTFORM 　%"第" + TOSTR(L_CNT + MAX(0, EXP:MOTHER:出産経験 - 子供人数)) + "子", 7, LEFT% 
			PRINTFORM %子供名前%
			SELECTCASE GET_PERSON_性別(RESULT:L_CNT)
				CASE 1
					PRINT (♀)
				CASE 2
					PRINT (♂)
				CASE 3
					PRINT (♀?)
				CASEELSE
					PRINT (？)
			ENDSELECT
			PRINTL
			子供名前表示 = 1
		ENDIF
	NEXT
ENDIF
SIF !子供名前表示
	PRINTL 无
DRAWLINE
SELECTCASE OPTION変数:オート名付け関連
	CASE 0
		;★EDIT:選択肢表示はオート選択をしない場合のみ
		PRINTFORML \@ (子供順番 > 0) ? 第{子供順番}位 # \@的姓名是…
		PRINTBUTTON "[0]自行命名", 0
		IF PARENT:0 != MASTER
			PRINTL 
			PRINTBUTTON @"[1]交由%CALLNAME:(PARENT:0)%决定", 1
		ELSE
			PRINTL 
			PRINTBUTTON "[1]使用脑海中突然浮现的名字", 1
		ENDIF
		IF PARENT:1 >= 0
			PRINTL 
			PRINTBUTTON @"[2]交由%CALLNAME:(PARENT:1)%决定", 2
		ENDIF
		PRINTL 
		BINPUT
	CASE 1
		;★EDIT:自分で名付ける場合は0が正しい？
		;RESULT = 1
		RESULT = 0
	CASE 2
		;★EDIT:片親に-1が入っている場合の分岐を追加　自動的にもう片方を指定する
		IF 0
		ELSEIF PARENT:1 == -1
			RESULT = 1
		ELSEIF PARENT:0 == -1
			RESULT = 2
		ELSEIF PARENT:0 != MASTER && PARENT:1 != MASTER
			RESULT = RAND:2 + 1
		ELSEIF PARENT:0 != MASTER
			RESULT = 1
		ELSE
			RESULT = 2
		ENDIF
ENDSELECT
SELECTCASE RESULT
	CASE 0
		PRINTL 请输入名字。（最多9个字符）
		INPUTS
		IF RESULTS == ""
			PRINTL 未输入名字
			RESTART
		ENDIF
		子供名前 = %SUBSTRING(RESULTS, 0, 18)%
		PRINTFORML 确定要使用%子供名前%吗？
		PRINTBUTTONLC "[0]确定", 0
		PRINTBUTTONLC "[1]取消", 1
		BINPUT
		SIF RESULT == 1
			RESTART
	CASE 1, 2
		命名者 = PARENT:(RESULT - 1)
		命名者種族 = TALENT:命名者:種族
		案出し回数 = 0
		WHILE 1
			SIF 案出し回数 == __INT_MAX__
				案出し回数 = 100
			IF 案出し回数 < 100
				TRYCCALLFORM 固有子供命名設定_{NO:命名者}(命名者, 子供性別, 子供順番, 案出し回数)
					;RESULTSに命名が返ってくる
					子供名前 = %RESULTS%
					IF 子供名前 == ""
						;空の場合は汎用ランダム
						子供名前 = %ランダムキャラ名前生成(命名者種族, 子供性別)%
					ELSEIF 子供名前 == ";子供の名前"
						;テンプレそのままの場合もランダム
						子供名前 = %ランダムキャラ名前生成(命名者種族, 子供性別)%
					ELSEIF 名前重複チェック(子供名前) || FIND_PERSON_FROM_CALLNAME(子供名前) > 0
						案出し回数++
						CONTINUE
					ENDIF
				CATCH
					子供名前 = %ランダムキャラ名前生成(命名者種族, 子供性別)%
				ENDCATCH
			ELSE
				子供名前 = %ランダムキャラ名前生成(命名者種族, 子供性別)%
			ENDIF

			;オプションで設定してる場合は選択肢を出さない
			SIF OPTION変数:オート名付け関連 == 2
				BREAK

			PRINTFORML %CALLNAME:命名者%似乎想取名为%子供名前%…
			PRINTBUTTON "[0]确定使用", 0
			PRINTL 
			PRINTBUTTON "[1]请对方重新取名", 1
			PRINTL 
			PRINTBUTTON "[2]还是自己来想吧", 2
			BINPUT
			SELECTCASE RESULT
				CASE 0
					BREAK
				CASE 1
					案出し回数++
					CONTINUE
				CASE 2
					RESTART
			ENDSELECT
		WEND
ENDSELECT
SIF PARENT:0 != MASTER
	PRINTFORM %CALLNAME:(PARENT:0)%\@ (PARENT:1 >= 0) ? 和%CALLNAME:(PARENT:1)% # \@经过商议，
PRINTFORML 孩子的名字定为%子供名前%！
RESULTS '= 子供名前
RETURN


