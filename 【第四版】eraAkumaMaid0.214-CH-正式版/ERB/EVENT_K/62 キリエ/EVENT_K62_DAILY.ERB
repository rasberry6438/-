	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育らしいわね？　いいな～、%CNAME("一人称")%も運動したーい」
	ELSE
		IF DAYEV("周六", "游戏内") == 0 && DAYEV("周日", "游戏内") == 0
			PRINTFORMW 「じゃ、%CNAME("一人称")%先に行くから。遅刻不会うにね」
		ELSE
			PRINTFORMW 「出かけよっかな？　どうしよっかな～」
		ENDIF
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")
		IF CFLAG:勉強時間 >= 5
			PRINTFORMW 「今回は勉強したし、余裕余裕♪……多分」
		ELSE
			PRINTFORMW 「……ヤバい。勉強してない。どうしよ絶対ヤバい」
		ENDIF
	ELSEIF DAYEV("試験返却日")
		IF CALCF("試験の予想点数") >= 90
			PRINTFORMW 「良し！　お母桑から何かご褒美貰おっかな～？」
		ELSEIF CALCF("試験の予想点数") >= 70 
			PRINTFORMW 「ん～、まあいつも街道かな？」
		ELSEIF CALCF("試験の予想点数") >= 50 
			PRINTFORMW 「良かった。助かった……」
		ELSE
			PRINTFORMW 「ヤバい、ヤバい！　絶対怒られる……！」
		ENDIF
	ELSEIF COND("朝袭相手")
		IF TALENT:相思相愛
			PRINTFORMW 「どうしてもって言うなら後で続きを、ね？」
			PRINTFORMW そっぽを向きながら何でも無いように言っているが红色耳が隠せていない。
		ELSEIF TALENT:恋慕
			PRINTFORMW 「んっ、言うまでもないけどカレにはナイショよ」
		ELSE
			PRINTFORMW 「……何よ？　変な勘ぐりされるような行動は慎むようにしなさいよ」
		ENDIF
;	ELSEIF COND("体育")
;	ELSEIF COND("水泳")
	ELSE
		IF TALENT:相思相愛
			IF RAND:3 == 0
				PRINTFORMW 「ほら、一緒に登校するんでしょ？」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「周りに関係がバレないようにね♪」
			ELSE
				PRINTFORMW 「今日は他の娘と一緒は嫌だからね」
			ENDIF
		ELSEIF TALENT:恋慕
			IF RAND:3 == 0
				PRINTFORMW 「一緒に登校したいの？　はぁ、しょうがないわね」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「カレと一緒だけど不穏なことを言わないって約束できるなら、一緒でいいわよ」
			ELSE
				PRINTFORMW 「他の娘は一緒じゃないの？」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「……カレと登校约会する予定なんだけど？」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「カレと登校するから3m位出口てるなら一緒でいいわよ」
			ELSE
				PRINTFORMW 「他の娘と一緒に登校しなさいよ」
			ENDIF
		ENDIF
	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
	IF TALENT:相思相愛
		PRINTFORMW 「あ……、早く来過ぎたみたいね……」
		PRINTFORMW 「えっと、二人っきりだし、……どうする？」
	ELSEIF TALENT:恋慕
		PRINTFORMW 「%CNAME("一人称")%たちが一番乗りみたいね」
