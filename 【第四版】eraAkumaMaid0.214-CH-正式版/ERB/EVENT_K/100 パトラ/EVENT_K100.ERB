@CNAME_K100, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_私
CASE "二人称"
	;相手が主人　日本語ではこっちのが自然かもなので変えさせてもらいました
	IF PLAYER == MASTER
		LOCALS = 您_您
	;相手が主人ではない
	ELSE
		LOCALS = 您_您
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 您_您
CASE "助手"
	LOCALS = %CALLNAME:ASSI%桑_%CALLNAME:ASSI%桑
ENDSELECT
RETURNF LOCALS

