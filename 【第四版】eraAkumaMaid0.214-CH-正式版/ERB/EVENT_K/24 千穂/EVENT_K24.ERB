;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K24, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
CASE "主人"
	LOCALS = 主人様_主人大人
CASE "助手"
	LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
ENDSELECT
RETURNF LOCALS

