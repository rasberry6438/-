		ENDIF
	CASE "中出し後恍惚"
		CALL PRINT_STRW, @"「%TEXTR("呼~ _HH_、呼~ _UH_/哈~_HH_、哈啊~_UH_")%」"
		RETURN 1
	CASEELSE
