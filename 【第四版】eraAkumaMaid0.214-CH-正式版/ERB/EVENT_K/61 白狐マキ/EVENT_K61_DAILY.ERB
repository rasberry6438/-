;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K61, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
	CASE "朝袭"
		SELECTCASE CSTR:朝袭
			CASE "朝口交"
				CALL PRINT_STRW, @"「%TEXTR("嗯、早上好~/嗯嗯嗯~/嗯、嗯_HH_")%」"
				RETURN 1
			CASE "朝騎乗位"
				CALL PRINT_STRW, @"「%TEXTR("嗯、早上好_HH_/嗯~/嗯、嗯_HH_/啊_HH_、醒了吗？")%」"
				DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
				RETURN 1
			CASE "朝足交"
				CALL PRINT_STRW, @"「%TEXTR("嗯、早安/嗯、醒了吗？")%」"
				RETURN 1
			CASE "朝の新妻PLAY"
				CALL PRINT_STRW, @"「%TEXTR("嗯……/嗯")%」"
				RETURN 1
			CASEELSE
				DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, 朝袭 %CSTR:朝袭%
		ENDSELECT
	CASE "打招呼"
		;朝のＨイベント終了後にする打招呼
		SELECTCASE CSTR:朝袭
			CASE "朝足交"
				CALL PRINT_STRW, @"「%TEXTR("小鸡鸡舒服吗？/舒服吗_HH_？/再来一次？")%」"
				RETURN 1
			CASE "朝口交"
				CALL PRINT_STRW, @"「%TEXTR("小鸡鸡舒服吗？/舒服吗_HH_？/再来一次？/好像还有感觉？/嗯_HH_、小鸡鸡的味道_HH_/嗯、咸咸的_HH_")%」"
				RETURN 1
			CASE "朝の新妻PLAY"
				CALL PRINT_STRW, @"「%TEXTR("唔~_HH_……/厨房、很危险哦？")%」"
				RETURN 1
			CASEELSE
				DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, 朝袭 %CSTR:朝袭%
		ENDSELECT

		PRINTDATAW
			DATA 「嗯」
			DATA 「嗯~」
			DATA 「早~安」
			DATA 「早安」
			DATA 「早啊早啊」
			DATA 「早上好哦」
			DATA 「嗯、早安」
		ENDDATA
		RETURN 1
	CASE "登校"
		IF DAYEV("試験当日")
			PRINTDATAW
				DATA 「测验好讨厌……」
				DATA 「测验好麻烦……」
				DATA 「……嗯」
				DATA 「嗯……」
			ENDDATA
			RETURN 1

		ELSEIF DAYEV("試験返却日")

		ELSEIF COND("朝袭相手")
			CALL PRINT_STRW, @"「%TEXTR("午休时再见吧/午休时再来吗_UH_？")%」"
			RETURN 1

		ELSEIF COND("体育")

		ELSEIF COND("水泳")

		ELSE
			PRINTDATAW
				DATA 「上学~」
				DATA 「学~校 学~校」
			ENDDATA
			RETURN 1

		ENDIF
	CASE "一番乗り"
		;この角色と一緒に部室に入った際に誰も他にいない場合
		PRINTDATAW
			DATA 「嗯」
			DATA 「嗯~」
			DATA 「嗯 嗯~」
			DATA 「欢迎光临~」
			DATA 「承蒙関照~」
			DATA 「惯例、第一个达到咯~」
		ENDDATA
		RETURN 1
	CASE "ただいま"
		PRINTDATAW
			DATA 「嗯」
			DATA 「嗯~」
			DATA 「回家啦」
			DATA 「回家啦~」
			DATA 「我回来了」
			DATA 「我回来啦~」
			DATA 「回来啦~」
			DATA 「到家啦~」
			DATA 「到家啦到家啦~」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "既に入室"
		;家で出迎えてくれる時
		IF COND("夜")
			PRINTDATAW
				DATA 「嗯」
				DATA 「嗯~」
				DATA 「嗯、欢迎回来」
				DATA 「欢迎回家~」
				DATA 「欢~迎回来」
				DATA 「欢迎光临~」
				DATA 「欢迎光临呀~」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSE
			PRINTDATAW
				DATA 「嗯」
				DATA 「嗯~」
				DATA 「你好啊~」
				DATA 「你好呀~」
				DATA 「你好~」
				DATA 「好~」
				DATA 「呀~」
				DATA 「你好」
				DATA 「你好哇」
				DATA 「欢迎光临」
				DATA 「欢迎光临~」
				DATA 「欢迎光临呀~」

				;便利店の入店音
				DATA 「特列特列 特列~嗯 特列列列~嗯」
			ENDDATA
			RETURN 1
		ENDIF

	CASE "入室打招呼"
		IF CFLAG:呼び出された
			PRINTDATAW
				DATA 「嗯」
				DATA 「嗯~」
				DATA 「发生什么事啦~」
				DATA 「一叫我就来啦~」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSEIF COND("Ｈ的いちゃつき中", MASTER)
			;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
			IF (TALENT:淫乱 && !RAND:2)
				PRINTDATAW
					DATALIST
						DATA 「嗯……」
						DATAFORM %CALLNAME:PLAYER%注意到、%CALLNAME:TARGET%看着这边的眼神里满是欲望。
					ENDLIST
				ENDDATA
				DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
				RETURN 1
			ENDIF

			PRINTDATAW
				DATALIST
					DATA 「嗯……呼嗯」
					DATAFORM 正一脸欲求不满地盯着%CALLNAME:PLAYER%……。
				ENDLIST
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSE
			PRINTDATAW
				DATA 「承蒙関照~」
				DATA 「嗯~」
				DATA 「嗯 嗯~」
				DATA 「还在忙吗？」

				;入ってきたのは你だ
				DATA 「欢迎光临~」
			ENDDATA
			RETURN 1
		ENDIF
	CASE "簡易入室打招呼"
		;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
		IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			;遅れた時のセリフ（既に部員の４分の３が集合済）
			PRINTDATAW
				DATA 「嗯」
				DATA 「嗯~」
				DATA 「还在忙吗？」
				DATA 「迟到了啦」
				DATA 「来晚了」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSEIF CFLAG:呼び出された
			PRINTDATAW
				DATA 「嗯」
				DATA 「嗯~」
				DATA 「发生什么事啦~」
				DATA 「一叫我就来啦~」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ELSE
			PRINTDATAW
				DATA 「承蒙関照~」
				DATA 「嗯~」
				DATA 「嗯 嗯~」

				;入ってきたのは你だ
				DATA 「欢迎光临~」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
			RETURN 1
		ENDIF
	CASE "打招呼返し"
		;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
		;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
		;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる

		;打招呼してきた人の中から一人選ぶ
		ACTOR = FIND_COND("入室打招呼中")
		;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
		CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

		;ここでは括弧に『』を使う

		;相手が現れるのが遅かった（既に部員の４分の３が集合済）
		IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			PRINTDATAW
				DATA 『嗯』
				DATA 『嗯~』
				DATA 『欢迎呀~』
				DATA 『欢迎哟~』
				DATA 『欢迎光临~』
				DATA 『欢迎呀~』
				DATA 『辛苦了啦~』
			ENDDATA
			RETURN 1
		ELSE
			IF CHARA_NUM("入室打招呼中") > 1
				PRINTDATAW
					DATA 『嗯』
					DATA 『嗯~』
					DATA 『欢迎呀~』
					DATA 『欢迎呀~』
					DATA 『团体客这边请~』
				ENDDATA
				RETURN 1
			ELSE
				PRINTDATAW
					DATA 『嗯』
					DATA 『嗯~』
					DATA 『你好~』
					DATA 『你好哇~』
					DATA 『竹轮~』
					DATA 『欢迎呀~』
					DATA 『欢迎哟~』
					DATA 『欢迎光临~』
					DATA 『欢迎呀~』
				ENDDATA
				RETURN 1
			ENDIF
		ENDIF
	CASE "便当"
		PRINTDATAW
			DATA 「饭饭~」
			DATA 「唔呼~」
			DATA 「嗯、好吃吗？」
			DATA 「感觉如何？」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "换地方"
		PRINTDATAL
			DATA 「怎么啦？」
			DATA 「怎么啦~？」
			DATA 「嗯？」
			DATA 「嗯~？」
			DATA 「要去哪里？」
		ENDDATA
		RETURN 1
	CASEELSE
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
ENDSELECT
RETURN 0

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;动画ーションの関係上、１ターンに何度も呼ばれたりするのでフラグ操作をする場合には
;IF CFLAG:KOJO_DAILY済 == 0
;で分岐させるのが良いでしょう
;-------------------------------------------------
@KOJO_DAILY_K61, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
	CASE "初回台詞"
		;初回のセリフ
		PRINTFORML 「唔~」
		PRINTFORML %CALLNAME:TARGET%早已摆出一副主人姿态、悠然自得。
		RETURN 1
	CASE "浑身是汗"
		PRINTDATAL
			DATA 「……嗯」
			DATA 「……黏糊糊的」
			DATA 「……哈啾」
			DATA 「……好热」
		ENDDATA
		RETURN 1
	CASE "刚睡醒"
		PRINTDATAL
			DATALIST
				DATAFORM 「……%TEXTR("嗯呜/早上/中午/晚上")%了？」
				DATAFORM %CALLNAME:TARGET%好像还没睡醒。
			ENDLIST
			DATALIST
				DATAFORM 「……%TEXTR("哈啊/嗯唔/好困")%」
				DATAFORM %CALLNAME:TARGET%正在打哈欠。
			ENDLIST
		ENDDATA
		RETURN 1
	CASE "点心の感想"
		;表記ゆれ対応
		IF K61_INCLUDES(SAVESTR:点心, CSTR:讨厌的东西+"/巧克力") != ""
			;ものをもらっておいてなんて野郎だ
			PRINTFORML 「嗯呜……这个不喜欢」
			PRINTFORML %CALLNAME:TARGET%似乎不怎么喜欢%K61_INCLUDES(SAVESTR:点心, CSTR:讨厌的东西 + "/巧克力")%。
		ELSEIF COND("点心当番")
			;今日の点心を用意したのは自分
			PRINTFORML 「饭饭~」
			PRINTFORML %CALLNAME:TARGET%正在大口吃着%TEXTS("点心")%。
		ELSE
			;点心が亲手自制でない
			PRINTFORML 「唔~」
			PRINTFORML %CALLNAME:TARGET%正在大口吃着%TEXTS("点心")%。
		ENDIF
	CASE "睡眠"
		;沉眠著
		;梦话
		CALL KOJO_SLEEP_K61
		;梦话用の汎用地の文呼出
		CALL DAILY_MESSAGE, "梦话"
	CASE ""
		;nothing
	CASEELSE
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
	CASE "愛の告白"
		CALL PRINT_STRL, "「……ん_H_」"
		PRINTFORML %CALLNAME:TARGET%脸颊泛红害羞地低下了頭。
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "主人公的房間に連れ込まれた"
		;你と一緒に、主人公的房間に移動した
		PRINTDATAL
			;他人的房間を物色するデリカシーのなさ
			DATAFORM %CALLNAME:TARGET%正不知所措地东张西望。
			DATAFORM %CALLNAME:TARGET%耸动鼻子嗅来嗅去。
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "自室に招待"
		;你と一緒に、partner的房間に移動した
		PRINTDATAL
			;でもかたづけない
			DATA 「虽然乱糟糟的」

			DATA 「嗯、欢迎光临」
			DATA 「嗯、怎么啦？」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "露脐装超短裙胖次チラ"
		PRINTDATAL
			DATA 「嗯？」
			DATA 「嗯、怎么啦？」
			DATA 「发生什么了吗？」
			DATA 「发生什么事了？」
			DATA 「……看到了？」
		ENDDATA
		RETURN 1
	CASE "掀起裙子"
		PRINTDATAL
			DATA 「变态……」
			DATA 「掀裙子什么的……」
		ENDDATA
		RETURN 1
	CASE "撫摸股間"
		CALL PRINT_STRL, @"「%TEXTR("ん、へんたい……_HH_/もちょっとする？/もちょっといいよ？")%」"
		RETURN 1
	CASE "拜托処理性欲"
		PRINTDATAL
			DATA 「舒服吗？」
			DATA 「舒服吗？」
			DATA 「……再来一次？」
			DATA 「……再继续做会儿？」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
		RETURN 1
	CASE "检查内褲"
		IF COND("合意：检查内褲")
			PRINTDATAL
				DATA 「嗯、变态……」
				DATA 「再稍微看看？」
				DATA 「再稍微摸摸？」
			ENDDATA
			RETURN 1
		ELSE
			PRINTDATAL
				DATA 「哔哔、正在发布变态注意警报」
				DATA 「变态……」
			ENDDATA
			RETURN 1
		ENDIF
	CASE "检查阴部"
		IF COND("合意：检查阴部")
			PRINTDATAL
				DATA 「嗯、变态……」
				DATA 「再稍微看看？」
			ENDDATA
			RETURN 1
		ELSE
			PRINTDATAL
				DATA 「哔哔、正在发布变态注意警报」
				DATA 「变态……」
			ENDDATA
			RETURN 1
		ENDIF
	CASE "打招呼舐陰"
		PRINTDATAL
			DATA 「变态……」
			DATA 「喜欢吗？」
		ENDDATA
		RETURN 1
	CASE "股間撮影", "阴部摄影", "裙下撮影", "内褲摄影"
		PRINTDATAL
			DATA 「变态……」
			DATA 「喜欢吗？」
			DATA 「拍的漂亮吗？」
			DATA 「那个要怎么办？」
		ENDDATA
		RETURN 1
	CASE "沏茶"
		PRINTDATAL
			DATALIST
				DATAFORM ｽﾞｽﾞ……
				DATAFORM 「呼噜噜」
				DATAFORM %CALLNAME:TARGET%正心满意足地啜饮着%SAVESTR:茶%、似乎是她的心頭好。
			ENDLIST
		ENDDATA
		RETURN 1
	CASE "泡茶"
		IF K61_INCLUDES(SAVESTR:点心, CSTR:讨厌的东西) != ""
			PRINTFORML %CALLNAME:TARGET%一边瞪着%TEXTS("点心")%一边小口喝着%SAVESTR:茶%。
		ELSE
			PRINTFORML %CALLNAME:TARGET%正小口地吃着茶点。
		ENDIF
		RETURN 1
	CASE "膝枕"
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%正昏昏欲睡地打着盹。
			DATAFORM %CALLNAME:TARGET%在%CALLNAME:PLAYER%的膝頭翻来滚去。
		ENDDATA
		RETURN 1
	CASE "抚摸尾巴"
		;嫌ではないが気になるので他のことはできない。
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%正昏昏欲睡地打着盹。
			DATAFORM %CALLNAME:TARGET%正呆愣楞地出神。
			DATAFORM %CALLNAME:TARGET%似乎很在意%CALLNAME:PLAYER%抚摸她尾巴时的小动作。
		ENDDATA
		RETURN 1
	CASE "胖次交易"
		PRINTDATAL
			DATA 「多谢惠顾~」
			DATA 「那么想要吗？」
			DATA 「那个要怎么办？」
			;本人は返してもらうつもり
			DATA 「要好好还回来哦？」
		ENDDATA
		RETURN 1
	CASE "隐秘摄影失敗"
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%对着镜頭摆出双V手势。
			DATAFORM %CALLNAME:TARGET%的得意表情在镜頭前留下大特写。
		ENDDATA
		RETURN 1
	CASE "一起学习"
		;REF 教材の参考 https://happylilac.net/mu1605121851.html
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%用直尺弹飞了钢笔。
			DATAFORM %CALLNAME:TARGET%正来回转动着钢笔……啊、掉了。
			DATAFORM %CALLNAME:TARGET%在笔记本的边角涂鸦。
			DATAFORM %CALLNAME:TARGET%直勾勾地盯着量角器发呆……。
			DATAFORM %CALLNAME:TARGET%正一圈圈地转动圆规。
			;摩擦で抜けなくなるやつ
			DATAFORM %CALLNAME:TARGET%把两本教科书的书页交叉叠在一起。
			DATA 「二等边、直角三角形」
			DATA 「锐角、直角、钝角」
			DATA 「3、4、5、构成直角」
			;正三角形を二等分した30°, 60°, 90°の三角定規の各辺の長さの比は3:4:5じゃないので裏切られた気がしている。
			DATA 「边长原来不是3:4:5啊……」不知为何她看起来有点消沉。
			DATA 「球体的展開图、是什么样子的？」
			;地図記号
			DATA 「工厂上面长出手掌、就是发电站」
		ENDDATA
		RETURN 1
	CASE "甜言蜜语"
		IF CFLAG:甜言蜜语
			;成功
			PRINTDATAL
				DATAFORM %CALLNAME:TARGET%用尾巴缠着%CALLNAME:PLAYER%的胳膊。
				DATAFORM %CALLNAME:TARGET%紧紧抱着%CALLNAME:PLAYER%。
			ENDDATA
			RETURN 1
		ELSE
			;nothing			
		ENDIF
	CASE "膝枕"
		IF !RAND:3
			PRINTDATAL
				DATAFORM %CALLNAME:TARGET%用渴望被関注的眼神注视着%CALLNAME:PLAYER%。
			ENDDATA
			RETURN 1
		ENDIF
	CASE "亲脸頬"
		PRINTDATAL
			DATAFORM %CALLNAME:TARGET%扭扭捏捏地坐在%CALLNAME:PLAYER%的身旁。
			DATAFORM %CALLNAME:TARGET%用渴望被関注的眼神注视着%CALLNAME:PLAYER%。
			DATAFORM %CALLNAME:TARGET%撒娇似地依偎过来。
		ENDDATA
		RETURN 1
