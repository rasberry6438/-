			DATA 「唔呼~」
			;こなみ
			DATA 「好開心」
		ENDDATA
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, %STR:日常イベント名%, %STR:PLAY内容%
		RETURN 1

	CASE "约会終了"
