			RETURN 1
		ELSE
			PRINTDATAW
				DATA 「下次再一起玩吧」
				DATA 「下次再来吧」
			ENDDATA
			DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%, %STR:日常イベント名%, %STR:PLAY内容%
			RETURN 1
		ENDIF
	CASEELSE
		DEBUGPRINTFORML %__FILE__% %__FUNCTION__% line:{__LINE__} ARGS %ARGS%
ENDSELECT
RETURN 0

