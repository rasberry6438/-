;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K48, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		CALL PRINT_STRW, @"「嗯、呼……_UH_ 啊、%CNAME("主人")%……吵醒了吗……？」"
		PRINTFORMW 「非常抱歉……这样随便的用着……嗯」
	CASE "朝舐陰"
		PRINTFORMW 「嗯、呼……啊、起床了吗？」
		PRINTFORMW 「非常抱歉、稍微……有点想恶作剧了。呵呵」
	CASE "朝足交"
		PRINTFORMW 「啊……终于起来了吗、%CNAME("主人")%？」
		PRINTFORMW 「嗯哼哼……因为你完全没有醒来的想法、所以有点想玩弄一下呢」
	CASE "朝の新妻PLAY"
		PRINTFORMW 「啊、等……请不要打扰我」
		PRINTFORMW 「真是的……万一料理搞砸了怎么办。到时候也要全部吃光哦？」
	CASE "朝乳交"
		PRINTFORMW 「嘿咻……诶。醒了吗？」
		PRINTFORMW 「不好意思啊、只是作为个人的练习台使用而已……」
	CASE "朝接吻", "朝添い寝"
		PRINTFORMW 「……那个……早上好」
	CASE "朝口交", "朝六九式"
		PRINTFORMW 「唔嗯……阿姆……早上好、%CNAME("主人")%」
	CASE "朝騎乗位", "朝Ａ騎乗位"
		CALL PRINT_STRW, "「嗯、唔嗯……_UH_ 啊啊……早上好……」"
		PRINTFORMW 「嗯哼哼……偶尔任性的用一下也没関系吧……？」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	PRINTFORMW 「hola、%TEXTR("要睡到什么时候。/我是特意来叫醒你的、")%请快点起来吧」
CASE "登校"
	IF DAYEV("試験当日")
		CALL EMOTION_PLUS, "重置/闭眼"
		PRINTFORMW 「呼唔……」
		PRINTFORMW （考试当天……没関系。%CNAME("一人称")%能做到……）
	ELSEIF DAYEV("試験返却日")
		IF CFLAG:予想点 >= 80
			CALL EMOTION_PLUS, "重置/闭眼"
			PRINTFORMW （考试结果……这次感觉。上位应该没问题……）
		ELSEIF CFLAG:予想点 >= 60
			CALL EMOTION_PLUS, "重置/闭眼/斜眉"
			PRINTFORMW （……这次的考试、感觉有点微妙啊……感觉不会是前几名……）
		ELSE
			CALL EMOTION_PLUS, "重置/闭眼/困扰眉"
			PRINTFORMW （……怎么办、这次的考试……可能在平均以下。要怎么蒙混过去呢……）
		ENDIF
	ELSEIF COND("朝袭相手")
		CALL EMOTION_PLUS, "重置/斜眼/斜眉/半開"
		PRINTFORMW （哈啊……一大早就做了蠢事……）
	ELSEIF COND("体育")
		CALL EMOTION_PLUS, "重置/闭眼/困扰眉/小口"
		PRINTFORMW 「啊啊、真是的体育课啊……好想休息啊……」
	ELSEIF COND("水泳")
		CALL EMOTION_PLUS, "重置/困扰眉"
		PRINTFORMW （游泳啊……不太擅长啊、好讨厌的感觉……）
	ELSE

	ENDIF
CASE "一番乗り"
	PRINTFORMW 「%TEXTR("啊咧/诶诶")%……只有你一个吗……？」
CASE "ただいま"
	PRINTFORMW 「……%TEXTR("打扰了/回来了/我回来了")%」
CASE "既に入室"
	PRINTFORM 「ああ……
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW お帰りなさい」
	ELSE
		PRINTFORMW どうも。お邪魔してます」
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORMW 「……%TEXTR(@"来了啊、%CNAME("主人")%/久等了")%」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		PRINTFORMW 「%TEXTR("打扰了/失礼了")%……%TEXTR("诶、在做什么/啊、来的时机不对吗……")%」
	ELSE
		PRINTFORMW 「%TEXTR("打扰了/失礼了")%……」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR("……非常抱歉、迟到了/唔……大家都到了吗")%」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「……来了」
	ELSE
		PRINTFORMW 「……打扰了」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『啊、老师……%CNAME("你好/您好")%』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF FIND_COND("入室打招呼中", "真")
			PRINTFORMW 『诶……啊啊、姐姐。你在做什么？』
		ELSEIF FIND_COND("入室打招呼中", "灯")
			PRINTFORMW 『啊啊、哥哥。好晚啊』
		ELSE
			PRINTFORMW 『啊啊……抱歉。来晚了』
		ENDIF
	ELSEIF FIND_COND("入室打招呼中", "真")
		PRINTFORMW 『啊……还以为是谁、原来是姐姐啊』
	ELSEIF FIND_COND("入室打招呼中", "灯")
		PRINTFORMW 『啊、是哥哥啊』
	ELSE
		PRINTFORMW 『啊……%TEXTR("您好/你好/欢迎")%』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「……有什么事吗？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「刚刚……在街道上」
	ELSE
		PRINTFORMW 「啊啊、有人在吗。先失陪了」
	ENDIF
CASE "抜け出された"
	PRINTFORMW 「啊啊……真是%TEXTR("健康/繁盛")%呢……」
CASE "便当：地の文前"
	IF TALENT:叛逆 == 0 && CEVENT("初めての便当") == 0
		PRINTFORMW 「……是」
		PRINTFORMW 午休时、到了%CALLNAME:MASTER%约好的地点、%CALLNAME:TARGET%移開了视线递上了便当。
		PRINTFORMW 「那么%CNAME("一人称")%、就走了」
		PRINTFORMW 突然想要离開的%CALLNAME:TARGET%、被%CALLNAME:MASTER%连忙拦住。
		PRINTFORMW 「什、什么事。%CNAME("一人称")%已经吃过了、你一个人吃吧」
		PRINTFORMW 找了个理由拦下了想要回去的%CALLNAME:TARGET%、%CALLNAME:MASTER%让她坐下然后打開了便当的盖子。
		PRINTL
		PRINTFORMW ……只有饭团蛋卷和香肠……！
		PRINTL
		PRINTFORMW 「哇、真是抱歉。这么寒酸不像女孩子做出来的吧」
		PRINTFORMW %CALLNAME:TARGET%抱着膝盖转过脸去、而%CALLNAME:MASTER%却被大块的蛋卷吸引、咬了一口。
		PRINTFORMW ……虽然很在意味道的浓度、但是很好吃……！
		PRINTFORMW %CALLNAME:MASTER%想说做得很好、于是向%CALLNAME:TARGET%打了个招呼。
		PRINTFORMW 「――这次的」
		PRINTFORMW ……%CALLNAME:TARGET%的口中、漏出了轻轻的打嗝声。
		PRINTFORMW 「真是的……我不想看到鸡蛋……」
		PRINTFORMW ……好像是很努力做的……
		CALL SET_CEVENT, "初めての便当", TARGET
	;↑を閲覧済みだと↓の分岐は使われない
	ELSEIF CEVENT("初めての便当：冷凍") == 0 && CEVENT("初めての便当") == 0
		PRINTFORMW 「是」
		PRINTFORMW 午休的时候、按照约定%CALLNAME:TARGET%做了%CALLNAME:MASTER%的便当带来了。
		PRINTFORMW 立刻看了看里面的菜、里面是五颜六色的菜肴。
		PRINTFORMW 吃了一口、让人觉得不可思议的无可非议的味道在口中蔓延開来……
		PRINTFORMW 「因为、那个是冷冻的」
		PRINTFORMW ……看起来、这些都是解冻后的便当和配菜……
		PRINTFORMW 「啊、不用在意%CNAME("一人称")%」
		PRINTFORMW 说着%CALLNAME:TARGET%拿出了均衡营养餐……
		CALL SET_CEVENT, "初めての便当：冷凍", TARGET
	;ELSEIF TALENT:叛逆 == 0
	ELSE
		PRINTFORMW 
		PRINTFORMW 「……」
		PRINTFORMW %CALLNAME:TARGET%一直看着心满意足地吃着自己手中的料理的%CALLNAME:MASTER%的脸。
	ENDIF
	TFLAG:地の文スキップ = 1
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	CALL EMOTION_PLUS, "惊嚇", TARGET
	PRINTFORMW 「啊？ 等下、突然想要要什么？」
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K48, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「你好……%NAME:TARGET%。请多関照」
	PRINTFORML %CALLNAME:TARGET%简单地打了个招呼、随便找了张空椅子坐下。
CASE "浑身是汗"
	PRINTFORML 「%TEXTR(@"好热/好累/要死了")%……」
	PRINTFORML %CALLNAME:TARGET%奄奄一息得说着……
CASE "刚睡醒"
	PRINTFORML 「唔嗯……呀？」
	PRINTFORML 「……做了什么？」
CASE "Cosplay中"
	PRINTFORML 「请不要这样一直盯着看。因为还是中学生」
	PRINTFORML %CALLNAME:TARGET%的动作既清楚又有力、但脸有点红……
CASE "点心の感想"
	;今日の点心を用意したのは自分
	IF COND("点心当番")
		PRINTFORML 「啊啊、这个。%TEXTR("是刚刚买的/请随便吃吧")%」
	;今日の点心は誰かの亲手自制
	ELSEIF COND("亲手自制点心")
		;今日の点心当番の人の亲手自制を初めて食べた
		IF DEVENT(@"%TEXTS("点心当番")%の亲手自制") == 0
			;点心当番が[料理擅長]
			IF COND("点心の作り手が料理擅長")
				PRINTFORML 「啊……真好吃……」
				PRINTFORML %CALLNAME:TARGET%冷漠的表情看起来有点动摇了。
			ELSE
				PRINTFORML 「哎……很棒啊、这个」
				PRINTFORML %CALLNAME:TARGET%一边钦佩一边大口地吃着点心。
			ENDIF
		ELSE
			PRINTFORML 「啊、亲手做的吗……那么、拿一个」
			PRINTFORML 顺手拿起桌上的零食、%CALLNAME:TARGET%坐到了座位上。
		ENDIF
	;点心が亲手自制でない
	;ELSE
		;PRINTFORML 点心当番へのコメントも点心自体へのコメントも幅が広すぎて難しすぐる…
	ENDIF
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K48
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	IF TALENT:相思相愛
		PRINTFORML %CALLNAME:TARGET%哭肿了眼睛把頭扭向一边……
	ELSE
		PRINTFORML %CALLNAME:TARGET%假装没听见%CALLNAME:MASTER%的话……
	ENDIF
CASE "约会的约定"
	IF TALENT:恋慕
		PRINTFORML 「…………～♪」
		PRINTFORML %CALLNAME:TARGET%好像在小声哼着歌。
	ELSE
		PRINTFORML 「真是的……本想星期天好好休息的……」
		PRINTFORML 虽然嘴上这么说着、但是%CALLNAME:TARGET%看起来有点不冷静……
	ENDIF
;これは悪魔や天使や驱魔人のみ
CASE "契約成立"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K48, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
;你にお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	PRINTFORML 
CASE "摆脱"
	IF CFLAG:躁动难忍
		PRINTFORML 「真是的……快点解决吧……」
		PRINTFORML %CALLNAME:TARGET%看起来有点脸红……
	ELSE
		PRINTFORML 「真是的……吧这么小的后辈带到这种地方、在想什么呢？」
		PRINTFORML %CALLNAME:TARGET%露出惊讶的眼神……
	ENDIF
;你と一緒に、主人公的房間に移動した
CASE "主人公的房間に連れ込まれた"
	IF CFLAG:躁动难忍
		PRINTFORML 「然后……想对%CNAME("一人称")%做什么呢、%CNAME("主人")%？」
		PRINTFORML 感受到了%CALLNAME:TARGET%热情的视线……
	ELSE
		PRINTFORML 「有什么事吗？ 看漫画、打游戏什么的」
		PRINTFORML %CALLNAME:TARGET%一副无视%CALLNAME:MASTER%的样子……
	ENDIF
;你と一緒に、partner的房間に移動した
CASE "自室に招待"
	IF CFLAG:躁动难忍
		PRINTFORML 「……那么、到%CNAME("一人称")%的房间是打算做什么呢？ %CNAME("主人")%」
		PRINTFORML %CALLNAME:MASTER%盯着%CALLNAME:TARGET%有点红的脸蛋……
	ELSE
		PRINTFORML 「房间里的东西请不要随便乱动哦」
		PRINTFORML 「%TEXTR(@"因为是步入青春期的人的房间。有点/能好好听话吗？ 比%CNAME("一人称")%更成熟的大人")%」
	ENDIF
;partnerが既に居る部屋に、你が移動した
CASE "自室に訪問された"
	;指导者を伴っている場合
	IF FIND_COND("現在存在", "助手")
		IF FIND_COND("真")
			PRINTFORML 「……你来干什么？ 姐姐」
			PRINTFORML 『真是的、又是这么冷淡的反应……』
			PRINTFORML 对于彼此的态度、两人都叹了口气……
		ELSEIF FIND_COND("灯")
			PRINTFORML 「还以为是谁、原来是哥哥啊……」
			PRINTFORML 『哦哦、打扰了』
			PRINTFORML %CALLNAME:FINDCHARA(NO, 35)%毫不客气地走进房间、%CALLNAME:TARGET%则是叹了口气……
		ELSE
			PRINTFORML 「啊咧。这次是两个人吗？」
			PRINTFORML 对于同行者的存在、%CALLNAME:TARGET%点慌张。
		ENDIF
	ELSE
		PRINTFORML 「%TEXTR(@"唔嗯、来了……/你是故意进来的吗/……你来干什么？")%」
		PRINTFORML %CALLNAME:TARGET%用冷淡的目光迎接着……
	ENDIF
CASE "指导者部屋訪問"
	SELECTCASE SAVESTR:現在位置
	CASE "真的房間"
		PRINTFORML 『啊咧、莉酱？ 怎么是和部长一起来的？』
		PRINTFORML 「没什么……只是被人擅自带来的」。
	CASE "灯的房間"
		PRINTFORML 『哦、是你们啊。是来玩的吗？』
		PRINTFORML 「不、不知道。有什么事问%CNAME("主人")%吧」
	CASEELSE
		PRINTFORML 「抱歉、打扰了」
	ENDSELECT
;自宅移動の後に你に連れられて居間に回来了場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 「啊啊、累了……」
CASE "オナバレ"
	PRINTFORML 「哈啊……在瞄着哪里啊、%CNAME("主人")%」
	PRINTFORML %CALLNAME:TARGET%飞来怨恨的视线……
CASE "露脐装超短裙胖次チラ"
	IF EQUIP:胖次 == 0
		PRINTFORML 「――！」
		PRINTFORML %CALLNAME:TARGET%注意到自己的下面正在被注视着、脸一下子就红了……
	ELSE
		PRINTFORML 「喂、眼睛往哪看呢」
		PRINTFORML %CALLNAME:TARGET%立刻按住裙子蹲了下去。
	ENDIF
CASE "低腰臀圍"
	PRINTFORML 「……请不要再盯着看了。毕竟是小孩子的屁股」
	PRINTFORML %CALLNAME:TARGET%投来责备的目光……
CASE "掀起裙子"
	IF EQUIP:胖次 == 0
		PRINTFORML 「……什么事。有什么意见吗」
		PRINTFORML 面色绯红、有些羞耻的%CALLNAME:TARGET%瞪了过来……
	ELSE
		PRINTFORML 「真是的、为什么要掀開这么小的孩子的裙子呢……」
		PRINTFORML 正在吐槽的%CALLNAME:TARGET%虽然假装平静、但还是遮挡不住脸颊的粉红……
	ENDIF
CASE "检查内褲"
	IF COND("合意：检查内褲")
		PRINTFORML 「……已经可以了吗？」
		PRINTFORML %CALLNAME:TARGET%急躁的问着%CALLNAME:MASTER%。
	ENDIF
CASE "检查阴部"
	IF COND("合意：检查阴部")
		PRINTFORML 「……真是个独裁者……」
		PRINTFORML %CALLNAME:TARGET%咬牙切齿地转过脸……
	ENDIF
CASE "拜托処理性欲"
	PRINTFORML 「呼……请不要让我太累哦……」
	PRINTFORML %CALLNAME:TARGET%的呼吸有些急促……
CASE "进行性処理"
	PRINTFORML 「这样很高兴吗、%CNAME("主人")%？」
	PRINTFORML %CALLNAME:TARGET%一脸无奈地歪着頭。
CASE "帮忙消除性欲"
	PRINTFORML 「……那个、真是抱歉。让您费心了」
	PRINTFORML %CALLNAME:TARGET%看都不敢看、支支吾吾的说着……
CASE "清洁口交"
	PRINTFORML 「……姆……呼唔……满意了吗？」
	PRINTFORML %CALLNAME:TARGET%擦了擦嘴、迅速地离開了%CALLNAME:MASTER%。
CASE "買東西"
	IF COND("巨大振動棒購入") && DEVENT("巨大振動棒感想") == 0
		PRINTFORML 「呜哇、好大……」
		IF TALENT:MASTER:男性
			PRINTFORML 「男人们、是不是都觉得越大越好？」
		ELSE
