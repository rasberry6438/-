;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K71, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		CALL PRINT_STRW, @"「早安ー！嘿嘿、在%CNAME("二人称")%的里面……好舒服哦_BH_」"
		PRINTFORMW 「其实、还想再……享受一会儿、但是……」
		CALL PRINT_STRW, @"「已经、忍不住了……要全部射进去了哦_BH_」"
	CASE "朝舐陰"
		CALL PRINT_STRW, @"「啾……嗯_BH_  咕噜」"
		PRINTFORMW 「……啊、已经醒了吗？」
		PRINTFORMW 「再等一下下ー！现在到関键的地方了……啾」
	CASE "朝足交"
		PRINTFORMW 「嗯ー……啊、早安！」
		PRINTFORMW 「今天呢、正在练习用脚……」
		PRINTFORMW 「舒服吗？痛的话要告诉我哦？」
	CASE "朝の新妻PLAY"
		PRINTFORMW 「早安！爸爸！」
		PRINTFORMW 「饭马上就……诶！？」
		CALL PRINT_STRW, @"「嗯…_BH_  再、再这样下去……要烤焦了、所以就一下下哦ー？」"
	CASE "朝乳交"
		PRINTFORMW 「嗯哼、嗯哼……啊、抱歉ー！」
		PRINTFORMW 「虽然有在努力、但是有点难……」
		PRINTFORMW 「但是我会尽力服侍你的！」
	CASE "朝面部騎乗"
		PRINTFORMW 「起床啦～？早上好！」
		PRINTFORMW 「能在一大早就看到%CNAME("二人称")%最喜欢的阴部，我会很開心的！」
		CALL PRINT_STRW, @"「呀っ_BH_ 啊，不可以对我太粗鲁哦_BH_」"
	CASE "朝騎乗位", "朝Ａ騎乗位"
		IF CANTRAIN("騎乗位") && CANTRAIN("肛門騎乗位")
			CALL SETFLAG, "地の文スキップ"
			PRINTFORMW 被腹部周围的压迫感弄醒睁開眼睛，%CALLNAME:TARGET%正在一心一意地摇摆着腰
			PRINTL 
			CALL PRINT_STRW, @"「唔……哈啊_BH_……啊，早上好，%CNAME("二人称")% _BH_」"
			PRINTFORMW 「嘿嘿嘿，怎么样？今天我找到了一款感觉很棒的游戏哦！」
			PRINTFORMW 「你看，虽然被街道裙遮住了……」
			PRINTFORMW 「猜猜看在阴部还是屁股里？」
			CALL PRINT_STRW, @"「如果猜错了……要怎么办呢_BH_」"
			CALL SIMULATE_SHOW_NEWDAY
		ELSE
			CALL PRINT_STRW, @"「嗯啊_BH_ 早上好_BH_ %CNAME("二人称")% _BH_」"
			CALL PRINT_STRW, @"「等等……马上就……射给你了_BH_」"
		ENDIF
	CASE "朝接吻"
		PRINTFORMW 「嗯……啾」
		PRINTFORMW 「……嘿嘿嘿，早上好，%CNAME("二人称")%」
	CASE "朝添い寝"
		PRINTFORMW 「嗯唔……哈啊……」
		PRINTFORMW 「……嗯嗯……再五分钟……」
		PRINTFORMW %CALLNAME:TARGET%感受着%CALLNAME:MASTER%的温暖，安心地睡着了。
	CASE "朝口交", "朝六九式"
		CALL PRINT_STRW, @"「嗞……嗞嘟嗞嘟，咕噜_BH_」"
		CALL PRINT_STRW, @"「……嗯，呼啊呼啊……呼啊嘿呼啊嘿嘿……嗞啦嗞啦_BH_」"
	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		CALL PRINT_STRW, @"「嗯唔……哈啊……好浓_BH_」"
		CALL PRINT_STRW, @"「嗯，舔舔……嘿嘿嘿_BH_ 非常美味哦，%CNAME("二人称")%_BH_」"
	CASE "朝騎乗位"
		CALL PRINT_STRW, @"「嘿嘿嘿……_BH_ 出了好多呢_BH_」"
		PRINTFORMW 「我还会再来抽取的，等着我哦！」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今天是体育节啊」
		PRINTFORMW 「希望是可以尽情玩耍的运动！」
	ELSE
		PRINTFORMW 「%CNAME("二人称")%！我们快点走吧！」
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")
		PRINTFORMW 「啊，对了！要考试了！虽然不太懂，但总之加油吧！」
	ELSEIF DAYEV("試験返却日")
		PRINTFORMW 「哎，考试成绩要出来了？」
		IF CALCF("試験の予想点数") >= 90
			PRINTFORMW 「没问题！这次很简单！」
		ELSEIF CALCF("試験の予想点数") >= 70 
			PRINTFORMW 「嗯～，因为和小酱用游戏复习过了，所以没问题！」
		ELSEIF CALCF("試験の予想点数") >= 50 
			PRINTFORMW 「那、那个，比起这个，%CNAME("一人称")%想快点玩游戏！」
		ELSE
			PRINTFORMW 「骗人，不敢相信！」
		ENDIF
	ELSEIF COND("朝袭相手")
		PRINTFORMW 「啊，要对大家保密哦！」
	ELSEIF COND("体育")
		PRINTFORMW 「体操服真凉快啊～」
	ELSEIF COND("水泳")
		PRINTFORMW 「因为和小酱都穿着泳衣，所以没问题！」
		PRINTFORMW 「……内褲？那个，我带了吗……」
	ELSE
		SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「游戏！游戏！想玩游戏！」
				PRINTFORMW 「啊，%CNAME("二人称")%的那份我也准备好了哦！」
			CASE 0
				PRINTFORMW 「今天和谁玩游戏？」
				PRINTFORMW 「%CNAME("一人称")%？%CNAME("一人称")%没有……想和纱南玩！」
			CASEELSE
				PRINTFORMW 「最近即使扭蛋运气不好也没関系！」
				PRINTFORMW 「下次一定会好运连连的！」
		ENDSELECT
	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
	PRINTFORMW 「板根蓝第一个到～」
	IF CEVENT("調教開始：初調教") == 0 || (TALENT:恋慕 == 0 && TALENT:淫乱 == 0)
		PRINTFORMW 「玩什么呢？游戏？还是学习？」
	ELSE
		PRINTFORMW 「玩什么呢？游戏？还是色色的事？」
	ENDIF
CASE "ただいま"
	PRINTFORMW 「我回来啦！」
	PRINTFORMW 「得玩新的游戏才行！」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「欢迎回来！今天又熬夜了！」
	ELSE
		PRINTFORMW 「欢迎回来！回来得有点晚呢」
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORMW 「叫我干嘛～？」
		PRINTFORMW 「要做什么呢？」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF TALENT:恋慕 || TALENT:淫乱 || TALENT:相思相愛
			PRINTFORMW 「啊！狡猾！%CNAME("一人称")%也要玩！」
		ELSE
			PRINTFORMW 「已经在玩了呀！玩什么呢」
		ENDIF
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「你好～！大家基本都到齐了呢～」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「%CNAME("二人称")%！你来了！」
	ELSE
		PRINTFORMW 「呀吼！嗯，还很宽敞呢！」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『啊！老师好！』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『你来晚了！要喝茶吗？』
	ELSE
		PRINTFORMW 『你好～！』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「叫我了吗？我现在正到関键时刻～」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「我回来啦！在做什么呢？」
	ELSE
		PRINTFORMW 「我渴了！还有零食吗？」
	ENDIF
;你と一緒に摆脱人はPARTNER()で参照できる。名前はCALLNAME:PARTNER()です
CASE "抜け出された"
	IF TALENT:恋慕 || TALENT:淫乱 || TALENT:相思相愛
		PRINTFORMW 「哎～！不要丢下我～！%CNAME("一人称")%也要一起去！」
	ELSE
		PRINTFORMW 「辛苦了！好～，今天也要练习～」
	ENDIF
CASE "便当"
	PRINTFORMW 「是！是便当！要是和小酱一起做就好了……」
	SIF CFLAG:料理経験 <= 10
		PRINTFORMW 「……等等，真的没问题吗？米是用水洗的而不是洗衣液，盐没有用糖代替吧……」
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	IF TALENT:恋慕 || TALENT:淫乱 || TALENT:相思相愛
		PRINTFORMW 「太好了！%CNAME("一人称")%也想做色色的事～」
	ELSE
		PRINTFORMW 「啊，那我要玩游戏了～」
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;动画ーションの関係上、１ターンに何度も呼ばれたりするのでフラグ操作をする場合には
;IF CFLAG:KOJO_DAILY済 == 0
;で分岐させるのが良いでしょう
;-------------------------------------------------
@KOJO_DAILY_K71, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「这里是活动室啊～，真不错呢！」
	PRINTFORML 「啊，有零食！而且还是很好吃的那种！」
	PRINTFORML 「应有尽有……从今天起这里就是%CNAME("一人称")%的营地了！」
CASE "浑身是汗"
	PRINTFORML 「好热……要被晒干了……」
	IF ABL:露出癖 >= 1
		PRINTFORML 「要不要脱掉呢……凉快一点比较好……」
	ELSE
		PRINTFORML 「冰在哪里……？没有吗？真不敢相信……！」
	ENDIF
CASE "刚睡醒"
	PRINTFORML 「唔……不要拿走游戏……」
	PRINTFORML %CALLNAME:TARGET%睡眼惺忪地伸出手。
	PRINTFORML 悄悄地把游戏机递了过去。
CASE "Cosplay中"
	SELECTCASE SHIRT(TARGET)
		CASE "女教師"
			PRINTFORML 「咳咳，開始上课了～」
			PRINTFORML %CALLNAME:TARGET%扶了扶眼镜，拿起教鞭
			PRINTFORML 「2个布丁加1个冰棒，一共是几个呢～！」
			PRINTFORML 「……呐，有在听吗——？」
		CASE "和服"
			PRINTFORML 「是七五三节和正月的时候穿的吧！」
			PRINTFORML %CALLNAME:TARGET%漫不经心地挥舞着袖子。
			PRINTFORML 「嗯——……突然想去参拜了呢」
			PRINTFORML 「呐%CNAME("二人称")%！……是哪里来着，莲台野？我们去吧！」
		CASE "女仆装"
			PRINTFORML 「锵锵，是女仆装哦！」
			PRINTFORML 「怎么样？很可爱吧？很适合我吧？」
			PRINTFORML %CALLNAME:TARGET%拿着拖把模仿着打扫地板的样子。
			IF TALENT:恋慕
				PRINTFORML 「嘿嘿，只为%CNAME("二人称")%一个人服务的女仆桑哦！」
			ELSE
				PRINTFORML 「嘿嘿，好像变成了真正的女仆桑一样！」
			ENDIF
		CASE "体操服"
			PRINTFORML 「明明不做运动，这样真的好吗？」
			PRINTFORML %CALLNAME:TARGET%歪着頭看着这边。
			PRINTFORML 「明明偶尔会看到的，真奇怪——」
			PRINTFORML 「……虽然确实很方便活动啦，但是要更有主人样！对吧？」
		CASE "护士服"
			PRINTFORML 「治疗时间到喽——！」
			PRINTFORML %CALLNAME:TARGET%不知道从哪里拿来了一个巨大的注射器。
			PRINTFORML 「诶？这个？在保健室找到的！是用来干什么的呢？」
			PRINTFORML 「别说那么多了，快躺下——！」
			IF TALENT:恋慕
				PRINTFORML 「……就算你在我不知道的地方倒下，我也会立刻赶到的哦？」
			ELSE
				PRINTFORML 「呐呐，哪里不舒服吗——？」
			ENDIF
		CASE "旗袍"
			PRINTFORML 「哎呀——！Spinning……什么什么的！」
			PRINTFORML %CALLNAME:TARGET%開心地模仿着格斗游戏的样子。
			PRINTFORML 「活动起来很方便呢！我好喜欢！」
			PRINTFORML 「……但是为什么前后都要垂着布呢？」
		CASE "水手服"
			PRINTFORML 「和平时不一样的制服也不错呢——」
			PRINTFORML %CALLNAME:TARGET%拍打着袖子。
			PRINTFORML 「不过袖子是不是有点长啊」
			PRINTFORML 「那个——……嘿嘿，是幽灵桑哦——！」
		CASE "巫女"
			PRINTFORML 「祓魂净土——」
			PRINTFORML %CALLNAME:TARGET%挥舞着御币。
			PRINTFORML 「Good Luck！向勇往直前的%CNAME("二人称")%致敬！」
			PRINTFORML 「虽然不能成为真正的巫女桑，但是祈祷什么的还是可以做到的哦！」
		CASE "情趣内衣"
			PRINTFORML 「这是什么啊，也太透了吧！」
			PRINTFORML %CALLNAME:TARGET%疑惑地看着这边。
			PRINTFORML 「诶，这也是睡衣的一种吗？」
			PRINTFORML 「嘿诶——，原来还有这么奇特的睡衣啊——」
		CASE "婚纱礼服"
			PRINTFORML 「哇……好漂亮啊」
			PRINTFORML %CALLNAME:TARGET%活动着身体看着衣服。
			IF TALENT:相思相愛
				PRINTFORML 「……这样随时都可以举行仪式了呢」
			ELSE
				PRINTFORML 「嘿嘿，今天是新娘桑哦——！」
			ENDIF
		CASE "哥特蘿莉礼服"
			PRINTFORML 「哇ー！好轻飘飘的呀ー！」
			PRINTFORML %CALLNAME:TARGET%在镜子面前挥舞着裙摆。
			PRINTFORML 「居然还有这样的服装呢！真羞人ー！」
			PRINTFORML 「啊！对了！」
			PRINTFORML 她猛地一转身，提起裙摆行了个礼。
			PRINTFORML 「看，是不是很有大小姐的感觉！怎么样？很棒吧！」
		CASE "啦啦队制服"
			PRINTFORML 「啊，这不是啦啦队的衣服嘛！」
			PRINTFORML %CALLNAME:TARGET%一边拿着绒球，一边摇来摇去。
			PRINTFORML 「那个，该怎么做来着……」
			PRINTFORML 「加油！加油！要努力哦！要努力哦！……这样对吗？」
		CASE "睡衣"
			PRINTFORML 「这睡衣好舒服呀ー！花样我也很喜欢！」
			PRINTFORML 「而且……感觉很适合躺着玩游戏呢！」
			PRINTFORML %CALLNAME:TARGET%像是想到了什么，拍了一下手。
			PRINTFORML 「对了！我们一起躺着玩游戏怎么样？」
			PRINTFORML 「在同一个地方排排躺着一定很有趣！」
		CASE "怪獣睡衣"
			PRINTFORML 「嗷呜ー！我要吃了你ー！」
			PRINTFORML 「嘿嘿，这个真的很可爱，又软乎乎的！」
			PRINTFORML %CALLNAME:TARGET%一边抓着袖子，一边把帽子摇来摇去。
			PRINTFORML 「虽然我不是懒虫，要一起睡觉觉吗ー？」
		CASEELSE
			PRINTFORML 「怎么样？很适合我吧ー？」
			PRINTFORML %CALLNAME:TARGET%挥舞着手臂，转着圈展示自己。
			PRINTFORML 「%CNAME("一人称")%也是可以转职什么的！」
		ENDSELECT
CASE "点心の感想"
	;今日の点心を用意したのは自分
	IF COND("点心当番")
		PRINTFORML 「好不容易做好了！来！吃吃看！」
	;今日の点心は誰かの亲手自制
	ELSEIF COND("亲手自制点心")
		;今日の点心当番の人の亲手自制を初めて食べた
		IF DEVENT(@"%TEXTS("点心当番")%の亲手自制") == 0
			;点心当番が[料理擅長]
			IF COND("点心の作り手が料理擅長")
				PRINTFORML 「好吃！感觉能吃个不停！」
				PRINTFORML 「还想再吃一点……不行吗？」
				PRINTFORML ……%CALLNAME:TARGET%好像还想吃这边的那份，不停地看过来。
					SIF TALENT:淫乱
						PRINTFORML 「要不……%CNAME("二人称")%的鸡鸡，让我吃一口？」
			ELSE
				PRINTFORML 「美味的点心是游戏的源泉！」
			ENDIF
		ELSE
			IF COND("点心の作り手が料理擅長")
				PRINTFORML 「果然还是好吃啊……再来一份！」
			ELSE
				PRINTFORML 「这样免费吃点心真的好吗？」
			ENDIF
		ENDIF
	;点心が亲手自制でない
	ELSE
		PRINTFORML 「啊，点心出现啦！我要開动啦，%CNAME("二人称")%！」
	ENDIF
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K71
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「嘿嘿……好開心！」
CASE "约会的约定"
	IF TALENT:恋慕
		SELECTCASE RAND:3
			CASE 0
