;-------------------------------------------------
;各種打招呼
;ここではTARGETではなくARG番の角色で呼ばれる。CNAMEの代わりにCNAMETを使う事
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でCALL EMOTION_PLUS, ARGS, ARGとすることで変える事が出来ます。
;ARGSに"甜言蜜语", "夸奖", "心情差", "躁动难忍", "交流中", "惊嚇"を入れる事で対応したCFLAGがON变成了状態の颜绘が表示されます。ARGは変更しないでください。
;-------------------------------------------------
@KOJO_GREETING_K49, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		CALL SETFLAG, "变成内衣姿态", TARGET
	CASE "朝舐陰"
		CALL SETFLAG, "变成内衣姿态", TARGET
	CASE "朝足交"
		CALL SETFLAG, "变成内衣姿态", TARGET
	CASE "朝の新妻PLAY"
		CALL SETFLAG, "变成内衣姿态", TARGET
	CASE "朝乳交"
		CALL SETFLAG, "变成内衣姿态", TARGET
	CASE "朝騎乗位"
		CALL SETFLAG, "地の文スキップ"
		CALL SETFLAG, "变成内衣姿态", TARGET
		PRINTFORMW 「ん……ふ、ぅ……ぁっ……んっ……」
		PRINTFORMW 快感に睁開眼睛と%CALLNAME:TARGET%が%CALLNAME:MASTER%の上に跨っていた。
		CALL K49_NewDay_Cowgirl
		PRINTFORMW 「ん…ふ、あっ……起こし、ちゃった……？ごめんね？」
		PRINTFORMW 「ホントは%CNAME("二人称")%の寝顔を見てるだけで良かったんだけれどちょっと、ね？」
		PRINTFORMW そう言うなら最後までしろ、とそう言う意味で下から突き上げる。
		CALL PRINT_STRW, @"「────っ_HH_然う、だったね……」"
		CALL PRINT_STRW, @"「ぃっ_HH_んっ……ぁっ_HH_はぅっ……_HH_」"
		PRINTFORMW 甜吐息を漏らしながら%CALLNAME:TARGET%通过%CALLNAME:MASTER%への奉仕を再開した……
		CALL SIMULATE_SHOW_NEWDAY
		CALL PRINT_STRW, @"「んはぅ……_HH_はぁ……_HH_はぁ……_HH_んふふ……」"
		CALL KOJO_GREETING, "打招呼", TARGET
	CASE "朝Ａ騎乗位"
		CALL SETFLAG, "变成内衣姿态", TARGET
	CASE "朝接吻"
		CALL SETFLAG, "变成内衣姿态", TARGET
	CASE "朝添い寝"
		CALL SETFLAG, "地の文スキップ"
		CALL SETFLAG, "变成内衣姿态", TARGET
		PRINTFORMW 髪を梳くように温柔的撫でられる感覚がする……
		PRINTFORMW その手付きは幼い頃に母に撫でられていたアレに似ていて、
		PRINTFORMW とても心地良く、このまま曖昧な目覚めを睡眠にしても良いのじゃないかと思うモノだった。
		PRINTFORMW けれども、予定があった様な気がして睁開眼睛。
		PRINTFORMW 「ん……、起きたかい？」
		PRINTFORMW ぼやけた視界に移ったのは真っ白な%CALLNAME:TARGET%だった。
		PRINTFORMW 「何となくね、%CNAME("二人称")%の寝顔を見たくて、早くに来ちゃったんだ」
		PRINTFORMW 「気持ち良さそうに寝てたから、起こさない様にしてたのだけれど」
		PRINTFORMW 「でも、頭撫でたくなっちゃったから……つい、ね？」
		PRINTFORMW 悪戯が成功したかの様に楽し気に笑着から
		CALL KOJO_GREETING, "打招呼", TARGET
	CASE "朝口交", "朝六九式"
		CALL SETFLAG, "变成内衣姿态", TARGET
	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		IF EXP_GAIN("射精経験", PLAYER)
			PRINTFORMW 「んく……んく……ふぅ……」
			PRINTFORMW 「ふふっ……朝からいっぱい射精したね？」
			PRINTFORMW 「私の奉仕が気持ち良かったのだったら……」
			PRINTFORMW 「シて良かったって、少し安心してるよ」
		ENDIF
		PRINTFORMW 涎と精液で弄脏的顔を拭い
		PRINTFORMW 「改めて、早上好」
		PRINTFORMW 「今日も%CNAME("二人称")%にとって良い一日だと良いね」
		PRINTFORMW %CALLNAME:TARGET%通过そう%CALLNAME:MASTER%に微笑みかけた。
	CASE "朝騎乗位"
		IF EXP_GAIN("射精経験", PLAYER)
			PRINTFORMW 「舒服吗？」
			PRINTFORMW 「なんて聞くのは無粋だったかな？」
			PRINTFORMW 「有がとね」
		ENDIF
		PRINTFORMW 乱れた衣服を整えてから白く長髪を轻轻地ふわりとさせて、
		PRINTFORMW 「改めて、早上好」
		PRINTFORMW 「今日も%CNAME("二人称")%にとって良い一日だと良いね」
		PRINTFORMW %CALLNAME:TARGET%通过そう%CALLNAME:MASTER%に微笑みかけた。
	CASE "朝添い寝"
		CALL EMOTION_PLUS, "重置/微笑/红晕/对话框爱心"
		PRINTFORMW 「早上好、今日も%CNAME("二人称")%にとって良い日で有ますように」
		PRINTFORMW いつも街道の打招呼をしてきた……
	CASEELSE
		PRINTFORMW 「ん、早上好」
	ENDSELECT
	;何か発言してたらここまで

	IF COND("周日")
		PRINTFORMW 「然う言えば、今日は安息日だね」
		IF COND("约会的约定")
			PRINTFORMW 「逢引きの準備は万端だよ」
			CALL PRINT_STRW, @"「……楽しみにしてたんだよ？……悪い？_／／／_」" 
			CALL EMOTION_PLUS, "害羞", TARGET
			PRINTFORMW %CALLNAME:TARGET%通过少しだけ照れ臭そうにそっぽ向いた。
		ELSEIF FIND_COND("约会的约定")
			PRINTFORMW 「確か他の子と約束が在るのだっけ？」
			PRINTFORMW 「羨ましい、なんて感情は少し前まで思わなかったけれど」
			PRINTFORMW 「今は然う思ってしまうね」
			PRINTFORMW 「だから、出掛けるまで%CNAME("二人称")%を独り占めさせてほしいな」
		ELSE
			PRINTFORMW 「今日は何か予定はないのかい？」
			PRINTFORMW 「だったら、私と少し出掛けないかい？」
			PRINTFORMW 「まぁ、%CNAME("二人称")%さえ良ければ、だけれどね」
		ENDIF
	ELSEIF CEVENT("学校に通う死神")
		PRINTFORMW 「朝餉と学校の準備はしておいたから一緒に行こうね？」
	ELSE
		PRINTFORMW 「今日も良い一日だと良いね？」
	ENDIF
	IF COND("体育")
		PRINTFORMW 「今日は体育があったでしょ？」
		PRINTFORMW 「用意して在るから忘れないでね？」
	ENDIF
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1
CASE "一番乗り"
	PRINTFORML 「おや、%CNAME("一人称")%が一番乗りかい？」
	PRINTFORMW 「死神が一番最初に来るのは死と切り離せない%CNAME("二人称")%らしいね」
CASE "ただいま"
	PRINTFORML 「ただいま」
	PRINTFORML 「今、帰ったよ」
	PRINTFORMW 「……うん、有がと」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:恋慕
			PRINTFORML 「あっ……おかえり」
			PRINTFORML 「……少しだけ寂しかったって言うと」
			PRINTFORMW 「面倒臭いって思うかな？」
		ELSE
			PRINTFORMW 「やぁ、おかえり」
		ENDIF
	ELSEIF COND("昼")
		IF RAND:2 == 0
			PRINTFORMW 「やぁ、今日は%CNAME("一人称")%の方が先だったね」
		ELSE
			PRINTFORML 「やぁ、駆け付け三杯とは言わないけれど」
			PRINTFORMW 「%CSTR:茶%でも如何だい？」
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORML 「ととっ、ごめんね」
		IF RAND:3 == 0
			PRINTFORML 「少し周りをふらふらしてたんだ」
		ELSEIF RAND:2 == 0
			PRINTFORML 「ほら同級の彼の子に少し呼び止められていたんだ」
		ELSE
			PRINTFORML 「実は統括機体の子から緊急の業務報告受けてたんだ」
		ENDIF
		PRINTFORMW 「%CNAME("二人称")%に手間かけさせちゃったね」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF RAND:3 == 0
			PRINTFORML 「やぁ………ふぅ～ん」
			PRINTFORMW じとーっとした瞳で%CALLNAME:TARGET%通过%CALLNAME:MASTER%を看了过来。
		ELSEIF RAND:2 == 0
			PRINTFORML 「やぁ……成る程」
			PRINTFORML 「成る程成る程成る程」
			PRINTFORMW 笑みを作って頷いているが%CALLNAME:TARGET%の瞳は虹色を帯び全く笑着いない……
		ELSE
			PRINTFORML 「やぁ…っと、如何やらお邪魔みたいだね？」
			PRINTFORMW 就这様ドアを開けて出て行こうとする%CALLNAME:TARGET%を止めて何とかなった。
		ENDIF
	ELSE
		PRINTFORML 「やぁ、您好」
		IF RAND:3 == 0
			PRINTFORML 「少し散步してから来たから%CNAME("二人称")%の方が先だったね」
		ELSEIF RAND:2 == 0
			PRINTFORML 「少し同級の彼の子と話しててね、遅れてしまったね」
		ELSE
			PRINTFORML 「統括機体の子から緊急の業務報告を受けてたから少し遅れちゃった」
		ENDIF
		PRINTFORMW そう言いながら%CALLNAME:TARGET%通过指定席の様に自然な流れで%CALLNAME:MASTER%の隣に座った。
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORML 「やぁ、您好」
		PRINTFORMW 「ととっ、もう勢ぞろいだね」
	ELSEIF CFLAG:呼び出された
		PRINTFORML 「ととっ、您好」
		PRINTFORMW 「ごめんね、%CNAME("二人称")%に手間かけさせたみたいだね」
	ELSE
		PRINTFORML 「やぁ、您好」
		PRINTFORMW 轻轻地打招呼した後、%CALLNAME:TARGET%通过%CALLNAME:MASTER%の横に座った。
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("入室打招呼中","诺伊")
		IF DEVENT("诺伊を認識") == 0
			PRINTFORML 『機体を検知、ΔΑ-Kyrios-06……』
			PRINTFORML 『検索、検索完了。識別名称诺伊.德米那』
			PRINTFORML 『まさか某个畜生から離反した機体が教職をやっているとはね……』
			CALL SET_DEVENT, "诺伊を認識", TARGET
		ELSE
			PRINTFORML 『おや、%CSVCALLNAME(NO:ACTOR)%じゃないか』
		ENDIF
		PRINTFORML 『おっと、学校では%CSVCALLNAME(NO:ACTOR)%老师だったね』
		PRINTFORMW 『改めて、您好』
	ELSEIF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『%CSVCALLNAME(NO:ACTOR)%老师、您好』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『やぁ、%CSVCALLNAME(NO:ACTOR)%。少し遅れたようだね』
	ELSE
		PRINTFORMW 『やぁ、%CSVCALLNAME(NO:ACTOR)%。您好』
	ENDIF
CASE "居間に現れた"
	IF CFLAG:呼び出された
		IF TALENT:恋慕
			PRINTFORML 「ん、%CNAME("一人称")%に何か用かい？」
			PRINTFORMW 「何て、用が無くても%CNAME("二人称")%に呼び出されたら来るけれどね？」
		ELSE
			PRINTFORMW 「ん、呼ばれてやって来たけれど、何か用でも在るのかな？」
		ENDIF
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「ん……戻ったよ」
	ELSE
		IF COND("夜")
			IF TALENT:恋慕
				PRINTFORMW 「やぁ、晚上好。良い夜だね」
			ELSE
				PRINTFORMW 「ん……。おや、晚上好」
			ENDIF
		ELSE
			IF TALENT:恋慕
				PRINTFORMW 「やぁ、您好。今日も良い日になるといいね」
			ELSE
				PRINTFORMW 「ん……。おや、您好」
			ENDIF
		ENDIF
	ENDIF
CASE "抜け出された"
	IF TALENT:恋慕
		PRINTFORML 「然うかい、然うかい、然うなのかい」
		PRINTFORMW %CALLNAME:TARGET%の少し膨れた顔が見えた気がする。
	ELSE
		PRINTFORMW 「おや……、ふむ……。いってらっしゃい」
	ENDIF
CASE "便当"
	IF TALENT:恋慕
		PRINTFORML 「是、便当だよ」
		IF DEVENT("便当同伴初めて") == 0
			PRINTFORML 「ん……？%CNAME("一人称")%もかい？」
			PRINTFORMW 「ちょっと待っててね、購買部で何か買ってくるから」
			CALL SET_DEVENT, "便当同伴初めて", TARGET
		ELSE
			PRINTFORML %CALLNAME:MASTER%に便当を渡した%CALLNAME:TARGET%通过横にちょこんと座り、
			IF RAND:3 == 0
				PRINTFORML 渡された便当とは違う内容の便当を広げた。
				PRINTFORMW 「斯うやって違う内容の便当だったら、交換とか出来るから少しだけ頑張ったんだ」
			ELSEIF RAND:2 == 0
				PRINTFORML 「じゃあ、いただきます。ふふっ♪」
				PRINTFORMW 何故だか%CALLNAME:TARGET%通过朗らかに笑着いる。
			ELSE
				PRINTFORML 「あーんって言うのをやってみたいんだけれど……ダメ、かな？」
				PRINTFORML %CALLNAME:TARGET%通过便当のオカズを箸で取って%CALLNAME:MASTER%に差し出してきた。
				PRINTFORMW 少し気恥ずかしいが差し出されたオカズを食べた。
			ENDIF
		ENDIF
	ELSE
		PRINTFORML 「是、頼まれてた便当だよ」
		IF DEVENT("便当同伴初めて") == 0
			PRINTFORML 「ん……？%CNAME("一人称")%もかい？」
			PRINTFORMW 「ちょっと待っててね、購買部？って場所で何か買ってくるから」
			CALL SET_DEVENT, "便当同伴初めて", TARGET
		ELSEIF RAND:3 == 0
			PRINTFORML 「では、いただきます」
			PRINTFORMW 「……此の食べ物に感謝するのって嫌いじゃないな」
		ELSEIF RAND:2 == 0
			PRINTFORML 「味の方は如何だい？」
			PRINTFORMW 「%CNAME("二人称")%の口に合えば良いんだけれど……」
		ELSE
			PRINTFORMW 「斯うやって一緒に吃飯をするって、存外心地いいね」
		ENDIF
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	IF TALENT:恋慕
		PRINTFORML 「ふぅ～ん……ふぅ～ん……」
		PRINTFORMW 気のせいだが%CALLNAME:TARGET%に若干半目で睨まれている気がする……
	ELSE
		PRINTFORML 「お盛んだねぇ……」
		PRINTFORMW %CALLNAME:TARGET%通过苦笑している。
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K49, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「ん、此から厄介になるね」
CASE "浑身是汗"
	IF DEVENT("一言浑身是汗：はじめて") == 0
		PRINTFORML 「…………なんで某个畜生はこんな機能を追加したのだろ？」
		PRINTFORML %CALLNAME:TARGET%通过若干心情差そうな顔をしている。
		CALL SET_DEVENT, "一言浑身是汗：はじめて"
	ELSE
		IF RAND:3 == 0
			PRINTFORML 「斯う暑いと氷菓子が欲しくなるねぇ」
			PRINTFORML ……最近冰のゴミが増えたのは%CALLNAME:TARGET%の所為だったのか……
		ELSEIF RAND:2 == 0
			PRINTFORML 「冷房って便利だねぇ」
			PRINTFORML 「今までは暑さとか寒さを感じなかったから、%CNAME("二人称")%達の文明に感謝だよ」
		ELSE
			PRINTFORML 「よく太陽が憎いって言葉を聞いたけれど斯う言う事だったんだね……」
			PRINTFORML 「確かに此処まで不快指数が高いと憎くなっても仕方ないね」
		ENDIF
	ENDIF
CASE "刚睡醒"
	IF CFLAG:囁き悪戯
		CALL EMOTEFACE, "斜眼４飘忽/困扰眉/小口/紅潮/頬手"
		CALL PRINT_STRL, @"「………確信犯なのは分かってるけれど_／／／_」"
		PRINTFORML 「まぁ……悪い気は不会」
		CALL PRINT_STRL, @"「けれど、出来れば面と向かって言ってほしいな……_／／／_」"
	ELSE
		PRINTFORML 「ん……保全作業完了」
		IF CHOSE_DEVENT("睡眠姦Ｖ中出し")
			IF TALENT:恋慕
				CALL EMOTEFACE, "斜眼４/微笑３/紅潮"
				CALL PRINT_STRL, @"「ん……_HH_ねぇ、もっと欲しいなって言ったら、してくれる？」"
				PRINTFORML 少し上気した顔で熱っぽく%CALLNAME:TARGET%通过%CALLNAME:MASTER%を看了过来。
				PRINTFORML 【%CALLNAME:TARGET%の欲求が深まった】
				BASE:欲求不満 += 50
			ELSE
				CALL EMOTEFACE, "笑颜２/会話口/困扰眉/紅潮"
				PRINTFORML 「まったく……%CNAME("二人称")%は仕方がないなぁ」
				PRINTFORML 少し呆れつつも%CALLNAME:TARGET%通过少し笑みをつくっている。
			ENDIF
		ELSEIF CHOSE_DEVENT("睡眠姦口交")
			IF CHOSE_DEVENT("睡眠姦顔射")
				IF TALENT:恋慕
					CALL EMOTEFACE, "斜眼４/通常眉/紅潮"
					PRINTFORML 「顔にかけるだけで満足しちゃったんだ……」
					PRINTFORML なんだか%CALLNAME:TARGET%通过少し不満気の様だ……
					PRINTFORML 【%CALLNAME:TARGET%の欲求が少し深まった】
					BASE:欲求不満 += 15
				ELSE
					CALL EMOTEFACE, "笑颜２/会話口/困扰眉/紅潮"
					PRINTFORML 「うーん、酱と拭いて呉れたみたいだけれど、臭いが凄いよ？」
					PRINTFORML %CALLNAME:TARGET%の光輪が少し光を強めた後、精子の臭いが無くなった。
				ENDIF
			ELSEIF CHOSE_DEVENT("睡眠姦精飲")
