;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K11, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR
#DIMS LUNCHBOX

MEMO_LINECOUNT = LINECOUNT

$KOJO_K11_GREETING_LP1

SELECTCASE ARGS
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝の新妻PLAY"
		;それ以外
		SELECTCASE RAND:10
			CASE 0
				CALL PRINT_STR, @"「朝から莉雅拉酱食べ放題_H_　ですねー……_H_」_W"
			CASE 1
				CALL PRINT_STR, @"「もぉ%TEXTR("……/＿")%_H_　珍しく莉雅拉酱が朝ごはん作ってたのに……」_W"
				CALL PRINT_STR,  "「肚子里面满满的……_H_　已经没法做了呀……_HH_」_W"

		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0
		GOTO KOJO_K11_GREETING_LP1
	CASE "朝騎乗位"
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:10
				CASE 0
					CALL PRINT_STR, @"「%TEXTR("哈/啊呜/啊")%……_HH_　早上開始就%TEXTR("很有精神/硬邦邦")%的呢……_H_」_W"
					CALL PRINT_STR, @"「请……_HH_　%TEXTR("满满地/大量地/尽情地")%喷射_H_　出来捏……_HH_」_W"
				CASE 1
					CALL PRINT_STR, @"「%TEXTR("啊唔/噫咕/呼嗯")%っ_H_　唔、唔唔、嗯っ_H_」_W"
					CALL PRINT_STR,  "「非、非常抱歉……っ_H_　是、想着普、普通地_H_　叫你起床的哟？_H_」_W"
					CALL PRINT_STR, @"「但是%CNAME("二人称")%的%TEXTR("这个/这里")%……_H_　%TEXTR("看起来很辛苦的样子/看起来很好吃的样子")%……_H_」_W"
					CALL PRINT_STR,  "「没用办法……忍耐了、啊……っ_HH_」_W"
				CASE 2
					CALL PRINT_STR, @"「%CNAME("一人称")%……最近、%TEXTR("稍微/有一点点")%%TEXTR("任性了/占有欲太强了")%……」_W"
					CALL PRINT_STR, @"「朝一番の%TEXTR("孕みやすそうな/こってり濃厚的/どっろどろの")%%CNAME("精液")%はぁ……%TEXTR("他の娘に渡したくない/自己堪能したい")%んですぅ……_H_」_W"
					CALL PRINT_STR,  "「所以、请就这样在里面……_H_　浓厚地射出来……_HH_HH_HH_」_W"
			ENDSELECT
		ENDIF
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0

		;それ以外
		SELECTCASE RAND:10
			CASE 0
				CALL PRINT_STR,  "「早上開始就这样有精神……不愧是雄性呢……_H_」_W"
				CALL PRINT_STR, @"「我慢しないで……んっ_H_　どろっどろな%CNAME("精液")%、%CALLNAME:TARGET%老师のナカに……ぁんっ_HH_HH_」_W"
				CALL PRINT_STR, @"「……啊_H_　暖暖的……_HH_」_W"
			CASE 1
				CALL PRINT_STR,  "「啊_H_　早上好っ_H_」_W"
				CALL PRINT_STR,  "「嗯っ_H_　不是啦、只是看起来、感觉那个っ_H_　攒了很多的样子_H_　所以_HH_」_W"
				CALL PRINT_STR, @"「总之想让%CALLNAME:TARGET%酱_H_　把精液_H_　榨出来吧_H_」_W"
				CALL PRINT_STR,  "「这也是一个好妻子的、义务……哟っ_HH_　啊嗯っ_HH_」_W"
		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0
		GOTO KOJO_K11_GREETING_LP1
	CASE "朝口交", "朝六九式"
		; 恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:10
				CASE 0
					CALL PRINT_STR, @"「%TEXTR("嗯っ/唔呜/哈/哈啊")%_HH_　早晨%TEXTR("最早榨出来的精液/最浓厚的牛奶")%、多谢款待_HH_」_W"
					CALL PRINT_STR,  "「要一起、吃早饭吗？」"
				CASE 1
					CALL PRINT_STR, @"「早上好%TEXTR("、亲爱的/＿")%_H_　嗯っ_H_　%CALLNAME:MASTER%跟%TEXTR("这里/肉棒君/大鸡鸡")%不一样、是喜欢睡懒觉的孩子呢……_H_」_W"
				CASE 2
					CALL PRINT_STR, @"「这样子用%TEXTR("口交/嘴")%%TEXTR("叫起来/弄醒")%、作为男孩子有什么感想吗？」_W"
					CALL PRINT_STR, @"「如果很喜欢的话……%CNAME("一人称")%这么早起来也是有价值的呢_HH_」_W"
				CASE 3
					CALL PRINT_STR,  "「早安、主人大人_HH_　今天也是一样地这么有精神、嗯、真是太好了_H_」_W"
					CALL PRINT_STR,  "「……呼呼_H_　这样的PLAY喜欢吗？」_W"
			ENDSELECT
		ENDIF
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0

		; それ以外
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR, @"「%TEXTR("えへへ/あは")%_H_　%CNAME("二人称")%の刚睡醒%CNAME("精液")%、%CALLNAME:TARGET%酱が堪能しちゃいました_H_」_W"
				CALL PRINT_STR, @"「早晨開始就能享受到%TEXTR("这么浓的/这么浓厚的/这样黏黏的")%%TEXTR("量又大的/这么大量的/快要溢出来的")%精液、%CALLNAME:TARGET%酱大满足……_HH_」_W"
			CASE 1
				CALL PRINT_STR, "「欸嘿嘿……早上好_H_」_W"
				CALL PRINT_STR, @"「因为看起来存了很多在里面的样子%TEXTR("、一不小心就想偷吃了/就想用嘴帮你弄出来")%♪」_W"
			CASE 2
				CALL PRINT_STR, "「嗯……咕噜……_HH_」_W"
				CALL PRINT_STR, "「欸嘿嘿……早上就能喝到这样的浓厚的饮品、不用吃早饭也没有问题的啦……_HH_」_W"
		ENDSELECT

		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0
		GOTO KOJO_K11_GREETING_LP1
	CASEELSE
		RETURN 0
	ENDSELECT
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝六九式"

	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1

$KOJO_K11_GREETING_LP2
	SELECTCASE RAND:30
		CASE  0
			PRINTFORMW 「早安♪　今天也一定会是%TEXTR("美好的/不错的")%一天呢」
		CASE  1
			LOCAL = FIND_COND("同居中")
			SIF LOCAL <= 0
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「今天的早餐是%CALLNAME:LOCAL%酱%TEXTR("亲手制作的哦/精心准备的哦")%~」
			PRINTFORMW 「……诶？　问为什么不是%CNAME("一人称")%做的？」
			IF TALENT:恋慕
				PRINTFORMW 「那、那个、%TEXTR("如、如果下次还有机会的话/下次肯定会做的")%……」
			ELSE
				PRINTFORMW 「嗯呼。那下次就让%CALLNAME:TARGET%酱为%CNAME("二人称")%制作特制的早餐吧～」
				CALL PRINT_STR, "「敬请期待！_HH_」_W"
			ENDIF
		CASE  2
			SIF TALENT:恋慕
				GOTO KOJO_K11_GREETING_LP2
			CALL PRINT_STR, @"「今早是%CALLNAME:TARGET%酱%TEXTR("特製的/手制的/满含爱意地做的")%早餐哟_H_」_W"
			CALL PRINT_STR, "「会啊～_HH_ 地喂给你吃的　、快点起来吧」_W"
		CASE  3
			SIF !TALENT:恋慕
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「那个……早餐、做好了哟？」
			PRINTFORMW 「希望能合%CNAME("二人称")%的口味……」
		CASE 10
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「今天是有体育课的日子捏？　%TEXTR("呼呼/诶嘿嘿~")%、已经做好充足的准备了哟♪」
		CASE 11
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「偶尔%CNAME("一人称")%也会混到体育课的队伍之中哦？」
			PRINTFORMW 「这样、对周围进行认知阻碍的话。反正对%CNAME("二人称")%来说这也是听不懂的东西捏」
		CASE 12
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「%CNAME("二人称")%喜欢泳池之类的地方吗？」
			PRINTFORMW 「泳衣的颜色啊设计样式啊什么的……想问一下是不是有什么讲究？」
		CASE 13
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「喜欢上体育课吗？　%TEXTR("呼呼/嘛")%、如果不是很讨厌就好」
			PRINTFORMW 「%TEXTR("两人一起做“运动”/两个人在床上缠在一起做体操")%什么的充沛的体力还是必要的呢」
			PRINTFORMW 「……特别是%CNAME("二人称")%、有那么多的女孩子等着　呼呼、加油哦♪」
		CASE 14
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「%CNAME("一人称")%穿着体操服的样子……这样说着、这么想看穿着布鲁马的样子吗？」
			IF TALENT:恋慕
				PRINTFORMW 「诶多、那个……co、cosplay的话什么时候都行哦？」
				PRINTFORMW 「运动之后……去洗澡、然后就做……对吧？」
				CALL PRINT_STR, @"「浑身是汗也可以？　讨、讨厌、真是%TEXTR("色狼老师/变态老师")%会说的话呢……_H_」_W"
			ELSE
				PRINTFORMW 「%TEXTR("姆呼~/嗯呼~")%♪　啊啦啊啦、满脸都写着期待的表情的孩子呢~♪」
			ENDIF
		CASE 20
			SIF BASE:PLAYER:絶頂 * 3 >= MAXBASE:PLAYER:絶頂
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「%TEXTR("恩呼呼/啊啦啊啦/啊啦~")%、还在睡懒觉啊♪」
			PRINTFORMW 「喂、快点起床啦」
		CASE 21
			SIF BASE:PLAYER:絶頂 * 2 < MAXBASE:PLAYER:絶頂
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「%TEXTR("啊啦~/嘛～嘛~/唔呼呼")%♪　这边也这么早起来了啊♪」
			PRINTFORMW 「如果还有时间能再稍微多一点的话……真是可惜」
	CASEELSE
		GOTO KOJO_K11_GREETING_LP2
	ENDSELECT

CASE "一番乗り"

CASE "ただいま"

CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR, "「欢迎回家~♪」_W"
			CASE 1
				CALL PRINT_STR, "「迟到了呢……都等得有点累了」_W"
			CASE 2
				CALL PRINT_STR, "「想要一个回家的啾～_H_」_W"
			CASE 3
				CALL PRINT_STR, "「即使是稍微迟到了一小会也是不行的哦？」_W"
		CASEELSE
			GOTO KOJO_K11_GREETING_LP1
		ENDSELECT
$KOJO_K11_GREETING_LP3
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR, "「要先吃饭？　还是先洗澡？　还是说……_H_」_W"
				CALL PRINT_STR, "「唔呼呼♪」_W"
			CASE 1
				CALL PRINT_STR, "「饭菜已经准备好了、一起吃吗？」_W"
		CASEELSE
			GOTO KOJO_K11_GREETING_LP3
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 「%TEXTR("唔呼呼/诶嘿嘿")%、久等了哦～♪」
				PRINTFORMW 「交给能干的%CALLNAME:TARGET%酱的话、%TEXTR("整理被子/准备零食")%这种事情当然是做得非常完美的啦」
			CASE 1
				PRINTFORMW 「%TEXTR("欢迎光临/已经久候多时了呢")%～♪」
				PRINTFORM 「欢迎来到%CALLNAME:TARGET%酱和
				IF CHARA_NUM("室内の邪魔者") > 4
					PRINTFORM 其他女孩子们
				ELSEIF CHARA_NUM("室内の邪魔者") > 2
					PRINTFORM 其他几个女孩子
				ELSE
					PRINTFORM 另一个的女孩
				ENDIF
				PRINTFORMW 精心准备的爱巢~♪」
			CASE 2
				PRINTFORMW 「真是的、%TEXTR("太慢了啦/迟到了哟/等了很久了啦")%」
				PRINTFORMW 「为了证明%CALLNAME:MASTER%的诚意、作为惩罚、%CALLNAME:MASTER%要听从%CNAME("一人称")%一个要求哦」
				PRINTFORMW 「……当然、愿意接受更加严厉的惩罚的话也是很欢迎的哦？」
			CASE 3
				PRINTFORMW 「已经恭候多时了、%TEXTR("亲爱的/主人大人")%」
				PRINTFORMW 「……什么的♪　呼呼、只是这个样子就%TEXTR("兴奋起来了吗/心跳不已了吗")%？」
				IF TALENT:恋慕 == 0
					PRINTFORMW 「想要%TEXTR("真心真意/从心底")%地被那样称呼的话、要努力赚取%CALLNAME:TARGET%酱的好感度哦♪」
				ENDIF
		CASEELSE
			GOTO KOJO_K11_GREETING_LP1
		ENDSELECT
	ENDIF

CASE "入室打招呼"
	SELECTCASE RAND:5
		CASE 0
			PRINTFORMW 「%TEXTR("Hi~/嗨~")%、可爱的%CALLNAME:TARGET%酱登场了哟%TEXTR("＿/ぉ/～")%♪」
			PRINTFORM 「%TEXTR("希望/想要")%被温柔地送到软乎乎的%TEXTR("床上/沙发上")%~
			CALL HEARTMARK
			PRINTFORMW 」

		CASE 1
			PRINTFORMW 「%CALLNAME:TARGET%酱%TEXTR("＿/今天也/终于")%来了哟~♪」

		CASE 4
			PRINTFORMW 「谢谢主人的指名%TEXTR("＿/ぅ/～")%。%CNAME("一人称")%是%CALLNAME:TARGET%♪」
			CALL PRINT_STR, @"「要%TEXTR("立刻/马上/立即")%開始%TEXTR("＿/相関的设置")%吗_H_」_W"
			PRINTFORM 　%CALLNAME:TARGET%一边用奇怪的腔调说着
			IF EQUIP:裙
				PRINTFORMW %SKIRT(TARGET, "略")%の下摆を摘んで僅かに持ち動作著……
			ELSE
				PRINTFORMW 一边静静地坐到%CALLNAME:PLAYER%的身边。
			ENDIF
	CASEELSE
		GOTO KOJO_K11_GREETING_LP1
	ENDSELECT

CASE "抜け出された"
$K11_GREETING_ESCAPE_LP1
	SELECTCASE RAND:5
		CASE 0
			PRINTFORMW 「要和%CALLNAME:TARGET%出去了吗？」
			PRINTFORMW 「%TEXTR("唔呼呼/啊啦啊啦/嗯呼~")%♪　请多加小心♪」
		CASE 1
			PRINTFORMW 「要去其他的场所休息了吗？」
		CASE 2
			PRINTFORMW 「呼姆呼姆、如果可以的话想要两个人一直独处下去呢～」
			PRINTFORMW 「不用一直想着%CNAME("一人称")%的事情好好去玩吧♪」
		CASE 4
			SIF TALENT:恋慕
				GOTO K11_GREETING_ESCAPE_LP1
			PRINTFORMW 「那个……%CNAME("一人称")%可以一起去吗？」
			PRINTFORMW 「啊、不行吗……唔。会老老实实地看家的……」
	CASEELSE
		GOTO K11_GREETING_ESCAPE_LP1
	ENDSELECT

CASE "便当"
$KOJO_GREETING_K11_LUNCHTIME_LP1
	LUNCHBOX = %KOJO_K11_LOCAL_STRING("便当")%

	IF TALENT:恋慕
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR, "「今天去哪里吃饭好捏~」_W"
				CALL PRINT_STR, "「啊、茶水也已经买好了哦♪」_W"
			CASE 1
				CALL PRINT_STR, @"「呼呼♪　那么期待%TEXTR("便当/午饭")%吗？」_W"
			CASE 2
				CALL PRINT_STR, "「今天……到屋顶上去吃吧」_W"
				CALL PRINT_STR, "「不、不是、那个……偶、偶尔两个人独处一会也不错呢……」_W"
				CALL PRINT_STR, "「……请不要让人家像一个还不会说话的孩子一样说着害羞的话啊」_W"
		CASEELSE
			GOTO KOJO_GREETING_K11_LUNCHTIME_LP1
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				SIF STRCOUNT(LUNCHBOX, "飯団") + STRCOUNT(LUNCHBOX, "三明治")
					GOTO KOJO_GREETING_K11_LUNCHTIME_LP1
				SPLIT LUNCHBOX, "/", LOCALS
				PRINTFORMW 「今天的便当是%LOCALS:0%~」
			;CASE 1
			;	SIF !STRCOUNT(LUNCHBOX, "没有")
			;		GOTO KOJO_GREETING_K11_LUNCHTIME_LP1
			CASE 4
				SIF RAND:3 == 0
					PRINTFORMW 「已经是正午了哦～」
				LOCALS = %TEXTR("部室/教室/房顶/中庭/食堂")%
				PRINTFORMW 「今天在哪里吃好捏」
				PRINTFORMW 「%TEXTR("＿/……/嗯～、")%%TEXTR("＿/那里、/")%%LOCALS%怎么样？」
		CASEELSE
			GOTO KOJO_GREETING_K11_LUNCHTIME_LP1
		ENDSELECT
	ENDIF

ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K11, ARGS
;これは主にFOR-NEXTのCOUNT用
#DIM LCOUNT
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

$KOJO_DAILY_K11_LP2

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「嗨、%CNAME("一人称")%来啦—！　噗卡噗卡噗卡——」
	PRINTFORML 　%CALLNAME:TARGET%声势浩大地走进房间……

	; 確定で弗理希艾尔が居る(筈)ので、最低値1
	IF CHARA_NUM("室内の邪魔者") > 1
		; 既に多数ヒロインが居る
		IF CHARA_NUM("室内の邪魔者") >= 4
			PRINTFORML 「哦呀哦呀哦呀？　这里比想象的要厉害很多啊——」
		ENDIF
	ELSE
		; 他には弗理希艾尔のみ
		IF CHARA_NUM("室内の邪魔者") != CHARA_NUM("集合予定")
			; 他にもヒロインが居る
		ELSE
			; 他のヒロインはまだ居ない
			PRINTFORML 「嗯？　……吼—。原来如此原来如此」
		ENDIF
	ENDIF

	IF CHARA_NUM("室内の邪魔者") >= 2 && CHARA_NUM("陥落済み") >= CHARA_NUM("室内の邪魔者")
		PRINTFORML 「不过稍微有点意外捏—。原本还只是以为%CNAME("二人称")%奴役着孩子们享受着淫乱的后宫生活」
		PRINTFORML 「连这么乖巧的好女孩都能乖乖地呆在这里……啊、说的话稍微有点失礼了、对不起」
	ENDIF

	PRINTFORML 「嘛、从此以后大家都是情同姐妹的伙伴了、对吗？」
	PRINTFORML 　%CALLNAME:TARGET%笑嘻嘻地那样说着……
CASE "浑身是汗"
	; たまに(25%)体育に参加、ということで特殊処理
	;IF RAND:3
		; 汗と汚れを重置して随机会話に飛ばす
	;	CFLAG:汗 = 0
	;	CALL  RESET_STAIN_EX, "全部", TARGET
	;	GOTO KOJO_DAILY_K11_SKIP1
	;ENDIF

	PRINTFORML 「咿呀~、偶尔做一下健全的健身运动也不错捏~」
	PRINTFORML 　%CALLNAME:TARGET%就这样满身是汗地坐到沙发里面……
