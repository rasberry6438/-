;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K55, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE  "朝接吻", "朝の新妻PLAY", "朝添い寝"
		SELECTCASE RAND:3
		CASE 0
			CALL PRINT_STR, @" 「嗯、早上好……_HH_」_W"
		CASE 1
			PRINTFORML 「嗯呢……啊、醒了吗？」
			IF TEXTS("曜日") == "日"
				PRINTFORMW 「差不多该吃早饭了吧？」
			ELSE
				PRINTFORMW 「快要到上学的时间了、再不起来可不行」
				CALL PRINT_STRW, @" 「……诶、再睡一会？ 真没办法……_HH_」"
			ENDIF
		CASEELSE
			PRINTFORML 「啊、早上好、%CNAME("二人称")%。」
		ENDSELECT
	CASE "朝騎乗位", "朝Ａ騎乗位","朝乳交", "朝口交", "朝足交", "朝舐陰"
		PRINTFORMW 「%CNAME("二人称")%起床啦～」
		CALL PRINT_STRW, @" 「%TEXTR("む～っ、下の体はもう起きてるのにぃ_HH_/朝一番_HH_いっぱい出してね_HH_/えへへ、起きないからぁ……_HH_朝から独り占め_HH_")%」"
	CASE "朝六九式"
		PRINTFORMW 「%CNAME("二人称")%、起来了？」
		CALL PRINT_STRW, @" 「%TEXTR("下面的那位已经起来的说……_HH_/恋人的早上_HH_好好品尝哦_HH_")%」"
	CASE "朝逆正常位"
		CALL PRINT_STRW,@"「%CNAME("二人称")%、哦哇 哟…_HH_」"
		CALL PRINT_STRW,@"「%CNAME("二人称")%%TEXTR("に朝出し_HH_きもちよかったぁ……_HH_/のナカ_HH_朝から独り占めしちゃった_HH_/の寝顔がえっちすぎたから、朝えっち_HH_しちゃった_HH_")%」"
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今天有体育课哦。%CNAME("二人称")%、体操服什么的没有忘记吧？」
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「今天有什么计划吗吗？」
		CASE 1
			PRINTFORMW 「稍等哦、我去沏茶」
		CASEELSE
			PRINTFORMW 「早上好。有好好睡吗？」
			PRINTFORMW %CNAME("二人称")%温柔的微笑着。
		ENDSELECT
	ENDIF
CASE "一番乗り"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORMW 「あ、%CNAME("二人称")%」
		PRINTFORMW 「今天%CNAME("一人称")%是第一呢」
	CASE 1
		PRINTFORMW 「太好了第一个到！」
		PRINTFORMW 「……今天干什么好呢～♪」
	CASEELSE
		PRINTFORMW 「……好像其他人都没到呢」
	ENDSELECT
CASE "ただいま"
	PRINTFORML 「我回来了」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜") && TALENT:相思相愛
		PRINTFORMW 「欢迎回来～」
		CALL PRINT_STRW, @" 「先吃饭吗？先洗澡吗？……还是说……_HH_」"
		CALL PRINT_STRW, @" 「没什么没什么、保~密_HH_」"
	ELSEIF COND("夜")
		PRINTFORMW 「欢迎回来～」
	ELSE
		PRINTFORMW 「啊、等着你呢」
	ENDIF
CASE "入室打招呼"
	SELECTCASE CSTR:PREV_DAILYCOM
	CASE "甜言蜜语"
		PRINTFORMW (欸、难道说打扰你了？)
	CASE "打招呼舐陰", "打招呼口交"
		PRINTFORMW 「欸？等等、在做什么呢……？」
	CASEELSE
		PRINTFORMW 「打扰了」
	ENDSELECT
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「打扰了」
		PRINTFORMW 「……啊阿、大家都到齐了啊」
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「呐、…今天干什么？」
		CASE 1
			PRINTFORMW %CALLNAME:TARGET%轻轻点頭。
		CASEELSE
			PRINTFORMW 「下午好。」
		ENDSELECT
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『你好、%CNAME(CALLNAME_ACTOR)%。』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 『大家差不多都到齐了哦。』
		CASE 1
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、你好。』
		CASEELSE
			PRINTFORMW 『您好。』
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 『你好。』
		CASE 1
			PRINTFORMW %CALLNAME:TARGET%轻轻点頭。
		CASEELSE
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、你好。』
		ENDSELECT
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 「我回来了～」
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "便当"
	IF TALENT:相思相愛 && RAND:100 >= 95
		PRINTFORML 「今天啊、居然！！」
		CALL PRINT_STRL, @" 「做了妻便当哦_HH_」"
		CALL PRINT_STRL, @" 「快快、来品尝_HH_」"
	ELSE
		PRINTFORML 「今日は、%TEXTR("海苔弁当/漢堡肉弁当/飯団の詰め合わせ/鮭の塩焼き弁当/")%を作ってきたよ～」
	ENDIF
	
	SELECTCASE RAND:4
	CASE 0
		PRINTFORML 「给、这是你的%TEXTR("梨/橘子/草莓/苹果/桃子")%。」
	CASE 1
		PRINTFORML 「%CNAME("一人称")%的自信作。」
		PRINTFORML 「怎么样、好吃吗？」
	CASE 2
		PRINTFORML %CALLNAME:TARGET%夹起便当、送到%CALLNAME:MASTER%嘴前。
		CALL PRINT_STRW, @" 「哈、然有些含羞……、来～啊_HH_」"
	CASEELSE
		PRINTFORML 「%CNAME("二称")%、吃饱了吗？」
	ENDSELECT
ENDSELECT

