;-------------------------------------------------
;各種打招呼
;注意！ここのセリフは基本的に颜绘が付きます！！
;颜绘に変化を付けたい場合にはセリフの前でEMOTION_PLUSを用いる事で変えられます。
;例）恥じらった顔にさせたい場合には CALL EMOTION_PLUS, "害羞", TARGET
;-------------------------------------------------
@KOJO_GREETING_K65, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝面部騎乗"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		CALL EMOTION_PLUS, "重置/紅潮２/小口２/闭眼", TARGET
		PRINTFORMW 「哈姆、嗯、呲」
		PRINTFORMW 「啊，醒了啊……早安~」
		PRINTFORMW 「……为什么，说……啊……」
		CALL EMOTEFACE, "躁动难忍", TARGET
		PRINTFORMW 「比起那个，这边好像也起来了呢~？」
		PRINTFORMW %CALLNAME:TARGET%在遮掩脸红的同时，指向了%CALLNAME:MASTER%的下半身……
	CASE "朝騎乗位"
		CALL EMOTEFACE, "惊嚇", TARGET
		PRINTFORMW 「啊」
		PRINTFORMW 「……起来了吗~」
		PRINTFORMW 「早安~，那个，……想继续吗？」
		PRINTFORMW 「……」
		CALL EMOTEFACE, "害羞", TARGET
		PRINTFORMW 「呀，别问%CNAME("一人称")%啦」
	;ろりっこ添い寝！ろりっこ添い寝！
	CASE "朝添い寝"
		IF TALENT:恋慕
			IF COND("欲求不満") >= 2
				;甘えたがり
				PRINTFORMW 「……嗯、嘶……哈、啊……？」
				PRINTFORMW 当%CALLNAME:MASTER%想要离開被窝，留下困倦的%CALLNAME:TARGET%时，下半身突然感到一阵沉重。当他去看发生了什么时……
				PRINTFORMW 「……呀」
				PRINTFORMW %CALLNAME:TARGET%正紧紧地抱着%CALLNAME:MASTER%的腰。
				PRINTFORMW 「……不要啊」
				PRINTFORMW 「……呀、不……！」
				PRINTFORMW 即使被拖拽着，%CALLNAME:TARGET%依然紧贴着%CALLNAME:MASTER%，不肯离開。
				PRINTFORMW 不仅如此，她还就那样趴在%CALLNAME:MASTER%的背上，让他动弹不得。
				PRINTFORMW 「不要，把我一个人留下……！」
				PRINTFORMW 「要一起，待在一起……不要走……」
				PRINTFORMW 「要一起……%CNAME("一人称")%……彻底地，把我……」
				PRINTFORMW 「……嗯~，喜欢、喜欢你、喜欢、喜欢」
				PRINTFORMW %CALLNAME:TARGET%用尽全力，手脚都缠住%CALLNAME:MASTER%的同时，腰也蹭了上来。
				PRINTFORMW 纤细柔软的身体，温暖的肌肤，一遍又一遍地触碰着%CALLNAME:MASTER%。
				PRINTFORMW 然而，那动作渐渐慢了下来?
				PRINTFORMW 「喜欢、喜欢你、喜……欢……？~~~！！！？」
				PRINTFORMW 「噗咻！！！！！」
				PRINTFORMW %CALLNAME:TARGET%醒了过来。脸上冒着如同爆炸般的蒸汽……
				PRINTFORML
				PRINTFORML
				PRINTFORML
				PRINTFORML
				PRINTFORMW ……
				PRINTFORMW 「……早、早上好……呜」
				PRINTFORMW %CALLNAME:TARGET%乖巧地坐好，再次向我问早。
				PRINTFORMW 「不要太、太冷落%CNAME("一人称")%了哦……」
			ELSEIF RAND:4 == 0
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「嗯，早上好~」
				CALL EMOTEFACE, "甜言蜜语", TARGET
				PRINTFORMW 「因为太暖和了，就不小心抱上去了，呵呵」
				PRINTFORMW 「要怎么办呢？要不再稍微放松一下吗？呵呵」
			ELSEIF RAND:3 == 0
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「……嗯……？啊、哦、早上好~」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「呵呵，感觉很舒服，就不知不觉地贴上去了」
				PRINTFORMW 「……还想再一起待一会儿呢~，什么的」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「嗯……呼……啊……啊？」
				CALL EMOTEFACE, "刚睡醒/紅潮２", TARGET
				PRINTFORMW 「嗯哈……呀，等等，还……」
				PRINTFORMW 「再稍微，睡吧？睡吧~，睡吧~……不要，不行，不行……不要走……呜呜……」
				PRINTFORMW 好不容易才把困倦地抱上来的%CALLNAME:TARGET%扒開，或者稍微黏在一起后，%CALLNAME:MASTER%从被窝里出来了……
			ELSE
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「呼……嗯，嗯啊……？哈啊……」
				CALL EMOTEFACE, "害羞", TARGET
				PRINTFORMW 「咦，%CNAME("一人称")%……什么时候……」
				PRINTFORMW 「……也许是安心了吧，呵呵」
			ENDIF
		ELSE
			IF RAND:4
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「……嗯，呜……？啊，诶？……！？」
				CALL EMOTEFACE, "害羞", TARGET
				PRINTFORMW 「喔，喔哈哟……呀，因为有点在意你晚上怎么样了」
				PRINTFORMW 「去了%CNAME("二人称")%那里，不知不觉就，钻进去了」
				PRINTFORMW 「就那样睡着了……好像……」
				CALL EMOTEFACE, "害羞/闭眼", TARGET
				PRINTFORMW 「……」
				CALL EMOTEFACE, "ガチ害羞", TARGET
				PRINTFORMW 「……偶尔也会有这样的啦！不要说人家是小孩子！」
			ELSEIF RAND:3 == 0
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「……嗯……？啊、哦、早上好~」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「呵呵，感觉很舒服，就不知不觉地贴上去了」
				PRINTFORMW 「……还想再一起待一会儿呢~，什么的」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「嗯……哈，喔，喔哈哟~……」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「呀，不知不觉就想黏在一起嘛」
				PRINTFORMW 「偶尔也会有这种情况的嘛~，是有的啦」
			ELSE
				CALL EMOTION_PLUS, "刚睡醒", TARGET
				PRINTFORMW 「呼……嗯，嗯啊……？哈啊……」
				CALL EMOTEFACE, "害羞", TARGET
				PRINTFORMW 「咦，%CNAME("一人称")%……什么时候……」
				PRINTFORMW 「因为太安心了，真是的……好害羞……」
			ENDIF
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		IF TALENT:恋慕
			PRINTFORMW 「今天有体育课~，是吗」
			PRINTFORMW 「要好好活动一下会很累呢~」
			PRINTFORMW 「……那样的话，就「休息」一下吧，呵呵……」
		ELSE
			PRINTFORMW 「今天有体育课啊，嘿~」
			PRINTFORMW 「身体活动后会很累呢，疲惫了，就，不太那个，色色的……事情……」
			CALL EMOTEFACE, "心情差", TARGET
			PRINTFORMW 「疲惫了就会想「休息」一下吗？呜~啊~，是吗~……」
		ENDIF
	ELSEIF COND("水泳")
		PRINTFORMW 「今天有游泳课~」
		IF RAND:2
			PRINTFORMW 「%CNAME("二人称")%游泳，很擅长吗？」
		ELSE
			PRINTFORMW 「是吗，要加油哦」
		ENDIF

	ELSE
		;朝ごはんイベント
		IF CFLAG:料理回数 < 6
			IF RAND:3 == 0
				CALL EMOTION_PLUS, "笑顔", TARGET
				PRINTFORMW 「早安~，来来，吃早饭吧」
				PRINTFORMW 「今天可是%CNAME("一人称")%做的哦~」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「可能稍微有点失败……不嫌弃的话就吃吧~？」
			ELSEIF RAND:2
				CALL EMOTION_PLUS, "重置", TARGET
				PRINTFORMW 「哼哼，哼，哼……♪」
				PRINTFORMW 「啊，早安~，正好饭快做好了~」
				CALL EMOTEFACE, "重置/斜眉", TARGET
				PRINTFORMW 「味增汤，要不要稍微尝一下？总觉得有点咸……」
			ELSE
				PRINTFORMW 「早安~，那个，那个……我试着做了早饭，怎么样？」
			ENDIF
		ELSE
			IF RAND:3 == 0
				CALL EMOTION_PLUS, "笑顔", TARGET
				PRINTFORMW 「早安~，快，快点坐下坐下」
				PRINTFORMW 「今天的饭是%CNAME("一人称")%做的哦~」
				PRINTFORMW 「我可是很有自信的，快吃快吃」
			ELSEIF RAND:2
				PRINTFORMW 醒来的%CALLNAME:MASTER%进入客厅，从厨房方向传来愉快的哼歌声。
				CALL EMOTION_PLUS, "笑顔", TARGET
				PRINTFORMW 「哼哼，哼，哼……♪」
				CALL EMOTEFACE, "重置", TARGET
				PRINTFORMW 「啊，早安~，正好饭快做好了~」
				PRINTFORMW 「呵呵，今天的菜是%TEXTR("煎蛋和培根/烤鱼/凉拌菜和炒菜/烤鲭鱼/鸡蛋卷/香肠/生姜烧肉")%哟~」
			ELSE
				PRINTFORMW 醒来的%CALLNAME:MASTER%进入客厅，注意到从厨房飘来令人食欲大增的香味。
				CALL EMOTION_PLUS, "重置", TARGET
				PRINTFORMW 「早安~，闻到香味了吗？我在做味增汤~」
				PRINTFORMW 「我试着加了%TEXTR("炸油豆腐/山药/茄子/卷心菜/豆腐/裙带菜/蛋液")%，觉得怎么样~」
				PRINTFORMW 「再稍等一下哦，呵呵」
			ENDIF
		ENDIF
		CFLAG:料理回数 += 1
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")

	ELSEIF DAYEV("試験返却日")

	ELSEIF COND("朝袭相手")

	ELSEIF COND("体育")

	ELSEIF COND("水泳")

	ELSE

	ENDIF
;この角色と一緒に部室に入った際に誰も他にいない場合
CASE "一番乗り"
		PRINTFORMW 「哎，好像谁也不在呢~」
		CALL EMOTEFACE, "笑顔", TARGET
		PRINTFORMW 「可以尽情玩耍了，是吗？」
CASE "ただいま"
	IF RAND:3 == 0
		PRINTFORMW 「回来了哟～」
	ELSEIF RAND:2
		PRINTFORMW 「我回来了~」
	ELSE
		PRINTFORMW 「我回来了，有没有觉得寂寞啊~？」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		;確率を偏らせる == 0の入れ忘れでは有ません
		IF RAND:3
			PRINTFORMW 「你回来啦~」
		ELSEIF RAND:2
			PRINTFORMW 「你回来啦~，呵呵」
		ELSE
			PRINTFORMW 「哦，你回来了……稍微有点困了……」
		ENDIF
	ELSE
		;確率を偏らせる == 0の入れ忘れでは有ません
		IF RAND:3
			PRINTFORMW 「啊，辛苦啦~」
		ELSEIF RAND:2
			PRINTFORMW 「你好呀~」
		ELSE
			PRINTFORMW 「hofuhwarehyama」
			PRINTFORMW 「……我先放松一下咯~」
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
			IF TALENT:恋慕
				IF RAND:2
					PRINTFORMW 「呼呼，我是你的%CALLNAME:TARGET%酱哦~」
					PRINTFORMW 「骗你的啦~，呵呵呵」
				ELSE
					PRINTFORMW 「我来啦~，怎么了？ 」
				ENDIF
			ELSE
				PRINTFORMW 「我来啦~，怎么了？ 」
			ENDIF
	;你が現在的partnerにＨ的悪戯をしている最中に入室した（partnerへの呼びかけはCNAME("partner")で出来る）
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF TALENT:恋慕
			CALL EMOTEFACE, "害羞", TARGET
			PRINTFORMW 「你、哇、等、等等，你在干什么啦~……」
		ELSE
			CALL EMOTEFACE, "害羞", TARGET
			PRINTFORMW 「你、哇哇、欸……你在做什么……」
		ENDIF
	ELSE
			PRINTFORMW 「你好呀~」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			PRINTFORMW 「那个，打扰了~……稍微绕路太多了吧」
	ELSEIF CFLAG:呼び出された
			PRINTFORMW 「你好呀~，我来啦~，怎么了？」
	ELSE
			PRINTFORMW 「打扰了~」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
			PRINTFORML 『你好~……啊』
			PRINTFORML 『啊，%CNAME("一人称")%我，那个，虽然不是学生，但正在参观……』
			PRINTFORMW 『你、你辛苦了大人？』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			PRINTFORML 『honghihifua』
			PRINTFORMW %CALLNAME:TARGET%正塞着%TEXTS("点心")%放松着……
	ELSE
			PRINTFORMW 『你好~，辛苦了大人~』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「我来啦~，然后呢，怎么了？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「回来了哟～」
	ELSE
		PRINTFORMW 「哦，在呢在呢」
	ENDIF
CASE "抜け出された"
		PRINTFORML 「哎呀，要走了吗？」
		PRINTFORMW （盯着看）
CASE "便当"
	;初めて
	IF CFLAG:料理回数 == 0
		PRINTFORMW 「便当，怎么样？好吃吗？」
		CALL EMOTEFACE, "害羞", TARGET
		PRINTFORMW 「因为是非常非常久违地做的，所以有点担心」
		PRINTFORMW 「味道很单薄吗？嗯，确实可能这样……」
		PRINTFORMW 「……看来还得继续练习呢」
		CALL EMOTEFACE, "重置", TARGET
		PRINTFORML 
		PRINTFORMW 「下次，我会再做的哦？下次我会再努力一点的」
	;不慣れ
	ELSEIF CFLAG:料理回数 >= 1 && CFLAG:料理回数 < 6
		IF RAND:3 == 0
			PRINTFORMW 「怎、怎么样……？」
			PRINTFORMW 「很咸？果然吗？」
			PRINTFORMW 「……对不起，啊，不、不用全部吃完的」
			PRINTFORMW 「毕竟是特意为我做的，要吃吗？这样啊，……谢谢」
		ELSEIF RAND:2
			PRINTFORMW 「唔嗯……」
			PRINTFORMW 「是不是稍微多煮一会儿比较好呢~」
			PRINTFORMW 「下次一定要注意」
			PRINTFORMW 「总之，谢谢你吃光了」
		ELSE
			;多分煮崩れした具材がべちゃっとしてるやつ
			PRINTFORMW 「这次我觉得比平时做得好……」
			PRINTFORMW 「啊，真的，变得软趴趴的……」
			PRINTFORMW 「呵呵，看来要做出美味的料理，还需要一段时间呢」
			PRINTFORMW 「但是能一起吃，感觉很開心……」
		ENDIF
	;好吃！好吃！
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「怎么样，好吃吗？」
			CALL EMOTEFACE, "笑顔", TARGET
			PRINTFORMW 「太好了~，呵呵，这次我对自己的手艺很有自信呢~」
			PRINTFORMW 「呵呵呵，呵呵……」
		ELSEIF RAND:2
			CALL EMOTEFACE, "笑顔", TARGET
			PRINTFORMW 「像这样一起吃便当，真開心呢」
			PRINTFORMW 「感觉就像是好朋友一样」
			PRINTFORMW 「能让你觉得好吃，我很高兴」
		ELSE
			CALL EMOTEFACE, "笑顔", TARGET
			PRINTFORMW 「哼哼~，好吃吧？」
			PRINTFORMW 「来，啊~」
			PRINTFORMW 「呵呵……啊，因为做得很好，所以有点高兴啦」
			CALL EMOTEFACE, "笑顔/紅潮２", TARGET
			PRINTFORMW 「没、没办法啦，来，再吃一口」
		ENDIF
	ENDIF
	CFLAG:料理回数 += 1
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	PRINTFORMW 「哇哇，好、好有精神呢……」
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;动画ーションの関係上、１ターンに何度も呼ばれたりするのでフラグ操作をする場合には
;IF CFLAG:KOJO_DAILY済 == 0
;で分岐させるのが良いでしょう
;-------------------------------------------------
@KOJO_DAILY_K65, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
		PRINTFORML 「那个，%CNAME("一人称")%是叫%CALLNAME:TARGET%吗，请多関照……」
		PRINTFORML 或许是对于陌生人感到紧张，%CALLNAME:TARGET%正不安地四处张望。
CASE "浑身是汗"
		PRINTFORML 「啊，好热……感觉要融化了~」
CASE "刚睡醒"
		IF CHOSE_DEVENT("睡眠姦口交")
			CALL EMOTEFACE, "憤怒", TARGET
			PRINTFORML 「唔……呜欸，总觉得喉咙，感觉怪怪的……」
			PRINTFORML 「唔~……」
			PRINTFORML 「嗯」
			PRINTFORML 「嗯……！」
			PRINTFORML %CALLNAME:TARGET%不快地，用仿佛想要诉说什么的眼神看向了%CALLNAME:MASTER%……
		ELSEIF CHOSE_DEVENT("睡眠姦Ｖ中出し")
			CALL EMOTEFACE, "害羞", TARGET
