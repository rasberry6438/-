;-------------------------------------------------
;口上テンプレート
;休托莉口上（角色クターの名前を入れる、下にコンセプトなど一言あれば入れる）
;製作者　ブリート
;-------------------------------------------------
;まだぜんぜん完成してないよ！
;-------------------------------------------------
;元気で甘えてくる女の子、悪魔ですが容姿だけで出来ることはあまり人类の女の子と変わりません
;女性型の魔物のため魔界にいた頃にえっちに関して勉強をしています。（知識だけ実践はない）
;力が弱いために驱魔人を相手にするのはちょっと怖い
;悪魔である事がばれない様に認識障害を使っていますが……精度があまり高くないので看破できる人にはできます
;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K50, ARGS
#FUNCTIONS
#DIM ACTOR

VARSET LOCALS

LOCALS =

SELECTCASE ARGS
CASE "男性器"
	SELECTCASE CEVENTS("男性器呼")
	CASE ""
		LOCALS = おちんちん_おちんちん
	CASEELSE
		LOCALS = %CEVENTS("男性器呼")%
	ENDSELECT
CASE "女性器"
	SELECTCASE CEVENTS("女性器呼")
	CASE ""
		LOCALS = おまんこ_おまんこ
	CASEELSE
		LOCALS = %CEVENTS("女性器呼")%
	ENDSELECT
CASE "一人称"
	LOCALS = 我_我

CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF TALENT:PLAYER:男性
			LOCALS = 哥哥_哥哥
		ELSE
			LOCALS = 姐姐_姐姐
		ENDIF
	;相手が主人ではない
	ELSE
		IF COND("教員", PLAYER)
			LOCALS = %CALLNAME:PLAYER%老师_%CSTR:PLAYER:ふりがな%老师
		ELSE
			SELECTCASE CSTR:PLAYER:学年
			CASE "初等部", "中等部"
				IF TALENT:PLAYER:男性
					LOCALS = %CALLNAME:PLAYER%君_%CSTR:PLAYER:ふりがな%君
				ELSE
					LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
				ENDIF
			CASE "１年生", "２年生", "３年生"
				LOCALS = %CALLNAME:PLAYER%同学_%CALLNAME:PLAYER%同学
			CASEELSE
				IF TALENT:PLAYER:外見年齢 < 3 && TALENT:PLAYER:娇小 && TALENT:PLAYER:萝莉老太婆 == 0
					IF TALENT:PLAYER:男性
						LOCALS = %CALLNAME:PLAYER%君_%CSTR:PLAYER:ふりがな%君
					ELSE
						LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
					ENDIF
				ELSE
					LOCALS = %CALLNAME:PLAYER%同学_%CALLNAME:PLAYER%同学
				ENDIF
			ENDSELECT
		ENDIF
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 部長桑_部長桑
CASE "助手"
	IF COND("教員", ASSI)
		LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部"
			IF TALENT:PLAYER:男性
				LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
			ENDIF
		CASE "１年生", "２年生", "３年生"
			LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
		CASEELSE
			IF TALENT:ASSI:外見年齢 < 3 && TALENT:ASSI:娇小 && TALENT:ASSI:萝莉老太婆 == 0
				IF TALENT:PLAYER:男性
					LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
				ELSE
					LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
				ENDIF
			ELSE
				LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
			ENDIF
		ENDSELECT
	ENDIF
CASEELSE
	;ARGSに数字だけが入ってる時はその加入番号の角色
	;例えばCNAME(@"TARGET")とか
	;ARGSにCALLNAMEが入ってる場合にはその角色への呼び方
	IF ISNUMERIC(ARGS)
		ACTOR = TOINT(ARGS)
	ELSEIF FIND_NO(ARGS) > 0
		ACTOR = FIND_NO(ARGS)
	ENDIF

	IF ACTOR == MASTER
		IF TALENT:ACTOR:男性
			LOCALS = 哥哥_哥哥
		ELSE
			LOCALS = 姐姐_姐姐
		ENDIF
	ELSEIF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%老师_%CALLNAME:ACTOR%老师
	ELSE
		SELECTCASE CSTR:ACTOR:学年
		CASE "初等部", "中等部"
			IF TALENT:ACTOR:男性
				LOCALS = %CALLNAME:ACTOR%君_%CSTR:ACTOR:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:ACTOR%酱_%CALLNAME:ACTOR%酱
			ENDIF
		CASE "１年生", "２年生", "３年生"
			LOCALS = %CALLNAME:ACTOR%桑_%CALLNAME:ACTOR%桑
		CASEELSE
			IF TALENT:ACTOR:外見年齢 < 3 && TALENT:ACTOR:娇小 && TALENT:ACTOR:萝莉老太婆 == 0
				IF TALENT:ACTOR:男性
					LOCALS = %CALLNAME:ACTOR%君_%CSTR:ACTOR:ふりがな%君
				ELSE
					LOCALS = %CALLNAME:ACTOR%酱_%CALLNAME:ACTOR%酱
				ENDIF
			ELSE
				LOCALS = %CALLNAME:ACTOR%桑_%CALLNAME:ACTOR%桑
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

