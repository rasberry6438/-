;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K35, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		PRINTFORMW 「やーっと起きたな、この寝ぼすけめ」
		PRINTFORMW 「あんまり可爱顔してるから、我慢できなくなっちゃったよ」
	CASE "朝舐陰"
		PRINTFORMW 「へへ、起きた起きた」
		PRINTFORMW 「ちょっと寝てる間にお掃除をと思ってね」
	CASE "朝足交"
		PRINTFORMW 「やーっと起きたな、この寝ぼすけめ」
		PRINTFORMW 「こんだけやっても起きないとか、大した奴だなぁ。くっく」
	CASE "朝の新妻PLAY"
		PRINTFORMW 「ったく、この%CNAME("一人称")%様が珍しく飯作ってやってるってのに……」
		PRINTFORMW 「ほら、先に食ってろ。%CNAME("一人称")%はちょっと洗い物ができた」
	CASE "朝乳交"
		PRINTFORMW 「んっ……あ、起きた？」
		PRINTFORMW 「わりぃわりぃ。ちょっとコイツの使い心地を確かめようかなって。ふふっ」
	CASE "朝接吻"
		PRINTFORMW 「ふふっ……王子様の接吻」
		PRINTFORMW 「なんつって。%CNAME("二人称")%にとってはお姫様かな？」
	CASE "朝添い寝"
		PRINTFORMW 「なんだよ、もう起きちまったの？」
		PRINTFORMW 「折角だから二度寝でもしようぜ……ふふっ」
	CASE "朝口交", "朝六九式"
		PRINTFORMW 「んっふふ……一番搾り、佳肴桑」
		PRINTFORMW 「んじゃ、ちょっと洗面台借りるから」
	CASE "朝騎乗位", "朝Ａ騎乗位"
		CALL PRINT_STRW, "「んっ……_UH_ あー……やっと起きた？」"
		PRINTFORMW 「よし、じゃあちょっと……手ぇ貸せ。立てねぇから」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		CALL EMOTION_PLUS, "心情差", TARGET
		PRINTFORMW 「%TEXTR("哦呀，要加油哦。今天是体育课呢。/听说今天是体育课哦亲爱的。")%」
	ELSE
		PRINTFORMW 「%TEXTR("啊，早上好/起床啦起床啦~")%」
	ENDIF
CASE "登校"
	IF DAYEV("試験当日")
		CALL EMOTION_PLUS, "重置/眯眯眼/半開"
		PRINTFORMW 「あーあ、試験かったり……」
	ELSEIF DAYEV("試験返却日")
		IF CFLAG:予想点 >= 80
			CALL EMOTION_PLUS, "重置/眯眯眼/ω"
			PRINTFORMW （今日は試験返却か……ま、今回は余裕だったし、上は取ってるだろ）
		ELSEIF CFLAG:予想点 >= 60
			CALL EMOTION_PLUS, "重置/無口系"
			PRINTFORMW （試験返却日来たなぁ……ま、いつも街道だろ）
		ELSE
			CALL EMOTION_PLUS, "重置/闭眼/斜眉/半開"
			PRINTFORMW （あぁー、やっちまったよなぁ……今回の試験結果微妙だろ絶対……）
		ENDIF
	ELSEIF COND("朝袭相手")
		CALL EMOTION_PLUS, "重置/ω"
		PRINTFORMW （ふふふーん……いやぁ、この独特の高揚感たまんねえよなあ。くっく）
	ELSEIF COND("体育")
		CALL EMOTION_PLUS, "重置/半開"
		PRINTFORMW 「はーあ、体育めんどくせ」
	ELSEIF COND("水泳")
		CALL EMOTION_PLUS, "重置/眯眯眼/ω"
		PRINTFORMW 「くっくっく……今日も%CNAME("一人称")%の肉体美見せつけてやるかぁ～」
	ELSE

	ENDIF

CASE "一番乗り"
	PRINTFORMW 「%TEXTR("什么啊，没有其他人啊.../嗯..只有我们两个人呢。")%」
CASE "ただいま"
	CALL EMOTION_PLUS, "心情差", TARGET
	PRINTFORMW 「我回来了。%TEXTR("啊~好累啊。/呐，来玩点什么吧~")%」

CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「%TEXTR("什么嘛、太迟了哦。/欢迎回来~现在有空吗？")%」
	ELSE
		PRINTFORMW 「%TEXTR("哟，你先来啦。/辛苦了，今天要做点什么呢？")%」
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORMW 「久等啦，难道说等了很久 ……%TEXTR("什么的/之类的")%」
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF TALENT:恋慕
			CALL EMOTION_PLUS, "心情差", TARGET
			PRINTFORMW 「%TEXTR("哦、已经開始了呀/啊啊？什么嘛，让我好好享受一下吧。")%」
		ELSE
			PRINTFORMW 「哦？做的不错嘛」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR("打扰了/唔，来了吗？")%」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR("啊~迟到了迟到了.../哦，大家都已经来了呀。")%」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「%TEXTR("久等了/我来了")%ー」
	ELSE
		PRINTFORMW 「%TEXTR("你们好/各位好啊")%」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『老师好啊』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF FIND_COND("入室打招呼中", "真", "莉")
			PRINTFORMW 『ずいぶん遅かったなー你。何してたん？』
		ELSE
			PRINTFORMW 『哟，真晚啊。』
		ENDIF
	ELSEIF FIND_COND("入室打招呼中", "真", "莉")
		PRINTFORMW 『%TEXTR("おう/ん")%、なんだ你か』
	ELSE
		PRINTFORMW 『%TEXTR("你好~/辛苦啦~/唔，你来啦")%』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「呼んだかー？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「ただいま、っと」
	ELSE
		PRINTFORMW 「うーっす、お邪魔しますよっと」
	ENDIF
CASE "抜け出された"
	IF TALENT:恋慕
		CALL EMOTION_PLUS, "心情差", TARGET
		PRINTFORMW 「%TEXTR("やれやれ/元気だなぁ")%……」
	ELSE
		PRINTFORMW 「%TEXTR("なんだ、お楽しみか～？/おーおー、お盛んなこって")%」
	ENDIF
CASE "便当"
	IF CEVENT("初めての便当") == 0
		PRINTFORMW 「给你，尝尝看吧」
		PRINTFORMW 在强硬地把饭盒递出来的%CALLNAME:TARGET%的脸上、难得看到了紧张的神色。
		PRINTFORMW 「干什么，快点吃啦！」
		PRINTFORMW 不高兴地打開了便当盒的盖子、出现了比想象中更整齐的便当的样子。
		PRINTFORMW 尝试着吃了一口、味道无可挑剔。
		PRINTFORMW 「...好吃吗？」
		PRINTFORMW 毫不犹豫地点了点頭、%CALLNAME:TARGET%叹了一口气。
		PRINTFORMW 「真是的，还装模作样的」
		PRINTFORMW 虽然说着语气不佳的话，但嘴角却露出一丝微笑。
		CALL SET_CEVENT, "初めての便当", TARGET
	ELSE
		PRINTFORMW 「给，都是你喜欢吃的东西、%CNAME("二人称")%」
		PRINTFORMW %CALLNAME:TARGET%一直看着心满意足地吃着自己手中的料理的%CALLNAME:MASTER%的脸。
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	IF TALENT:恋慕
		CALL EMOTION_PLUS, "心情差", TARGET
		PRINTFORMW 「%TEXTR("おーおー、気にせず始めやがって……/おいおい、こっちは生殺しかー？")%」
	ELSE
		CALL EMOTION_PLUS, "惊嚇", TARGET
		PRINTFORMW 「%TEXTR("おいマジか。遠慮しねえな/え、なに。見てていいの？")%」
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K35, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「我是%NAME:TARGET%♪ 还请多多指教哦♪」
	PRINTFORML %CALLNAME:TARGET%假惺惺地用着天真地笑容打着招呼。
CASE "浑身是汗"
	IF RAND:2
		PRINTFORML 「%TEXTR(@"喂~给我擦一下吧……/%CNAME("主人")%~好想吃冰淇淋呀~")%」
		PRINTFORML %CALLNAME:TARGET%浑身无力地呼喊着%CALLNAME:MASTER%
	ELSE
		PRINTFORML 「%TEXTR("阿——嚏！……真的好讨厌夏天啊……/又出汗了……！ 真的可恶……")%」
		PRINTFORML %CALLNAME:TARGET%看起来非常不高兴的样子……
	ENDIF
CASE "刚睡醒"
	PRINTFORML 「……%TEXTR("啊/什么/什么时候")%？」
CASE "Cosplay中"
	IF CHARA_NUM("室内の邪魔者")
		PRINTFORML 「%TEXTR("どーよ？ 様になってるだろ/ふふーん。これくらいなんてこたぁねえぜ")%」
	ELSE
		PRINTFORML 「%TEXTR("さ、個撮でもするか？/ちょうど二人きりだ。じっくり堪能していいんだぜ")%」
	ENDIF
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K35
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
		PRINTFORML %CALLNAME:TARGET%转过身去、一幅心神不宁的样子，腿都有些哆嗦……
CASE "约会的约定"
	PRINTFORML %CALLNAME:TARGET%通过鼻歌を歌っている……
;これは悪魔や天使や驱魔人のみ
CASE "契約成立"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K35, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
;你にお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	PRINTFORML 
CASE "摆脱"
	IF CFLAG:躁动难忍
		PRINTFORML 「……期待していいんだよな？」
		PRINTFORML 感受到了%CALLNAME:TARGET%热情的视线……
	ELSE
		PRINTFORML 「%TEXTR(@"さて、なんの御用で？/で？ 何を始めようってんだ、%CNAME("主人")%桑よ")%」
		PRINTFORML %CALLNAME:TARGET%通过余裕そうに構えている。
	ENDIF
CASE "主人公的房間に連れ込まれた"
	IF CFLAG:躁动难忍
		PRINTFORML %CALLNAME:TARGET%比%CALLNAME:MASTER%更快一步的坐在了床上。
		PRINTFORML 「……%TEXTR("で、どうする/そういうことで良いんだよな")%？」
	ELSE
		PRINTFORML 「%TEXTR("有书可以看呢/有漫画什么的嘛？ 游戏呢？/嗯嗯？床底下有什么呢？嗯……")%」
		PRINTFORML %CALLNAME:TARGET%早早地放松了下来……
	ENDIF
CASE "自室に招待"
	IF CFLAG:躁动难忍
		PRINTFORML 「%TEXTR(@"いらっしゃーい。さっそく始めるか？/ようこそ%CNAME("主人")%。歓迎するぜ")%」
	ELSE
		PRINTFORML 「%TEXTR("棚ん中とか荒らすなよ。衣装とか入ってっから/漫画とかその辺にあるから。随意戻すなよー/游戏はそっちの余ったモニター使えな")%」
	ENDIF
CASE "自室に訪問された"
	;指导者を伴っている場合
	IF FIND_COND("現在存在", "助手")
		IF FIND_COND("真", "莉")
			PRINTFORML 「%TEXTR(@"おー、どうした揃って/ん、なんだ%CNAME("二人称")%らか")%」
			PRINTFORML %CALLNAME:MASTER%の隣に見慣れた妹の姿を認め、%CALLNAME:TARGET%通过姿勢を崩している……
		ELSE
			PRINTFORML 「%TEXTR("お、らっしゃい/なんだ、今日は連れがいるのか")%」
			PRINTFORML %CALLNAME:MASTER%以外の来訪者の姿を認め、%CALLNAME:TARGET%通过幾らか姿勢を正した。
		ENDIF
	ELSE
		PRINTFORML 「%TEXTR(@"んー？ なんだ%CNAME("二人称")%かぁ/おう、なんか用かー？")%」
	ENDIF
CASE "指导者部屋訪問"
	SELECTCASE SAVESTR:現在位置
	CASE "真的房間";個別イベントCEVENTによる分岐はカットしました
		PRINTFORML 「さてと、お邪魔するぜー」
		PRINTFORML 『……言っとくけど、変なことしないでよ兄桑』
		PRINTFORML 「そんなのは%CNAME("主人")%に言ってくれよ」
	CASE "莉的房間"
		PRINTFORML 『……で、三人で何するの？』
		PRINTFORML 「何って、そりゃあ……なあ？」
		PRINTFORML %CALLNAME:TARGET%通过%CALLNAME:MASTER%に流し目を送った。
	CASEELSE
		PRINTFORML 「おじゃーましますよっと……」
	ENDSELECT
;自宅移動の後に你に連れられて居間に回来了場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 「ただーいま、っと」
CASE "オナバレ"
	PRINTFORML 「……なんだよ」
	PRINTFORML %CALLNAME:TARGET%通过叉叉の悪そうな顔でそっぽを向いている……
CASE "露脐装超短裙胖次チラ"
	IF TALENT:男性
		PRINTFORML 「%TEXTR(@"喂——不要偷看哦/唔哇、想看看男人的内褲呢/好喜欢啊、%CNAME("二人称")%も")%」
		PRINTFORML %CALLNAME:TARGET%毫不羞涩地笑着。
	ELSE
		PRINTFORML 「%TEXTR("おっと、流石に女の胖次チラは訳が違うぞ。マジで/うわ、ちょ……待った待った。ダメだって")%」
		PRINTFORML %CALLNAME:TARGET%只能苦笑着。
	ENDIF
CASE "低腰臀圍"
	PRINTFORML 「%TEXTR("なんだ、男の尻に興味でもあんのかぁ？/どうよ、ケツにゃ結構自信あんだぜ")%」
	PRINTFORML %CALLNAME:TARGET%通过自己的尻を惜しげもなく見せつけている……
CASE "掀起裙子"
	PRINTFORML 「ったく、野郎の裙の中なんか見てはしゃぎやがってよー」
CASE "检查内褲"
	IF COND("合意：检查内褲")
		PRINTFORML 「%TEXTR(@"やれやれ。満足かい、%CNAME("主人")%桑よ/いい趣味してるぜ、まったく")%」
	ENDIF
CASE "检查阴部"
	IF COND("合意：检查阴部")
		PRINTFORML 「%TEXTR("ふー……ちょっと落ち着かねえな/あんまりやらせるなよな……汚れがさ……")%」
	ENDIF
CASE "检查菊穴"
	IF COND("合意：检查菊穴")
		PRINTFORML 「%TEXTR(@"ったく……本物だよ、%CNAME("二人称")%/いるところに是るもんだよなぁ、変態って")%」
	ENDIF
CASE "拜托処理性欲"
	PRINTFORML 「%TEXTR("ふぅ……満足したか？/収まったか？ それとも……")%」
	PRINTFORML %CALLNAME:TARGET%通过%CALLNAME:MASTER%の様子を伺っている……
CASE "进行性処理"
	PRINTFORML 「ふー。おつかれおつかれ」
	PRINTFORML %CALLNAME:TARGET%通过一瞥もくれずに平然としている……
CASE "帮忙消除性欲"
	PRINTFORML 「うんうん、ここの部活らしいことしたな」
	PRINTFORML %CALLNAME:TARGET%通过満足そうに頷いている……
CASE "清洁口交"
	PRINTFORML 「ふーっ……ほれ、終わったぞ」
	PRINTFORML %CALLNAME:TARGET%通过%CALLNAME:MASTER%の体を轻轻地叩きながら出口た。
CASE "買東西"
	IF COND("巨大振動棒購入") && DEVENT("巨大振動棒感想") == 0
		PRINTFORML 「っはー、これまたでっけえの買ったなぁ」
		IF TALENT:MASTER:男性
			PRINTFORML 「%CNAME("二人称")%、少しは気遣いってもんを考えた方がいいぞー」
		ELSE
			PRINTFORML 「……やっぱサイズ無えと駄目だよなぁ……」
			PRINTFORML %CALLNAME:TARGET%通过溜め息をついている……
		ENDIF
		CALL SET_DEVENT, "巨大振動棒感想"
	ELSE
		;単純にいっぱい買ったかどうかで分岐
		SELECTCASE COND("買上品数")
		CASE IS >= 4
			PRINTFORML 「いやぁー買ってきたねぇ。壮観なこって」
		CASEELSE
			PRINTFORML 「ふーん、これ使うつもりなんだ？ へえー」
		ENDSELECT
		PRINTFORML %CALLNAME:TARGET%通过%CALLNAME:MASTER%が持ってきた袋を見て含み笑いをしている……
	ENDIF
CASE "隐秘摄影失敗"
	PRINTFORML 「ははは、まだまだ甜なー%CNAME("二人称")%も」
CASE "变成裸体"
	;他に人がいる
	IF CHARA_NUM("室内の邪魔者")
		PRINTFORML 「ったく、人前は勘弁してくれよなー……」
		PRINTFORML さすがの%CALLNAME:TARGET%も恥ずかしいのか、平静を装いながらも目を逸らしている……
	;まさかの野外
	ELSEIF PLACE("野外")
		PRINTFORML 「……で、いつまでこうしてりゃいいんだ」
		PRINTFORML さすがの%CALLNAME:TARGET%もこの状況では落ち着かないようだ……
	;不在個室
	ELSEIF PLACE("個室") == 0
		PRINTFORML 「ったく……人が来ないうちに満足してくれよ？」
	;個室で二人きり
	ELSE
		IF TALENT:男性
