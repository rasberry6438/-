@KOJO_GREETING_K8, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STRW, "「ふぅーーーーっ_H_」"
	PRINTFORMW 「いいよね？ね？こんな可爱寝顔見せられたら我慢出来ないじゃんね！？」
;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"
	IF EXP_GAIN("射精経験", PLAYER)
		CALL PRINT_STR, @"「んふふ、%CNAME("二人称")%ご馳走様サマ_H_」_W"
	ELSE
		PRINTFORMW 「%CNAME("二人称")%，舒服吗？」
	ENDIF
ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%, %CSTR:朝袭%
		[ENDIF]
	CASE "朝騎乗位"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%, %CSTR:朝袭%
		[ENDIF]
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	PRINTFORMW 「おっはー、%CNAME("二人称")%」
	IF COND("体育")
		IF RAND:2
			PRINTFORMW 「女の子のお尻ばっかり見てたらおこだかんね」
			PRINTFORMW 「%CNAME("一人称")%の是ーけど♪」
		ELSE
			PRINTFORMW 「%CNAME("一人称")%の体操服サイズがちっさいからパツパツなんだよねぇー」
			PRINTFORMW 「んふふ、想像した？」
		ENDIF
	ELSEIF RAND:2
		PRINTFORMW 「%CNAME("一人称")%朝ごはんとかもバッチリ作っちゃうんだよねぇ」
		PRINTFORMW 「……惚れた？」
	ELSE
		PRINTFORMW 「早上好うのギュー♪」
	ENDIF
CASE "一番乗り"
	[IF_DEBUG]
		PRINTFORMW KOJO_GREETING_K8, %ARGS%
	[ENDIF]
CASE "ただいま"
	[IF_DEBUG]
		PRINTFORMW KOJO_GREETING_K8, %ARGS%
	[ENDIF]
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%(夜)
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%(昼)
		[ENDIF]
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%
		[ENDIF]
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%
		[ENDIF]
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%(既に部員の大半集合済み)
		[ENDIF]
	ELSEIF CFLAG:呼び出された
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K8, %ARGS%
		[ENDIF]
	ENDIF
