;-------------------------------------------------
;部員同士での交流。自分はTARGETとして呼ばれ、話相手の部員はPARTNER()として呼ばれる
;話相手への呼びかけはCNAME("partner")で出来る
;TFLAG:台詞スキップ = 1で話している相手のセリフのスキップ、TFLAG:地の文スキップ = 1で続く地の文のスキップをする事が出来る
;-------------------------------------------------
@KOJO_COMM_K46, ARGS
#DIM MEMO_LINECOUNT
;対話相手にしたい角色の名前を/区切りでこれに突っ込んでFIND_TaiTa(EVENT_TABLE)とするとそのうち一人が選ばれる
#DIMS EVENT_TABLE
;避難というかPARTNER()で大丈夫なのか不安なのでACTOR
#DIM ACTOR

EVENT_TABLE = 
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;話しかける側のセリフ。COND("対話可能")を満たす相手を選んでPARTNER()にする事で、特定の相手に話しかけることもできる
CASE "お誘いする"
	;特定の相手に話しかける場合はEVENT_TABLEに対話相手の呼び名(CALLNAME)を / 区切りで並べて ; を消す
	;例１）弗理希艾尔or濡烏の君or杏珠から一人選ぶ場合
	EVENT_TABLE = 白色死神/玛丽/奥丝卡/卡莉斯塔/千紗/小夜/麦莉爱娜/綾乃/紬希/灯/弗理希艾尔/真/蔻卡/安娜

	;例２）部員全てから一人選ぶ場合
	;EVENT_TABLE = 全員

	IF FIND_TaiTa(EVENT_TABLE)
		TFLAG:台詞スキップ = 1
		;話し相手をpartnerにする
		CALL SET_PARTNER, FIND_TaiTa(EVENT_TABLE)
		CALL EMOTION_PLUS, "交流中", TARGET, PARTNER()

		SELECTCASE CALLNAME:PARTNER()
		CASE "弗理希艾尔"
			IF CEVENT("フェリ初会話") == 0
				PRINTFORMW 「好啊%CNAME("partner")%、想不到你竟然是幕后黑手」
				PRINTFORMW 『什么叫幕后黑手。%CNAMET(PARTNER(), "一人称")%只是答应了%CNAMET(PARTNER(), "主人")%的请求罢了』
				PRINTFORMW 「少骗人了、大体方向可能是%CNAME("主人")%指出来的、具体的细节肯定是你在安排」
				PRINTFORMW 『也算吧。虽然%CNAMET(PARTNER(), "主人")%是社团长、但作为提出要求的人什么都不做确实有点难以理解』
				PRINTFORMW 「对吧？　准备活动室和道具、还有外人一看就明白但实际上根本不会照着做的活动表……」
				PRINTFORMW 「%CNAME("二人称")%的存在非常重要。倒不如说整个社团活动都是以%CNAME("二人称")%为中心的」
				PRINTFORMW 『哈哈、得到夸奖很高兴、但都是多亏了%CNAMET(PARTNER(), "主人")%才能做好社团活动』
				PRINTFORMW 『%CNAMET(PARTNER(), "一人称")%一个人的话恐怕不能做到这样』
				PRINTFORMW 「だがな%CNAME("partner")%。もしアイツがひとりでやったとして、ここまで洒落た空間变成了と思うか？　本当に性交しかしない部活になってそうじゃないか？」
				PRINTFORMW 『……不太好否定呢』
				CALL SET_CEVENT, "フェリ初会話"
			ELSE
				IF RAND:2
					PRINTFORMW 「%CNAME("partner")%很厉害啊、%CNAME("一人称")%的话根本不想做饭」
					PRINTFORMW 『习惯就好了』
					PRINTFORMW 「虽然这样说、%CNAME("一人称")%的话给钱也不会做的……会做饭确实不一样」
					PRINTFORMW 『%CNAMET(PARTNER(), "一人称")%也有偷懒的时候。太累了或者没时间就会准备速冻食品。最近做的都不错』
					PRINTFORMW 「唔呣、强者的余韵啊……」PRINTFORMW 『天使や堕天使の翼って鳥のような形をしていますけど、？』	PRINTFORMW 『天使和堕天使都鸟翅膀？』
				ELSE
					PRINTFORMW 『天使や堕天使の翼って鳥のような形をしていますけど、梅雨の時は問題ないのですか？』
					PRINTFORMW 「嗯？　是这个啊。看起来像这样，但是灵体之类的东西。雨这样普通的水是没法打湿的」
					PRINTFORMW 「但是、习惯温度的影响、夏天还是很热……」
					PRINTFORMW 「黑色翅膀是最糟的。你看、黑色非常吸收太阳光是吧？　虽然是灵体不会烧起来、但还是热的要命啊」
					PRINTFORMW 『这样啊、天使也有天使的烦恼啊』
				ENDIF
			ENDIF
		;メリィ妈妈の場合
		CASE "麦莉爱娜"
			IF CEVENT("メリィ初会話") == 0
				PRINTFORMW 「……真没想到这家伙竟然是你儿子、%CNAME("partner")%」
				PRINTFORMW 『抱歉、%CNAMET(PARTNER(), "一人称")%也没想到会弄出这么大的社团……』
				PRINTFORMW 「说是灵魂特殊、其他方面跟常人也没什么不同。为了不让他和没用的事情烦恼、让他健康成长的想法也可以理解」
				PRINTFORMW 「……嗯、像无限的一样真不可思议」
				PRINTFORM 『不好说。
				IF TALENT:好色
					PRINTFORMW ……成长的过程中有些奇怪的地方还很像的』
				ELSE
					PRINTFORMW ……真是的、到底像谁呢』
				ENDIF
				PRINTFORMW %CALLNAME:PARTNER()%和%CALLNAME:TARGET%看着%CALLNAME:MASTER%一起叹了口气。
				CALL SET_CEVENT, "メリィ初会話"
			ELSE
				IF RAND:2
					PRINTFORMW 「还是人间伙食好。不仅质量好还口味丰富，根本不会腻」
					PRINTFORMW 『……啊、%CALLNAME:TARGET%老师曾经在魔界待过一段时间吧』
					PRINTFORMW 「那是很久以前的事了。我所在的地区是自然环境比较恶劣的」
					PRINTFORMW 『原来如此……那么，魔界猪排咖喱味噌乌冬面定食你不知道吧?』
					PRINTFORMW 「哈……魔界什么……？」
					SIF CEVENT("未知のカレー") == 0
						CALL SET_CEVENT, "未知のカレー"
				ELSE
					PRINTFORMW 『又到了这种时候……』
					PRINTFORMW 「啊、考试打分啊」
					PRINTFORMW 『嗯……为什么必须无偿加班啊』
					PRINTFORMW 「就是啊。还不能让学生看见这样子。特别是初等部学生」
					PRINTFORMW 「『对啊……』」
				ENDIF
			ENDIF
		;卡莉斯塔酱の場合
		CASE "卡莉斯塔"
			IF CEVENT("卡莉斯塔初対面") == 0
				PRINTFORMW 「哎……%CNAME("partner")%吗。你在这个社团里啊」
				PRINTFORMW 『其实我也才知道%CALLNAME:TARGET%您是堕天使。很吃惊啊』
				PRINTFORMW 「我为了不被一些奇奇怪怪的发现身份还是做了不少伪装的。以后好好相处吧」
				PRINTFORMW 『好的，今后也请多関照……』
				PRINTFORMW 「……这是？」
				PRINTFORMW 『……嗯，游戏机啊？ 那个，我听说老师喜欢玩游戏，所以就……』
				PRINTFORMW 「不，我玩过的也认得出来……呃？ 现在的天使这么散漫吗？」
				CALL SET_CEVENT, "卡莉斯塔初対面"
			ELSE
				IF RAND:2
					PRINTFORMW 「……啊啊啊啊！！ 再来打一盘！」
					PRINTFORMW 『……老师，时间是不是太晚了？』
					PRINTFORMW 「还早！ 可恶……我是有实力的……！」
				ELSE
					PRINTFORMW 『啊，你死了。……』
					PRINTFORMW 「还没结束！」
				ENDIF
				PRINTFORMW %CALLNAME:PARTNER()%和%CALLNAME:TARGET%玩起了游戏……
			ENDIF
		;玛丽桑の場合
		CASE "玛丽"
			IF CONFIG("部員間Ｈ") && TALENT:PARTNER():淫乱 && COND("欲求不満", PARTNER()) && CEVENT("玛丽逆レ") == 0
				;玛丽暴走了
				;角色崩壊みたいなもんだけど割とコメントアウトしてない所でもやらかしてるからええやろ・・・
				PRINTFORMW 『说实话，%CNAMET(PARTNER(), "二人称")%好矮啊』
				PRINTFORMW 「哈？ 怎么这么说」
				PRINTFORMW 『这算是个玛丽苏设定吗，还是说是后宫的一部分然后要一起收掉』
				CALL PRINT_STRW, @"『像这种巨根正太肯定是大姐姐在那里说『啊、好大……_H_』然后插入反转的类型』"
				PRINTFORMW 「说正经的，不要搞黄色」
				PRINTFORMW 『小孩巨根这样的反转是重罪、绝对不可以原谅』
				PRINTFORMW 『小个子就应该在大姐姐身上拼命摇摆时大姐姐有微笑摸頭的余韵，怎么能有巨根呢。肛門開发虽然不坏但还是不一样的、圣经上面是这样说的』
				PRINTFORMW 「你在说什么伪典」
				PRINTFORMW 『你以为成年了就可以了吗，想抢%CNAMET(PARTNER(), "一人称")%的位子吗』
				PRINTFORMW 『必须要天诛！ 责任和义务要求我挑战%CNAMET(PARTNER(), "二人称")%！』
				PRINTFORMW 「不对、你只是单纯的想做爱吧……喂！」
				PRINTFORML 
				;ここからエッチ内容の設定
				;大半を交流イベントから流用する
				TFLAG:PLAY回数 = (RAND:3+1) * MIN(ABL:欲望 / 5 + 1, 10)
				ACTOR = PLAYER
				SWAP PLAYER, TARGET
				TARGET = PARTNER()
				CALL SIMULATE_TRAIN_FIRST
				CALL SIMULATE_TRAIN_COM, TNUM("騎乗位"), TFLAG:PLAY回数
				CALL SIMULATE_TRAIN_CALC
				;ここで入れ替えていた角色クター達を戻していく
				SWAP PLAYER, TARGET
				PLAYER = ACTOR
				PRINTFORMW %CALLNAME:TARGET%和%CALLNAME:PARTNER()%做了。
				PRINTFORML 
			ELSEIF RAND:3 == 0
				PRINTFORMW 「……对了%CNAME("partner")%。为什么把手机関了？」
				PRINTFORMW 『……嘘！　不行不行。我的手机出问题了我正在调整』
				PRINTFORMW 「……你啊」
			ELSEIF RAND:2
				PRINTFORMW 『堕天使的话、对聖遺物应该有抗性吧』
				PRINTFORMW 「我们的身体构造完全不同啊。事实上就算你把它和魔鬼放在一起来看也是很麻烦的分类」
			ELSE
				;ここでは出来るだけ玛丽桑のCOMMとコール＆レスポンスが出来るように
				;その関係でTFLAG:台詞スキップが0に返回
				TFLAG:台詞スキップ = 0
				IF COND("相性☆", PARTNER()) && COND("欲求不満") && COND("欲求不満", PARTNER())
					PRINTFORMW 「……%TEXTR("嗯、原来如此/确实可以这么做/那就简单了")%」
				ELSE
					PRINTFORMW 「%TEXTR("啊、能帮我一下吗/好闲啊、不聊点什么吗/现在你是学生的话、要不要学习一下？")%」
				ENDIF
			ENDIF
		;真酱の場合
		CASE "真"
			IF CEVENT("真初会話") == 0
				PRINTFORMW 『嘿呀、老、老师！？』
				PRINTFORMW 「啊%CNAME("partner")%。请多関照啊？」
				PRINTFORMW 『为、为什么%CALLNAME:TARGET%老师也加入了社团啊！？』
				PRINTFORMW 「喂喂、你这说法――」
				PRINTFORMW 『因为老师你长得比女孩子还漂亮！』
				PRINTFORMW 『%CNAMET(PARTNER(), "一人称")%不用像我这样想尽办法来打扮自己就能受人喜欢了！』
				PRINTFORMW 「……哦」
				IF TALENT:MASTER:男性
					PRINTFORMW 『……哈！　目标也是%CNAMET(PARTNER(), "主人")%吗！？』
					IF CNUM("灯")
						PRINTFORMW 『像拿下哥哥的%CNAMET(PARTNER(), "主人")%一样、老师也是肉食系吗！？』
					ELSE
						PRINTFORMW 『连老师都在%CNAMET(PARTNER(), "主人")%的狩猎范围内吗……！』
					ENDIF
				ELSE
					PRINTFORMW 『……哈！　您的目标是弗理希艾尔小姐吗！？』
					PRINTFORMW 『想要独占恶魔小姐的胸部吗！？』
				ENDIF
				PRINTFORMW 「喂、正常点」
				CALL SET_CEVENT, "真初会話"
			ELSEIF CEVENT("真初会話") && CEVENT("真謝罪") == 0
				PRINTFORMW 『之前真的很抱歉』
				PRINTFORMW 「啊、没関系。……嗯、确实有点惊讶就是了」
				PRINTFORMW 「嗯、不断克服失败的人会变得更坚强。因为脑子里有了更多的经验和对策」
				PRINTFORMW 「虽然不知道%CNAME("partner")%是怎么想的、但了解到%CNAME("二人称")%的真实想法让%CNAME("一人称")%很高兴」
				PRINTFORMW 『老师……！』
				PRINTFORMW 『……老师的话，用%CNAMET(PARTNER(), "一人称")%的屁股也没関系』
				PRINTFORMW 「哦……嗯？　嗯！？」
			ELSE
				IF RAND:2
					PRINTFORMW 「喂、是不是喝太多了？」
					PRINTFORMW 『真是的！　怎么说%CALLNAME:TARGET%老师也不知道%CNAMET(PARTNER(), "一人称")%的事情吗！　哈！』
					PRINTFORMW 『怎么说%CNAMET(PARTNER(), "一人称")%也只是个普通人！　老师给人一种遗世独立的感觉？　我可不是躺在别人膝盖上就会開心的女孩子哦～！』
					PRINTFORMW 「……在找呕吐袋吗」
				ELSE
					PRINTFORMW 「哦，那个钥匙圈……」
					PRINTFORMW 『咦，老师知道吗？』
					PRINTFORMW 「啊、我的朋友有专門收集这个的人。%CNAME("二人称")%喜欢这个吗？」
					PRINTFORMW 『嗯，怎么说呢……』
					PRINTFORMW 「……嗯？　那个，算是中等偏小的尺寸吧？」
					PRINTFORMW 『是的。以前偶尔看见过一次、后来就再也没见过了』
					PRINTFORMW 『怎么说呢。当时有种奇妙的共鸣感……』
				ENDIF
			ENDIF
		;奥丝卡桑の場合
		CASE "奥丝卡"
			IF CEVENT("奥丝卡との邂逅")
				IF CEVENT("クラーケン？", PARTNER() )
					PRINTFORMW 『对了，前几天我和邻居一起去水族馆了……』
					PRINTFORMW 「……你看到那个了吗」
					PRINTFORMW 『……不要紧吧？』
					PRINTFORMW 「没问题。……我和馆长认识、包括%CNAME("一人称")%在内，所有听到的人都反对。不过结果还是被强硬摊派了，后来变成了受欢迎的展览，到现在也不好意思说要撤了……」
				ELSE
					PRINTFORMW 『%CALLNAME:TARGET%啊，可以告诉我这里的规矩吗？』
					PRINTFORMW 「哦，好啊。那么，首先……」
				ENDIF
			ENDIF
		;灯君の場合
		CASE "灯"
			IF CEVENT("灯初会話") == 0
				PRINTFORMW 「啊、听说过。好像做的很华丽」
				PRINTFORMW 『喂、这种地方说话要像个老师啊』
				PRINTFORMW 「嗯、%CNAME("二人称")%说什么%CNAME("一人称")%都无所谓」
				PRINTFORMW 「但是、这么想我像个老师的话也行、不要一边做爱一边考虑这种麻烦事」
				PRINTFORMW 『哈？ 你觉得你在干什么』
				PRINTFORMW 「就是这种感觉。……算了、往比起做爱来逃避、不管是发泄还是释怀，或者转换都更有效果。这就是人生老前辈的劝解」
				PRINTFORMW 『……人类じゃないだろ』
				CALL SET_CEVENT, "灯初会話"
			;灯君がTSした時
			ELSEIF TALENT:男性 == 0 && CEVENT("灯ＴＳ") == 0
				PRINTFORMW 「哦、变成女人了吗」
				PRINTFORMW 『……有什么不好吗』
				IF CEVENT("伪娘の理由", PARTNER())
					PRINTFORMW 「不，我觉得很适合你」
					PRINTFORMW 「大概很多事情都下定了决心吧?既然如此，又有什么関系呢?」
					PRINTFORMW 『对、对啊』
					PRINTFORMW 『……啊、%CNAMET(PARTNER(), "二人称")%、那时候的事，谢谢――』
					PRINTFORMW 「反正不管怎么样都是注定的」
					PRINTFORMW 『哦』
				ELSE
					PRINTFORMW 「如果你说没有，那就不说谎」
					PRINTFORMW 「本来%CNAME("二人称")%需要的不是炮友、而是好好听话的人」
					PRINTFORMW 「这也是%CNAME("二人称")%希望的……不是吗？」
					PRINTFORMW 『……随便想把』
				ENDIF
				CALL SET_CEVENT, "灯ＴＳ"
			ELSE
				IF RAND:2
					PRINTFORMW 『长生不老到底是什么感觉呢……』
					PRINTFORMW 「别想了、没%CNAME("二人称")%想的那样舒服」
					PRINTFORMW 『就算这样说……』
				ELSE
					PRINTFORMW 「啊……可以吗？」
					PRINTFORMW 『怎么这么客气』
					PRINTFORMW 「哎呀、%CNAME("二人称")%大概在高中结束后会离開吧？」
					PRINTFORMW 『……对啊』
					PRINTFORMW 「%CNAME("二人称")%很聪明。这里人手一直不足、甚至需要猫来搭把手。更别说优秀的人了」
					PRINTFORMW 「……%CNAME("partner")%、我们的教师职位、或者研究职位。你有兴趣加入吗？」
					PRINTFORMW 『不嫌弃的话太好了』
				ENDIF
			ENDIF
		;綾乃酱の場合
		CASE "綾乃"
			IF CEVENT("綾乃初会話") == 0
				PRINTFORMW 「啊、%CNAME("二人称")%也在这里工作吗」
				PRINTFORM 『嗯。
				IF TALENT:PARTNER():恋慕
					PRINTFORMW 最初是ろんな男の人を探さなくてもいいからだったんだけど、いつの間にか%CNAMET(PARTNER(), "二人称")%のことが好きになっちゃって……』
					PRINTFORMW 「这样啊……嗯、也没规矩说魅魔不能幸福。我觉得你不用为此感到内疚」
					PRINTFORMW 『是啊、尽力吧』
				ELSE
					PRINTFORM 他の男の子を探さなくても済むし、何より
					IF PENIS(MASTER) == 0
						PRINTFORMW %CNAMET(PARTNER(), "二人称")%很可爱不是吗？』
						CALL PRINT_STRW, @"『あの可爱子が%CNAMET(PARTNER(), "一人称")%の手でびくびく_BH_ってしながら精子をいっぱい出したり、逆にオスみたいな顔つきで推倒てきたりするのがクセになっちゃったんだ_BH_』"
					ELSEIF TALENT:PLAYER:童貞
						CALL PRINT_STR, @"いろんなハジメテに戸惑う%CNAMET(PARTNER(), "二人称")%の反応がかわいくてきゅんきゅんしちゃうから……_BH_』_W_"
					ELSE
						PRINTFORM %CNAMET(PARTNER(), "二人称")%
						IF ABL:PLAYER:技巧 >= 3
							SELECTCASE CONDS("基本阴茎尺寸", MASTER)
							CASE "短小"
								;CALL PRINT_STR, @"的技巧很厉害、小鸡鸡也能给人强烈的快感……_BH_』_W_"
								CALL PRINT_STR, @"のテクニックがすごくて、入り口のところでいっぱいイカされちゃうから……_BH_』_W_"
								CALL PRINT_STRW, @"『%CNAMET(PARTNER(), "二人称")%の可爱おちんちんでおまんこの浅い所をいっぱい突かれると、それだけで気持ちよくて頭が真っ白になっちゃうんだ_BH_』"								;CALL PRINT_STRW, @"『太舒服了子宫都在抽搐_BH_但小鸡鸡根本够不着那里、不过这种焦躁地感觉也习惯了_BH_』"
							CASE "普通"
								CALL PRINT_STR, @"的技巧很厉害……_BH_』_W_"
								CALL PRINT_STRW, @"『胸部でぎゅってしてあげられちゃうおちんちんにいっぱいご奉仕するんだけど、礼物にＧスポットの辺りをいっぱい突いてもらって何回もイっちゃうんだ_BH_』"
