	SELECTCASE CALLNAME:PARTNER()
	CASE "玛丽"
		IF COND("相性○", PARTNER())
			SELECTCASE TEXTS("季節詳細")
			CASE "春"
				IF RAND:2
					PRINTFORMW 『说是这么说、大家開心就好……』
					PRINTFORMW 『……那么。和其他人一起晚上去看樱花怎么样？　安静的和白天的有不一样的魅力』
				ELSE
					PRINTFORMW 『%TEXTR("嗯、好啊/可以、换换口味/哦、真的吗")%』
				ENDIF
			CASE "初夏"
				IF COND("今年の泳装選択済", PARTNER())
					PRINTFORMW 『嗯、你觉得没事就行』
					IF COND("今年の泳装選択済")
						PRINTFORMW 『啊、%CNAME("一人称")%可以』
					ELSE
						PRINTFORMW 『……那么、约%CNAME("一人称")%我出去是情侣还是类似的事情吗？』
					ENDIF
				ELSE
					PRINTFORMW 『%TEXTR("嗯、好啊/可以、换换口味/哦、真的吗")%』
				ENDIF
			CASE "夏"
				IF RAND:2
					PRINTFORMW 『%CNAME("一人称")%想去山那里怎么样』
					PRINTFORMW 『海也不是不行、山上也挺湿润的。……哎、看到时候的天气吧』
				ELSE
					PRINTFORMW 『%TEXTR("哦……去凉快点的地方吧……/那里有空调吗……？/有没有冰激淋啊……")%』
				ENDIF
			CASE "秋"
				IF RAND:2
					PRINTFORMW 『%CNAME("一人称")%不是很想。不喜欢』
					PRINTFORMW 『但是，你的决定的话我就去』
				ELSE
					PRINTFORMW 『%TEXTR("嗯、好啊/是的、可以换换口味/有点凉快，不过这样也好")%』
				ENDIF
			CASE "冬"
				IF RAND:2
					PRINTFORMW 『哦、很期待啊』
					PRINTFORMW 『重要的是你们学生開心……開个玩笑』
				ELSE
					PRINTFORMW 『%TEXTR("嗯、好啊/要戴好围巾/冬天去吗")%』
				ENDIF
			ENDSELECT
		ELSE
			PRINTFORMW 『嗯，没関系……怎么了？』
		ENDIF
	ENDSELECT

ENDSELECT

