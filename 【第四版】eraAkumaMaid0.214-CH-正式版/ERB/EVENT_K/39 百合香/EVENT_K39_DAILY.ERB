;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K39, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"打招呼"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "打招呼"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝舐陰", "朝六九式"
		SELECTCASE RAND:3
		CASE 0
			CALL PRINT_STR, @" 「っん、早上好……_HH_」_W"
		CASE 1
			PRINTFORML 「あ、起きちゃいましたか？」
			IF TEXTS("曜日") == "日"
				PRINTFORMW 「え、続きですか？その前に朝ご飯にしません？」
			ELSE
				PRINTFORMW 「そろそろ学校行く時間だから、続きは学校でしましょう」
				PRINTFORMW 「……ダメでしょうか？」
			ENDIF
		CASEELSE
			PRINTFORML 「早上好、%CNAME("二人称")%桑」
		ENDSELECT
	CASE "朝騎乗位"
		PRINTFORML 「%CNAME("二人称")%桑、起きてくださ～い」
		PRINTFORMW 「……むー、下はしっかり起きてるのに……」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育ですよ。%CNAME("二人称")%桑、体操服とかは忘れてないですよね？」
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「今日は何か予定有ますか？」
		CASE 1
			PRINTFORMW 「ちょっと待って下さい、茶入れてきますね」
		CASEELSE
			PRINTFORMW 「早上好。しっかり眠れましたか？」
			PRINTFORMW %CNAME("二人称")%温柔的微笑着。
		ENDSELECT
	ENDIF
CASE "一番乗り"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORMW 「あ、%CNAME("二人称")%桑」
		PRINTFORMW 「今日は%CNAME("一人称")%が一番のようですね」
	CASE 1
		PRINTFORMW 「いっちばーん！……って、たまには言ってみてもいいのでしょうか。」
		PRINTFORMW 「兒童っぽいけど……たまには。ですね。」
	CASEELSE
		PRINTFORMW 「他に誰も来てないみたいですね。」
	ENDSELECT
CASE "ただいま"
	PRINTFORML 「我回来了」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「欢迎回来」
	ELSE
		PRINTFORMW 「あ、待ってましたよ」
	ENDIF
CASE "入室打招呼"
	SELECTCASE CSTR:PREV_DAILYCOM
	CASE "甜言蜜语"
		PRINTFORMW (あれっ、お邪魔になっちゃってます？)
	CASE "打招呼舐陰", "打招呼口交"
		PRINTFORMW 「ちょ、ちょっと何してるんですか……？」
	CASEELSE
		PRINTFORMW 「お邪魔します。」
	ENDSELECT
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「打扰了」
		PRINTFORMW 「……あらあら、皆桑お揃いなんですね」
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「…今日は何をしますか？」
		CASE 1
			PRINTFORMW %CALLNAME:TARGET%轻轻点頭。
		CASEELSE
			PRINTFORMW 「下午好。」
		ENDSELECT
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『你好、%CNAME(CALLNAME_ACTOR)%。』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 『皆桑ほとんど揃ってますよ。』
		CASE 1
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、你好。』
		CASEELSE
			PRINTFORMW 『您好。』
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 『你好。』
		CASE 1
			PRINTFORMW %CALLNAME:TARGET%轻轻点頭。
		CASEELSE
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、你好。』
		ENDSELECT
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 「ただいま戻りました」
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "便当"
		PRINTFORML 「今日は、%TEXTR("海苔弁当/姜汁焼肉弁当/飯団の詰め合わせ/鮭の塩焼き弁当/")%を作ってきたんです。」
	SELECTCASE RAND:4
	CASE 0
		PRINTFORML 「给、这是你的%TEXTR("梨/橘子/草莓/苹果/桃子")%。」
	CASE 1
		PRINTFORML 「%CNAME("一人称")%の自信作です。お味はどうでしょうか？」
	CASE 2
		PRINTFORML %CALLNAME:TARGET%夹起便当、送到%CALLNAME:MASTER%嘴前。
		CALL PRINT_STR, @" 「恥ずかしいですけど……、あ～ん_HH_」_W"
	CASEELSE
		PRINTFORML 「%CNAME("二人称")%、満足していただけたでしょうか？」
	ENDSELECT
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面随机属性しても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;覚え書き：ここは基本的にMLで書く
;-------------------------------------------------
@KOJO_DAILY_K39, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「本日からお世話になります、小早川百合香です」
	PRINTFORML 「よろしくお願いします」
CASE "浑身是汗"
	IF TALENT:恋慕
		PRINTFORML 「運動は心地いいですね」
		PRINTFORML 「……气味、嗅ぎたいのですか？ ……後で嗅がせてあげますから、待ってくださいね」
	ELSE
		PRINTFORML 「運動は心地いいですね」
		PRINTFORML 「あまり气味嗅がれると、その……恥ずかしいのですけど」
	ENDIF
CASE "刚睡醒"
	PRINTFORML 「ふぁ、あふ……早上好……」
CASE "Cosplay中"
	PRINTFORML 「这个、どうでしょうか？」
	PRINTFORML 「%CNAME("一人称")%に似合いますか……？」
;沉眠著
CASE "睡眠"
	;梦话
	CALL KOJO_SLEEP_K39
	;梦话用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回指令対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「%CNAME("一人称", "かな")%に恋人なんて出来ないと思っていましたけど」
	CALL PRINT_STRL, @" 「……嬉しいです……_HH_」"
CASE "约会的约定"
	PRINTFORML 
;これは悪魔や天使や驱魔人のみ
CASE "契約成立"
;你にお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	PRINTFORML 
CASE "摆脱"
	CALL PRINT_STR, @" 「……_HH_」_L"
CASE "主人公的房間に連れ込まれた"
	PRINTFORML 「お邪魔します。」
CASE "自室に招待"
	PRINTFORML 「%CNAME("一人称")%的房間で何をしましょうか？」
CASE "自室に訪問された"
	;指导者を伴っている場合
	IF FIND_COND("現在存在", "助手")
		PRINTFORML 
	ELSE
		PRINTFORML 
	ENDIF
CASE "指导者部屋訪問"
	PRINTFORML 
;自宅移動の後に你に連れられて居間に回来了場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 「我回来了」
CASE "オナバレ"
	;この辺は初期の合意(口上側で用意した合意の方)がある場合と無い場合で分岐
	;あったら誘ってくる、なくてもちょっと恥ずかしがるぐらい
	PRINTFORML 「……！？ちょっと、%CNAME("二人称")%桑！？ あっちいってくださいっ、えっち、スケベ、のぞき魔ーーーー！」
	PRINTFORML %CALLNAME:TARGET%红着脸低下了頭、不想让你看见。
	PRINTFORML 虽说是不自觉的、但还是让你眼前一亮…思考了一下、还是暂时不要说了。
CASE "露脐装超短裙胖次チラ"
	PRINTFORML 「きゃっ！？ もう、あんまりじろじろ見ないでください……」
CASE "低腰臀圍"
	PRINTFORML 
CASE "掀起裙子"
	PRINTFORML 「……%CNAME("二人称")%桑だけですよ？こういうことしていいよって言うのは……」
CASE "检查内褲"
	IF COND("合意：检查内褲")
		PRINTFORML 「%CNAME("二人称")%桑、他の人にはやっちゃダメですからね……？」
	ENDIF
CASE "检查阴部"
	;人が居るとき分岐させる
	IF COND("合意：检查阴部")
		PRINTFORML 「%CNAME("二人称")%桑……人前でこういうことは、いけないことだと思うんですが……」
	ENDIF
CASE "拜托処理性欲"
	PRINTFORML 「……しょうがないですね」
CASE "进行性処理"
	PRINTFORML 「片手間になりますけど、それでもいいですか？」
CASE "清洁口交"
	PRINTFORML 
CASE "買東西"
	;買ってきた道具などで分岐
	;IF COND("巨大振動棒購入")

	;	CALL SET_DEVENT, "巨大振動棒感想"
	;ELSE
		;単純にいっぱい買ったかどうかで分岐
		SELECTCASE COND("買上品数")
		CASE IS >= 4
			PRINTFORML 「今日是っぱい買いましたね」
		CASEELSE
			PRINTFORML 「购物行く時はまた誘って下さいね」
		ENDSELECT
	;ENDIF
CASE "隐秘摄影失敗"
	PRINTFORML 「……あら、何をしているんですか……？」
CASE "变成裸体"
	;他に人がいる
	IF CHARA_NUM("室内の邪魔者")
		PRINTFORML 「……流石にここで裸は人目を引きますね」
	;まさかの野外
	ELSEIF PLACE("野外")
		PRINTFORML 「呜呜、在外面赤裸着……」
	;そのうち書きたいです。ちょっと思いつかない。
	;不在個室
	ELSEIF PLACE("個室") == 0
		PRINTFORML 「就算在室内……这样子……」
	;個室で二人きり
	ELSE
		CALL PRINT_STR, @"「……_HH_」_L"
	ENDIF
CASE "Ｈ目的で呼び出された"
	IF PLACE("情人旅館")
		PRINTFORML 「呼んでくれて有がとうございます」
	;做爱房
	ELSE
		PRINTFORML 「……校内でするというのも、緊張しますね」
		PRINTFORML 「私はもうそういう時期を過ぎてしまったので、ちょっとうれしさも有ますけど」
	ENDIF
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;標語の反応。書いたらコメントアウトを外してください
;各分岐を細かく埋めずに、上の分岐を消して望みの学生服に着替えてやったぞ発言をここに書くのも良いと思います
IF CFLAG:標語実践
	IF STRCOUNT(SAVESTR:標語, "注意口渴")
		PRINTFORML 「のど乾くとやっぱり気になりますよね」
		PRINTFORML 「スポーツドリンクとかも部室におけないでしょうか……」
		PRINTFORML 「あ、ジュース類だからだめですか？」
	ELSEIF STRCOUNT(SAVESTR:標語, "体操服")
		SELECTCASE SAVESTR:標語
		CASE "体操服はやっぱり紧身短褲","体操服はやっぱり运动衫","体操服はやっぱり运动短褲"
			PRINTFORML 「着替えた是いですけど、普段と違うから落ち着かないですね……」
		CASE "体操服はやっぱり布鲁马"
			PRINTFORML 「やっぱり体操服は布鲁马ですよね」
			PRINTFORML 「勒紧的感觉非常喜欢」
		CASEELSE

		ENDSELECT
	ELSEIF STRCOUNT(SAVESTR:標語, "学生服")
		SELECTCASE SAVESTR:標語
		CASE "学生服は露脐装超短裙に限る","学生服は超短迷你に限る"
			PRINTFORML 「着替えた是いですけど、普段と違うから落ち着かないですね……」
		CASEELSE
			;夏服でも短い、気になる
		ENDSELECT
	ENDIF
ENDIF

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;イベント台詞。CFLAG:日常事件回数が10増えるたびに一つ叫醒ぐらいで良いでしょう
IF CFLAG:日常事件回数 >= 5 && DEVENT("質問１") == 0 && TALENT:相思相愛 == 0
	PRINTFORML 「苦手なもの……やっぱり男の人でしょうか。男の人は……意識的に遠ざけてしまうんです」
	PRINTFORML 「どうしてか是つか言えたらいいと思ってるんですが……」
	CALL SET_DEVENT, "質問１"
ELSEIF CFLAG:日常事件回数 >= 15 && DEVENT("質問２") == 0
	PRINTFORML 「好きなモノですか？葱が好きですよ。玉ねぎも長葱も大好きです」
	PRINTFORML 「なんなら玉ねぎサラダは毎日食べられちゃいます」
	CALL SET_DEVENT, "質問２"
ELSEIF DEVENT("質問１") == 1 && DEVENT("質問３") == 0 && TALENT:相思相愛
	PRINTFORML 「苦手なもの……やっぱり男の人でしょうか。男の人は……未だに意識的に遠ざけてしまうんです」
	PRINTFORML 「ずっとそうして過ごしてきたから、今更変えづらいのもあるんですけれど」
	CALL SET_DEVENT, "質問３"
ELSEIF CFLAG:日常事件回数 >= 25 && DEVENT("質問４") == 0 && ABL:信頼 >= 5
	PRINTFORML 「I字平衡ですか？んーーー……Y字ならギリギリ出来ますけど……」
	PRINTFORML 「……ポーズを撮る時に見せてあげますから、それで……ダメでしょうか」
	CALL SET_DEVENT, "質問４"
ENDIF

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;他の人といちゃついた後にpartner変更
IF COND("いちゃいちゃ") == 0 && FIND_COND("Ｈ的いちゃつき中")
	PRINTFORML 何が、とは言わないが、別の場所でお願いします、という視線を向けている……
ENDIF

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;以下は随机台詞など

;時間帯と場所と関係で変化有些
IF COND("夜")
	IF RAND:4
		PRINTFORML 突然感觉到了谁的视线、环顾四周、发现%CALLNAME:TARGET%正朝这边微笑着。
		PRINTFORML 「……%CNAME("二人称")%桑、どうしましたか？」
	ELSEIF RAND:3 == 0
		PRINTFORML %CALLNAME:TARGET%坐在沙发上看书。
		PRINTFORML 仔细看了看、%CALLNAME:TARGET%似乎在读的是%TEXTR("杂志/漫画书/食谱")%的样子……。
	ELSEIF RAND:2 == 0
		PRINTFORML %CALLNAME:TARGET%坐在沙发上阅读着参考书。
		PRINTFORML 仔细观察了一下、%CALLNAME:TARGET%好像正在阅读%TEXTR("国语/数学/英语/理科/地歴公民")%的参考书……。
	ELSE
		PRINTFORML %CALLNAME:TARGET%通过頭戴式耳机を付けて%TEXTR("iPad/スマートフォン/Switc○")%で和我一起玩いるようだ。
	ENDIF
ENDIF

;-------------------------------------------------
;梦话。随意むにゃむにゃ言わせてください
;-------------------------------------------------
@KOJO_SLEEP_K39
;上から順に発言頻度が高いようにする(== 0の付け忘れではない)
IF RAND:3
	IF RAND:3 == 0
		PRINTFORML 「zzz…………」
	ELSEIF RAND:2 == 0
		PRINTFORML 「嘶…………嘶…………」
	ELSE
		PRINTFORML 「ん……それは……だめですよ……」
	ENDIF
ELSEIF RAND:4
	IF RAND:4 == 0
		PRINTFORML 「%CNAME("二人称", "かな")%桑……んぅ、むにゃ……」
	ELSEIF RAND:3 == 0
		PRINTFORML 「もー……%CNAME("二人称", "かな")%桑ってば……ふふ……♪」
	ELSEIF RAND:2 == 0
		CALL PRINT_STRL, @"「%CNAME("二人称", "かな")%桑は……ほんとに……えっちなんですね……_HH_」"
	ELSE
		PRINTFORML 「欸哆……再多来一点……」
	ENDIF
ENDIF

;-------------------------------------------------
;メイン画面で選択肢として独立している日常イベントでのセリフ。WAIT使って大丈夫です
;こっちはあくまで選択肢で選ばれた場合に限るのに注意。
;-------------------------------------------------
@KOJO_DAILYCOM_K39, ARGS
;打招呼
#DIMS GREETING
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR


SELECTCASE ARGS
;通話などでの呼出なので括弧は「」ではなく『』推奨
CASE "Ｈ目的で呼び出された"
	;台詞を書いたらRETURN 0 を消してください
	;RETURN 0
	IF PLACE("情人旅館")
		CALL PRINT_STR, @" 『好的、那么就到那里吧……_HH_』_W"
	;做爱房
	ELSEIF PLACE("做爱房")
		PRINTFORMW 『んっ、じゃあこれから行きますね♪』
	ELSE
		PRINTFORMW 『待ち合わせですね、我明白了』
	ENDIF
CASE "学園での日常"

CASE "お仕事"

CASE "归途"
	;SAVESTR:現在位置 と SAVESTR:归途の行動 を決定する
	CALL SETFLAG, "遭遇地点"

	;地の文の前の台詞がある場合はここに書く
	SELECTCASE SAVESTR:現在位置
	CASE "阵雨"
		IF RAND:2 == 0
			PRINTFORML 「突然下大雨……」
			PRINTFORML 「あっ、有がとうございます、%CNAME("二人称")%桑」
			PRINTFORML (ホントは折りたたみ傘あるんですけど……こういうのも、いいですよね)
		ELSE
			PRINTFORML 「%CNAME("二人称")%桑、すみません！傘持ってきてなかったのに雨降ってきちゃって……」
			PRINTFORML 「……え？　入れてくれるんですか？有がとうございます！」
		ENDIF
	ENDSELECT

	;地の文を出す
	CALL DAILY_MESSAGE, "归途"

	SELECTCASE SAVESTR:归途の行動
	CASE "相合傘"
		PRINTFORMW 「相合い傘もついに高校生までの間には叶わなかったんですよね」
