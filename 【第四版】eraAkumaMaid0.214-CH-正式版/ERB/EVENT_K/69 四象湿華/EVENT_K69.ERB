;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K69, ARGS
#FUNCTIONS
;名前を呼ぶ相手の角色番号の代入に使う
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF TALENT:PLAYER:男性
			LOCALS = 貴方_你
		ELSE
			LOCALS = 貴女_你
		ENDIF
	;相手が主人ではない
	ELSE
		LOCALS = 您_您
	ENDIF
;この下は三人称。
CASE "主人"
	LOCALS = 您_您
CASEELSE
	;名前を呼ぶ相手の角色番号をACTORに代入する
	SELECTCASE ARGS
	;相手が助手
	CASE "助手"
		ACTOR = ASSI
	;ここにはARGSに角色のCALLNAMEが入ったりしている場合(ARGS == "弗理希艾尔" など)に来る
	CASEELSE
		ACTOR = FIND_NO(ARGS)
	ENDSELECT

	;相手が教員
	IF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%老师_%CSTR:ACTOR:ふりがな%老师

	;自分も相手も学生なら学年比較
	ELSEIF COND("学生") && COND("学生", ACTOR)
		;相手が同学年
		IF COND("学年") == COND("学年", ACTOR)
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		;相手が下の学年
		ELSEIF COND("学年") > COND("学年", ACTOR)
			IF TALENT:ACTOR:男性
				LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
			ELSE
				LOCALS = %CALLNAME:ACTOR%酱_%CSTR:ACTOR:ふりがな%酱
			ENDIF
		;相手が上級生
		ELSE
			LOCALS = %CALLNAME:ACTOR%前輩_%CSTR:ACTOR:ふりがな%前輩
		ENDIF

	;それ以外
	ELSE
		LOCALS = %CALLNAME:ACTOR%桑_%CSTR:ACTOR:ふりがな%桑
	ENDIF
ENDSELECT
RETURNF LOCALS

