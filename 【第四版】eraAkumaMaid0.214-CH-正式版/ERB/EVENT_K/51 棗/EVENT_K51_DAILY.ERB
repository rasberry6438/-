@KOJO_DAILY_K51, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「%CALLNAME:TARGET%です。よろしくお願いします！」
CASE "浑身是汗"
	PRINTFORML 「うう……べとべとする……」
CASE "刚睡醒"
	IF RAND:4 == 0
		PRINTFORML 「むな……？」
	ELSEIF RAND:3
		PRINTFORML 「あふ……」
	ELSEIF RAND:2
		PRINTFORML 「ふわぁ……」
	ELSE
		PRINTFORML 「ううん……」
	ENDIF
CASE "Cosplay中"
	PRINTFORML 「えへへ、変じゃないかな？」
CASE "点心の感想"
	;今日の点心を用意したのは自分
	IF COND("点心当番")
		PRINTFORML 「えへへ、喜んでくれると嬉しいな！」
	;今日の点心は誰かの亲手自制
	ELSEIF COND("亲手自制点心")
		;今日の点心当番の人の亲手自制を初めて食べた
