			CALL PRINT_STRW, @"「あっ、早上好う%CNAME("二人称")%」"
			CALL PRINT_STRW, @"「今は……%CNAME("二人称")%のおまんこを舐めてたの_H_勝手にごめんね……_H_」"
		ELSE
			PRINTFORMW 「……あっ、早上好う……」
			PRINTFORMW 「えっーっと……出来心で……ごめんね？」
		ENDIF
	CASE "朝足交"
		IF TALENT:相思相愛
			PRINTFORMW 「あっ、早上好う%CNAME("二人称")%」
			CALL PRINT_STRW, "「今はね、足でおちんちんを……あ、起きたらよりご心情变成了ね_H_」"
