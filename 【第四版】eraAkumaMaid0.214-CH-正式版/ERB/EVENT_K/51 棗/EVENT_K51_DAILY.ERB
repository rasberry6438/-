			PRINTFORMW 「～♪あっ、早上好う%CNAME("二人称")%！」
			CALL PRINT_STRW, @"「今はね、胸部がだ～い好き_H_な%CNAME("二人称")%のおちんちんをー」"
			CALL PRINT_STRW, "「いい子_H_いい子_H_って……あっ嬉しいんだ_H_」"
		ELSEIF TALENT:恋慕
			PRINTFORMW 「あっ、早上好う%CNAME("二人称")%！」
			CALL PRINT_STRW, "「今はね、おちんちんが寂しそうにしてたから胸部で……あっ、びくびくしてる_H_」"
		ELSE
			PRINTFORMW 「よいしょ……あっ、早上好う！」
			PRINTFORMW 「えっと、これは、あそこが大きくなってたから……つい……」
