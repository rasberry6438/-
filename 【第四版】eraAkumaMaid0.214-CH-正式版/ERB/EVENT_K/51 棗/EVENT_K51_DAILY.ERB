			CALL PRINT_STRW, @"「今はぁ、%CNAME("二人称")%のおちんちんを、おまんこに借りてるっ、のっ_H_はぅ_H_」"
			CALL PRINT_STRW, "「ごめんね_H_すぐっ_H_イくからっ_H_はうっ_H_きもちいっ_H_うぅん_H_」"
		ELSEIF TALENT:恋慕
			CALL PRINT_STRW, @"「はあっ……んうっ、あっ、早上好っ、%CNAME("二人称")%っ……はうっ_H_」"
			CALL PRINT_STRW, @"「おちんちん、ちょっと、借りて……んうっ、%CNAME("一人称")%、イっちゃ……うぅ_H_」"
		ELSE
			PRINTFORMW 「はっ……ふぅ……あっ、おはっ、んはっ！」
			PRINTFORMW 「ごめ、すぐにぃ……はっ……あっうっ」
		ENDIF
	CASE "朝Ａ騎乗位"
		IF TALENT:相思相愛
			CALL PRINT_STRW, @"「んっ……はっ……あ、早上好うっ、%CNAME("二人称")%っ、んんっ_H_」"
			CALL PRINT_STRW, @"「今はぁ、%CNAME("二人称")%のおちんちんを、おしりに借りてるっ、のっ_H_はぅ_H_」"
			CALL PRINT_STRW, "「ごめんね_H_すぐっ_H_イくからっ_H_おしり_H_ダメなのに_H_きもちいっ_H_うぅん_H_」"
		ELSEIF TALENT:恋慕
			CALL PRINT_STRW, @"「はあっ……んうっ、あっ、早上好っ、%CNAME("二人称")%っ……はうっ_H_」"
			CALL PRINT_STRW, "「おちんちん、ちょっと、借りて……んうっ、おしりっ、違うのni↘、イっちゃ……うぅ_H_」"
		ELSE
			PRINTFORMW 「はっ……ふぅ……あっ、おはっ、んはっ」
			PRINTFORMW 「ごめ、すぐにぃ……はっ……おしりぃ……あっうっ」
		ENDIF
	CASE "朝接吻"
		IF TALENT:相思相愛
			CALL PRINT_STRW, @"「んっ……ちゅ……はぁ_H_早上好う、%CNAME("二人称")%……ふっ……んちゅ_H_」"
			CALL PRINT_STRW, "「朝からどうしてもちゅーしたくて……んっ……好き_H_大好き_H_……ん……ふっ……_H_」"
		ELSEIF TALENT:恋慕
			PRINTFORMW 「んっ……ちゅ……あ、早上好う%CNAME("二人称")%」
			CALL PRINT_STRW, "「ちゅーしてたんだけど……もっとしていいよね……んっ、ふっ……_H_」"
		ELSE
			PRINTFORMW 「んっ……ちゅっ……はっ……あっ、早上好う%CNAME("二人称")%」
			PRINTFORMW 「えっーっと……もうちょっと、ちゅーしてて、いい？」
		ENDIF
