			CALL PRINT_STRW, @"「んぶっ……ふぅ……んはぁ……早上好う、%CNAME("二人称")%……_H_」"
			CALL PRINT_STRW, "「今日も朝からおちんちんが元気だから……んん～_H_こうやっへ_H_んっ_H_おと无くさせてるの_H_んうっ_H_」"
		ELSEIF TALENT:恋慕
			PRINTFORMW 「んんっ……んぶっ……はぁ……早上好う、%CNAME("二人称")%」
			CALL PRINT_STRW, "「今、元気なおちんちんを落ち着かせるから……んっ……まっふぇふぇめ_H_」"
		ELSE
			PRINTFORMW 「んんっ……んっ……はっ……あっ、早上好う……」
			PRINTFORMW 「えっーっと……出来心で………ごめんね？」
		ENDIF
	ENDSELECT
;朝のＨイベント終了後にする打招呼
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝六九式", "朝乳交", "朝舐陰", "朝騎乗位", "朝Ａ騎乗位", "朝足交", "朝逆正常位", "朝接吻"
		IF TALENT:恋慕
			PRINTFORMW 「えへへ、ついつい……」
			PRINTFORMW 「今度は%CNAME("二人称")%が……なーんて……えへへ……」
		ELSE
			PRINTFORMW 「ごめんね……魔が差したって言うか……」
			PRINTFORMW %CALLNAME:TARGET%通过背中を丸くして照れている……
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1
CASE "登校"
	IF DAYEV("試験当日")
		PRINTFORMW 「早上好う、テストだけど%CNAME("二人称")%は自信ある？%CNAME("一人称")%はあんまり……」
	ELSEIF DAYEV("試験返却日")
		PRINTFORMW 「早上好う～……返却怖いなぁ……はぁ……」
	ELSEIF COND("朝袭相手")
		IF TALENT:恋慕
			
		ENDIF
	ELSEIF COND("体育")
		PRINTFORMW 「早上好う、%CNAME("二人称")%。今日は体育だけど体操服忘れてたりしてない？」
	ELSEIF COND("水泳")

	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「早上好う、%CNAME("二人称")%。今日も元気？」
		CASE 1
			PRINTFORMW 「おっはよう！%CNAME("二人称")%！えへへ、目が覚めたでしょ」
		CASEELSE
			PRINTFORMW 「あっ、早上好う、%CNAME("二人称")%！」
		ENDSELECT
	ENDIF
CASE "一番乗り"
		PRINTFORMW 「您好ー、あ、%CNAME("二人称")%！」
		PRINTFORMW 「まだ他の皆は来てないよー」
CASE "ただいま"
		PRINTFORMW 「我回来了——」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「おかえりなさい、%CNAME("二人称")%。今日もお疲れ様」
			PRINTFORMW 「……なんだか、新婚桑みたいだね……えへへ……」
		CASE 1
			PRINTFORMW 「あ、%CNAME("二人称")%。おかえり、遅かったね？」
		CASEELSE
			PRINTFORMW 「欢迎回来、%CNAME("二人称")%」
		ENDSELECT
	ELSE
		PRINTFORMW 「あ、%CNAME("二人称")%！遅かったね」
	ENDIF
CASE "入室打招呼"
	IF CFLAG:呼び出された
		IF COND("Ｈ的いちゃつき中", MASTER) && TALENT:恋慕
			PRINTFORMW 「……………………………ふぅん。要件は何？」
			PRINTFORMW 普段の%CALLNAME:TARGET%とはかけ出口た冷たい視線で%CALLNAME:MASTER%を見下ろしている……
		ELSE
			PRINTFORMW 「どうかしたの？%CNAME("二人称")%？」
		ENDIF
	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF TALENT:恋慕
			PRINTFORMW 「……………………………」
			PRINTFORMW 普段の%CALLNAME:TARGET%とはかけ出口た冷たい視線で%CALLNAME:MASTER%を見下ろしている……
		ELSE
			PRINTFORMW 「……えっーと……お邪魔じゃない？%CNAME("一人称")%」
		ENDIF
	ELSE
		PRINTFORMW 「你好」
	ENDIF
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「わわっ、遅れちゃった！ごめんなさい！」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「来たよ、何か用事かな？」
	ELSE
		PRINTFORMW 「你好」
	ENDIF
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
			PRINTFORMW 『あ、%CNAME(CALLNAME_ACTOR)%、您好』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、遅かったね？』
	ELSE
		PRINTFORMW 『您好、%CNAME(CALLNAME_ACTOR)%』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「なぁに？%CNAME("二人称")%」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "便当"
	SELECTCASE RAND:4
	CASE 0
		PRINTFORMW 「%CNAME("二人称")%と便当食べてるとね」
		PRINTFORMW 「こう、小さい頃に戻ったみたいで……えへへ、なんだか嬉しいんだ」
	CASE 1
		PRINTFORMW 「本日の～おすすめおかず～♪」
		PRINTFORMW 「え～本日のおすすめは%TEXTR("迷你オムレツ/ミートボール/チキンナゲット/漢堡肉/玉子焼き")%です。よーく味わってね？」
	CASE 2
		PRINTFORMW 「一人で食べてるよりもなんだかおいしく感じるな……なんて」
	CASE 3
		PRINTFORMW 「%CNAME("二人称")%、おいしい？」
	ENDSELECT
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"

ENDSELECT

