	ENDSELECT
	
CASE "Cosplay中"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「为什么%CNAME("一人称")%会变成这样……」
		PRINTFORML 「%CNAME("二人称", "どもり")%……%TEXTR("喂，想打你！/可以打你吗？/我要打爆你的頭！")%」
	CASE 1
		PRINTFORML 「为什么%CNAME("一人称")%穿成这样……」
		PRINTFORML 「喂，喂！？ %TEXTR("禁止拍照/不要拍视频")%！！」
	CASEELSE
		PRINTFORML 「……」
		PRINTFORML 「%CNAME("一人称")%的尊严正在消失……」
	ENDSELECT

;沉眠著
CASE "睡眠"
	;梦话
