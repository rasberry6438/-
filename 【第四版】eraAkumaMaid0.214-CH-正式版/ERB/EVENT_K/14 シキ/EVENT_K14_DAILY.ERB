;-------------------------------------------------
;各種打招呼
;-------------------------------------------------
@KOJO_GREETING_K14, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中の角色
#DIM ACTOR
;イベント発生中の角色の默认のCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交", "朝六九式"
		IF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				CALL PRINT_STR, @"「%TEXTR("嗯……/成熟的/噗哈")%_H_ ……什么嘛.%CNAME("一人称")%已经起床了？」_L"
				PRINTFORMW %CALLNAME:TARGET%舔了舔口边的精液
			CASE 1
				CALL PRINT_STR, @"「%TEXTR("吐舌/嗯哼/呵呵")%_H_ 怎么了？%CNAME("主人")%_H_」_L"
				PRINTFORMW %CALLNAME:TARGET%像什么事都没有发生一样開始做早上的准备。
			CASEELSE
				CALL PRINT_STR, @"「%TEXTR("么么嗯/噗哈/嗯~～")%_H_ 什么啊？你喜欢这样吧？」_L"
				PRINTFORMW 当%CALLNAME:MASTER%点頭时，%CALLNAME:TARGET%看起来很满足地開始收拾起来
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……%TEXTR("嗯，嗯/咳咳")%！？什、什么啊……起来了啊……」
				PRINTFORMW %CALLNAME:TARGET%是u%TEXTR("看起来不舒服/像是被骂了一样/兴冲冲地")%開始收拾
			CASE 1
				PRINTFORML 「……%TEXTR("嗯，嗯/咳咳")%！？ ……%CNAME("一人称")%为了尽快恢复原状……正因为如此……」
				PRINTFORMW 好像是那样的事的呢
			CASEELSE
				PRINTFORML 「……啊 ……%CNAME("二人称")%什么都没看见。%CNAME("一人称")%什么都没做」
				PRINTFORMW %CALLNAME:TARGET%很快就从%CALLNAME:MASTER%的房间里走了出来。
			ENDSELECT
		ENDIF

	CASE "朝騎乗位"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K14, %ARGS%, %CSTR:朝袭%
		[ENDIF]
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「今天有体育课吗？」
			PRINTFORMW 看起来%CALLNAME:TARGET%很%TEXTR("兴奋/高兴")%。
		CASE 1
			PRINTFORML 「今天有体育课……但是，太阳光太强了」
			PRINTFORMW %CALLNAME:TARGET%正在认真地读着标题为怀恨在心地盯着太阳。
		CASEELSE
			PRINTFORML 「等一下有体育课，别忘了穿体操服哦？」
			PRINTFORMW %CALLNAME:TARGET%把%CALLNAME:MASTER%体操服交给我。
		ENDSELECT

	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「%TEXTR("嗯……/打着哈欠、/早……")%早上好……」
			PRINTFORMW %CALLNAME:TARGET%正在认真地读着标题为揉着困倦的眼睛
		CASE 1
			PRINTFORML 「嗯……哈哈」
			PRINTFORMW %CALLNAME:TARGET%正在认真地读着标题为眠そうに朝の打招呼をしてきた。
		CASEELSE
			PRINTFORML 「饭已经做好了吗？」
			PRINTFORMW %CALLNAME:TARGET%好像肚子饿了。
		ENDSELECT
	ENDIF

CASE "一番乗り"
	IF TALENT:恋慕
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「嗯……和%CNAME("二人称")%两人独处」
			PRINTFORMW %CALLNAME:TARGET%坐在%CALLNAME:MASTER%的旁边。
		CASE 1
			PRINTFORML 「哟！……什么啊，只有%CNAME("二人称")%在啊？」
			PRINTFORMW %CALLNAME:TARGET%虽然这么说，但好像很開心的样子
		CASEELSE
			PRINTFORML 「ウイーっす……お、%CNAME("一人称")%だけか」
			PRINTFORMW %CALLNAME:MASTER%确认了%CALLNAME:TARGET%微笑着。
		ENDSELECT

	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「什么啊，和%CNAME("二人称")%两个人啊……」
			PRINTFORMW %CALLNAME:TARGET%与%CALLNAME:MASTER%稍微拉開距离坐了下来。
		CASE 1
			PRINTFORML 「哟！……只有%CNAME("二人称")%在吗？」
			PRINTFORMW %CALLNAME:TARGET%的表情很微妙。
		CASEELSE
			PRINTFORML 「嗯……唉」
			PRINTFORMW %CALLNAME:TARGET%看到%CALLNAME:MASTER%を的脸，叹了口气。
		ENDSELECT
	ENDIF

CASE "ただいま"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「我回来啦，肚子饿了~」
		PRINTFORMW %CALLNAME:TARGET%吧嗒吧嗒地回来了。
	CASE 1
		PRINTFORML 「啊~累坏了……哎呀，我回来了」
		PRINTFORMW %CALLNAME:TARGET%疲惫地回来了。
	CASEELSE
		PRINTFORML 「我回来了……今天阳光好刺眼。」
		PRINTFORMW %CALLNAME:TARGET%懒洋洋地回来了。
	ENDSELECT

CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「不好意思……什么嘛，%CNAME("一人称")%来接你不好吗！？」
				PRINTFORMW 到門口迎接了%CALLNAME:TARGET%有点害羞。
			CASE 1
				PRINTFORML 「欢迎回来，%CNAME("主人")%…………怎么说呢？」
				PRINTFORMW 女仆装身姿的%CALLNAME:TARGET%迎接了%CALLNAME:MASTER%。
				IF FIND_COND("弗理希艾尔", "現在存在")
					PRINTFORML 「弗理希艾尔桑为我准备的……怎么样？」
				ELSE
					PRINTFORML 「意外的很适合我吧？」
				ENDIF
				PRINTFORMW %CALLNAME:TARGET%是%TEXTR("用笑容/装出一副若无其事的样子/一边挺着腰")%转了一圈给我看。
				CALL SET_COORDINATE, "女仆装", TARGET
			CASEELSE
				PRINTFORML 「欢迎回来，……绕道过来了吗？」
				PRINTFORMW  看来%CALLNAME:TARGET%很担心晚回来的%CALLNAME:MASTER%。
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「嗯~回来了~」
				PRINTFORMW %CALLNAME:TARGET%正在认真地读着标题为在客厅里看电视。
			CASE 1
				PRINTFORML 「喂～……啊，可恶！？」
				PRINTFORMW %CALLNAME:TARGET%一边玩游戏一边向%CALLNAME:MASTER%扑腾扑腾挥手。
			CASEELSE
				PRINTFORML 「欢迎回来……你是怎么绕道来这的？」
				PRINTFORMW 看来%CALLNAME:TARGET%在期待着特产。
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「哟！怎么这么晚啊！」
				PRINTFORMW %CALLNAME:TARGET%が前来迎接。
			CASE 1
				PRINTFORML 「哦……%CNAME("二人称")%也要吃吗？」
				PRINTFORMW %CALLNAME:TARGET%拿出了自己带的点心。
			CASEELSE
				PRINTFORML 「迟到了吧？是……%TEXTR("被老师骂了/还捡到了色情杂志呢/钱包丢了")%？」
				PRINTFORMW %CALLNAME:TARGET%開始对迟到的%CALLNAME:MASTER%做出偏离目标的推理。
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「嘿咻嘿咻……」
				PRINTFORMW %CALLNAME:TARGET%好像在锻炼肌肉。
			CASE 1
				PRINTFORML 「嗯，……呦」
				PRINTFORMW %CALLNAME:TARGET%很费事地打了招呼。
			CASEELSE
				PRINTFORML 「啊～……好无聊……」
				PRINTFORMW %CALLNAME:TARGET%像是在躲避阳光一样地懒散着。
			ENDSELECT
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		IF TALENT:恋慕
			PRINTFORML 「有什么事吗？」
			PRINTFORMW 被%CALLNAME:MASTER%叫到的%CALLNAME:TARGET%进了房间。
		ELSE
			PRINTFORML 「……你有什么事吗？」
			PRINTFORMW 被%CALLNAME:MASTER%叫到的的%CALLNAME:TARGET%が%TEXTR("粗鲁地/看起来很麻烦的样子/一边打哈欠")%进入房间了
		ENDIF

	;你が現在的partnerにＨ的悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		CALL EMOTION_PLUS, "心情差", TARGET
		PRINTFORML 「嗯……很好玩吗？」
		PRINTFORML 　%CALLNAME:TARGET%确认了捣乱的是%CALLNAME:MASTER%后眯起了眼睛。
		PRINTFORML 
		CFLAG:好感度 -= 20
		PRINTFORMW 　好感度 -20

	ELSE
		IF TALENT:恋慕
			PRINTFORML 「失礼了」
			PRINTFORMW %CALLNAME:TARGET%进入了房间。
		ELSE
			PRINTFORML 「……失礼了」
			PRINTFORMW %CALLNAME:TARGET%%TEXTR("粗鲁地/看起来很麻烦的样子/一边打哈欠")%进入房间了。
		ENDIF
	ENDIF
	
;部屋にいる他の人から打招呼を返される打招呼。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「在闲聊吗？」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「在闲聊吗？」
	ELSE
		PRINTFORMW 「在闲聊吗？」
	ENDIF
	
;簡易入室打招呼に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室打招呼中")がONなのでFIND_COND("入室打招呼中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室打招呼中")で調べられる
CASE "打招呼返し"
	;打招呼してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれた角色の默认のCALLNAME。この角色への呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;打招呼してきた人に老师が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『啊……老师早上好』

	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『闲聊.JPG』
		
	ELSE
		PRINTFORMW 『闲聊.JPG』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		IF TALENT:恋慕
			PRINTFORML 「有什么事吗？」
			PRINTFORMW 被%CALLNAME:MASTER%叫到的%CALLNAME:TARGET%进了房间。
		ELSE
			PRINTFORML 「……你有什么事吗？」
			PRINTFORMW 被%CALLNAME:MASTER%叫到的的%CALLNAME:TARGET%が%TEXTR("粗鲁地/看起来很麻烦的样子/一边打哈欠")%进入房间了
		ENDIF

	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「啊……喉咙好渴」
	ELSE
		PRINTFORMW 「啊……喉咙好渴」
	ENDIF

CASE "抜け出された"
	IF TALENT:恋慕
		CALL EMOTION_PLUS, "心情差", TARGET
		PRINTFORML 「……色鬼，就这！」
		PRINTFORML %CALLNAME:TARGET%用轻蔑的眼光目送着%CALLNAME:MASTER%。
		PRINTFORML
		CFLAG:好感度 -= 20
		PRINTFORMW 　好感度 -20
	ELSE
		PRINTFORML 「啊？今天就结束可以吗？」
		PRINTFORMW %CALLNAME:TARGET%是%TEXTR("兴冲冲地/麻利")%開始准备回去了。
	ENDIF

CASE "便当"
	SELECTCASE RAND:7
	CASE 0
		PRINTFORML 「真是的……不要把便当拜托给%CNAME("一人称")%哦」
		PRINTFORMW 　和%CALLNAME:TARGET%一起吃了茶色的便当。
	CASE 1
		PRINTFORML 「不好意思，今天是简单的食物哦？」
		PRINTFORMW 　和%CALLNAME:TARGET%亲手做的三明治一起吃了。
	CASE 2
		PRINTFORML 「这好像是の弗理希艾尔桑的便当啊」
		PRINTFORMW 　和%CALLNAME:TARGET%昨天剩下的东西一起吃了便当。
	CASE 3
		PRINTFORML 「嗯……肚子饱了……是胃变小了吗？」
		PRINTFORML 　%CALLNAME:TARGET%好像吃到一半就吃饱了。
		PRINTFORMW 「……给你的，啊」
	CASE 4
		PRINTFORML 「偶尔会起得早啊，给这是你的」
		PRINTFORMW 　一起吃了%CALLNAME:TARGET%亲手做的便当。
	CASE 5
		PRINTFORML 「%CNAME("二人称")%不做饭吗？」
		PRINTFORMW 　和%CALLNAME:TARGET%一起吃了简单的海苔便当。
	CASEELSE
		PRINTFORML 「如果是男性料理的话多少还能做出来啊……」
		PRINTFORMW 　和%CALLNAME:TARGET%一起吃了日之丸便当。
	ENDSELECT

;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	IF TALENT:恋慕
		SELECTCASE RAND:3
		CASE 0
			CALL EMOTION_PLUS, "惊嚇", TARGET
			PRINTFORML 「等一下！？不要一下子就起哄了！」
			PRINTFORMW %CALLNAME:TARGET%是%TEXTR("慌慌张张/快点儿")%走了~了。
			TFLAG:退出する = 1
		CASE 1
			CALL EMOTION_PLUS, "心情差", TARGET
			PRINTFORML 「嗯……呼……嘿！不要吵到我」
			PRINTFORMW %CALLNAME:TARGET%的视线%TEXTR("很冷/很痛/刺进去")%。
		CASEELSE
			PRINTFORML 「是是，还是老样子呢……不，讨厌习惯了的自己……」
			PRINTFORMW %CALLNAME:TARGET%表情复杂。
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			CALL EMOTION_PLUS, "惊嚇", TARGET
			PRINTFORML 「等一下！？不要一下子就起哄了！」
			PRINTFORMW %CALLNAME:TARGET%是%TEXTR("慌慌张张/快点儿")%走了~了。
			TFLAG:退出する = 1
		CASE 1
			CALL EMOTION_PLUS, "惊嚇", TARGET
			PRINTFORML 「……真是没节操啊！？」
			PRINTFORMW %CALLNAME:TARGET%吐槽了%CALLNAME:MASTER%。
		CASEELSE
			CALL EMOTION_PLUS, "躁动难忍", TARGET
			PRINTFORML 「啊……嗯，我出去了」
			PRINTFORMW %CALLNAME:TARGET%%TEXTR("そ慌慌张张/红着脸")%出去了。
			TFLAG:退出する = 1
		ENDSELECT
	ENDIF

ENDSELECT

