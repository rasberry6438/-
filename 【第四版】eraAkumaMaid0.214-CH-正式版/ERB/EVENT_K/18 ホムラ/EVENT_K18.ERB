@CNAME_K18, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSE
		IF COND("教員", ASSI)
			LOCALS = %CALLNAME:PLAYER%老师_%CSTR:PLAYER:ふりがな%老师
		ELSE
			LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
		ENDIF
	ENDIF
CASE "主人"
	LOCALS = 主人様_主人大人
CASE "助手"
	LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
ENDSELECT
RETURNF LOCALS

