	;胖次没収中
	IF COND("買い物胖次没収中")
		CALL PRINT_STRW, @"「はぁぁ～～……_／／／_ ど、どきどきしたぁ…_／／／_」"
		CALL PRINT_STRW, @"「ね、ねぇ%CNAME("二人称")%…帰ったら米娅の相手をお願いしますねっ…_HH_」"
