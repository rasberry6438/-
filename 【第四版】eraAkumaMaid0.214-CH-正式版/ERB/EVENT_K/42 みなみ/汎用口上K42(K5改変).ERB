;-------------------------------------------------
;口上随机生成関数
;ARGSは種別
;式中関数として叫出来のはひとまず保留
;
;@KOJO_RAND_K5をベースに使用して良いと元の作者桑から許可を得ています
;使いたい人は自分の角色の口調に合わせてどうぞ
;
;改変：表示されるセリフを二行に修正
;-------------------------------------------------
@KOJO_RAND_K42(ARGS)
#DIM LCOUNT
#DIM LIMIT_MEMO
LOCAL = 0
LOCALS = 
SELECTCASE ARGS
CASE "接吻"
	;情緒による口上変化度合いを決定
	;情緒30につき爱心一個分に相当する
	;情緒が150以上
	IF FLAG:情緒 >= 150
		LOCAL = 5
	;情緒が120以上なら確定、かつ90未満は範囲外
	ELSEIF FLAG:情緒 >= 120 || (FLAG:情緒 >= 90 && FLAG:情緒 >= RAND:180)
		LOCAL = 4
	;情緒が90以上なら確定、かつ60未満は範囲外
	ELSEIF FLAG:情緒 >= 90 || (FLAG:情緒 >= 60 && FLAG:情緒 >= RAND:180)
		LOCAL = 3
	;情緒が60以上なら確定、かつ30未満は範囲外
	ELSEIF FLAG:情緒 >= 60 || (FLAG:情緒 >= 30 && FLAG:情緒 >= RAND:180)
		LOCAL = 2
	ELSE
		LOCAL = 1
	ENDIF
	;1.情緒が高い場合の追加口上　一行目　爱心4つ
	IF LOCAL >= 4
		LOCALS:1 = %TEXTR("啾~/嗯啾~/嗯~/嗯嗯~")%
		IF LOCAL == 5
			LOCALS:2 = 　
		ENDIF
	ENDIF
	;1.に続いて表示される　一行目
	;2.情緒がさらに高いと追加口上　爱心5つ
	;☆☆☆ここを改変すれば汎用で使用可能☆☆☆
	;汎用口上の一行目を削除して、口上側で一行目にセリフ+二行目に汎用喘ぎの方が便利に使えるかもしれない
	IF LOCAL == 5 && TALENT:恋慕
		LOCALS:3 = %TEXTR(@"%CNAME("主人", "甘え")%…/%CNAME("主人")%、")%%TEXTR("喜欢……/最喜欢了……/请一直……")%%TEXTR(@"%CNAME("一人称")%和我亲吻吧……/%CALLNAME%请宠爱我……/请永远都不要离開我……")%%UNICODE(0x2665)%
	ENDIF
	;長くても2回まで　二行目
	LIMIT_MEMO = LIMIT(RAND:LOCAL, 2, 3)
	FOR LCOUNT, 0, LIMIT_MEMO
		;ちゅっちゅ系
		IF RAND:2 == 0
			LOCALS = %LOCALS%%TEXTR("啾嗯……、/啾…/啾唔…/啾…………/唔嗯……、")%%TEXTR("啾呜……/啾唔…/啾……嗯/哈啊…………/啾………啾呜……")%
		;んっんっ系
		ELSE
			LOCALS = %LOCALS%%TEXTR("＿/嗯、/嗯唔、/嗯……")%%TEXTR("嗯唔…/啾、嗯嗯…/嗯嗯……/嗯嗯…唔、嗯…/嗯……唔……、啾…/呼啊……、嗯…/嗯唔、啾…")%%TEXTR("…/…/…/……呜……/…呜嗯……")%
		ENDIF
		;情緒に依存して爱心マークである確率が上がる
		IF LOCAL >= RAND:4 && LOCAL >= 4
			LOCALS = %LOCALS%%UNICODE(0x2665)%%UNICODE(0x2665)%%UNICODE(0x2665)%
		ELSEIF LOCAL >= 3
			LOCALS = %LOCALS%%UNICODE(0x2665)%
		ENDIF
		;行末以外の記号の後に全角スペースを入れる
		SIF LIMIT_MEMO - LCOUNT > 1
			LOCALS = %LOCALS%　
		RESETCOLOR
	NEXT
CASE "口交"
	;情緒による口上変化度合いを決定
	;情緒が高いほど深く咥えこんでいることを意識
	;情緒が150以上
	IF FLAG:情緒 >= 150
		LOCAL = 5
	;情緒が120以上なら確定、かつ90未満は範囲外
	ELSEIF FLAG:情緒 >= 120 || (FLAG:情緒 >= 90 && FLAG:情緒 >= RAND:180)
		LOCAL = 4
	;情緒が90以上なら確定、かつ60未満は範囲外
	ELSEIF FLAG:情緒 >= 90 || (FLAG:情緒 >= 60 && FLAG:情緒 >= RAND:180)
		LOCAL = 3
	;情緒が60以上なら確定、かつ30未満は範囲外
	ELSEIF FLAG:情緒 >= 60 || (FLAG:情緒 >= 30 && FLAG:情緒 >= RAND:180)
		LOCAL = 2
	ELSE
		LOCAL = 1
