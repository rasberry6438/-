;-------------------------------------------------
;調教開始時のセリフ
;-------------------------------------------------
@EVENTTRAIN
	SIF NO:TARGET != 15
		RETURN 1

	DRAWLINE
	;-------------------------------------------------
	;初調教時
	;-------------------------------------------------
	IF CEVENT("調教開始：初調教") == 0
		[IF_DEBUG]
			PRINTFORMW EVENTTRAIN, Ｈ初回
		[ENDIF]
		CALL K15_VW(@"「Hai'! Nande shor kar?」")
		PRINTFORMW 槞彁は%CALLNAME:MASTER%に話しかけられた事に気が付くと、何の疑いも無く近づいてきた。
		CALL K15_V(@"「Ehe't...? Dor yiu immy deshor kar?」")
		PRINTFORMW 慢慢地、何を言われているのかを槞彁は聞き返す。
		CALL K15_VW(@"「Gommeng nar say? Hocar nor hyor geng vor onegay shimass...」")
		CALL K15_VW(@"「...」")
		CLEARLINE 1
		CALL K15_VW(@"「... ...」")
		CLEARLINE 1
		CALL K15_VW(@"「... ... ...！」")
		CLEARLINE 1
		PRINTFORMW 初め戸惑っていた槞彁の表情は、%CALLNAME:MASTER%が何を述べているのかの意味を理解すると同時に頬を染めた。
		CALL K15_VW(@"「E',A''r Sorevar...Sonnor...E'tchi yennor ossasoy... deshch yonye...?」")
		PRINTFORMW 槞彁は耳まで真っ赤にし的同时、俯いている。
		CALL K15_VW(@"「Motch long...」")
		CALL SET_CEVENT, "調教開始：初調教"
	;-------------------------------------------------
	;調教開始時（一回のみ）
	;-------------------------------------------------
	;初めてのご褒美Ｈ
	ELSEIF EQUIP:Ｈ的奖励 && CEVENT("調教開始：ご褒美Ｈ") == 0
		PRINTFORMW 
		CALL SET_CEVENT, "調教開始：ご褒美Ｈ"
	;屈服刻印Lv1
	ELSEIF MARK:屈服刻印 == 1 && TALENT:恋慕 == 0 && CEVENT("調教開始：屈服刻印ＬＶ１") == 0
		[IF_DEBUG]
			PRINTFORMW EVENTTRAIN, 屈服刻印Lv1
		[ENDIF]
		CALL SET_CEVENT, "調教開始：屈服刻印ＬＶ１"
	;屈服刻印Lv2
	ELSEIF MARK:屈服刻印 == 2 && TALENT:恋慕 == 0 && CEVENT("調教開始：屈服刻印ＬＶ２") == 0
		[IF_DEBUG]
			PRINTFORMW EVENTTRAIN, 屈服刻印Lv2
		[ENDIF]
		CALL SET_CEVENT, "調教開始：屈服刻印ＬＶ２"
	;屈服刻印Lv3
	ELSEIF MARK:屈服刻印 == 3 && TALENT:恋慕 == 0 && CEVENT("調教開始：屈服刻印ＬＶ３") == 0
		[IF_DEBUG]
			PRINTFORMW EVENTTRAIN, 屈服刻印Lv3
		[ENDIF]
		CALL SET_CEVENT, "調教開始：屈服刻印ＬＶ３"
	;恋慕
	ELSEIF TALENT:恋慕 && CEVENT("調教開始：恋慕") == 0
		;淫乱を先に獲得したかで分岐させたい場合(別にこの分岐は無くても良いと思います)
		IF CEVENT("先に淫乱獲得")
