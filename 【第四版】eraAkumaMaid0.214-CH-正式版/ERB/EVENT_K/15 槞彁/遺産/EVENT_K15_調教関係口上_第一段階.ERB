				PRINTFORMW EVENTTRAIN, 恋慕
			[ENDIF]
		ENDIF
		CALL SET_CEVENT, "調教開始：恋慕"
	;相思相愛
	ELSEIF TALENT:相思相愛 && CEVENT("調教開始：相思相愛") == 0
		PRINTFORMW 
