	;			PRINTFORMW 
	;		ENDIF
	;		CALL SET_CEVENT, "助手会話：ＸＸ"
		;二回目以降
	;	ELSE
			;恋慕
	;		IF TALENT:恋慕
	;			PRINTFORMW 
