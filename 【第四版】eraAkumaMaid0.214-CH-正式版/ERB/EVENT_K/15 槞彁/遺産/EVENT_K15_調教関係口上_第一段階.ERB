;-------------------------------------------------
;指令で絶頂する際のセリフ
;NOWEX:Ｖ絶頂 などで何由来の絶頂か分岐出来る
;-------------------------------------------------
@KOJO_MESSAGE_COMING_K15_1
	;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
	SIF ASSIPLAY
		RETURN 0

	;汎用絶頂台詞。ここだけは淫語很多でＯＫ
	IF COND("絶頂") && CHOSECOM(SELECTCOM)
		;自慰や推倒中は自己するので他と台詞の趣が異なる
		IF CHECK_COM("快楽模索中")
			;これは２回連続で同じ値を取ることが無いRANDです
			SELECTCASE KABURAND(4)
			CASE 3
				[IF_DEBUG]
					PRINTFORMW KOJO_MESSAGE_COMING_K15_1, 汎用絶頂台詞４：快楽模索中
				[ENDIF]
			CASE 2
				[IF_DEBUG]
					PRINTFORMW KOJO_MESSAGE_COMING_K15_1, 汎用絶頂台詞３：快楽模索中
				[ENDIF]
			CASE 1
				[IF_DEBUG]
					PRINTFORMW KOJO_MESSAGE_COMING_K15_1, 汎用絶頂台詞２：快楽模索中
				[ENDIF]
			CASEELSE
				[IF_DEBUG]
					PRINTFORMW KOJO_MESSAGE_COMING_K15_1, 汎用絶頂台詞１：快楽模索中
				[ENDIF]
			ENDSELECT
		;汎用喘ぎ声
		ELSE
			SELECTCASE KABURAND(4)
			CASE 3
				[IF_DEBUG]
					PRINTFORMW KOJO_MESSAGE_COMING_K15_1, 汎用絶頂台詞４
				[ENDIF]
			CASE 2
				[IF_DEBUG]
					PRINTFORMW KOJO_MESSAGE_COMING_K15_1, 汎用絶頂台詞３
				[ENDIF]
			CASE 1
				[IF_DEBUG]
					PRINTFORMW KOJO_MESSAGE_COMING_K15_1, 汎用絶頂台詞２
				[ENDIF]
			CASEELSE
				[IF_DEBUG]
					PRINTFORMW KOJO_MESSAGE_COMING_K15_1, 汎用絶頂台詞１
				[ENDIF]
			ENDSELECT
		ENDIF
	ENDIF
	RETURN 1

