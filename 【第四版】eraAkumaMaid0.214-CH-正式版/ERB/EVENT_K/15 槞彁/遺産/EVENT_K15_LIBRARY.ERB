;----------------------------------------------------------------
;セリフ参照 
;ARGS = 検索するセリフ
;----------------------------------------------------------------
@CALL_LIB(ARGS)
	TRYCALLFORM EVENT_K15_LIBRARY(ARGS)
	RETURN 0
;----------------------------------------------------------------
;プロパティ
;ARG:1 = 言語能力
;ARGS = 検索するセリフ
;----------------------------------------------------------------
@EVENT_K15_LIBRARY(ARGS)
	CALL TRANS_CHECK(ARGS)
	RETURN 0
;----------------------------------------------------------------
;翻訳機の設定確認
;ARG = 言語能力
;ARGS = 検索するセリフ
;----------------------------------------------------------------
@TRANS_CHECK(ARGS)
IF CEVENT("槞彁：機械翻訳OFF", MASTER)
	CALL NOT_USE_TRANS(ARGS)
ELSE
	CALL USE_TRANS(ARGS)
ENDIF
RETURN 0
;----------------------------------------------------------------
;翻訳機使用モード
;ARG = 言語能力
;ARGS = 検索するセリフ
;----------------------------------------------------------------
@USE_TRANS(ARGS)
#DIM LCOUNT
#DIM DYNAMIC BASE_RESULT
#DIM DYNAMIC BEFORE_RESULT
#DIM DYNAMIC AFTER_RESULT
	RESULT = 0
	FOR LCOUNT, 0, 100
		RESETCOLOR 
		CALL SEARCH_LIB(0, LCOUNT, ARGS)
			SIF RESULT == 1
				BASE_RESULT = 1
		SETCOLOR TOINT(@"0x%K15_VOICE_COLOR%")
		
		CALL SEARCH_LIB(1, LCOUNT, ARGS)
			SIF RESULT == 1
				BEFORE_RESULT = 1
		SETCOLOR K15_MACHINE_COLOR
		CALL SEARCH_LIB(100, LCOUNT, ARGS)
		RESETCOLOR
			SIF RESULT == 1
				AFTER_RESULT = 1
		SIF (BEFORE_RESULT == 1)&&(AFTER_RESULT == 1)&&(BASE_RESULT == 1)
			BREAK
	NEXT
	RESETCOLOR 
;----------------------------------------------------------------
;翻訳機不使用モード
;ARG = 言語能力
;ARGS = 検索するセリフ
;----------------------------------------------------------------
@NOT_USE_TRANS(ARGS)
#DIM LCOUNT
#DIM DYNAMIC BASE_RESULT
#DIM DYNAMIC BEFORE_RESULT
	RESULT = 0
	FOR LCOUNT, 0, 100
		CALL SEARCH_LIB(0, LCOUNT, ARGS)
			SIF RESULT == 1
				BASE_RESULT = 1
		CALL SEARCH_LIB(1, LCOUNT, ARGS)
			SIF RESULT == 1
				BEFORE_RESULT = 1
		SIF (BEFORE_RESULT == 1)&&(BASE_RESULT == 1)
			BREAK
	NEXT

