;
@K15_VOCAB_REPLACE(Text,Word,Dictionary)
	#DIMS DYNAMIC Text
	#DIMS DYNAMIC Word
	#DIMS DYNAMIC Dictionary
	#DIM DYNAMIC TagCount
	#DIM DYNAMIC Lcount
	#DIM DYNAMIC Lcount2
	#DIMS DYNAMIC MEMO_RESULTS,300

	#DIM DYNAMIC EndButtonTagCount
	#DIM DYNAMIC StartButtonTagCount
	#DIM DYNAMIC EndNonbuttonTagCount
	#DIM DYNAMIC StartNonbuttonTagCount

	HTML_TAGSPLIT Text
	TagCount = RESULT
	EndButtonTagCount = 0
	StartButtonTagCount = 0
	EndNonbuttonTagCount = 0
	StartNonbuttonTagCount = 0
	
	FOR Lcount, 0, TagCount
		MEMO_RESULTS:LCOUNT = %RESULTS:LCOUNT%
	NEXT
	;辞書情報を挿入して良いかの確認
	FOR Lcount, 0, TagCount
		IF STRCOUNTS(MEMO_RESULTS:Lcount,Word) > -1

			FOR Lcount2, 0, Lcount
				IF STRCOUNTS(MEMO_RESULTS:Lcount2,"</button") > 0
					EndButtonTagCount++
				ELSEIF STRCOUNTS(MEMO_RESULTS:Lcount2,"<button") > 0
					StartButtonTagCount++
				ELSEIF STRCOUNTS(MEMO_RESULTS:Lcount2,"</nonbutton") > 0
					EndNonbuttonTagCount++
				ELSEIF STRCOUNTS(MEMO_RESULTS:Lcount2,"<nonbutton") > 0
					StartNonbuttonTagCount++
				ENDIF

			NEXT
			SIF EndButtonTagCount <= StartButtonTagCount
				RESULTS = </button>%MEMO_RESULTS:Lcount%<button>
			SIF EndNonbuttonTagCount <= StartNonbuttonTagCount
				RESULTS = </nonbutton>%MEMO_RESULTS:Lcount%<nonbutton>
			;IF StartTagCount == EndTagCount
				;【メモ】なんでRESULTSに一回入れないとREPLACEしてくれないんだ...?
				RESULTS = %MEMO_RESULTS:Lcount%
				;なぜか分からないけど、Window内でホバーコメント表示する為にはnonbuttonではなくてbuttonである必要があるらしい
				REPLACE (MEMO_RESULTS:Lcount), Word, @"<button title='%Dictionary%' value='-1'><font color='#%KnownWordColor%' bcolor='#%KnownWordSelectingColor%'><u><i>%Word%</i></u></font></button>"
				;REPLACE (MEMO_RESULTS:Lcount), Word, @"<nonbutton title='%Dictionary%'><font color='%KnownWordColor%'>%Word%</font></nonbutton>"
				MEMO_RESULTS:Lcount = %RESULTS%
			;ENDIF
		ENDIF
	NEXT

	VARSET RESULTS:0, ""
	FOR Lcount, 0, TagCount
		RESULTS += @"%MEMO_RESULTS:Lcount%"
	NEXT
