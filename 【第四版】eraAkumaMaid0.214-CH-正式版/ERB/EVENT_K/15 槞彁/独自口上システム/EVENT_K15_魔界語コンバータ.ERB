	;CALL SET_CEVENT("сакана")
	;CALL SET_CEVENT("Аната")
	;CALL SET_CEVENT("Коннйчивар")
	;CALL SET_CEVENT("Vatashi")
	
	IF CEVENT("⊥лыъвьыкцлр")
		Dictionary = 【名词】槞彁：好像是魔界语的本名
		CALL K15_VOCAB_REPLACE(Text, CNAME("本名"),Dictionary)
		Text = %RESULTS%
	ENDIF
	IF CEVENT("U二人称")
		Dictionary = 【名词】你的名字在魔界语中是这样发音
		CALL K15_VOCAB_REPLACE(Text, 魔界語PlayerName, Dictionary)
		Text = %RESULTS%
	ENDIF
	;友達
	IF CEVENT("Tommordatch")
		Dictionary = 【名词】朋友
		CALL K15_VOCAB_REPLACE(Text, K15_CyrillicConverter("Tommordatch"),Dictionary)
