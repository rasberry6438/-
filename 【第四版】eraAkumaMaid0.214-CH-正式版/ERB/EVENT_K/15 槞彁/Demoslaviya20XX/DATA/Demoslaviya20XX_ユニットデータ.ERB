@D20XX_GenerateUnitComment(TargetUnit)
	#FUNCTIONS
	#DIM DYNAMIC TargetUnit
	#DIM DYNAMIC LCount
	#DIMS DYNAMIC UnitName
	#DIMS DYNAMIC ResultData
	#DIMS DYNAMIC Weapon,3
	UnitName = %D20XX_Unit:TargetUnit:1%

	ResultData += @"%D20XX_GET_UNITDATA(UnitName,"ユ针织名")%\n"
	ResultData += @"HP：%D20XX_Unit:TargetUnit:2%/%D20XX_GET_UNITDATA(UnitName,"HP")%\n"
	ResultData += @"行動力：%D20XX_Unit:TargetUnit:3%/%D20XX_GET_UNITDATA(UnitName,"行動力")%\n"
	ResultData += @"装甲：%D20XX_GET_UNITDATA(UnitName,"装甲")%\n"
	ResultData += @"残era：%D20XX_Unit:TargetUnit:5%/%D20XX_GET_UNITDATA(UnitName,"ERA")%\n"
	ResultData += @"指揮統制率：%D20XX_Unit:TargetUnit:4%/%D20XX_GET_UNITDATA(UnitName,"指揮統制率")%\n"
	
	SPLIT D20XX_Unit:TargetUnit:6,",",Weapon
	FOR LCount, 0, 3
		SIF Weapon:LCount != ""
			ResultData += @"武装{LCount + 1}：%D20XX_GET_WEAPONDATA(Weapon:LCount,"武器名")%\n"
	NEXT
	ResultData += @"状態：%D20XX_Unit:TargetUnit:7%\n"

	ResultData += @"～解説～\n%D20XX_GET_UNITDATA(UnitName,"解説")%"
	RETURNF ResultData