@CREATE_MOB_K15, ARGS
#DIM DYNAMIC Lcount
SELECTCASE ARGS
	CASE "槞彁"
		FOR Lcount,0,120
			CSTR:Lcount = %CSTR:FINDCHARA(NO,15):Lcount%
		NEXT
		FOR Lcount,0,200
			TALENT:Lcount = TALENT:FINDCHARA(NO,15):Lcount
		NEXT
	CASE "イェニェル"
		CSTR:種族=天使
		CSTR:髪色=銀髪
		CSTR:髪長=中等長度
		CSTR:前髪=自然刘海３
		CSTR:後髪=自然
		CSTR:改髪=侧盘頭
		CSTR:呆毛=呆毛
		CSTR:蝴蝶結=无蝴蝶結
		CSTR:瞳色=青
		CSTR:眼形=普通眼２
		CSTR:白眼=无眼白
		CSTR:眉形=困扰眉短
		CSTR:嘴形=笑顔２
		CSTR:高光=高光有
		CSTR:颜绘変化=楕円
		TALENT:精灵耳 = 1
		CALL SET_SHIRT, "西装外衣"
ENDSELECT


@D20XX_Generate_UniqueFcsv, ARGS
	#DIM DYNAMIC MEMO_TARGET
	#DIM DYNAMIC LCOUNT
	#DIMS DYNAMIC CSTRS
	#DIMS DYNAMIC TALENTS

	MEMO_TARGET = TARGET
	CSTRS = 
	TALENTS = 
	;ベースはドッペル你
	ADDCHARA 99

	;記述の簡略化のために路人をTARGETにする
	TARGET = CHARANUM - 1
	;颜绘出力に必要な共通フラグ設定
	CFLAG:颜绘 = 1
	CFLAG:路人 = 1

	CALLNAME:TARGET = %ARGS%
	NAME:TARGET = %ARGS%
	CSTR:ふりがな = %ARGS%

	SELECTCASE ARGS
		CASE "槞彁"
			FOR Lcount,0,120
				CSTR:Lcount = %CSTR:MEMO_TARGET:Lcount%
			NEXT
			FOR Lcount,0,200
				TALENT:TARGET:Lcount = TALENT:MEMO_TARGET:Lcount
			NEXT

		CASE "イェニェル"
			CSTR:種族=天使
			CSTR:髪色=銀髪
			CSTR:髪長=中等長度
			CSTR:前髪=自然刘海３
			CSTR:後髪=自然
			CSTR:改髪=侧盘頭
			CSTR:呆毛=呆毛
			CSTR:蝴蝶結=无蝴蝶結
			CSTR:瞳色=青
			CSTR:眼形=普通眼２
			CSTR:白眼=无眼白
			CSTR:眉形=困扰眉短
			CSTR:嘴形=笑顔２
			CSTR:高光=高光有
			CSTR:颜绘変化=楕円
			TALENT:精灵耳 = 1
			CALL SET_SHIRT, "西装外衣"
		CASE "女仆兵士"
			CSTR:種族=イヴァ阴蒂ナ
			CSTR:髪色=焦茶髪
			CSTR:髪長=長
			CSTR:前髪=Ｍ字刘海３
			CSTR:後髪=卷髪
			CSTR:改髪=马尾辫
			CSTR:蝴蝶結=无蝴蝶結
			CSTR:瞳色=赤
			CSTR:眼形=吊眼２飘忽
			CSTR:白眼=白眼
			CSTR:眉形=憤怒眉
			CSTR:嘴形=嘲笑
			CSTR:高光=高光有
			TALENT:精灵耳 = 2
			CALL SET_OTHER, "颈环/黑框眼鏡/礼服頭花"
			CALL SET_SHIRT, "女仆装"
			CALL SET_SOCKS, "吊帯襪"
			CALL SET_SHOES, "小皮靴"
		CASE "椦彁"
			CSTR:種族=イヴァ阴蒂ナ
			CSTR:髪色=槞髪
			CSTR:髪長=長
			CSTR:前髪=斜刘海
			CSTR:後髪=直髪
			CSTR:改髪=无特殊髪型
			CSTR:呆毛=呆毛
			CSTR:蝴蝶結=无蝴蝶結
			CSTR:瞳色=紫
			CSTR:眼形=普通眼２
			CSTR:白眼=无眼白
			CSTR:眉形=斜眉
			CSTR:嘴形=微笑４
			TALENT:精灵耳 = 2
			TALENT:角 = 1
			TALENT:翼 = 1
			CALL SET_SHIRT, "V领毛衣/黒"
			CALL SET_SKIRT, "紧身裙"
			CALL SET_BOTTOM, "连褲襪/黒"
			CALL SET_PANTIES, "內褲/正面蝴蝶結紐白/黒"
			CALL SET_SHOES, "小皮靴"
			CALL SET_OTHER, "黑色黑框眼鏡/水兵帽"
		CASE "将校"
			CALL SET_OTHER, "黒将校"
		CASE "兵士"
			CALL SET_OTHER, "茶兵士"
		CASE "黒服"
			CALL SET_OTHER, "黒制服"
		CASE "無人"	
			CALL SET_OTHER, "無人"
	ENDSELECT
	;ここで、TARGETの設定を丸々保存したい
	;VariableSize.CSVを参照
	FOR LCOUNT,0,120
		CSTRS += @"%CSTR:LCOUNT%+"
	NEXT

	FOR LCOUNT,0,300
		TALENTS += @"{TALENT:LCOUNT}+"
	NEXT


	RESULTS = 
	RESULTS += @"%CSTRS%%TALENTS%"

	TARGET = MEMO_TARGET
	CALL DELETE_CHARA, CHARANUM - 1
	CALL DEL_MOB
;