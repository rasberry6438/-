@K15_V(ARGS)
	CALL K15_STR(ARGS)
	CALL K15_VOCAB(RESULTS)
	CALL K15_HTML_PRINT_STR(RESULTS)
	HTML_PRINT @"<font color='#%K15_VOICE_COLOR%'>%RESULTS%</font>"
@K15_VW(ARGS)
	CALL K15_V(ARGS)
	WAIT

@K15_T(ARGS)
	SETCOLOR 204,255,229;ccffe5
	CALL PRINT_STR(ARGS)
	RESETCOLOR
	PRINTL

@K15_TW(ARGS)
	CALL K15_T(ARGS)
	WAIT


@K15_HTML_PRINT_STR(ARGS)
	#DIM Lcount
	#DIMS LReturn
	SPLIT ARGS,"_",LOCALS
	LReturn = 
	FOR Lcount,0,RESULT
		LReturn += @"%HUNI(LOCALS:Lcount)%"
	NEXT
	RESULTS = %LReturn%