;=========================
;D20XX_MAPコンストラクタ
;=========================
@D20XX_Map_Constructor
	#DIM DYNAMIC Y_COUNT
	#DIM DYNAMIC X_COUNT
		FOR Y_COUNT, 0, 200
			FOR X_COUNT, 0, 200
				D20XX_MAP:Y_COUNT:X_COUNT:0 = 無
				D20XX_MAP:Y_COUNT:X_COUNT:1 = None
				D20XX_MAP:Y_COUNT:X_COUNT:2 = false
			NEXT
		NEXT
;=========================
;D20XX_Unitコンストラクタ
;=========================
@D20XX_Unit_Constructor
	VARSET D20XX_Unit:0:0, "None"

@D20XX_UnitMovePower_Constructor
	#DIM STATIC LCOUNT
		FOR LCOUNT, 0, 10000
			D20XX_Unit:LCOUNT:3 = %D20XX_GET_UNITDATA(D20XX_Unit:LCOUNT:1,"行動力")%
		NEXT
