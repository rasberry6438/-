;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K15, ARGS
	#FUNCTIONS
	;名前を呼ぶ相手の角色番号の代入に使う
	#DIM ACTOR

	VARSET LOCALS

	SELECTCASE ARGS
	CASE "本名"
		LOCALS = ⊥лыъвьыкцлр
	CASE "一人称"
		LOCALS = Ваташ
	CASE "一人称翻訳後"
		LOCALS = 我_我
	CASE "二人称"
		;相手が主人
		IF PLAYER == MASTER
			SIF 魔界語PlayerName == ""
				魔界語PlayerName = Аната
			LOCALS = %魔界語PlayerName%
			SIF LOCALS != "Аната"
				LOCALS = %魔界語PlayerName%-сан

		;相手が主人ではない
		ELSE
			LOCALS = Аната
		ENDIF
	CASE "二人称日本語"
		LOCALS = %日本語PlayerName%
	CASE "二人称翻訳後"
		LOCALS = 貴方_你
	;この下は三人称。
	CASE "主人"
		LOCALS = Анор-кат
	CASE "三人称"
		LOCALS = そこ人
	CASEELSE
		;名前を呼ぶ相手の角色番号をACTORに代入する
		SELECTCASE ARGS
		;相手が助手
		CASE "助手"
			ACTOR = ASSI
		;ここにはARGSに角色のCALLNAMEが入ったりしている場合(ARGS == "弗理希艾尔" など)に来る
		CASEELSE
			ACTOR = FIND_NO(ARGS)
		ENDSELECT

		;相手が教員
		IF COND("教員", ACTOR)
			LOCALS = %CALLNAME:ACTOR%Сенсеы
		;自分も相手も学生なら学年比較
		ELSEIF COND("学生") && COND("学生", ACTOR)
			;相手が同学年
			IF COND("学年") == COND("学年", ACTOR)
				LOCALS = %CALLNAME:ACTOR%сан
			;相手が下の学年
			ELSEIF COND("学年") > COND("学年", ACTOR)
				IF TALENT:ACTOR:男性
					LOCALS = %CALLNAME:ACTOR%кюм
				ELSE
					LOCALS = %CALLNAME:ACTOR%чан
				ENDIF
			;相手が上級生
			ELSE
				LOCALS = %CALLNAME:ACTOR%самар
			ENDIF

		;それ以外
		ELSE
			LOCALS = %CALLNAME:ACTOR%сан
		ENDIF
	ENDSELECT
	RETURNF LOCALS
