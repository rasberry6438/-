;Talk指令で増颜绘会話やした選択肢の地の文と台詞
@KOJO_DAILY1_K15_ADDSELECT, ARGS
	
	SELECTCASE ARGS
		CASE "おまけ1立ち絵シム"
			CALL 槞彁_TachieGenerate_Sim
		CASE "おまけ2添い寝シム"
			CALL K15_添い寝背景描画_Sim
		CASE "デバッグターミナル"
			CALL K15_DebugTerminal(ARGS)
		CASE "電話"

			CALL D20XX_Generate_UniqueFcsv, "イェニェル"
			CALL D20XX_FACE_FcsvToSprite(RESULTS)
			CALL D20XX_FACE_Packaging(RESULTS)
			RESULTS = %CONVERTSTR(RESULTS)%
			CALL D20XX_NO_NOBR(RESULTS)
			CALL D20XX_OS_SpriteYposReverse(RESULTS)
			CALL D20XX_OS_WindowInput_Constructor

			RESULTS:1 = %RESULTS:0%

			;#region
				D20XX_OS_WindowInput:0 += @"<nonbutton pos='1100'>- <font color = 'Yellow'>耶涅尔</font> -</nonbutton>"
				D20XX_OS_WindowInput:1 += @"<nonbutton pos='1100'>%PRINT_NamariF("おう、你かどうした？")%</nonbutton>"
				D20XX_OS_WindowInput:2 += @"<nonbutton pos='1100'></nonbutton>"
				D20XX_OS_WindowInput:3 += @"<nonbutton pos='1100'></nonbutton>"
				D20XX_OS_WindowInput:4 += @"<nonbutton pos='1100'></nonbutton>"
				D20XX_OS_WindowInput:5 += @"<nonbutton pos='1100'></nonbutton>"
				D20XX_OS_WindowInput:6 += @"<button pos='1100' value = '100'><font color = 'Yellow'>[魔界に行きたい]</font></button>"
				D20XX_OS_WindowInput:7 += @"<nonbutton pos='1100'></nonbutton>"
				D20XX_OS_WindowInput:8 += @"%RESULTS:1%<button pos='1100' value = '200'><font color = 'Yellow'>[何でもない]</font></button>"
			;#endregion


			CALL D20XX_OS_Window(500,12,4000,10)
			$PhoneLoop
				INPUT
				SELECTCASE RESULT
					CASE 100
						CALL D20XX_Generate_UniqueFcsv, "イェニェル"
						CALL D20XX_FACE_FcsvToSprite(RESULTS)
						CALL D20XX_FACE_Packaging(RESULTS)
						RESULTS = %CONVERTSTR(RESULTS)%
						CALL D20XX_NO_NOBR(RESULTS)
						CALL D20XX_OS_SpriteYposReverse(RESULTS)
						CALL D20XX_OS_WindowInput_Constructor
							RESULTS:1 = %RESULTS:0%
						;#region
							D20XX_OS_WindowInput:0 += @"<nonbutton pos='1100'>- <font color = 'Yellow'>耶涅尔</font> -</nonbutton>"
							D20XX_OS_WindowInput:1 += @"<nonbutton pos='1100'>%PRINT_NamariF("分かった、今転送する")%</nonbutton>"
							D20XX_OS_WindowInput:2 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:3 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:4 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:5 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:6 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:7 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:8 += @"%RESULTS:1%<nonbutton pos='1100'></nonbutton>"
						;#endregion
						CALL D20XX_OS_Window(500,12,4000,10)
						WAIT
						CALL D20XX_Start
						RETURN
					CASE 200
						CALL D20XX_Generate_UniqueFcsv, "イェニェル"
						CALL D20XX_FACE_FcsvToSprite(RESULTS,"憤怒/本")
						CALL D20XX_FACE_Packaging(RESULTS)
						RESULTS = %CONVERTSTR(RESULTS)%
						CALL D20XX_NO_NOBR(RESULTS)
						CALL D20XX_OS_SpriteYposReverse(RESULTS)
						CALL D20XX_OS_WindowInput_Constructor
							RESULTS:1 = %RESULTS:0%
						;#region
							D20XX_OS_WindowInput:0 += @"<nonbutton pos='1100'>- <font color = 'Yellow'>耶涅尔</font> -</nonbutton>"
							D20XX_OS_WindowInput:1 += @"<nonbutton pos='1100'>%PRINT_NamariF("用もないのに電話してくるな ボケ")%</nonbutton>"
							D20XX_OS_WindowInput:2 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:3 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:4 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:5 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:6 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:7 += @"<nonbutton pos='1100'></nonbutton>"
							D20XX_OS_WindowInput:8 += @"%RESULTS:1%<nonbutton pos='1000'></nonbutton>"
						;#endregion
						CALL D20XX_OS_Window(500,12,4000,10)
						WAIT
						RETURN
				ENDSELECT
				CLEARLINE 1
				GOTO PhoneLoop
		CASE "颜绘会話テスト"
			CALL CALL_LIBFW("魔界語講座：な")
		CASE "立ち絵会話テスト"
			CFLAG:非フラグ変動指令 = 1
			;CALL K15_StoryEvents("自己紹介")
			CALL K15_StoryEvents("出会い")
		CASE "表示テスト"
			CALL K15_NuclearTestSite_A4
			;CALL CALL_LIBFW("刺身")
		CASE "立ち絵シミュレータ"
			CALL 槞彁_TachieGenerate_Sim
		CASE "イェニェル立ち絵シム"
			CALL イェニェル_TachieGenerate_Sim
		CASE "添い寝テスト"
			;前編
			IF CEVENTS("追加選択肢から结束回合") == ""
				FLAG:SELECT_USERSHOP = 0 ;念のためね
				CALL K15_添い寝_main
				CALL SET_CEVENT, @"追加選択肢から结束回合：%ARGS%"
				FLAG:SELECT_USERSHOP = 998
			ELSE
				IF TFLAG:结束回合
					
				ELSE
					CALL K15_添い寝_main
				ENDIF
				;#region ABLUP_CHARA_MAIN出やってる事の模倣
				CLEARLINE LINECOUNT
				CALL EFFECT_ALLRESET
				FONTREGULAR
				$添い寝ABLUP_Loop
				CALL SHOW_JUEL
				CALL SHOW_ABLUP_SELECT
				INPUT
				SELECTCASE RESULT
				CASE 0 TO 99
					TRYCALLFORM ABLUP{RESULT}
				CASE 100
					CALL AUTO_LVUP
				CASE 200
					CALL JUEL_TO_EXP, 10000
				CASE 999
					;終了するでよ
					RETURN 1
				CASEELSE
				ENDSELECT
				GOTO 添い寝ABLUP_Loop
				;#endregion
			ENDIF
		CASE "添い寝グラテスト"
			CALL K15_添い寝背景描画_Sim
		CASE "ストーリーイベントテスト"
			CALL K15_StoryEventDebug
		CASE "追加選択肢之２"
			PRINTFORMW %CALLNAME:MASTER%做了%ARGS%。
			PRINTL 
			PRINTFORMW 「台詞」
			PRINTL 
			PRINTFORMW 受到%ARGS%后%CALLNAME:TARGET%的反応
		;直接Ｈしたい場合
		CASE "追加選択肢からＨ：Talk"
			;前編
			IF CEVENTS("追加選択肢からＨ") == ""
				PRINTFORMW 文本
				PRINTL [0]選択肢１(これを選ぶとＨします)
				PRINTL [1]選択肢２(これを選ぶとＨしません)
				CALL SET_CEVENT, @"追加選択肢からＨ：%ARGS%"
				FLAG:SELECT_USERSHOP = 100
			ELSE
				IF TFLAG:床イン
					PRINTFORMW Ｈする場合の展開です
				ELSE
					PRINTFORMW こっちはＨしない場合の展開です
				ENDIF
			ENDIF
		;直接结束回合したい場合
		CASE "ターンを終わらせる"
			;前編
			IF CEVENTS("追加選択肢から结束回合") == ""
				PRINTFORMW 文本
				PRINTL [0]選択肢１(これを選ぶと结束回合します)
				PRINTL [1]選択肢２(これを選ぶと结束回合しません)
				CALL SET_CEVENT, @"追加選択肢から结束回合：%ARGS%"
				FLAG:SELECT_USERSHOP = 998
			ELSE
				IF TFLAG:结束回合
					PRINTFORMW 结束回合する場合の展開です
				ELSE
					PRINTFORMW こっちは结束回合しない場合の展開です
				ENDIF
			ENDIF
	ENDSELECT
@K15_DebugTerminal(ARGS)
	$K15DebugTerminalLoop
	PRINTFORML [100]ストーリーイベントテスト
	PRINTFORML [101]添い寝テスト
	PRINTFORML [102]LINE風表示テスト
	PRINTFORML 
