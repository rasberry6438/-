	PRINTFORML [300]迷你游戏テスト
	PRINTFORML 
	PRINTFORML [400]簡易テスト枠
	PRINTFORML 
	PRINTFORML [999]返回
	INPUT

	SELECTCASE RESULT
		CASE 100;ストーリーイベントテスト
			CALL K15_StoryEventDebug
		CASE 101;添い寝テスト
			;前編
			IF CEVENTS("追加選択肢から结束回合") == ""
				FLAG:SELECT_USERSHOP = 0 ;念のためね
