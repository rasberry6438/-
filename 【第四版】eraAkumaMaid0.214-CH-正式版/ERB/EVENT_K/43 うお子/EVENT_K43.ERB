;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K43, ARGS
#FUNCTIONS
;名前を呼ぶ相手の角色番号の代入に使う
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 部長_ぶちょー
	;相手が教員
	ELSEIF COND("教員", PLAYER)
		LOCALS = 老师_老师
	;相手が上級生
	ELSEIF COND("学生", PLAYER) && COND("学年") < COND("学年", PLAYER)
		LOCALS = 前輩_前輩
	;相手が下の学年
	ELSEIF COND("学生", PLAYER) && COND("学年") > COND("学年", PLAYER)
		LOCALS = 你_你
	ELSE
		SELECTCASE CSTR:PLAYER:称号
		CASE "魔貴族", "没落魔貴族"
			LOCALS = %CALLNAME:PLAYER%様_%CSTR:PLAYER:ふりがな%大人
		CASEELSE
			LOCALS = 你_你
		ENDSELECT
	ENDIF
;この下は三人称。
CASE "主人"
	LOCALS = 部長_ぶちょー
CASEELSE
	;名前を呼ぶ相手の角色番号をACTORに代入する
	SELECTCASE ARGS
	;相手が助手
	CASE "助手"
		ACTOR = ASSI
	;ここにはARGSに角色のCALLNAMEが入ったりしている場合(ARGS == "弗理希艾尔" など)に来る
	CASEELSE
		ACTOR = FIND_NO(ARGS)
	ENDSELECT

	;相手が教員
	IF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%老师_%CALLNAME:ACTOR%老师
	;自分も相手も学生なら学年比較
	ELSEIF COND("学生") && COND("学生", ACTOR)
		;相手が同学年
		IF COND("学年") == COND("学年", ACTOR)
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		;相手が下の学年
		ELSEIF COND("学年") > COND("学年", ACTOR)
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		;相手が上級生
		ELSE
			IF CFLAG:心情好
				LOCALS = %CALLNAME:ACTOR%大前輩_%CSTR:ACTOR:ふりがな%だい前輩
			ELSE
				LOCALS = %CALLNAME:ACTOR%前輩_%CSTR:ACTOR:ふりがな%前輩
			ENDIF
		ENDIF
	;それ以外
	ELSE
		SELECTCASE CSTR:ACTOR:称号
		CASE "魔貴族", "没落魔貴族"
			LOCALS = %CALLNAME:ACTOR%様_%CSTR:ACTOR:ふりがな%大人
		CASEELSE
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

