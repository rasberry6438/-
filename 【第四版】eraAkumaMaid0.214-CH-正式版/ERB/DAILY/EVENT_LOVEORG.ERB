;此函数用于上半页面显示
@EVENT_LOVEORG_TOP
	PRINTFORML
	DRAWLINE
	DRAWLINE
	PRINTFORML
	PRINTFORML
	PRINTFORM %TEXTS("日期表示")% 
	PRINTFORML -%TEXTS("現在位置")%- 
	SIF CEVENT("LOVORG同行中",TARGET)
		CALL EVENT_LOVEORG_MEM_SHOW_NEW, TARGET
	IF CEVENT("助手LOVORG前往",ASSI)
		CALL EVENT_LOVEORG_MEM_SHOW_NEW, ASSI
		;PRINTFORML
		;PRINTFORML
		PRINTFORML 做爱部活动的参与情况
		CALL LOVEORG_EXP_MINI
		;CALL LOVEORG_EXP, TARGET
		;CALL LOVEORG_EXP, ASSI
		;PRINTFORML
		;PRINTFORML
		;两人同行
		IF CEVENT("LOVORG同行中",TARGET) && CEVENT("LOVORG同行中",ASSI)
			CALL PRINT_MAIDFACE, TARGET, ASSI
		;TARGET同行ASSI不同行
		ELSEIF CEVENT("LOVORG同行中",TARGET) && CEVENT("LOVORG同行中",ASSI) == 0
			CALL PRINT_MAIDFACE, TARGET
			PRINTFORML %CALLNAME:ASSI%不在这里....
		;TARGET不同行ASSI同行
		ELSEIF CEVENT("LOVORG同行中",TARGET)==0 && CEVENT("LOVORG同行中",ASSI) && CEVENT("助手LOVORG前往",ASSI)
			CALL PRINT_MAIDFACE, ASSI
			PRINTFORML %CALLNAME:TARGET%不在这里....
		;两人都不同行
		ELSE
			PRINTFORML %CALLNAME:TARGET%和%CALLNAME:ASSI%不在这里....
		ENDIF
		PRINTFORML
	ELSE
		;PRINTFORML
		;PRINTFORML
		PRINTFORML 做爱部活动的参与情况
		CALL LOVEORG_EXP_MINI
		;CALL LOVEORG_EXP, TARGET
		;PRINTFORML
		;PRINTFORML
		IF CEVENT("LOVORG同行中",TARGET)
			CALL PRINT_MAIDFACE, TARGET
		ELSE
			PRINTFORML %CALLNAME:TARGET%不在这里....
		ENDIF
	ENDIF

