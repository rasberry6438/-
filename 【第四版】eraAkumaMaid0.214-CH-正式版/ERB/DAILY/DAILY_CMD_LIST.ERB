;STR:選択肢側でボタン色指定できないのが色々面倒臭くしてる根源だと思うので
@ADD_MENU_SHOP(BtnNum,BtnNumMax = 100)
#DIM DYNAMIC BtnNum
#DIM DYNAMIC BtnNumMax
#DIM DYNAMIC Lcount

#DIMS DYNAMIC ColorSplit,2
VARSET LOCALS
VARSET ColorSplit
VARSET RESULTS

SPLIT STR:選択肢, "/", LOCALS

FOR LCOUNT, 0, BtnNumMax
	SELECTCASE LOCALS:LCOUNT
	CASE ""
		;BREAK
	CASEELSE
		SIF STRCOUNT(STR:追加選択肢, @"/%LOCALS:LCOUNT%/")
			RESULTS += @"緑/"
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "灰色_")
			SPLIT LOCALS:LCOUNT,"_",ColorSplit
			IF ColorSplit:1 != ""
				RESULTS += @"灰色/"
				LOCALS:LCOUNT = %ColorSplit:1%
			ELSE 
				CONTINUE 
			ENDIF
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "水色_")
			SPLIT LOCALS:LCOUNT,"_",ColorSplit
			IF ColorSplit:1 != ""
				RESULTS += @"水色/"
				LOCALS:LCOUNT = %ColorSplit:1%
			ELSE 
				CONTINUE 
			ENDIF
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "爱心粉紅_")
			SPLIT LOCALS:LCOUNT,"_",ColorSplit
			IF ColorSplit:1 != ""
				RESULTS += @"爱心粉紅/"
				LOCALS:LCOUNT = %ColorSplit:1%
			ELSE 
				CONTINUE 
			ENDIF
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "青_" )
			SPLIT LOCALS:LCOUNT,"_",ColorSplit
			IF ColorSplit:1 != ""
				RESULTS += @"青/"
				LOCALS:LCOUNT = %ColorSplit:1%
			ELSE 
				CONTINUE 
			ENDIF
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "緑_")
			SPLIT LOCALS:LCOUNT,"_",ColorSplit
			IF ColorSplit:1 != ""
				RESULTS += @"緑/"
				LOCALS:LCOUNT = %ColorSplit:1%
			ELSE 
				CONTINUE 
			ENDIF
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "緑色_") 
			SPLIT LOCALS:LCOUNT,"_",ColorSplit
			IF ColorSplit:1 != ""
				RESULTS += @"緑色/"
				LOCALS:LCOUNT = %ColorSplit:1%
			ELSE 
				CONTINUE 
			ENDIF
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "黄色_")
			SPLIT LOCALS:LCOUNT,"_",ColorSplit
			IF ColorSplit:1 != ""
				RESULTS += @"黄色/"
				LOCALS:LCOUNT = %ColorSplit:1%
			ELSE 
				CONTINUE 
			ENDIF
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "太字_")
			SPLIT LOCALS:LCOUNT,"_",ColorSplit
			IF ColorSplit:1 != ""
				RESULTS += @"太字/"
				LOCALS:LCOUNT = %ColorSplit:1%
			ELSE 
				CONTINUE 
			ENDIF
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "NOBUTTON")
			RESULTS += @"NOBUTTON/"
			CONTINUE
		ENDIF
		IF STRCOUNT(@"%LOCALS:LCOUNT%/", "改行")
			RESULTS += @"改行/"
			CONTINUE
		ENDIF
		RESULTS += @"[{BtnNum + LCOUNT, 2}]%DAILYCOMNAME_SELECT(LOCALS:LCOUNT)%/"
	ENDSELECT
NEXT

