		PRINTL ＜肛門馬型振動棒挿入中＞
	CASE 4
		PRINTL ＜肛門腕並振動棒挿入中＞
	CASE 3
		PRINTL ＜肛門極太振動棒挿入中＞
	CASEELSE
		PRINTL ＜肛用振動棒挿入中＞
	ENDSELECT
ENDIF

PLAY_ONANIE = %CHECK_ONANIE(SELECTCOM)%

LOCAL = 1
;自慰実行時は経験＋１
IF STRCOUNT(PLAY_ONANIE, "Ａ") && SELECTCOM == 3
	SIF ABL:Ａ拡張 < SIZE("Ａ振動棒")
		LOCAL += 1
ENDIF

EXP:Ａ経験 += LOCAL

VARSET LOCAL
;Ａ拡張経験
IF SELECTCOM == 2 || (STRCOUNT(PLAY_ONANIE, "Ａ") && SELECTCOM == 3)
	SIF SIZE("Ａ振動棒") >= 2
		LOCAL = MAX(SIZE("Ａ振動棒") - ABL:Ａ拡張 + 1, SIZE("Ａ振動棒")/3)
ENDIF

IF LOCAL
	EXP:Ａ拡張経験 += LOCAL
	;拡張経験による追加の疲労
	DOWNBASE:体力 += LOCAL*50
ENDIF

RETURN 1
