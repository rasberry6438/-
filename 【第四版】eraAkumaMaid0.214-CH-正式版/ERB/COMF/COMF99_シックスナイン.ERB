SELECTCASE CHECK_EQUIP_69(SELECTCOM)
CASE 1
	;前回の調教が口交、舐陰強制か六九式だと六九式に
	IF PREVCOM == TNUM("口交") || PREVCOM == TNUM("奉仕舐陰") || PREVCOM == TNUM("深喉") || TEQUIP:六九式
		SIF TEQUIP:六九式 == 0
			TEQUIP:六九式 = PREVCOM
		TEQUIP:PLAYER:六九式 = SELECTCOM
	ENDIF
CASE 2
	;前回の調教が舐陰、舐陰莖か六九式だと六九式に
	IF PREVCOM == TNUM("舐陰") || PREVCOM == TNUM("舐陰莖") || TEQUIP:面部騎乗 || TEQUIP:六九式
		;69に移行した時（TEQUIP:PLAYER:六九式は0も意味を持つので、TEQUIP:六九式で判定。）
		IF TEQUIP:六九式 == 0
			IF TEQUIP:面部騎乗
				TEQUIP:PLAYER:六九式 = TEQUIP:面部騎乗
			ELSE
				TEQUIP:PLAYER:六九式 = PREVCOM
			ENDIF
		ENDIF
		TEQUIP:六九式 = SELECTCOM
	ENDIF
CASE 3
	;六九式中だと継続
	IF TEQUIP:六九式
		TEQUIP:PLAYER:六九式 = SELECTCOM
		;partnerの行動に変化をつける
		IF RAND:3 == 0 && PENIS(PLAYER)
			IF CANTRAIN("乳交") && RAND:3 == 0
				TEQUIP:六九式 = TNUM("乳交")
			ELSEIF CANTRAIN("口交") && RAND:2
				TEQUIP:六九式 = TNUM("口交")
			ELSEIF CANTRAIN("手淫")
				TEQUIP:六九式 = TNUM("手淫")
			ENDIF
		ENDIF
	ENDIF
CASE 4
	;六九式中だと継続
	SIF TEQUIP:六九式
		TEQUIP:六九式 = SELECTCOM
;六九式も可能な指令(TEQUIP:六九式は変更させない)
CASE 6
	RETURN 0
CASEELSE
	IF SELECTCOM != 99
		TEQUIP:六九式 = 0
		TEQUIP:PLAYER:六九式 = 0
	ENDIF
ENDSELECT

@CHECK_EQUIP_69(ARG)
#FUNCTION

;時間を経過させない指令なら変化无
SIF NOTTIME_PLAY(ARG)
	RETURNF 6

SELECTCASE ARG
;PLAYERの行動　六九式に移行する股間弄り指令
CASE TNUM("舐陰"), TNUM("舐陰莖")
	RETURNF 1
;TARGETの行動　六九式に移行する奉仕指令
CASE TNUM("口交"), TNUM("奉仕舐陰"), TNUM("深喉")
	RETURNF 2
;PLAYERの行動　六九式体勢で可能な愛撫系の股間弄り指令
CASE 0 TO 2, 7 TO 10, 15
	RETURNF 3
;PLAYERの行動　道具指令や打屁股、拳交
CASE 30 TO 39, 130, 141 TO 143
	RETURNF 3
;TARGETの行動　六九式体勢で可能な奉仕指令
CASE TNUM("手淫") TO TNUM("乳交"), TNUM("内褲冲"), TNUM("奉仕舐陰"), TNUM("飞机杯冲")
	RETURNF 4
;六九式も可能な指令(TEQUIP:六九式は変更させない)
CASE 3, 13, 14, 24, 146, 176, 230 TO 300
	RETURNF 6
CASE 99
	RETURNF 7
ENDSELECT
RETURNF 0


