IF CHECK_COM("清洁口交", TNUM("口交")) + CHECK_COM("清洁舔阴", TNUM("奉仕舐陰")) && CHECK_COM("清洁口交", TNUM("舐陰莖")) + CHECK_COM("清洁舔阴", 1)
	IF CHECK_COM("清洁口交", TNUM("口交"))
		LOCAL:1 = COM_ORDER_STAIN("Ｐ", "Ｍ")
	ELSE
		LOCAL:1 = COM_ORDER_STAIN("Ｖ", "Ｍ")
	ENDIF
ELSEIF PENIS(PLAYER)
	LOCAL:1 = COM_ORDER_STAIN("Ｐ", "Ｍ")
ELSE
	LOCAL:1 = COM_ORDER_STAIN("Ｖ", "Ｍ")
ENDIF

