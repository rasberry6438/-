			CALL PRINT_STRW, @"「就这样回去也可以……嗯……之后还有时间吗_HH_」"
		ENDIF
	ENDIF
CASE "约会終了"
	;エッチをする場合
	IF TFLAG:床イン
		IF TALENT:恋慕
			CALL PRINT_STRL, @"「哈啊、%CNAME("一人称")%也快要受不了了……啊_HH_」"
			CALL PRINT_STRW, @"「%TEXTR(@"现在请忘记一切……尽情疼爱我吧/全部忘记……现在只想成为%CNAME("二人称")%的女人/请告诉%CNAME("一人称")%的身体……让%CNAME("一人称")%意识到没有%CNAME("二人称")%是不行的")%_HH_」"
		ELSE
			PRINTFORML 「毕竟是恋人啊……」
			CALL PRINT_STRW, @"「%TEXTR("知道了/好了")%_HH_……%TEXTR("现在好好相爱吧/让我们来确认两个人的爱吧")%_HH_」"
		ENDIF
	ELSE
		IF TALENT:恋慕
			CALL PRINT_STRL, @"「%TEXTR("今天真是太感谢了/今天非常開心呢")%_HH_」"
