#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSEIF NO:PLAYER == 35
		LOCALS = 你_你
	ELSEIF NO:PLAYER == 48
		LOCALS = 你_你
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:男性
				LOCALS = 君_你
			ELSE
				LOCALS = 你_你
			ENDIF
		CASEELSE
			LOCALS = 你_你
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	IF NAME:PLAYER == "你"
		LOCALS = 前輩_前輩
	ELSE
		LOCALS = %CALLNAME:PLAYER%前輩_%CSTR:PLAYER:ふりがな%前輩
	ENDIF
CASE "助手"
	IF NO:ASSI == 35
		LOCALS = 哥哥_哥哥
	ELSEIF NO:PLAYER == 48
		LOCALS = 莉酱_れい酱
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:女性 || TALENT:扶她
				LOCALS = %CALLNAME:ASSI%酱_%CSTR:ASSI:ふりがな%酱
			ENDIF
		CASE "２年生", "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
			ELSE
				IF TALENT:男性
					LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
				ELSE
					LOCALS = %CALLNAME:ASSI%同学_%CSTR:ASSI:ふりがな%同学
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

