;-------------------------------------------------
;射精先選択時に表示する口上
;頻繁に表示されるため随机や表示条件の設定推奨です
;-------------------------------------------------
@KOJO_MESSAGE_SHOOT_K34
;初期は表示されないようにしてあるため、書いた後はRETURN 0を消す
RETURN 0

;恋慕
IF TALENT:恋慕
	PRINTFORMW 
;それ以外
ELSE
	PRINTFORMW 
ENDIF

;-------------------------------------------------
;PLAYERの射精＆絶頂をトリガーにした口上
;それぞれの射精分岐内での初めてかどうかの判定は、FIRST_EV(ARGS)で行う
;例）ELSEIF SHOOT("脚")において初めてかどうかは、FIRST_EV("絶頂：脚")で判定する
;-------------------------------------------------
@KOJO_MESSAGE_SAMEN_AND_EXTACY_K34
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSIPLAY
;	RETURN 0

;-------------------------------------------------
;髪、顔、股間、腹、尻、くぱぁ、Ａくぱぁへのぶっかけ
;-------------------------------------------------
IF SHOOT("ぶっかけ")
	IF SHOOT("自慰")
		PRINTFORMW 
	;超大量射精
	ELSEIF COND("絶頂", PLAYER) >= 3
		PRINTFORMW 
	;大量射精
	ELSEIF COND("絶頂", PLAYER) == 2
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;自慰で射精された(ぶっかけではなく床などに射精した)
;-------------------------------------------------
ELSEIF SHOOT("自慰")
	PRINTFORMW 

;以下は通常の射精区分。
;-------------------------------------------------
;素股、足交や身体で射精させた
;-------------------------------------------------
ELSEIF SHOOT("脚")
	;初めて
	IF FIRST_EV("絶頂：脚") == 0
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ELSEIF SHOOT("素股")
	PRINTFORMW 
;-------------------------------------------------
;口で射精させた
;-------------------------------------------------
ELSEIF SHOOT("口")
	;初めて
	IF FIRST_EV("絶頂：口") == 0
		PRINTFORMW 
	;精液中毒Lv5
	ELSEIF ABL:精液中毒 >= 5
		PRINTFORMW 
	;精液中毒Lv3
	ELSEIF ABL:精液中毒 >= 3
		PRINTFORMW 
	;奉仕精神Lv3以上
	ELSEIF ABL:奉仕精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;胸で射精させた
;-------------------------------------------------
ELSEIF SHOOT("胸")
	;初めて
	IF FIRST_EV("絶頂：胸") == 0
		PRINTFORMW 
	;奉仕精神Lv5
	ELSEIF ABL:奉仕精神 >= 5
		PRINTFORMW 
	;奉仕精神Lv3以上
	ELSEIF ABL:奉仕精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;手で射精させた
;-------------------------------------------------
ELSEIF SHOOT("手")
	;初めて
	IF FIRST_EV("絶頂：手") == 0
		PRINTFORMW 
	;奉仕精神Lv5
	ELSEIF ABL:奉仕精神 >= 5
		PRINTFORMW 
	;奉仕精神Lv3
	ELSEIF ABL:奉仕精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;性交で射精させた
;-------------------------------------------------
ELSEIF SHOOT("Ｖ中出し")
	;初めて
	IF FIRST_EV("絶頂：Ｖ中出し") == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
	;屈服刻印Lv3
